#include <bits/stdc++.h>
using namespace std;
#define int long long
typedef long long ll;

int32_t main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int n, r, c;
    cin >> n >> r >> c;
    if(r+c-1 <= n) cout << "White\n";
    else cout << "Black\n";
    return 0;
}






















