#include<iostream>
#include<algorithm>
#include<string>
#include<vector>
#include<fstream>
#include<map>
#include<set>
#include<cmath>
using namespace std;
int main()
{
    long long n, x, y,i,z,a=0,b=0,c=0;
    cin >> n;
    for (i = 0; i < n; i++)
    {
        cin >> x;
        a += x;
    }
    for (i = 0; i < n - 1; i++)
    {
        cin >> y;
        b += y;
    }
    for (i = 0; i < n - 2; i++)
    {
        cin >> z;
        c += z;
    }
    cout << a - b << endl;
    cout << b - c << endl;

    return 0;
    
}