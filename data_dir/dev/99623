#include <bits/stdc++.h>
using namespace std;

pair<int, int> boot[100];
int ans = 0, n;

int main (int argc, char *argv[])
{
	scanf("%d", &n);
	for(int i = 0; i < n; ++i) {
		scanf("%d%d", &boot[i].first, &boot[i].second);
	}
	ans = n;
	for(int i = 0; i < n; ++i) {
		int v = boot[i].second;
		for(int j = 0; j < n; ++j) {
			if(j == i) {
				continue;
			}
			if(boot[j].first == v) {
				ans--;
				boot[j].first = -1;
			}
		}
	}
	printf("%d", ans);

	return 0;
}

