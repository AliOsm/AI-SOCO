#include<iostream>
#include<string>
#include<math.h>
#include<queue>
#include<stack>
#include<stdio.h>
#include<map>
#include<set>
#include<memory.h>
#include<algorithm>
#include<vector>
using namespace std;
int Abs(int x){
    if(x<0)return -x;
    return x;
}
int main(){
    long long a,b,c;
    cin>>a>>b;
    c=a;
    int cnt=0;
    while(c<b){
        ++cnt;
        c*=a;
    }
    if(c==b)cout<<"YES"<<endl<<cnt<<endl;
    else cout<<"NO";
}