#include<bits/stdc++.h>
#define ll long long
#define ld long double
#define pb push_back
#define x first
#define y second
#define fastread ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
#define PI (atan(1)*4)
#define mp make_pair
using namespace std;

int main()
{
	fastread;
	int x, y, z;
	cin >> x >> y >> z;
	x -= y;
	if (z == 0) {
		if (x < 0)
			cout << "-";
		else if (x == 0)
			cout << 0;
		else
			cout << "+";
	}
	else if (abs(x) > z) {
		if (x < 0)
			cout << "-";
		else if (x == 0)
			cout << 0;
		else
			cout << "+";
	} else {
		cout << "?";
	}
	return 0;
}