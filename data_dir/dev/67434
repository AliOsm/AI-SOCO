#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

const ll N=100011;
void snum(ll &a){scanf("%lld",&a);}
void snum(ll &a, ll &b){scanf("%lld %lld",&a,&b);}
void snum(ll &a, ll &b, ll &c){scanf("%lld %lld %lld",&a,&b,&c);}
void snum(ll &a, ll &b, ll &c, ll &d){scanf("%lld %lld %lld %lld",&a,&b,&c,&d);}

void solve(){
	string k;
	cin>>k;
	sort(k.begin(),k.end());
	ll sz=k.size();
	if(k[0]==k[sz-1]){
		printf("-1\n");
	}else{
		cout<<k<<endl;
	}
	return;
}

int main(){
	ll t;
	cin>>t;
	while(t--){
		solve();
	}
	return 0;
}

