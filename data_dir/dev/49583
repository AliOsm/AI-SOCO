#include<bits/stdc++.h>

using namespace std;

typedef complex<double> base;
typedef long double ld;
typedef long long ll;

#define pb push_back
#define pii pair<int,int>

const int maxn=(int)(1e5+5);
const ll mod=(ll)(1e9+7);
int a[maxn];

int main()
{
    ios_base::sync_with_stdio(0);

    int n;cin>>n;

    set<int> s;

    for(int i=0;i<n;i++)
    {
        cin>>a[i];

        if(a[i]!=0)
        {
            s.insert(a[i]);
        }
    }

    cout<<s.size()<<endl;return 0;
}
