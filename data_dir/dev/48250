#include<bits/stdc++.h>
using namespace std;

#define asc(v) sort(v.begin(),v.end())
#define des(v) sort(v.begin(),v.end(), greater<int>())
#define rep(i,n) for(int i=0;i<n;i++)
#define mod 1000000007

int main()
{
      int n,a,b,x=0;
      cin>>n;
      rep(i,n)
      {
        cin>>a>>b;
        if(b-a>=2)
        x++;
      }
      cout<<x;      
} 