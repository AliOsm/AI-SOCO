#include <bits/stdc++.h>
using namespace std;
long long  i,j,n,c[523456],d,b[323465],a[554123],e,l,r,s,t,tt,k,x, y,z,m;
string p,q,du,qq;
map <long long,long long> v;
int main() {
    cin >> tt;
    while (tt--){
        cin >> p>>q;
        for (i = 0; i <345; i ++)
            a[i] =0;
        l = p.size();
        x=0;
        for (i = 0; i < l; i ++)
            a[p[i]] =1;
        for (i = 0; i < l; i ++)
            if(a[q[i]] ==1)
                x=1;
        if (x == 1)
        cout << "YES"<<endl;
        else cout << "NO"<<endl;
    }
 
    return 0;   
}