#include <bits/stdc++.h>
#define rc(s) return cout<<s,0
using namespace std;
int main(){
    int i,n,t,j;
    string s[1000],a;
    cin>>t;
    for(i=0;i<t;i++)
        cin>>s[i];
        for(int c=0;c<t;c++){
        n=s[c].size();
    for(i=0,j=n-1;i<n,j>=0;i++,j--)
    if(s[c][i]!=s[c][j]){
        cout<<s[c]<<'\n';
        break;
    }else {
        a=s[c];
    sort(s[c].begin(),s[c].end());
    if(a==s[c]){
       cout<<"-1\n";
       break;
       }else {
        cout<<s[c]<<'\n';
        break;
       }
    }
        }
return 0;}
