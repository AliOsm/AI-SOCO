#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

int T, a[MAX], b[MAX], N;

string solve() {
    bool mn = 0, mx = 0;
    cin >> N;
    for( int i = 1; i <= N; i ++ )
        cin >> a[i];
    for( int i = 1; i <= N; i ++ )
        cin >> b[i];
    for( int i = 1; i <= N; i ++ ) {
        if( a[i] < b[i] && ! mx )
                return "NO";
        else if( a[i] > b[i] && ! mn )
            return "NO";
        if( a[i] == 1 )
            mx = 1;
        if( a[i] == -1 )
            mn = 1;
    }
    return "YES";
}

int main()
{
    optimiza_io
    cin >> T;
    while( T -- )
        cout << solve() << "\n";
    return 0;
}

