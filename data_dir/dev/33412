#include <bits/stdc++.h>
#define ll long long int
#define pb push_back
#define pii pair<int, int>
#define pll pair<ll, ll>
#pragma GCC optimize ("O3")
using namespace std;
int main () {
    ios::sync_with_stdio(false);
    cin.tie (0);
    int n; cin>>n;
    string col="ROYGBIV";
    for (int i=0;i<n/7;++i) cout<<col;
    if (n%7<5) cout<<col.substr(3, n%7);
    else cout<<col.substr(0, n%7);
    return 0;
}