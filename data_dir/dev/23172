#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
    ll a,b,c,d,e,i,j,k,l,n,m,x,y,t,p;
    scanf("%lld",&t);
    for(p=1;p<=t;p++)
    {
        scanf("%lld%lld%lld",&a,&b,&n);
        x=a^b;
        if(n%3==0)
        {
            printf("%lld\n",a);
        }
        else if(n%3==1)
        {
            printf("%lld\n",b);
        }
        else
        {
            printf("%lld\n",x);
        }
    }
}
