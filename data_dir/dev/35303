# include <stdio.h>
# include <math.h>
# include <string.h>

# define isDigit(x) ((x) >= 'A' && (x) <= 'Z')
# define isdigit(x) ((x) >= 'a' && (x) <= 'z')
# define convert(x) ( isdigit (x) ? (x)-'a'+'A' : (x)-'A'+'a' )


const int N = 5001;
const int K = 1000000000;

int n, m, k, aa, bb, cc;
int l [N], w [N], h [N];
int ol [N], ow [N], op [N];
int sum, lw;

void Print (int kol);

int main ()
{
# ifndef ONLINE_JUDGE
   freopen ("B.txt", "rt", stdin);
   freopen ("B.out", "wt", stdout);
# endif

   int i, j, mm, ii, kk;

   scanf ("%d", &n);
   for ( i=0; i < n; i++ )
   {
      scanf ("%d %d %d", &l [i], &w [i], &h [i]);
  }

   scanf ("%d", &m);
   for ( i=0; i < m; i++ )
   {
      scanf ("%d %d %d", &ol [i], &ow [i], &op [i]);
  }

   for ( j=0; j < n; j++ )
   {
      lw = (l [j] + w [j])*2;
      sum = K;
      for ( i=0; i < m; i++ )
      {
         ii = ol [i]/h [j];
         mm = (lw + ow [i]-1)/ow [i];
//         if ( (lw + ow [i]-1)%ow [i] > 0 )
  //          mm++;
         if ( ii == 0 ) continue;
         kk = mm/ii;
         if ( mm%ii > 0 ) kk++;
         if ( kk*op [i] < sum )
            sum = kk*op [i], aa=i;
//         printf ("i=%d k=%d ii=%d mm=%d\n", i, kk, ii, mm);
      }
 //     printf ("lw=%d sum=%d i=%d\n", lw, sum, aa);
      k += sum;
   }

   printf ("%d", k);

   return 0;
}

void Print (int kol)
{
   printf (" ");
   for ( int i=1, k=n-kol; i < k; i++ )
      printf ("  ");
}
