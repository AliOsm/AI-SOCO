#include<bits/stdc++.h>
#define pb push_back
#define F first
#define S second
#define mod (ll)1000000007
#define ll long long
#define acm ios_base::sync_with_stdio(0),ios::sync_with_stdio(0),cin.tie(0),cout.tie(0);
using namespace std;
int main()
{
    acm;
    int n;
    cin>>n;
    while (n--)
    {
        ll a,b;
        cin>>a>>b;
        ll ans=floor(cbrt(a*b));
        if ((a%ans==0&&b%ans==0)&&(a*b==ans*ans*ans)) cout<<"yes\n";
        else cout<<"no\n";
    }
    return 0;
}
