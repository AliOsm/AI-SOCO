#include <queue>
#include <list>
#include <string>
#include <string.h>
#include <algorithm>
using namespace std;

#define ll long long
#define MAX 200001

long long n, m, k;
int main()
{
	int a, b, ans = 0;
	cin >> a >> b;
	while (a <= b)
	{
		a *= 3;
		b *= 2;
		ans++;
	}
	cout << ans << endl;
	return 0;
}