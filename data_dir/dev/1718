#include <bits/stdc++.h>
 
using namespace std;
 
#define MOD 1000000007
#define ll long long int
#define ld long double
#define pb push_back
#define mkp make_pair
#define pii pair<int, int> 
#define pll pair<long long int, long long int>
#define sci(x) scanf("%d", &x)
#define scl(x) scanf("%lld", &x)
#define fi first
#define sc second
#define eps 1e-9
 
int A[200005];
set<int> se;
 
struct {
	int idx, a, b;
} Q[200005];
 
int main()
{
	ios_base::sync_with_stdio(false);cout.tie(0);cin.tie(0);
 
	int n, m, i, j, k, x, y, z, a, b, c;
	
	cin >> n;
	for (i = 1; i <= n; ++i) {
		cin >> A[i];
	}
	cin >> m;
	for (i = 1; i <= m; ++i) {
		cin >> Q[i].idx;
		if (Q[i].idx == 1) {
			cin >> Q[i].a >> Q[i].b;
		} else {
			cin >> Q[i].a;
		}
	}
	int mx = 0, idx;
	for (i = m; i > 0; --i) {
		if (Q[i].idx == 1) {
			idx = Q[i].a;
			if (se.find(idx) == se.end()) A[idx] = max(Q[i].b, mx);
			se.insert(idx);
		} else {
			if (Q[i].a > mx) mx = Q[i].a;
		}
	}
	for (i = 1; i <= n; ++i) if (se.find(i) == se.end()) A[i] = max(A[i], mx);
	for (i = 1; i <= n; ++i) cout << A[i] << " "; cout << endl;
 
	return 0;
}