#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

int main()
{
    int test;
    scanf("%d",&test);

    while(test--)
    {
        int n , m , i , j;
        scanf("%d %d",&n,&m);

        printf("W");
        for(i = 1 ; i < m ; i++)printf("B");
        printf("\n");
        for(i = 1 ; i < n ; i++)
        {
            for(j = 0 ; j < m ; j++)printf("B");
            printf("\n");
        }


    }


    return 0;
}

