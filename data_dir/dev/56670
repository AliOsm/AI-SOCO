#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define pi pair<ll,ll>
pi ans[1000005];
ll arr[10000];
ll brr[10000];
map<ll,ll>mp;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    ll i,j,n,m,t,a,b,x,y;
    cin>>t;

    for(i=0;i<t;i++){
        cin>>a>>b;
        if(__gcd(a,b)!=1) cout<<"Infinite"<<endl;
        else cout<<"Finite"<<endl;
    }
}


