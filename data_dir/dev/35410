#include <iostream>
#include <vector>
#include <algorithm>
#include <memory>
#include <map>
#include <unordered_map>
#include <set>
#include <queue>
#include <unordered_set>
#include <functional>
#include <climits>
#include <cstring>
#include <cmath>
#include <numeric>
#include <random>
#include <tuple>

using namespace std;
using ll = long long;
const int oo = numeric_limits<int>::max() / 2;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie();
  ll n, x, y;
  cin >> n >> x >> y;
  ll a = max(x - 1, y - 1);
  ll b = max(n - x, n - y);
  // cout << a << " " << b << endl;
  if (a <= b) {
    cout << "White" << endl;
  } else {
    cout << "Black" << endl;
  }
	return 0;
}
