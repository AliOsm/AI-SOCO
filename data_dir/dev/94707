#define int long long
using namespace std;
const int INF = 5e16;
const int MOD = 1e9 + 7;
const int MB = 30;
const int K = 500;

void solve() {
	int n, k;
	cin >> n >> k;
	int ans = n * 2 / k + (n * 2 % k ? 1 : 0) + n * 5 / k + (n * 5 % k ? 1 : 0) + n * 8 / k + (n * 8 % k ? 1 : 0);
	cout << ans;
}

signed main() {
	/*
