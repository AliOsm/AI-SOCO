int MOD = 1e9+7;
void ADD(LL& x,LL v){x=(x+v)%MOD;if(x<0)x+=MOD;}
/*}}}*/
const int SIZE = 1e6+10;
int n[3];
int d[3][SIZE];
LL f(LL v1,LL v2,LL v3){
    return 2*(v1*v1+v2*v2+v3*v3)-2*(v1*v2+v2*v3+v1*v3);
}
LL f(int p[]){
    int ll=0,rr=0;
    LL res=4e18;
    REP(i,n[p[1]]){
        while(ll+1<n[p[0]]&&d[p[0]][ll+1]<=d[p[1]][i])ll++;
        while(rr+1<n[p[2]]&&d[p[2]][rr]<d[p[1]][i])rr++;
        res=min(res,f(d[p[0]][ll],d[p[1]][i],d[p[2]][rr]));
    }
    return res;
}
void solve(){
    REP(i,3)R(n[i]);
    REP(i,3){
        REP(j,n[i])R(d[i][j]);
        sort(d[i],d[i]+n[i]);
    }
    int p[3]={0,1,2};
    LL an=4e18;
    do{
        an=min(an,f(p));
    }while(next_permutation(p,p+3));
    W(an);
}
int main(){
    CASET{solve();}
    return 0;
}
