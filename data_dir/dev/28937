typedef tree< i64, null_type, less<i64>, rb_tree_tag, tree_order_statistics_node_update> ordered_set;

const double eps = 1e-9 , pi = acos(-1.0);

int main()
{
    int i , j , k , m , n ;

    scanf("%d",&n) ;

    for(i=1 ; i<=n ; i++)
    {
        int l , r ;
        scanf("%d %d",&l,&r);
        printf("%d %d\n",l,2*l) ;
    }

    return 0 ;
}
