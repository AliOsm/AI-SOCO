using namespace std;
typedef vector<int> vint;
typedef long long ll;
typedef unsigned long long ull;
typedef pair<int, int> pii;
#define y1 vamdlsfmvldf
const int mod = (int)1e9 + 7;
const ll INF = (int)(1e9);
const int N = 100005;
string s;
int main(){
  cin >> s;
  int n = s.size();
  for(int i = 0; i < n - 1; ++i){
      if((s[i] == 'W')&&(s[i + 1] == 'U')&&(s[i + 2] == 'B')){
          s[i] = s[i + 1] = s[i + 2] = ' ';
      }
  }
  --n;
  while(s[n] == ' ') --n;
  ++n;
  int i = 0;
  while(s[i] == ' ') i++;
  while(i < n){
      while((s[i] != ' ')&&(i < n)){
          printf("%c", s[i]);
          ++i;
      }
      printf(" ");
      while((s[i] == ' ')&&(i < n)) ++i;
  }
  return 0;
}