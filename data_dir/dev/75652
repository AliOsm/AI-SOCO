#include <bits/stdc++.h>
using namespace std;
#define ll long long int
#define sep ios::sync_with_stdio(0),cin.tie(0)
const int MXi = 2e5 + 5;
vector <int> seen(MXi);
int n, t;

int main() {
    sep;
    cin >> n;
    int node = 0;
    for (int i = 1; i <= n; ++i) {
        cin >> t;
        if (seen[t]) {
            seen[t] = 0;
            seen[i] = 1;
        } else {
            ++node;
            seen[i] = 1;
        }
    }
    cout << node << "\n";
    return 0;
}
