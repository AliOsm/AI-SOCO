#include <bits/stdc++.h>
#define MOD (1000000007LL)
#define pb push_back
#define ff first
#define ss second
#define LEFT(n) (2*n)
#define RIGHT(n) (2*n+1)
using namespace std;

typedef long long ll;
typedef pair<int, int> ii;



int main()
{
    ios_base::sync_with_stdio(0);
    // freopen("input.txt", "r", stdin);

    ll a, b, c;
    cin>>a>>b>>c;

    if(c == 0){
        if(a == b)  cout<<"YES";
        else    cout<<"NO";
        return 0;
    }

    if((b-a) % c == 0 && (b-a)/c>=0)  cout<<"YES";
    else    cout<<"NO";
    return 0;
}






