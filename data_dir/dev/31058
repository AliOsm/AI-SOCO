#include <bits/stdc++.h>
using namespace std;

int main(){
	int n,o=0,z=0;
	string s;
	cin >> n >> s;
	for(int i = 0 ; i < n ; i += 1){
		if(s[i] == 'n'){
			o += 1;
		}
		if(s[i] == 'z'){
			z += 1;
		}
	}
	for(int i = 0 ; i < o ; i += 1){
		cout << 1 << " ";
	}
	for(int i = 0 ; i < z ; i += 1){
		cout << 0 << " ";
	}
}