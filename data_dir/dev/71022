#include<iostream>
#include<vector>
#include<algorithm>
#include<cmath>
#include<string>
#include<map>
#include<cstring>
#include<limits.h>
#include<stdio.h>
#include<stack>
#include<set>
using namespace std;
#define ll long long int
#define llu long long unsigned int
#define mod 1000000007
#define pb push_back
#define mp make_pair
#define get(n) scanf("%lld",&n)
#define yup(t) printf("%lld\n",t)
int main(){
ll n;cin>>n;
ll a[n];
ll s=0;
for(int i=0;i<n;i++) {
cin>>a[i];
s=s+a[i];
}
if(s%n==0){
    cout<<n;
}
else{
    cout<<n-1;
}
}