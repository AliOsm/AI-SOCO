#include <functional>

int bsf(int x) {
#ifdef __GNUC__
	return __builtin_ctz(x);
#else
	unsigned long i;
	_BitScanForward(&i, x);
	return i;
#endif
}
int bsr(int x) {
#ifdef __GNUC__
	return 31 - __builtin_clz(x);
#else
	unsigned long i;
	_BitScanReverse(&i, x);
	return i;
#endif
}

using namespace std;
typedef long long int64;
const double pi = 3.14159265358979323846;
const int inf = (int)2e+9 + 2;
const int mod = (int)1e+9 + 7;
const double eps = 1e-8;
const int N = 100000;

int n, m, k;

void solve()
{
	multiset<int> st;
	cin >> n;
	int x; cin >> x >> x;
	st.insert(x);
	auto it = st.begin();
	int mx = x;
	double s = x;
	double cnt = 1;
	cout.setf(ios::fixed);
	cout.precision(10);
	for (int i = 1; i < n; ++i) {
		int t;
		cin >> t;
		if (t == 1) {
			cin >> x;
			s -= mx;
			s += x;
			mx = x;
			st.insert(x);
			while (*it < s / cnt) {
				++cnt;
				s += *it;
				++it;
			}
		}
		else {
			cout << mx - s / cnt << '\n';
		}
	}
}

int main()
{
	ios::sync_with_stdio(0);
	cin.tie(0), cout.tie(0);
	int test = 1;
	//cin >> test;
	for (; test; --test)
		solve();
	return 0;
}