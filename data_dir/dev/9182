int sa(int x,int y){if(x%y==0)return x/y;return x/y+1;}
int binp(int x,int y){int ans=1;int t=x;while(y!=0){if(y%2==1){ans*=t;ans;}y/=2;t*=t;t;}return ans;}
main(){
	ios::sync_with_stdio(0) , cin.tie(0) , cout.tie(0);
	int n,k,t;
	cin>>n>>k>>t;
	if(t>n+k)
        return cout<<0,0;
    if(t<=k)
        return cout<<t,0;
    if(t<n+1)
        return cout<<k,0;
    cout<<(n+k)-t;
}
