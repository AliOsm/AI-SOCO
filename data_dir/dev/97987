#include<bits/stdc++.h>

using namespace std;

int main()
{
    int x;
    cin>>x;
    for(int i=0;i<=x;i++)
    {
        int l=ceil(pow(2,i));
        if(l==x)
        {
            cout<<"YES";
            return 0;
        }
        if(l>x)
            break;

    }
    cout<<"NO";
    return 0;
}
