#include <bits/stdc++.h>
#define ToyotaLandCruiser100 ios_base::sync_with_stdio; cin.tie(0); cout.tie(0);
#define tormoz return 0;

using namespace std;

typedef long long ll;
typedef long double ld;

const int N = 1e5 + 7;
const ld pi = 3.14159265358979323846;
const ll INF = 1e9 + 1;

ll n, a, b;

bool u1[1001], u2[1001];

int main(){
     cin >> n;
     for(int i = 1; i <= n * n; i++){
          cin >> a >> b;
          if(!u1[a] && !u2[b]){
               u1[a] = 1;
               u2[b] = 1;
               cout << i << " ";
          }
     }
}