//In The Name Of God
#include<bits/stdc++.h>
using namespace std;

const int MAXN = 1e5+22 ;

int n,k	;

int main()
{
	ios::sync_with_stdio(false),cin.tie(0);
	
	cin >> n >> k; 
	for(int i=1;i<=n;i++)
	{
		for(int j=1;j<=n;j++)	
		{
			if(i==j)
				cout << k << " ";
			else	
				cout << "0 ";
		}
		cout << endl;
	}	
	return 0;
}
