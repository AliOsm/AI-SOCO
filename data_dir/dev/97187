#include <bits/stdc++.h>
#define li 100005
#define fi first
#define se second
#define inf 1000000000
#define mpp make_pair
#define pb push_back
#define md 1000000007
#define lo long long
using namespace std;
lo int a,b,x,y;
int main(){
    scanf("%lld %lld %lld %lld",&a,&b,&x,&y);
    lo int ty=__gcd(x,y);
    x/=(ty);
    y/=(ty);
    printf("%lld\n",min(a/x,b/y));


    return 0;
}

