#include <bits/stdc++.h>
using namespace std;

const int N = 1e5 + 5;

int q;
long long a, b;


int main(){


    ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
    cin >> q;
    while (q--){
        cin >> a >> b;
        if (a == b) cout << 0 << '\n'; else
        if (a > b && (a - b) % 2 == 0) cout << 1 << '\n';
        else if (b > a && (b - a) % 2) cout << 1 << '\n';
        else cout << 2 << '\n';
    }

}
