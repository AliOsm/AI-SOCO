#define _CRT_SECURE_NO_WARNINGS
#include <iostream>
#include <cstdlib>
#include <ctime>
#include <cmath>
#include <cstring>
#include <string>
#include <vector>
#include <stdio.h>
#include <climits>
#include <Windows.h>
#include <iterator>
#include <algorithm>
#include <set>
#include <iomanip>
#include <fstream>
#include <list>

using namespace std;

typedef unsigned long long ull;
typedef long long ll;
#define all(a) a.begin(),a.end()

void solve(int argc, char *argv[])
{
    int n, temp(0);
    cin >> n;

    vector < int > v(n);

    for (int i(0); i < n; i++)
        cin >> v[i];

    sort(all(v));

    for (int i(0); i < n; i++)
        cout << v[i] << ' ';
}

int main(int argc, char *argv[])
{
    setlocale(LC_ALL, "Russian");

    solve(argc, argv);
    
//  cout << "\n\n"; system("pause");
    return 0;
}