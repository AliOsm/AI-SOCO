#include <bits/stdc++.h>

using namespace std;

#define ll long long
#define oo 1e9
#define OO 1e18
#define pb push_back
#define mp make_pair
#define ii pair<int,int>
#define dd pair<double,double>
#define ff first
#define ss second

int main(){
	ll n,x,y,k;
	char c;
	scanf("%lld%c",&n,&c);
	x = (n-1)/4;
	k = x;
	x++;
	x *= 2;
	if(n % 2)	x--;
	// printf("%lld\n",x);
	y = (x * 7) - 1;
	y += k * 2;
	if(c == 'a')	printf("%lld\n",y - 2);
	else if(c == 'b')	printf("%lld\n",y - 1);
	else if(c == 'c')	printf("%lld\n",y);
	else if(c == 'd')	printf("%lld\n",y - 3);
	else if(c == 'e')	printf("%lld\n",y - 4);
	else if(c == 'f')	printf("%lld\n",y - 5);
	return 0;
}