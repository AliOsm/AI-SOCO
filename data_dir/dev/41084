using namespace std;

int a[111],x[111],y[111],dp[111];


int main() {
 // freopen("input.txt","r",stdin);
 // freopen("output.txt","w",stdout);
    
 int j,i,n,k=0,t=0,tmax=0,d;
 pair <int,int> pii[111111];
 cin>>n;
 for (i=0;i<n;i++) cin>>pii[i].first>>pii[i].second;
 sort(pii,pii+n);
 t=pii[0].second;
 for (i=1;i<n;i++)
  if (pii[i].second<t) k++; else t=pii[i].second;
  
cout<<k;
  
  

 //system("pause");  
}
