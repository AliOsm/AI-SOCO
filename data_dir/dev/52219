#include <bits/stdc++.h>

#define ll long long
#define pb push_back
#define pii pair<int,int>
#define pll pair<ll,ll>
#define mp make_pair
#define ff first
#define ss second

using namespace std;

const double pi=acos(-1);
const int N=(int)1e6+7,mod=1000000007,M=2e9;

//scanf("%d",&);
string s;

int main()
{
//    freopen(".txt","r",stdin);
    cin>>s;
    int n=s.size();
    for(int i=0;i<n-1;i++){
        if(s[i]!=s[i+1])
            cout<<"1 ";
        else
            cout<<"0 ";
    }
    if(s.back()=='a')
        cout<<1;
    else
        cout<<0;
    return 0;
}
