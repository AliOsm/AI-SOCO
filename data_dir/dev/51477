#include<bits/stdc++.h>
using namespace std;

int main()
{
    string str;

    cin >> str;
    int cnt = 0,k=0;

    for(int i = 0; i < str.length(); i++)
    {
        if(str[i] == '-')cnt++;
        if(str[i] == 'o')k++;
    }

    if( k == 0 ||cnt % k == 0 )
        cout<<"YES";
    else
        cout<<"NO";
    return 0;
}
