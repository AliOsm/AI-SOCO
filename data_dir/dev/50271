#include <stdio.h>
#include <stdlib.h>
#include <string.h>
using namespace std;
int factorial(int n) {
    if (n == 1) {
        return 1;
    } else {
        return n * factorial(n - 1);
    }
}
int main(int argc, char **argv)
{
    int a, b;
    scanf("%d%d", &a, &b);
    printf("%d", (a > b) ? factorial(b) : factorial(a));
    return 0;
}
