#include <bits/stdc++.h>
using namespace std;

bool solve(){
    int64_t N;
    cin >> N;
    if(N <= 14) return false;
    int64_t r = N%14;
    return (1 <= r && r <= 6);
}

int main(){
    int T;
    cin >> T;
    while(T--) cout << (solve() ? "YES" : "NO") << endl;
    return 0;
}
