/* In The Name Of God */
#include <bits/stdc++.h>

# define xx first
# define yy second
# define pb push_back
# define pp pop_back
# define eps 1e-9

using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
typedef vector<int> vint;
int n,m,c;
int main(){
	ios_base::sync_with_stdio (0);
	cin>>n;
	while(n--){
		int a,b;cin>>a>>b;
		if(a>b)
			m++;
		if(a<b)
			c++;
	}
	if(m==c)
		cout<<"Friendship is magic!^^\n";
	else if(m<c)
		cout<<"Chris\n";
	else
		cout<<"Mishka\n";
	return 0;
}

