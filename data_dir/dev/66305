#include<bits/stdc++.h>
#define ff first
#define ss second
#define pb push_back
#define mp make_pair
#define ll long long
#define all(a) a.begin(),a.end()
#define int long long
using namespace std;
main ()
{
    int n,m;
    cin>>n>>m;
    if (n>33){
        cout<<m;
    }
    else {
        int k=2;
        for (int i=2;i<=n;++i){
            k*=2;
        }
        int ans=m%k;
        cout<<ans;
    }
}
