#include <bits/stdc++.h>
using namespace std;

#define mod 1000000007
#define pb push_back
#define mp make_pair

typedef long long ll;
const int N = 100005;

struct info {

};

int main(){
    int n; cin >> n;

    int yes = 0;
    for(int i=1;i<=n;i++){
        char s[55]; int b,e;
        cin >> s >> b >> e;
        if(b >= 2400 && (e - b) > 0) yes = 1;
    }

    puts(yes ? "YES" : "NO");

    //main();
    return 0;
}


