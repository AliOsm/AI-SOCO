#include<bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    int n;
    cin>>n;
    if (n==0) {cout<<15; return 0;}
    if (n==1) {cout<<14; return 0;}
    if (n==2) {cout<<12; return 0;}
    if (n==3) {cout<<13; return 0;}
    if (n==4) {cout<<8; return 0;}
    if (n==5) {cout<<9; return 0;}
    if (n==6) {cout<<10; return 0;}
    if (n==7) {cout<<11; return 0;}
    if (n==8) {cout<<0; return 0;}
    if (n==9) {cout<<1; return 0;}
    if (n==10) {cout<<2; return 0;}
    if (n==11) {cout<<3; return 0;}
    if (n==12) {cout<<4; return 0;}
    if (n==13) {cout<<5; return 0;}
    if (n==14) {cout<<6; return 0;}
    if (n==15) {cout<<7; return 0;}
    return 1;
}
