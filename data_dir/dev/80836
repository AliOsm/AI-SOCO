#pragma GCC optimize ("O3")
#include <bits/stdc++.h>
#define re return
#define ll long long
#define ull unsigned ll
#define ld long double
#define f first
#define s second
#define pi acos(-1)
#define oo (ll)1e9+8
#define OO 1e18
#define EPS 1e-9
#define Endl '\n'
#define all(v) (v).begin(),(v).end()
#define FX(n) fixed<<setprecision(n)
#define mm(o,k) memset(o,k,sizeof o)
#define IO ios_base::sync_with_stdio(0),cin.tie(0),cout.tie(0);
using namespace std;
const int N=5e4+5,M=1e6+5;
string s;
string f(int x){
    string ret;
    while(x){
        ret+=char((x%10)+'0');
        x/=10;
    }
    reverse(ret.begin(),ret.end());
    re ret;
}
int main()
{
    for(int i=1;i<=3000;++i)
        s+=f(i);
    int n;
    cin >> n;
    cout<<s[n-1]<<endl;
    re 0;
}
