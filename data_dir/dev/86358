#include <bits/stdc++.h>
using namespace std;
int main()
{
	
	int n,m;
	cin>>n>>m;
	bool ok=0;
	for (int i=0; i<n*m; i++)
	{
		char x;
		cin>>x;
		if (x!='B'&& x!='W' && x!='G')
		{
			ok=1;
		}
	}
	if (ok)
	{
		cout<<"#Color";
		return 0;
	}
	else
	{
		cout<<"#Black&White";
		return 0;
	}
}