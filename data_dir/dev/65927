//ios::sync_with_stdio(false);
#include <bits/stdc++.h>
#define pb push_back
#define adelkhah main
#define left(x) ((x)*2)
#define right(x) (((x)*2)+1)
//#define int long long
using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
int32_t adelkhah()
{
    int n,e,d; cin >> n >> d >> e;
    e *= 5; int ans = n;
    for(int i = 0; i*e <= n; i++){
        ans = min(ans,(n-i*e)%d);
    }
    cout << ans;
}
