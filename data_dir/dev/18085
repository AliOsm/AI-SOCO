#include <bits/stdc++.h>
using namespace std;
void go() {
    int n, x, sum = 0;
    cin >> n;
    for(int i = 0; i < n; i++) {
        cin >> x;
        sum += x;
    }
    cout << sum / n + bool(sum % n) << "\n";
    return;
}
int main() {
    int tests;
    cin >> tests;
    while(tests--)
        go();
    return 0;
}