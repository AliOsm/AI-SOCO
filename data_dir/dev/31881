struct mul{
  ll operator()(ll a,ll b){
    return (a*b)%mod;
  }
};
int identity_element(const mul&m){
  return 1;
}
//(≧∇≦) I won't go back on my word... that is my ninja way!
int n ;
int x ,l0 ,l1 ;
int main(){
    cin>>n;
    for(int i=1;i<=n;++i){
        cin>>x;
        if(!x)l0=i;
        else l1=i;
    }
    cout<<min(l0,l1);
