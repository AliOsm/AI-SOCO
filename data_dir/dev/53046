#include <iostream>
#include<cstdio>
#include<cmath>
#include<algorithm>
#include<map>
#include<set>
#include<vector>
#include<bitset>
#include<iomanip>

using namespace std;

#define ll long long

int main()
{
//freopen("a.in","r",stdin);
    long n,h,m=0;
    cin>>n;
    while(n--)
    {
        scanf("%I32d",&h);
        m=max(m,h);
    }
    printf("%I32d",m);
    return 0;
}
