#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
    ll a,b,c,d,e,i,j,k,l,n,m,x,y,t,p;
    scanf("%lld",&t);
    for(p=1;p<=t;p++)
    {
        scanf("%lld%lld",&a,&b);
        if(b==0)
        {
            x=(24-a)*60;
        }
        else
        {
            x=(60-b);
            a++;
            x+=(24-a)*60;
        }
        printf("%lld\n",x);
    }
}
