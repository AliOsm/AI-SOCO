#include <cstdio>
#include <cstring>
#include <algorithm>
#include <vector>

int main() {
  char s[3], t[5][3];
  scanf("%s", s);
  for (int i = 0; i < 5; ++ i)
    scanf("%s", t[i]);
  for (int i = 0; i < 5; ++ i) {
    if (s[0] == t[i][0] || s[1] == t[i][1]) {
      puts("YES");
      return 0;
    }
  }
  puts("NO");
}
