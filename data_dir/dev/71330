#include <bits/stdc++.h>
using namespace std;

using ll = long long;
const int sz = 5e5 + 10;
const ll inf = 1e18;

ll ar[4];

int main() {
#ifdef CLown1331
        freopen( "in.txt", "r", stdin );
#endif // CLown1331

        while (scanf("%lld %lld %lld %lld", &ar[0], &ar[1], &ar[2], &ar[3]) == 4) {

                
                if (ar[0] != ar[3]) {
                        printf("0\n");
                } else if (ar[2] && !ar[0]) {
                        printf("0\n");
                } else {
                        printf("1\n");
                }

                cerr << "-----\n";
        }

        return 0;
}