#include <bits/stdc++.h>
#define ll long long
#define mp make_pair
#define pb push_back
using namespace std;

int main()
{
  ios::sync_with_stdio(false);
    ll a,b;
    cin>>a>>b;
    ll t=0;
    while(a<=b)
    {
        a*=3;
        b*=2;
        t++;
    }
    cout<<t<<endl;
  return 0;
}
