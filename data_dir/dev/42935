#include "bits/stdc++.h"

using ll = long long;
//#define long Michael hÃ¶r auf long longs zu benutzen

using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr);
	string s;
	cin >> s;
	bool out = false;
	for (char c : s) {
		out |= c == 'H' || c == 'Q' || c == '9';
	}
	cout << (out ? "YES" : "NO") << endl;
}