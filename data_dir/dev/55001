
#include <bits/stdc++.h>
using namespace std;
long long  i, j, n, c[223456],d[123456], l,r, t, k,x,z,a[434568],m,b[445678];
string p,q,qq;
long long s;
int main() {
    cin >> n>>m>>k;
    k = k*5;
    s = n;
    for (i = 0; i <= k; i ++){
        x = i*m;
        if (x <= n){
            t = n-x;
            t = t%k;
            s = min (s,t);
        }
    }
    cout <<s;
    return 0;   
}