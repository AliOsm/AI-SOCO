#include <bits/stdc++.h>
using namespace std;
int finished [100000];
int main()
{
    //ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
    int n;
    cin >> n ;
    for(int i=0;i<n;i++){
        int l,r;
        cin >> l >> r;
        for(int j=l;j<=r;j++)finished[j]=i;
    }
    int now ;cin >> now;
    cout << n - finished [now];
    return 0;
}
