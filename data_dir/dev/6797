#include<bits/stdc++.h>
using namespace std;

int ara[5005];

int main()
{
    int n, i, j, k, p, q, x, y;

    cin>>n;
    for(i = 1; i <= n; i++) cin>>ara[i];

    for(i = 1; i <= n; i++)
    {
        if(ara[ara[ara[i]]] == i)
        {
            printf("YES\n");
            return 0;
        }
    }
    printf("NO\n");

    return 0;
}
