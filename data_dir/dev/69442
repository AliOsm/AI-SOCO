#include <algorithm>
#include <cmath>
#include <cstdio>
#include <cstdlib>
#include <cstring>
#include <iostream>
#include <string>
#include <vector>
#define SZ(x) ((int)(x).size())
#define FOR(it, c) for(__typeof((c).begin()) it = (c).begin(); it != (c).end(); ++it)
using namespace std;
typedef long long LL;

int main(void) {
	string s;
	cin >> s;
	int cnt=0;
	for(auto x: s) {
		if(x=='a' || x=='e' || x=='i' || x=='o' || x=='u') ++cnt;
		if(x=='1' || x=='3' || x=='5' || x=='7' || x=='9') ++cnt;
	}
	cout << cnt << endl;
  return 0;
}
