#include <bits/stdc++.h>

using namespace std ;

const int MAX = 1e5 + 10 ;

int arr[MAX] ;
int n ;

int main()
{
	ios_base::sync_with_stdio(0) ;
	cin.tie(0) ;
	int t ;
	cin>>t ;
	while(t--)
	{
		cin>>n ;
		set<int>s ;
		for(int i = 0 ; i < n ; ++i)
			cin>>arr[i] ;
		for(int i = 0 ; i < n ; ++i)
			s.insert(arr[i]) ;
		cout<<s.size()<<"\n" ;
	}
	return 0 ;
}		