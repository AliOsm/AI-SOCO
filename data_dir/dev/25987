/// NO PAIN <-> NO GAIN
/// FROM ASHES TO THE EMPIRE
#include <bits/stdc++.h>

#define ss second
#define ff first
#define pb push_back
#define faster ios::sync_with_stdio(0), cin.tie(0), cout.tie(0)

using namespace std;

typedef long long ll;
typedef long double ld;

const ll N=1000100,mod=1e9+7,mod2=1e9+9,mod3=998244353,sq=666,base=727,lg=30;

ll n,m,x,y,w,z,t,k,d,ans,a[N],b[N];
string s,f;
bool p(char c){
    if(c=='a' || c=='u' || c=='i' || c=='e' || c=='o')return true;
    return false;
}
int main(){
	faster;
    cin>>s>>f;
    if(s.size()!=f.size())return cout<<"No",0;
    for(int i=0;i<s.size();i++){
        if(p(s[i])!=p(f[i]))return cout<<"No",0;
    }
    cout<<"Yes";
	return 0;
}
