#include "cstdio"
using namespace std;
int n, before, after;
char name[11];

int main() {
    scanf("%d", &n);
    for (int i=0; i<n; i++) {
        scanf("%s %d%d", name, &before, &after);
        if ((before>=2400) && (after>before)) {
            printf("YES");
            return 0;
        }
    }
    printf("NO");
    return 0;
}