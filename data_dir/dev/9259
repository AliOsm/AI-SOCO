// KALAM
#include<bits/stdc++.h>

using namespace std;
typedef long long ll;
typedef long double ld;
const int N = 1e6 + 77;
const int MOD = 1e9 + 7;
const ll oo = 2e18;
int n,m,t;
int main(){
	scanf("%d",&t);
	for(int i = 1;i <= t;i++){
		scanf("%d%d",&n,&m);
		int far = 1;
		for(int j = 0;j < 2 * n + m;j++){
			printf("%d %d\n",(j % n) + 1,((j + far) % n) + 1);
			far = ((j + 1) / n) + 1;
		}
	}
	
	
	return 0;
}
