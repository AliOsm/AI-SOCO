int main()
{
    string khoj="idieh";
    string s;
    cin>>s;
    for(int i=0;s[i];i++){
        if(s[i]==khoj.back()){
            khoj.pop_back();
        }
        if(khoj.empty()){
            cout<<"YES"<<endl;
            End;
        }
    }
    cout<<"NO"<<endl;


    End;
}
