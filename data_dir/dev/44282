#include<bits/stdc++.h>

using namespace std;

int main()
{
    int line,len,i,j;
    cin>>line;
    while(line--)
    {
        cin>>len;
        string s;
        cin>>s;
        int indx=-1;
        for(i=0; i<len; i++)
        {
            if(s[i]=='8')
            {
                indx=i;
                break;
            }
        }
        if(indx==-1)
        {
            cout<<"NO"<<endl;
        }
        else if(len-indx>=11)
        {
            cout<<"YES"<<endl;
        }
        else cout<<"NO"<<endl;
    }




}
