#include "bits/stdc++.h"

using namespace std;
using LL = long long;

constexpr int N = 1e5 + 5;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    cin >> n;

    string s;
    cin >> s;

    int one = 0, zero = 0;
    for (char &c : s) {
        if (c == 'n') one++;
        if (c == 'z') zero++;
    }    

    while (one --) cout << "1 ";
    while (zero --) cout << "0 ";
}