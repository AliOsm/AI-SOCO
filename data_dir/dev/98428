#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
const ll N=2e5+5;
const ll MOD=1e9+7;

int main(){
    ios_base:: sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);
    
    ll n;cin>>n;
    string s;cin>>s;
    ll cnt[26]={0};
    for(ll i=0;i<n;i++)cnt[s[i]-'a']++;
    for(ll i=0;i<26;i++)while(cnt[i]--)cout<<char('a'+i);
}
