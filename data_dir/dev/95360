#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

int N;
map < char , int > mp;

int main()
{
    // optimiza_io
    cin >> N;
    string ini = "";
    for( int i = 0; i < N; i ++ )
        ini += "0";
    char yo = '1', otro = '0';
    int posyo = 1, posotro;
    int diff1, diff2;
    cout << "? " << ini << endl;
    cout.flush();
    cin >> diff1;
    mp['0'] = N - diff1;
    mp['1'] = diff1;
    ini[0] = '1';
    cout << "? " << ini << endl;
    cout.flush();
    cin >> diff2;
    if( diff2 > diff1 )
        swap( yo , otro );
    int inii = 1;
    int finn = N;
    while( inii < finn )
    {
        int mid = ( inii + finn + 1 ) / 2;
        string tmp = "";
        int aux = 0;
        for( int i = 1; i <= N; i ++ )
        {
            if( i <= mid )
                tmp += yo;
            else
            {
                tmp += otro;
                aux++;
            }
        }
        cout << "? " << tmp << endl;
        cout.flush();
        int diff;
        int deberia = mp[yo] - mid;
        cin >> diff;
        if( diff == deberia )
            inii = mid;
        else
            finn = mid-1;
    }
    posotro = inii + 1;
    if( yo == '0' )
        cout << "! " << posyo << " " << posotro << "\n";
    else
        cout << "! " << posotro << " " << posyo << "\n";
    return 0;
}

