#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
typedef long long ll;
const int maxn = 1e3+1;


int main()
{
    int n, k, t;
    scanf("%d %d %d", &n, &k, &t);
    int ans;
    if (t <= k)
        ans = t;
    else if (t > n)
        ans = k+n-t;
    else
        ans = k;
    printf("%d\n", ans);
}