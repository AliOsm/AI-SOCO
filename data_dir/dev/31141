#include <vector>
#include <list>
#include <map>
#include <set>
#include <deque>
#include <stack>
#include <bitset>
#include <algorithm>
#include <functional>
#include <iostream>
#include <iomanip>
#include <cstdio>
#include <cmath>
#define ll long long
using namespace std;

int main() {
    int n,m;
    cin>>n>>m;
    bool is_color = false;
    for(int i=0;i<n;i++)
        for(int j=0;j<m;j++) {
            string c;
            cin>>c;
            if (c == "C" || c =="M" || c == "Y")
                is_color = true;
        }
    if (!is_color)
        printf("#Black&White\n");
    else
        printf("#Color\n");

}