using namespace std;

lli modexp(lli a,lli n)
{
    lli r=1;
    while(n){
        if(n&1)
            r=(r*a)%mod;
        a=(a*a)%mod;
        n>>=1;
    }
    return r;
}

bool compare(pair<lli, lli> p,pair<lli, lli >  q){
    if(p.ff < q.ff)
        return true ;
    else if(p.ff == q.ff)
    {
        if(p.ss < q.ss)
            return true ;
        return false ;
    }
        return false ;
}

lli stoi(string s){lli ans = 0;lli t = 1;fd(i, sz(s) - 1, 0)ans += (s[i] - '0') * t, t *= 10;return ans;}
string itos(int x){//string ans = "";while(x > 0){lli r = x % 10;ans += (char)r;x = x / 10;}return ans;}
    //string s = to_string(x);
//    return s;
}
lli w[1000000];
lli ans[1000000];

lli get(lli x)
{
    if(x == w[x])
        return x;
    return w[x] = get(w[x]);
}

void merge(lli a, lli b)
{
    a = get(a);
    b = get(b);
    if(a == b)
        return ;
    ans[a] += ans[b];
    w[b] = a;
}

void solve()
{

    string s;
    cin>>s;
    lli a[26] = {0};
    rep(i, sz(s))
        a[s[i] - 'a'] ++;
    lli c = 0;
    rep(i, 26)
    {
        if(a[i] % 2 != 0)
            c++;
    }
    if(c)
        c--;
    if(c % 2 != 0)
        cout<<"Second";
    else
        cout<<"First";
    return ;
}


int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);

    lli t;
   // cin>>t;
    t=1;
    while(t--)
    {

        solve();
    }
    return 0;
}
