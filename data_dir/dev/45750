#include <iostream>
#include <cstdio>
#include <cstdlib>
#include <algorithm>
#include <functional>
#include <iomanip>
#include <cmath>
#include <set>
#include <string>
#include <map>
#include <deque>
#include <queue>
#include <vector>
using namespace std;

typedef int64_t _64;

int main()
{
	string s;
	int zeros=0;
	bool one = false;
	cin >> s >> s;
	for (int i = 0;i < s.length();++i)
	{
		if(s[i]=='0')++zeros;
		else one = true;
	}
	if(one)cout << 1;
	while (zeros--)
	{
		cout << 0;
	}
	return 0;
}