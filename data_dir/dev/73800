#include <iostream>

using namespace std;

int main()
{
    ios_base::sync_with_stdio(0);
    int n, i;
    cin >> n;
    if (n % 2 > 0) cout << -1;
    else
    {
        for (i = n; i >= 1; i--)
        {
            if (i > 1) cout << i << " ";
            else cout << i;
        }
    }
    return 0;
}