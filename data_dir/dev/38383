// In the name of Allah the Most Merciful.

#include<bits/stdc++.h>
using namespace std;

int main(void)
{
    int t;
    string n;

    cin >> t >> n;

    int si =n.size();


    if(t==1){

        if(n=="10")cout << -1 << endl;
        else cout << n << endl;

    }
    else{

        cout << n;

        for(int i = si;i<t;i++)cout << "0";
        cout << endl;
    }
    return 0;
}
