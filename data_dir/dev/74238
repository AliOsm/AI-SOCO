using namespace std;
typedef long long ll;
#define y1 fjlwekf
const int mod = 1e9 + 7;
const double eps = 1e-13;
const int N = 100005;
void pr(int cur){
    if(cur < 10){
        printf("0");
    }
    printf("%d", cur);
}
int main(){
    int a, b, c;
    scanf("%d:%d", &a, &b);
    scanf("%d", &c);
    int cur = (a * 60 + b + c) % (24 * 60);
    pr(cur / 60);
    printf(":");
    pr(cur % 60);
    return 0;
}

