#include <bits/stdc++.h>
using namespace std;

int main(){
	int n,m;
	cin >> n;
	vector<int> x(n);
	for(int i = 0 ; i < n ; i += 1){
		cin >> x[i];
	}
	cin >> m;
	vector<int> y(m);
	for(int i = 0 ; i < m ; i += 1){
		cin >> y[i];
	}
	sort(x.begin(),x.end());
	sort(y.begin(),y.end());
	cout << x[n-1] << " " << y[m-1];
}