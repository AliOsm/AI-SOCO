#include<stdio.h>

int main()
{
    int q;
    scanf("%d", &q);
    while(q--)
    {
        int n, a, sum=0, t;
        scanf("%d", &n);
        t=n;
        while(n--)
        {
            scanf("%d", &a);
            sum += a;
        }
        printf("%d\n", (sum+t-1)/t);
    }

    return 0;
}
