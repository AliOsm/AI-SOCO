#include<iostream>
#include<math.h>
#include<bits/stdc++.h>
#define ll long long int
#define hell 1000000007LL
using namespace std;
vector<int>v;
ll power(ll x, ll y){ 
    if(y==0)
     return 1;
    else
    {
        ll p=power(x,y/2);
        if(y%2==0)
         return (p*p)%hell;
        else
        return (x*((p*p)%hell))%hell;
    }}
ll gcd(ll x,ll y)
{
    if(y==0)
     return x;
    else
    return gcd(y,x%y);
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.precision(20);
    int t;
    cin>>t;
    while(t--)
    {
       ll n;
       cin>>n;
       ll a[n],x=n+1;
       for(int i=0;i<n;i++)
        cin>>a[i];
       map<ll,ll>mp;
       for(int i=0;i<n;i++)
       {
           if(mp[a[i]]==0)
           {
               mp[a[i]]=i+1;
           }
           else
           {
               x=min(x,i-mp[a[i]]+2);
               mp[a[i]]=i+1;
           }
       }
       if(x==n+1)
        cout<<"-1";
       else
        cout<<x;
       cout<<"\n";}}
    
       