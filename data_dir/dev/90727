#include <bits/stdc++.h>
using namespace std;
#define F first
#define S second

typedef long long lli;
const lli   N = 35000 + 5 , OO =1110111110111 , mod = 1000000007 , mod2=998244353;

int a[4];

int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
    for(int i=0;i<4;i++)cin>>a[i];
    sort(a,a+4);
    cout<<a[3]-a[0]<<" "<<a[3]-a[1]<<" "<<a[3]-a[2];
    return 0;
}
