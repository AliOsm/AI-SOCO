#include <iostream>
#include <cstdio>
#include <cmath>
using namespace std;



int main()
{
	double a, b, c;
	cin>> a >> b >> c;

	double v[3];
	v[0] = (a * a * a) / (sqrt(2) * 6);
	v[1] = (sqrt(2) / 6) * (b * b * b);
	v[2] = (c * c * c) * (5 + sqrt(5)) / 24;

	printf("%.9lf\n", v[0] + v[1] + v[2]);
}