#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define fi first
#define se second
#define pp pop_back
#define mp make_pair
#define ld long double
using namespace std;
string s,t;
int i,j,k,l,r;
int main()
{
    ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
    cin >>s;
    t="heidi";
    for (i=0;i<s.size();++i)
    {
        if (j>=t.size())return cout <<"YES",0;
        if (s[i]==t[j])j++;
    }
    if (j>=t.size())cout <<"YES";else cout <<"NO";
}
