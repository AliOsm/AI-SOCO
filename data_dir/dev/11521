#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <bitset>
#include <string>
#include <vector>
#include <algorithm>
#include <ctime>
#include <queue>
#include <cstring>
#include <set>
#include <map>
#include <sstream>
#include <cmath>
#include <fstream>
#include <list>
using namespace std;
#define rp(i,n) for(int (i)=0;(i)<(n);++(i))
#define pb push_back
#define L(s) (int)s.size()
#define mp make_pair
#define pii pair<int,int>
//#define x first 
//#define y second
#define inf 1000000000
#define VI vector<int>
#define ll long long
#define all(s) (s).begin(),(s).end()
#define C(u) memset((u),0,sizeof((u)))
#define ull unsigned ll
int a,b,c,d,e,f;
int main()
{
	cin>>a>>b>>c>>d>>e>>f;
	if (!d)
		cout<<"Hermione\n";
	else
		if (!c)
			cout<<"Ron\n";
		else
			if (!b)
				cout<<"Hermione\n";
			else
				if (!a)
					cout<<"Ron\n";
				else
					if (!f)
						cout<<"Hermione\n";
					else
						if (!e)
							cout<<"Ron\n";
						else
							if (((ll)2*inf)%a + (((((((ll)2*inf)/a)*b)/c)*d)/e)*f>(ll)2*inf)
								cout<<"Ron\n";
							else
								cout<<"Hermione\n";
}