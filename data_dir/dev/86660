int di[] = { 0, 1, 0, -1 };
int dj[] = { 1, 0, -1, 0 };
int dik[] = { 1, 1, 2, 2, -1, -1, -2, -2 };
int djk[] = { 2, -2, 1, -1, -2, 2, -1, 1 };

ll gcd(ll x, ll y){ return !y ? x : gcd(y, x%y); }

int main(){
    /*freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);*/
    int n, m, idx = 0, best = 0, ret = 0;
    cin >> n;
    vi a(n), ans;
    rep(i, n) cin >> a[i];
    cin >> m;
    vi b(m);
    rep(i, m) cin >> b[i];
    rep(i, m) rep(j, n) if (b[i] % a[j] == 0){
        ans.push_back(b[i] / a[j]);
        best = max(best, b[i] / a[j]);
    }
    sortb(ans);
    rep(i, siz(ans)) if (ans[i] == best) ret++;
    else break;
    cout << ret << endl;
    return 0;
}
