#include<iostream>
#include<set>
#include<cmath>
#include <algorithm>
#include <vector>
using namespace std;
int main()
{
	set<int> b;
    int n;
    cin>>n;
    int a,s;
    for(int i=0;i<n;i++)
    {
    cin>>a;
    if(a==0)
    s++;
    else
    b.insert(a);
    }
    cout<<b.size();
}

