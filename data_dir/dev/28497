/// NO PAIN <-> NO GAIN
/// FROM ASHES TO THE EMPIRE
#include <bits/stdc++.h>

#define ss second
#define ff first
#define pb push_back
#define faster ios::sync_with_stdio(0), cin.tie(0), cout.tie(0)

using namespace std;

typedef long long ll;
typedef long double ld;

const ll N=100100,SQ=330,LG=25,inf=1e17;

ll n,m,t,k,ans,x,y,z,w,a[N],b[N];
string s,f[N];
pair <ll,ll> p[N];

int main(){
    faster;
    cin>>n>>k;
    for(int i=0;i<n;i++)cin>>a[i];
    sort(a,a+n);
    for(int i=0;i<n;i++){
        a[i]-=x;
        if(a[i]==0)continue;
        cout<<a[i]<<endl;
        x+=a[i];
        a[i]=0;k--;if(k==0)return 0;
    }
    while(k--){
        cout<<0<<endl;
    }
    return 0;
}
