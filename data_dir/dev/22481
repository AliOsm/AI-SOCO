#include<iostream>
#include<fstream>
#include<string>
#include<vector>

using namespace std;

int i,j,n,c,t,q,l,a,b,r,w,y,m,x;

int main()
{
	ifstream fin("input.txt");
	m=0;
	for(i=0;i<5;i++)
	{
	 cin>>n;
	 m+=n;
	}
	if(m%5 || m==0)
		cout<<-1;
	else
		cout<<m/5<<endl;
	return 0;
}