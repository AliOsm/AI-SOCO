#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ld = long double;
#define nl '\n'
#define ff first
#define ss second
/* -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- */



int main() {
  ios::sync_with_stdio(0);
  cin.tie(0); cout.tie(0); srand(time(0));

  int n;
  cin >> n;
  string s; cin >> s;
  if( s.length() == 1 and s[0] == '0' ) {
    cout << 0 << nl;
  } else {
    cout << 1;
    for(int i=1; i<n; i++)
      if( s[i] == '0' ) cout << 0;
    cout << nl;
  }

  return 0;
}

/* -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- -*- */
