#include <iostream>

using namespace std;

int main()
{
    int n , t ;
    cin>>n>>t ;
    if(t == 10 && n == 1)
    {
       cout<<"-1";
       return 0 ;
    }
    for(int i = 0 ; i < n ; ++i)
     {
       if(i == n-1 && t == 10)
        {
          cout<<0;
          return 0 ;
        }
       if(t == 10)
         cout<<"1";
       else
        cout<<t ;
     }
    return 0 ;
}