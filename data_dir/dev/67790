#include<bits/stdc++.h>

using namespace std;

#define ll long long
const int maxn = 3e5;
string s;
int main()
{
    int a = 0 , n;
    cin >> n;
    cin >> s;
    for(int i = 0; i<n; i++){
        if(s[i] == '8')a++;
    }
    int ans = 0;
    for(int i = 1; i<=a; i++){
        if(i*11<= n)ans++;
    }
    cout << ans << endl;
    return 0;
}
