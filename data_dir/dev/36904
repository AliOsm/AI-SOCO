#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef long double ld;
#define F first
#define S second
#define ii pair <int,int>
#define ever (;;)

ll r;

int main()
{
    scanf("%lld",&r);

    if( r%2 == 0 || r <= 3 )
        printf("NO");
    else
        printf("1 %lld",(r-3)/2);
}
