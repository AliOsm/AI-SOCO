#include <bits/stdc++.h>
#define ToyotaLandCruiser100 ios_base::sync_with_stdio; cin.tie(0); cout.tie(0);
#define tormoz return 0;

using namespace std;

typedef long long ll;
typedef long double ld;

const int N = 1e5 + 7;
const ld pi = 3.14159;
const ll INF = 1e9 + 1;

ll y, k, n, ans;

bool u;

int main(){
  cin >> y >> k >> n;
  if(y >= n){
    cout << -1;
    return 0;
  }
  ans = k * (y / k + 1);
  for(int i = ans; i <= n - (n % k); i += k){
      cout << i - y << ' ';
      u = 1;
  }
  if(!u){
    cout << -1;
    return 0;
  }
}
