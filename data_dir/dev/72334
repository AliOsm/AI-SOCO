// 		file : codeforces2.cpp

# include <bits/stdc++.h>

using namespace std;


const int N = 3e5 + 10;

const double PI = 3.1415926535897932384626433832795;

int d, hh, v, ee;

double e, h;

int main()
{
	cin >> d >> hh >> v >> ee;
	h = PI * (d / 2.0) * (d / 2.0) * hh; 
	e = PI * (d / 2.0) * (d / 2.0) * ee;
	if(v < e )cout << "NO";
	
	else{
		double k = h / (v - e);
		cout << fixed << setprecision(20) << "YES" << endl << k; 
	}
}
