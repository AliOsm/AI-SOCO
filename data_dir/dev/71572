#include<bits/stdc++.h>
using namespace std;
#define int long long
const long double eps = 1e-8l;
int32_t main()
{
  ios::sync_with_stdio(false);
  int x,y;
  cin>>x>>y;
  long double a1 = y*logl(x), a2 = x*logl(y);
  if(abs(a1-a2)<eps)
    cout<<"=";
  else if(a1<a2)
    cout<<"<";
  else
    cout<<">";
}
