long long mod=1000000007;

#define all(c) (c).begin(),(c).end()
#define sz(c) (int)(c).size()
#define forn(i, a, b) for(int i = a; i < b; i++)

#define pb push_back
#define mp make_pair

int main()
{
#ifndef ONLINE_JUDGE
    freopen("input.txt", "rt", stdin);
    freopen("output.txt", "wt", stdout);
#endif
    vi a(4);
    cin>>a[0]>>a[1]>>a[2]>>a[3];
    sort(all(a));
    if(a[0]+a[1]>a[2]) cout<<"TRIANGLE";
    else if(a[2]+a[1]>a[3]) cout<<"TRIANGLE";
    else if(a[0]+a[1]==a[2]) cout<<"SEGMENT";
    else if(a[2]+a[1]==a[3]) cout<<"SEGMENT";
    else cout<<"IMPOSSIBLE";
    // cout<<ans;
}