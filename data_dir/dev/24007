#include <bits/stdc++.h>

#define pb push_back
#define F first
#define S second
#define mp make_pair

using namespace std;

vector <int> v;
long double a,b,c,ans;
int main()
{
    cin>>a>>b>>c;
  ans = (a*a*a*sqrt(2))/12 + (b*b*b*sqrt(2))/6 + (c*c*c*(5+sqrt(5)))/24 ;
  cout<<fixed<<setprecision(10)<<ans<<endl;

}

