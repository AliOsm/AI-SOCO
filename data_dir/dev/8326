#pragma GCC optimize ("O3")
#include <bits/stdc++.h>
#define ll long long
#define ld long double
using namespace std;
ll a, b, t;
int main()
{
    ios::sync_with_stdio(0), cin.tie(0), cout.tie(0);
    cin >> t;
    while(t--){
        cin >> a >> b;
        if(a % b == 0)
            cout << "0\n";
        else
            cout << b - a % b << '\n';
    }
    return 0;
}
