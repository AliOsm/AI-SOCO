#include <bits/stdc++.h>
using namespace std;
#define oo 1000000010
#define mod 998244353
const int N = 200010;
char s[N];
int main(){
	int t;
	int n;
	scanf("%d",&t);
	while(t--){
		scanf("%d",&n);
		scanf("%s",s);
		bool found = false;
		for(int i = 0 ;i < n - 10;i++){
			if(s[i] == '8')
				found = true;
		}
		if(found)
			puts("YES");
		else
			puts("NO");
	}
    return 0;
} 