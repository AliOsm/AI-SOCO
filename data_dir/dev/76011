using namespace std;

string s;
int a[111],sum=0;

int main(){
	cin>>s;
	foru(i,40) a[i]=0;
	foru(i,s.size()) a[(int)s[i]-'a']=1;
	foru(i,100) sum+=a[i];
	if (sum%2) cout<<"IGNORE HIM!"; else cout<<"CHAT WITH HER!";
