#include<bits/stdc++.h>

using namespace std;

typedef long long LL;
typedef unsigned long long uLL;
typedef long double ldb;
typedef pair<int,int> pii;

map<string, bool> ma;

int main()
{
    ios_base::sync_with_stdio(0);cin.tie(0);
    int n;
    cin>>n;
    while(n--)
    {
        string s;
        cin>>s;
        if(ma[s])
            cout<<"YES"<<'\n';
        else
            cout<<"NO"<<'\n';
        ma[s] = true;
    }
    return 0;
}
