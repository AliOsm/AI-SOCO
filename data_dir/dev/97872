#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define MAX_INT 100000
#define ld long double
int main()
{
   ll n,i,j,d,x,y,q,m;
   cin>>n;
   ll a[n];
   for(i=0;i<n;i++){
       cin>>a[i];
   }
   cin>>m;
   ll b[m];
   for(i=0;i<m;i++){
       cin>>b[i];
   }
   sort(a,a+n);
   sort(b,b+m);
   
   cout<<a[n-1]<<" "<<b[m-1]<<endl;
    return 0;
}