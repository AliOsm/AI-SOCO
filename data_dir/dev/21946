#include <cstdio>
#include <string>
#include <iostream>
#include <algorithm>
#include <vector>
#include <queue>
#include <map>

#define INT long long int
using namespace std;
INT n;
string s;
int main()
{
    ios::sync_with_stdio(false);
    cin>>n;
    for(int i = 0 ; i < 200002 ; i+=2 )
        if( (i/2)&1 ) s+="aa";
        else s+="bb";
    cout<<s.substr(0, n)<<endl;
    return 0;
}
