#pragma GCC optimize ("O3")
#include "bits/stdc++.h"
using namespace std;
#define F first
#define S second
#define pb push_back
using ll = long long;
using ii = pair<int, int>;
using db = long double;
using ull = unsigned long long;
const int N = 3e5 + 5, MOD =  998244353;
const db EPS = 1e-7;
string s;
long long dp[4];
int main(){
#ifdef ONLINE_JUDGE
  ios_base::sync_with_stdio(0); cin.tie(0);
#endif // ONLINE_JUDGE
  int w, h;

  cin >> w >> h;
  int ans = 1;

  for(int j = 0; j < w + h; j++){
    ans=(ans+ans)%MOD;
  }

  cout<<ans<<'\n';


  return 0;
}
