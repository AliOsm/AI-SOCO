#include <bits/stdc++.h>

using namespace std;

#define ll long long

int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, k;
	cin >> n >> k;
	cout << (6 * (n - 1) + 5) * k << endl;
	for (int i = 0; i < n; i++)
	{
		for (int j = 1; j <= 5; j++)
		{
			if (j == 4)
				continue;
			cout << (6 * i + j) * k << " ";
		}
		cout << endl;
	}
	return 0;
}
