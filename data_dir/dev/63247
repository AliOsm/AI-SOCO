#include <bits/stdc++.h>
#define ToyotaLandCruiser100 ios_base::sync_with_stdio; cin.tie(0); cout.tie(0);
#define tormoz return 0;

using namespace std;

typedef long long ll;
typedef long double ld;

const int N = 1e5 + 7;
const ld pi = 3.14159;
const ll INF = 1000000;

int n, x[10001], y[10001];

set < int > stx;

set < int > sty;

int main(){
  cin >> n;
  for(int i = 1; i <= n; i++){
    cin >> x[i] >> y[i];
    stx.insert(x[i]);
    sty.insert(y[i]);
  }
  cout << min(stx.size(), sty.size());
}