#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
const int maxn = 5e5;

int n;
string s;
int main(){
  cin >> s;
  for(int i = 0; i < s.size(); i++){
    if(s[i] == 'a' || s[i] == 'o' || s[i] == 'i' || s[i] == 'e' || s[i] == 'u' || (isdigit(s[i]) && (int)(s[i] - '0') % 2 == 1)){
      n++;
    }
  }
  cout << n;
}
