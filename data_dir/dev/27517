#include<bits/stdc++.h>
#define GO ios::sync_with_stdio(0), cin.tie(0), cout.tie(0)
using namespace std;
typedef long long ll;
typedef vector<ll> vec;
const int N = 1e5 + 5;
const int M = 1e6 + 5;
const int Mod = 998244353;


int main()
{
	GO;
	int t;
	cin >> t;
	while (t--)
	{
		ll a, b;
		cin >> a >> b;
		cout << (a % b ? b - a % b : 0) << endl;
	}
	return 0;
}