#include<iostream>
#include<string>
#include<cmath>
#include<algorithm>
#include<queue>
#include<vector>
#include<cmath>
#include<set>
#include<map>
#include<cstdio>
#include<cstdlib>
using namespace std;
int main(){
    
    int N = 0, ANSWER = 0;
    scanf("%i",&N);
    ANSWER = max(N,max(N/10,N/100*10 + N%10));
    cout << ANSWER;
    
    //system("pause");
    return 0;    
}