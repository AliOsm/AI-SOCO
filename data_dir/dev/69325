#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    bool ans = false;
    scanf("%d", &n);
    while(n--)
    {
        char name[12];
        int r1, r2;
        scanf("%s %d %d", name, &r1, &r2);
        if (r1 >= 2400 && r2 > r1)
            ans = true;
    }
    if (ans)
        printf("YES\n");
    else
        printf("NO\n");
    return 0;
}
