template<tnm T,tnm U>invd smax(T&a,U b){if(b>a)a=b;}template<tnm T,tnm U>invd smin(T&a,U b){if(b<a)a=b;}
template<tnm T,tnm U>inline T gcd(T a, U b){return __gcd(a,b);}typedef vector<int> vi;typedef vector<vi > vvi;typedef pair<int, int> pii;
typedef vector<pii > vpii;invd init();
const int MOD = 1E9+7;
const int INF=MOD;


char b[1000000];
vector<int> ans;
int twos;

int main(){
  init();
  int n,t;
  scan(n);
  while(n--){
    scan(t),t--,b[t]++,b[999999-t]+=2;
  }
  ff(i,0,1000000){
    if(b[i]==2)ans+=i+1;
    else if(b[i]==3)twos++;
  }
  int p=0;
  for(;twos;p++){
    if(b[p]||b[999999-p])continue;
    ans+=p+1;ans+=999999-p+1;twos-=2;
  }
  pr(ans.size()),ln;
  prsp(ans);
}

invd init(){
#ifndef ONLINE_JUDGE
    freopen("xx/3.txt","r",stdin);
#endif
}
