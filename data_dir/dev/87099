#include <bits/stdc++.h>
using namespace std;

long long f(long long x) {
	long long cnt = 0;
	for (long long k = 2; k * k * k <= x; k++) {
		cnt += x / (k * k * k);
	}
	return cnt;
}

int main() {
	long long m;
	cin >> m;

	long long pass = 1e17;
	long long fail = -1;

	while (pass - fail > 1) {
		long long mid = (pass + fail) / 2;

		if (f(mid) >= m) {
			pass = mid;
		} else {
			fail = mid;
		}
	}

	if (f(pass) != m) pass = -1;
	cout << pass << endl;
}