
#include <iostream>
#include <vector>
#include <algorithm>
#include <map>
#include <set>

using namespace std;

int main()
{
	int x1,x2,y1,y2;
	int ans = 0;
	int n;
	
	cin>>n;
	for (int i=0 ; i<n ; i++)
	{
		cin>>x1>>y1>>x2>>y2;
		ans += (abs(x1-x2)+1)*(abs(y1-y2)+1);
	}
	
	cout<<ans;
	
	return 0;
}

