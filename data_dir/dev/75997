void Init ()
{
   n = 0, m = 0, k = 0;
//   a = 0, b = 0;
   l = 0;
//   d = 0;
}

void Main ()
{
   ok = false;
   for ( int i=0, j=n-1; i <= j; ok = ! ok )
      if ( a [i] < a [j] )
      {
         if ( ok )
            k += a [j];
         else
            m += a [j];
         j--;
      }
      else
      {
         if ( ok )
            k += a [i];
         else
            m += a [i];
         i++;
      }

# ifndef ONLINE_JUDGE
   PrintData (1);
# endif
}

