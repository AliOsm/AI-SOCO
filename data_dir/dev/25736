#include <bits/stdc++.h>
#define ll long long
#define OO LONG_LONG_MAX
#define oo 1e9
#define yalla ios::sync_with_stdio(0),cin.tie(0),cout.tie(0);
#define sz  100005
#define re return
#define mod  1000000007
#define pi acos(-1)
using namespace std;
string s;
bool ok;
int main()
{yalla;
 cin>>s; bool vis[s.size()+1]={};
  for(int i=0;i<s.size();i++)
  {

      if(!ok && s[i]=='a'&& ( (i<s.size() &&s[i+1]=='b') || i+1==s.size()) )
         vis[i]=1,ok=1,s[i]=s[0],s[0]='a';

      else if( ok &&s[i]=='b' &&  i+1<s.size() &&s[i+1]=='a' )
        vis[i]=1,ok=0,s[i]=s[0],s[0]='b';
  }
for(int i=0;i<s.size();i++)
    cout<<vis[i]<<" ";
 return 0;
}

