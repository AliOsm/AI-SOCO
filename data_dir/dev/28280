#include<iostream>
#include<cmath>
using namespace std;
int main(){
         string a,b;
         int a1,b1;
         cin>>a>>b;
         if(a=="monday"){
                  a1=2;
         }
         if(a=="tuesday"){
                  a1=3;
         }
         if(a=="wednesday"){
                  a1=4;
         }
         if(a=="thursday"){
                  a1=5;
         }
         if(a=="friday"){
                  a1=6;
         }
         if(a=="saturday"){
                  a1=0;
         }
         if(a=="sunday"){
                  a1=1;
         }
         if(b=="monday"){
                  b1=2;
         }
         if(b=="tuesday"){
                  b1=3;
         }
         if(b=="wednesday"){
                  b1=4;
         }
         if(b=="thursday"){
                  b1=5;
         }
         if(b=="friday"){
                  b1=6;
         }
         if(b=="saturday"){
                  b1=0;
         }
         if(b=="sunday"){
                  b1=1;
         }
         if((a1+2)%7==b1){
                  cout<<"YES";
                  return 0;
         }
         if((a1+3)%7==b1){
                  cout<<"YES";
                  return 0;
         }
         
         if(a==b){
                  cout<<"YES";
                  return 0;
         }
         cout<<"NO";
         return 0;
}