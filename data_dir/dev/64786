#include <bits/stdc++.h>

using namespace std;

int main() {
    string s;
    cin >> s;
    bool ok = false;
    for (int i = 0; i < 5; ++i) {
        string t;
        cin >> t;
        if (s[0] == t[0] || s[1] == t[1]) {
            ok = true;
        }
    }
    if (ok) cout << "YES" << endl; else cout << "NO" << endl;
    return 0;
}
