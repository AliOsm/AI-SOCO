#include "bits/stdc++.h"

using namespace std;
using LL = long long;

constexpr int N = 1e5 + 5;

int main() {
	ios::sync_with_stdio(false); cin.tie(nullptr);
	
	int n, m, x, y;
	cin >> n >> m >> x >> y;

	vector< int> a(n), b(m);
	for (int &e : a) cin >> e;
	for (int &e : b) cin >> e;

	cout << (a[x - 1] < b[m - y]? "YES" : "NO") << endl;
}