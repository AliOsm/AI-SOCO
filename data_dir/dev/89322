#include <bits/stdc++.h>
using namespace std;

int n;
vector<vector<int> > g;
vector<int> arr, cur, ans;

void dfs(int num) {
	cur[num]++;
	ans.push_back(num + 1);
	for(int i = 0; i < g[num].size(); ++i) {
		int to = g[num][i];
		cur[to]++;
		if(arr[to] == cur[to]) {
			dfs(to);
		}
	}
}

int main (int argc, char *argv[])
{
	scanf("%d", &n);
	g.resize(n);
	arr.resize(n);
	cur.resize(n);
	int m;
	scanf("%d", &m);
	while(m--) {
		int from, to;
		scanf("%d%d", &from, &to);
		--from; --to;
		g[from].push_back(to);
		g[to].push_back(from);
	}
	for(int i = 0; i < n; ++i) {
		scanf("%d", &arr[i]);
	}
	for(int i = 0; i < n; ++i) {
		if(arr[i] == cur[i]) {
			dfs(i);
		}
	}
	printf("%d\n", int(ans.size()));
	for(int i = 0; i < ans.size(); ++i) {
		printf("%d ", ans[i]);
	}

	return 0;
}

