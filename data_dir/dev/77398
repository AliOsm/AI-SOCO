using namespace std;
 
 
double PI = 3.1415926535897932384626433832795;
double EPS = 1e-2;
 
typedef unsigned int uint32;
typedef long long int64;
typedef unsigned long long uint64;
 
typedef pair<int, int> pii;
const int MAXN = 1000100;
const int MAXC = 1e7;
const int mod = 1e9 + 7;
const int ROOT = 555;

int n, m;
vector < vector < int > > gr; 
vector < vector < int > > gr1;

int64 ans = 0;
int dist[MAXN];

int mas[600][600];

void solve()
{
	int h, w;
	cin >> h >> w;
	for(int i = 0; i < h; ++i)
	{
		for(int j = 0; j < w; ++j)
			cin >> mas[i][j];
	}
	int k = 0;
	for(int i = 0; i < w; ++i)
	{
		k += abs(mas[h / 2][i] - mas[h / 2 - 1][i]);
	}
	if (k >= 20 * w)
		cout << "YES\n";
	else
		cout << "NO\n";
}
 
 
int main()
{
    ios_base::sync_with_stdio(false); cin.tie(0);
#ifdef _MY_DEBUG
    freopen("input.txt", "rt", stdin); freopen("output.txt", "wt", stdout);
#else
    //freopen(TASK ".in", "rt", stdin); freopen(TASK ".out", "wt", stdout);
#endif
    srand(1313);
	
	int t = 1;
	cin >> t;
	for(int i = 0; i < t; ++i)
	{
		solve();
		//cout << endl;
	}
	
    return 0;
}   