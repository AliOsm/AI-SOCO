#include <bits/stdc++.h>
using namespace std;
#define mod 1000000007 
#define oo 1000000000
int main(){
	int q , a , b , c , d ;
	cin >> q;
	while(q--){
		cin >> a >> b >> c >> d;	
		while(a == c){
			if(a + 1 <= b) a++; else c++;
		}
		cout << a << " " << c << endl;
	}

	return 0;
}