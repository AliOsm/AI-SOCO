//In The Name Of God
//Let Our Voices Echo
#include<bits/stdc++.h>
using namespace std;
#define X real()
#define Y imag()
typedef long long ll;
typedef double ld; 
typedef complex<ld> point;
const ld eps=1e-9;
const int MAX=1e5+9;

int main()
{
	ios_base::sync_with_stdio(false),cin.tie(0),cout.tie(0);
	int n,ans;
	cin>>n;
	for (int i=1;i*i<=n;i++)
		if (n%i==0)	
			ans=i;
	cout<<ans<<" "<<n/ans;
	return 0;
}

