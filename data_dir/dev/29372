		int n, s; in >> n >> s;
		vector<int> res(n, 1);
		int mid = (n - 1) / 2;
		int max_element = (s - n) + 1;
		res[mid] = max_element;
		int target = s / 2;

		int can_use = n - mid - 1;

		auto check = [&can_use, &max_element, &out] (int t) {
			if (t <= can_use || (t >= max_element && t <= max_element + can_use)) {
				out << "NO" << endl;
				return true;
