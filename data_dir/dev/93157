#include <bits/stdc++.h>

using namespace std;
using ll = long long;

const int N = 111;

char s[N];
int n;

int main() {
	int t;
	scanf("%d", &t);
	while(t--) {
		scanf(" %s", s);
		n = (int)strlen(s);
		bool eq = 1;
		for(int i = 0; i + 1 < n; i++) if(s[i] != s[i + 1])
			eq = 0;
		if(eq) {
			printf("%s\n", s);
			continue;
		}
		for(int i = 0; i < n; i++) {
			printf("10");
		}
		printf("\n");
	}
}
