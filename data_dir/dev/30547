#include<bits/stdc++.h>
#define ll long long int
#define pb push_back
#define mp make_pair
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    string x,y;
    cin>>x>>y;
    string z;
    for(int i=0;i<x.length();i++)
    {
        if(y[i]>x[i])
        {
            cout<<-1<<endl;
            return 0;
        }
        z+=y[i];

    }
    cout<<z<<endl;
    return 0;

}
