const llong over999 = 1e9;
const ld over999ld = 1e18+1;
const llong md = 1e9+7;
const ld eps = 1e-9;
const ld Pi = acos(-1);
const llong nich = 0;
const int mnogo = 300;

/*We must have STRUCTURE Mr Reynolds*/

int n, a[100010];
int32_t main() {
    kchay;
    cin >> n;
    for(int i = 0; i < n; i++){
        cin >> a[i];
    }
    
    sort(a, a + n);
    int cnt = 1;
    for(int i = 1; i < n; i++){
        if(a[i] == a[i-1])cnt++;
            else {
                if(cnt&1)return cout << "Conan", 0;
                cnt = 1;
            }
            
    }
    
    if(cnt&1)return cout << "Conan", 0;
    cout << "Agasa";
    
    
    
    
    return 0;
}