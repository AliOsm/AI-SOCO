#include <bits/stdc++.h>

using namespace std;

int main () {
	int t;
	scanf("%d",&t);
	while (t--) {
		int a,b;
		scanf("%d%d",&a,&b);
		if (!b) puts("1");
		else if (a >= 4*b) printf("%0.7f\n",1.0 - 1.0*b/a );
		else printf("%0.7f\n",0.5*(1.0 + min(b*1.0,a/4.0)/(2.0*b)) );
		
	}
}
