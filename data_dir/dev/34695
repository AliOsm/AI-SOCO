#define mendl "\n"
#define N 1000004//10000001  1000001
#define M 1000001//(1<<19) + 100 10000001
#define P 2
#define K 40
#define MOD 1000000007LL//1000000007LL  
#define INF 100000000000001LL//14
#define msq(px) (p2(px.first) + p2(px.second)) 
#define PI 3.141592653589793238462643383279 
#define EPS 1e-5

char s[N];
bool eq;
mt a, b;

int main()
{
#ifdef _DEBUG	
	freopen("in.txt", "r", stdin);
#endif
	//std::ios::sync_with_stdio(false);
	scanf("%s", s);
	mt i = 0;
	while (s[i]) 
	{
		if (s[i] == '|') eq ? ++b : ++a;
		else if (s[i] == '=')
			eq = true;
		++i;
	}
	if (a == b)
		puts(s);
	else if (a == b + 2)
	{
		if (s[1] == '+')
		{
			printf("|+");
			rep(0, a - 2, i)
				putc('|', stdout);
			putc('=', stdout);
			rep(0, b + 1, i)
				putc('|', stdout);
			putc('\n', stdout);
		}
		else
			printf("%s|\n", s+1);
	}
	else if (a + 2 == b)
		s[i - 1] = 0, printf("|%s\n", s);
	else
		puts("Impossible");
	return 0;
}