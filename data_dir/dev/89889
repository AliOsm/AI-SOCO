#include <bits/stdc++.h>
using namespace std;

char s[15];

int main(){
	int n;
	while (true){
		printf("next %d %d\n", 0, 1);
		fflush(stdout);
		scanf("%d",&n);
		for (int i = 1; i <= n; i++) scanf("%s", s + 1);
		printf("next %d\n", 1);
		fflush(stdout);
		scanf("%d",&n);
		for (int i = 1; i <= n; i++) scanf("%s", s + 1);
		if (n == 2) break;
	}
	while (true){
		printf("next");
		for (int i = 0; i < 10; i++) printf(" %d", i);
		printf("\n");
		fflush(stdout);
		scanf("%d",&n);
		for (int i = 1; i <= n; i++) scanf("%s", s + 1);
		if (n == 1) break;
	}
	printf("done\n");
	fflush(stdout);
	return 0;
}