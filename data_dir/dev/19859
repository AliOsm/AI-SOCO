#include<bits/stdc++.h>

using namespace std;

typedef complex<double> base;
typedef long double ld;
typedef long long ll;

#define pb push_back
#define pii pair<int,int>

const int maxn=(int)(1e5+5);
const ll mod=(ll)(1e9+7);
int a[maxn];

int main()
{
    ios_base::sync_with_stdio(0);

    int a,b,c,n;

    cin>>a>>b>>c>>n;

    int zz=a+b-c;

    if(n-zz>=1 && c<=a && c<=b)
    {
        cout<<(n-zz)<<endl;
    }
    else
    {
        cout<<-1<<endl;
    }


    return 0;
}
