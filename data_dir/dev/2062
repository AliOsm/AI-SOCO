#include <bits/stdc++.h>

using namespace std;
#define endl "\n"
#define all(x) x.begin(),x.end()
#define rall(x) x.rbegin(),x.rend()
typedef long long ll;
const double PI=acos(-1);
const int mod=1e9+7;
const int INF=2e9;
const int MAX=1e5;

int main() {
	ios_base::sync_with_stdio(0);
	//	cin.tie(0);
	int n;
	cin>>n;
	set<int>s;
	for(int i=0;i<n;i++){
		int x;
		cin>>x;
		if(x)
			s.insert(x);
	}
	cout<<s.size()<<endl;

	return 0;
}
