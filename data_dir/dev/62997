#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define f(i, x, n) for(int i = x; i < (int)(n); ++i)

char s[6][3];

void win() { printf("YES\n"); exit(0); }

int main(){
	f(i, 0, 6)scanf("%s", s[i]);
	f(i, 1, 6)if (s[i][0] == s[0][0])win();
	f(i, 1, 6)if (s[i][1] == s[0][1])win();
	printf("NO\n");
}