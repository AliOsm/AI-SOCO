#include <algorithm>
#include <iostream>
#include <cstring>
#include <cstdlib>
#include <cassert>
#include <cstdio>
#include <vector>
#include <cmath>
#include <ctime>
#include <set>
#include <map>
#include <queue>
#include <stack>
#include <deque>

#define F first
#define S second
#define eps 1e-8
#define pb push_back
#define ppb pop_back
#define mp make_pair
#define lb lower_bound
#define ub upper_bound
#define INF 2000000000

typedef long long ll;

using namespace std;
int n, t;
int main()
{
	cin >> n >> t;
	if(t < 10){
		for(int i = 0; i < n; i++)
			cout << t;
	}
	else{
		if(n == 1){
			cout << "-1";
		}	
		else{
			for(int i = 0; i < n - 1; i++)
				cout <<"1";
			cout <<"0";
		}
	}
	
	
	return 0;
}
