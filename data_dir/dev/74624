#include<bits/stdc++.h>
#define ff first
#define ss second
#define ll long long
#define ld long double
#define all(a) a.begin(),a.end()
#define endl '\n'
#define pb push_back
#define mp make_pair
using namespace std;

const int N=2e5+7;

main ()
{
    int n;
    cin>>n;
    int ans=0;
    for (int i=2;i<=min(100000000,n);++i){
        ans=max(ans,n/i);
    }
    if (n>ans)++ans;
    cout<<ans<<endl;

}
