const ll MOD = 1e9 + 7;
const ll mod = 555557;
const double pi = acos (-1.0);

int n, n1, nn1, nn, cnt; 

int main ()
{
	speed;
	cin >> n;
	n1 = n;
	while (n1)
		nn1 = n1 % 10, cnt ++, n1 /= 10;
	nn = (nn1 + 1) * pow (10, cnt - 1);
	cout << nn - n;
}
