#include <bits/stdc++.h>
using namespace std;
#define sz(a) a.size()
#define rep(i,a,b) for (int i=a;i<b;i++)

int main (){
 ios_base::sync_with_stdio (0);
 cin.tie (0);
 string s;
 cin>>s;
 bool cond=false;
 //xdD
 rep (i,0,sz(s)){
    if (i==0 && s[i]=='9'){
          s[i]='9';
          continue;
    }
       s[i]='0'+min(s[i]-'0','9'-s[i]);
 }


cout<<s;



}
