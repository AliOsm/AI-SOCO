#include <bits/stdc++.h>
using namespace std;
#define speed ios::sync_with_stdio(false)
#define F first
#define S second
typedef long long ll;
bool cmp(string x,string y)   {return x+y<y+x;}
string s[50000];
int main ()
{
    speed;
    int n;
    cin>>n;
    for (int i=0;i<n;i++){
        cin>>s[i];
    }
    sort(s,s+n,cmp);
    for (int i=0;i<n;i++){
        cout<<s[i];
    }
    return 0;
}