#include <bits/stdc++.h>
#define ll long long
#define mp make_pair
#define pb push_back
#define fi first
#define se second
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    ll t;
    cin>>t;
    while(t--)
    {
        ll n;
        cin>>n;
        string s;
        cin>>s;
        bool can=false;
        for(int i=0;i<s.length();i++)
        {
            if(s[i]=='8')
            {
                if(s.length()-i>=11)
                {
                    can=true;
                    break;
                }
            }
        }
        if(can)
        {
            cout<<"YES"<<endl;
        }
        else cout<<"NO"<<endl;
    }
    return 0;
}
