#include <bits/stdc++.h>

using namespace std; 	
typedef long long ll;
typedef unsigned long long ull;
const int N = 1e6+50;
const int oo = 1e9;
const int mod = 1e9+7;

bool f[1001];
int main(){
	int n;
	cin>>n;
	int p=0;
	for(int i=0;i<n;i++){
		int a;
		cin>>a;
		if(!f[a] && a)p++;
		f[a] = 1;
	}
	printf("%d\n", p);
}