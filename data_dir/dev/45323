#include <bits/stdc++.h>

#define ll long long
#define mp make_pair
#define pb push_back
#define f first
#define s second
#define vi vector<int>
#define vl vector<long long>
#define sz(a) (int)((a).size())
#define all(a) ((a).begin(),(a).end())
#define pii pair<int, int>
#define pll pair<ll, ll>
#define ld long double

using namespace std;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	
	int ans = 0;
	for(int i = 0; i < 3; ++i) {
		int x;
		cin >> x;
		int c = (x + 1) / 2 - 1;
		ans = max(ans, 3 * c + i);
	}
	cout << ans + 30 << endl;
	

	return 0;
}