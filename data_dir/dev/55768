#include <bits/stdc++.h>
#define MOD 1000000007
#define endl "\n"
typedef long long ll;
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cout.precision(10);
    int n;
    cin >> n;
    set<int> st;
    for(int i = 1; i <= n; i++) {
        int x;
        cin >> x;
        if(x) {
            st.insert(x);
        }
    }
    cout << st.size() << endl;
    return 0;
}
