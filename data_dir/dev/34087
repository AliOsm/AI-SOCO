#include <iostream>
#include<cstdio>
#include<cmath>
#include<algorithm>
#include<map>
#include<set>
#include<vector>
#include<bitset>
#include<iomanip>
#include<cstring>
#include<string>
#include<deque>
#include<limits>

using namespace std;

#define ll long long

int main()
{
//    freopen("a.in","r",stdin);
//    freopen("a.out","w",stdout);
    int n,s,a[101],sum=0;
    cin>>n>>s;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    sort(a,a+n);
    for(int i=0;i<n-1;i++)
    {
        sum+=a[i];
    }
    cout<<((sum>s)?"NO":"YES");
}



