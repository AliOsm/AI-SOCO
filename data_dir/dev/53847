using namespace std;
#define forr(i, a, b) for(int i = (a); i < (int) (b); i++)
#define forn(i, n) forr(i, 0, n)
#define db(v) cerr << #v << " = " << v << endl
#define pb push_back
#define sz(x) ((int)x.size())
#define fst first
#define snd second
typedef long long ll;
typedef pair<int, int> ii;
const int MAXN = -1;

int q, x;
ll k;

int main(){
  scanf("%d", &q);
  while(q--){
    scanf("%lld %d", &k, &x);
    printf("%lld\n", (k-1)*9 + x);
  }
  return 0;
}
