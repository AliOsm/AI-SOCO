#include<bits/stdc++.h>
#define fin freopen( "input.txt", "r" , stdin )
#define fout freopen( "output.txt", "w" , stdout )
#define ll long long
#define T ios_base::sync_with_stdio(0) ;
#define pp pop_back
#define sz size()
#define r return
#define pb push_back
#define fr first
#define sc second
using namespace std;

const ll N = 1e4 + 1;
const ll INF = 1e18;

ll n , m , l , cnt , ans , k , x , y , z , mn = INF  , mx , sum ;
vector < ll > v , V;
deque < ll > d;
queue < ll > q;
map < ll , ll > mp;
pair < ll , ll > p [ N ];
string s , t;
bool b , boo;
float f , fl;
char c;
int main()
{
//fin ;
//fout ;
T
    cin >> n >> m >> x >> y ;
    while ( n > 0 && x > 0 && y > 0 ){
        cnt += 256;
        n -- , x -- , y -- ;
    }
    while ( n > 0 && m > 0 )
    {
        cnt += 32 ;
        n -- ;
        m -- ;
    }
    cout << cnt ;








}














