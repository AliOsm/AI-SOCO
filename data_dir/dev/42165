// Author: πα

#include <bits/stdc++.h>
using namespace std;
#define long int64_t

int main()
{
    ios::sync_with_stdio(false), cin.tie(nullptr);
    int T;
    cin >> T;
    while(T--)
    {
        int n;
        cin >> n;
        if(n == 1)
        {
            cout << "-1\n";
            continue;
        }
        cout << '5';
        for(int i = 1; i < n; ++i)
            cout << '7';
        cout << '\n';
    }
    return 0;
}
