#include <bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(NULL);cout.tie(NULL)
#define ll long long int
using namespace std;
const int N= 1e5 + 10;

int main(){ 
    int n, x, y = 0;
    cin >> n;
    for (int i = 0; i < n; ++i)
    {
      cin >> x;
      y = __gcd(y , x);
    }
    cout << y * n;


    
    return 0;
}
    
