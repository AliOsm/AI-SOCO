#include <bits/stdc++.h>
using namespace std;

int T, N;
string S;

int main () {
	ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
	
	cin>>T;
	
	for (int i=0;i<T;i++) {
		cin>>N>>S;
		
		for (int j=0;1;j++) {
			if (j+10>=N) {
				cout<<"NO\n";
				break;
			}
			
			if (S[j]=='8') {
				cout<<"YES\n";
				break;
			}
		}
	}
	
	return 0;
}
