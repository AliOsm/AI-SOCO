#include <cctype>
#include <string>
#include <iostream>
#include <algorithm>
#include <vector>
#include <set>
#include <cmath>
#include <map>
#include <queue>
#include <functional>
#include <iomanip>

void solve() {
  int64_t n, k;
  std::cin >> n >> k;
  n %= k + k;
  if (n >= k) {
    std::cout << "YES";
  } else {
    std::cout << "NO";
  }
}

int main () {
    std::ios_base::sync_with_stdio(false);
    std::cin.tie(nullptr);
    solve();
    return 0;
}

