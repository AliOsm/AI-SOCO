#include <bits/stdc++.h>

#define ll long long
#define sz size()
#define all(xx) xx.begin(),xx.end()
#define pb push_back
#define in insert
#define er erase
#define x1 pascal
#define y1 java
#define x2 php
#define y2 pyton
#define x3 ruby
#define y3 cpp
#define x4 delphi
#define y4 perl
#define S second
#define F first
#define tbc continue
#define mp make_pair
#define Yes cout<<"Yes"
#define No cout<<"No"
#define YES cout<<"YES"
#define NO cout<<"NO"
#define speed ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0) 
#define input freopen (".in", "r", stdin)
#define output freopen (".out", "w", stdout)

using namespace std;

const int N = 1e5 + 123;
const int M = 1e7 + 123;
const int inf = 1e9 + 123;
const ll INF = 1e18 + 123;
const ll MAXN = 1e10 + 123;
                     
int n, m, a[N], x, y, ans = inf, b[101][101];

int main ()
{
	speed;
	cin >> n >> m;
	for (int i = 1; i <= n; i ++)
		cin >> a[i];
	for (int i = 1; i <= m; i ++)
	{
		cin >> x >> y;
		b[x][y] = b[y][x] = 1;
	}
	for (int i = 1; i <= n; i ++)
		for (int j = 1 + i; j <= n; j ++)
			for (int k = 1 + j; k <= n; k ++)
				if (i != j && i != k && j != k && b[i][j] && b[j][k] && b[i][k])	
					ans = min (ans, a[i] + a[k] + a[j]); 
   	if (ans == inf || m <= 2)
   		cout << -1;
   	else
   		cout << ans;
}
