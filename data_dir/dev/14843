#include <bits/stdc++.h>

using namespace std;

struct point{
	double x, y;
};   

point a[10];


int main(){    	
	int n = 3;
	for(int i = 1; i <= n; i++){
		cin >> a[i].x >> a[i].y;
	}
	cout << 3 << endl;
	cout << a[1].x + a[2].x - a[3].x << " " << a[1].y + a[2].y - a[3].y << endl;
	cout << a[1].x + a[3].x - a[2].x << " " << a[1].y + a[3].y - a[2].y << endl;
	cout << a[2].x + a[3].x - a[1].x << " " << a[3].y + a[2].y - a[1].y << endl;
}