#include<bits/stdc++.h>

using namespace std;

int main()
{
    string s;

    int i,j,len;

    cin>>len>>s;

    set < char > st;

    for(i=0; i<len; i++)
    {
        st.insert(s[i]);
    }

    if(len==1)
    {
        printf("Yes\n");

        return 0;
    }

    if((int)st.size()==len)
    {
        printf("No\n");

        return 0;
    }

    printf("Yes\n");

    return 0;
}
