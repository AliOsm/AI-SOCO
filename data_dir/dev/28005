#include<bits/stdc++.h>
using namespace std;
#define forr(i,a,n) for(int i=a; i<n; i++)
#define loop(i,a,n)	for(int i=a; i>=n; i--)
#define conti continue
#define brk break
#define pb push_back
#define SPEED ios_base :: sync_with_stdio(0), cin.tie(0), cout.tie(0)
#define in cin>>
#define out cout<<
#define endl <<"\n"

int main() 
{
	SPEED;
	
	int a;
	in a;
	int b[a];
	double k=0;
	forr(i,0,a)	in b[i];
	forr(i,0,a)	k=k+b[i];
	out fixed << setprecision(12) << k/a; 
	
	return 0;
}