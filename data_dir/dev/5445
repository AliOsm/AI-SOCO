#include<bits/stdc++.h>
#define ll long long
#define f first
#define s second
#define pb push_back
using namespace std;

int main(){
    int t,n,a,b;
    cin >> t;
    while(t--){
        cin >> a >> b >> n;
        if(n%3==0){
            cout << a << endl;
        }
        if(n % 3 == 1){
            cout << b << endl;
        }
        if(n % 3 == 2)cout << (a ^ b) << endl;
    }
    
    
    
    
    return 0;
}
