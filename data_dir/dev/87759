#include <bits/stdc++.h>
using namespace std;
 
#define F first
#define S second
#define int long long
#define pb push_back
#define pp pop_back
#define mp make_pair
#define all(x) x.begin(), x.end()
#define debug(x) cerr << #x << " : " << x
 
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
typedef pair<ll, ll> pll;
typedef vector<pll> vpll;
 
const int maxx = 219;
 
int a, b;
int n, m;
 
int32_t main() {
    ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
    cin >> n;
    for (int i = 0; i < n; i++) {
        int x;
        cin >> x;
        a = max(a, x);
    }
    cin >> m;
    for (int i = 0; i < m; i++) {
        int y;
        cin >> y;
        b = max(b, y);
    }
    cout << a << ' '  << b << endl;
    return 0;
}