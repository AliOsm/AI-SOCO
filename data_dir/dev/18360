#include <bits/stdc++.h>

using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int ans=0,ans2=0;

        for(int i=0;i<n;i++){
            int j;
            cin>>j;
            if(j%2==0)
                ans++;
            if(j%2)
                ans2++;
            }

      if(ans==0||ans2==0){
        cout<<"YES"<<endl;
      }
      else cout<<"NO"<<endl;

    }
    return 0;
}

