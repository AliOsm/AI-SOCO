#include <bits/stdc++.h>

using namespace std;

#define pb push_back
#define mp make_pair
#define ff first
#define ss second
#define mod 1000000007 

int main(){
	int n,k,ans[6][3] = {{0,1,2},{1,0,2},{1,2,0},{2,1,0},{2,0,1},{0,2,1}};
	scanf("%d %d",&n,&k);
	n = n%6;
	printf("%d\n",ans[n][k] );
	return 0;
}