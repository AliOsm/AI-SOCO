#include<bits/stdc++.h>
#define ll long long
#define ld long double
#define PB push_back
#define MP make_pair
#define INF 1e18
#define inf 1e9
#define y1 dasdsvcvd
using namespace std;
ll ax,ay,bx,by,cx,cy;
int main()
{
ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
cin >> ax >> ay >> bx >> by >> cx >>cy;
ll d1=(ax-bx)*(ax-bx)+(ay-by)*(ay-by);
ll d2=(bx-cx)*(bx-cx)+(by-cy)*(by-cy);
ll mx=(by-ay);
ll m2=(bx-ax);
ll m3=(by-cy);
ll m4=(bx-cx);
if(d1==d2 && mx*m4!=m2*m3)
{
    cout <<"Yes";
}
else
{
    cout <<"No";
}

return 0;
}
