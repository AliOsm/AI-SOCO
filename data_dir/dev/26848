#include<bits/stdc++.h>
#define ll long long
using namespace std;
ll n,i,a[100];
int main()
{
    cin >>n;
    for (i=1;i<=n;++i)
        cin >>a[i];
    if (a[n]==15)return cout <<"DOWN",0;
    if (a[n]==0)return cout <<"UP",0;
    if (n==1)return cout <<-1,0;
    if (a[n]>a[n-1])return cout <<"UP",0;
    cout <<"DOWN";
}
