#include<bits/stdc++.h>
using namespace std;


#define ll long long
#define pi pair<ll,ll>

ll arr[150005];
ll cnt[150005];
map<ll,ll>mp;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    ll i,j,n,m,t;
    cin>>n;
    for(i=0;i<n;i++){
        cin>>arr[i];
    mp[arr[i]]=1;
    }

    sort(arr,arr+n);
    ll ans=0;
    for(i=1;;i++){
        if(mp[i]==0){
            cout<<i<<endl;
            return 0;
        }
    }
}