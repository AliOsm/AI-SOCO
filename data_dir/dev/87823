#include<cstring>
#include<cstdlib>
#include<cstdio>
#include<iostream>
#include<algorithm>
#define LiangJiaJun main
using namespace std;
int i,j,x = 0, y = 0, num;
char str[14];
int LiangJiaJun(){

	for (i = 0; i < 8; i++){
		scanf("%s", str);
		for (j = 0, num = 0; j < 8; j++){
			if (str[j] == 'B')num++;
		}
		if (num == 8)x++;
		else y = num;
	}
	printf("%d\n",x+y);
	return 0;

}
