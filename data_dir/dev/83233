#include <bits/stdc++.h>
#define ll long long
#define mp make_pair
#define pb push_back
using namespace std;

int main() {

    ios::sync_with_stdio(false);
    ll t;
    cin>>t;
    while(t--){
    ll x,y;
    cin>>x>>y;
    if(x-y==1)
    {
        cout<<"NO"<<endl;
    }
    else cout<<"YES"<<endl;
    }
    return 0;

}
