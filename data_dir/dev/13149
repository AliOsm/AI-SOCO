#include <bits/stdc++.h>
using namespace std;

long long e[500][500], d[500][500];
int n, req[500]; long long ans[500];
bool eg[500];

void fnd() {
	
}

int main (int argc, char *argv[])
{
	memset(eg, false, sizeof(eg));
	scanf("%d", &n);
	for(int i = 0; i < n; ++i) {
		for(int j = 0; j < n; ++j) {
			scanf("%lld", &e[i][j]);
		}
	}
	for(int i = 0; i < n; ++i) {
		scanf("%d", &req[i]);
		req[i]--;
	}
	for(int k = n - 1; k >= 0; --k) {
		eg[req[k]] = true;
		long long  ans_i = 0;
		for(int i = 0; i < n; ++i) {
			for(int j = 0; j < n; ++j) {
				e[i][j] = min(e[i][j], e[i][req[k]] + e[req[k]][j]);
				if(eg[i] && eg[j]) {
					ans_i += e[i][j];
				}
			}
		}
		ans[k] = ans_i;
	}
	for(int i = 0; i < n; ++i) {
		printf("%lld ", ans[i]);
	}
	
	return 0;
}

