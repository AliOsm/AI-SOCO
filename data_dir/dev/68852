#include <cstdio>
#include <iostream>
#include <cstring>
#include <vector>
#include <list>
#include <map>
#include <set>
#include <queue>
#include <deque>
#include <stack>
#include <algorithm>
#include <functional>
#include <cmath>
#include <sstream>
#include <utility>
#include <ctime>
#include <memory.h>
#include <cctype>
#include <cstdlib>
#include <limits>
#include <stdlib.h>

using namespace std;

#pragma comment (linker, "/STACK:4000000")

#define forn(j, n) for(j=1; j<=n; j++)
#define sz size()
#define pb push_back
#define o 1000000007LL 
#define fors(w, s) for(set <int> ::iterator w=s.begin(); w!=s.end(); w++)
#define pii pair<int, int>
#define uns unsigned
#define vi vector <int>
#define vvi vector <vi>
#define inf 2000000000

struct qwe
{
};

int n, j, i, h, q1, q2, q[3], x, y, k, dl=-1, cur=1, p[3], p1, p2;
string t1, t2;

int main()
{
    //freopen ("input.txt", "r", stdin); freopen ("output.txt", "w", stdout);
	//freopen ("nosimple.in","r",stdin); freopen ("nosimple.out","w",stdout);
	scanf ("%d\n", &n);
	getline (cin, t1);
	getline (cin, t2);
	for (j=0; j<(n<<1); j++)
	{
		if (t1[j]=='1' & t2[j]=='1')
		{
			p[0]++;
			continue;
		}
		if (t1[j]=='1')
		{
			p[1]++;
			continue;
		}
		if (t2[j]=='1')
		{
			p[2]++;
			continue;
		}
	}
	cur=1;
	forn (j, (n<<1))
	{
		if (p[0]>0)
		{
			q[cur]++;
			p[0]--;
			cur=3-cur;
			continue;
		}
		if (p[cur]>0)
		{
			p[cur]--;
			q[cur]++;
			cur=3-cur;
			continue;
		}
		if (p[3-cur]>0)
		{
			p[3-cur]--;
			cur=3-cur;
			continue;
		}
		cur=3-cur;
	}
	if (q[1]==q[2])
	{
		printf ("Draw");
		return 0;
	}
	if (q[1]>q[2])
	{
		printf ("First");
	}
	else
	{
		printf ("Second");
	}
	return 0;
}