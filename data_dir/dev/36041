#include <iostream>

using namespace std;

int main()
{
    int a , b , c , n ;
    cin>>a>>b>>c>>n ;
    if(c > a || c > b)
    {
        cout<<-1;
        return 0 ;
    }
    a -= c ;
    b -= c ;
    if(a + b + c < n)
        cout<<n-a-b-c;
    else
        cout<<-1;
    return 0 ;
}
