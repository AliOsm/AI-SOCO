#include <bits/stdc++.h>
using namespace std;
const char duom[] ="i.txt";
int main()
{
    //ifstream cin (duom);
    int n,k;
    cin >> n>>k;
    int red=n*2;
    int blue = n*8;
    int green = n*5;
    int total = red/k + blue/k + green/k;
    if(red%k!=0){total++;}
    if(blue%k!=0){total++;}
    if(green%k!=0){total++;}
    cout <<total;

    return 0;
}