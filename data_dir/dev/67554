#define ll long long
#define ss second
#define ff first
#define pb push_back
using namespace std;
ll n,m,x,y,z,w,t,a[100100],b[100100];
string s,f[100100];
int main(){
    //ios::sync_with_stdio(false) , cin.tie(0) , cout.tie(0);
    char c;
    cin>>n>>c>>m>>x;
    x%=(24*60);
    for(int i=0;i<x;i++){
        m++;
        if(m==60){m=0;n++;}
        if(n==24){n=0;}
    }
    if(n<10)cout<<0;
    cout<<n;
    cout<<c;
    if(m<10)cout<<0;
    cout<<m;
	return 0;
}