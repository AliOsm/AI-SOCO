#include <bits/stdc++.h>
using namespace std;
int a[1001];
unordered_map<char,int>freq;
int main(){
// 	freopen("readin.txt","r",stdin);
	int n;
	scanf("%d",&n);
	string s;
	cin >> s;
	int ans = 0,x = 0,y = 0;
	for(auto i : s)freq[i]++;
	cout << ((min(freq['U'],freq['D']) + min(freq['L'],freq['R']))<<1) << endl;
}