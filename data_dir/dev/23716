#include<bits/stdc++.h>
using namespace std;

int main()
{
    map<char,string>mymap;
    mymap['a']=" s";
    mymap['q']=" w";
    mymap['z']=" x";
    mymap['/']=". ";
    mymap[';']="l ";
    mymap['p']="o ";
    mymap['.']=",/";
    mymap[',']="m.";
    mymap['m']="n,";
    mymap['n']="bm";
    mymap['b']="vn";
    mymap['v']="cb";
    mymap['c']="xv";
    mymap['x']="zc";
    mymap['s']="ad";
    mymap['d']="sf";
    mymap['f']="dg";
    mymap['g']="fh";
    mymap['h']="gj";
    mymap['j']="hk";
    mymap['k']="jl";
    mymap['l']="k;";
    mymap['o']="ip";
    mymap['i']="uo";
    mymap['u']="yi";
    mymap['y']="tu";
    mymap['t']="ry";
    mymap['r']="et";
    mymap['e']="wr";
    mymap['w']="qe";
	string s,s1;
	char p,m;
	int len,i;
	while(cin>>p)
    {
        cin>>s;
        len=s.length();
        if(p=='R')
        {
            for(i=0;i<len;i++)
            {
               // m=mymap[s[i]];
                s1=mymap[s[i]];
                cout<<s1[0];
            }
            cout<<endl;
        }
        else
        {
            for(i=0;i<len;i++)
            {
                s1=mymap[s[i]];
                cout<<s1[1];
            }
            cout<<endl;
        }
    }
	return 0;
}
