//                             In The Name Of Allah
#include <bits/stdc++.h>
#define	ss second
#define ff first
#define use_fast ios::sync_with_stdio(false), cin.tie(0), cout.tie(0)
#define ret(n) return cout << n, 0
#define se(n) cout << setprecision(n) << fixed
#define pb push_back
#define int long long
#define ld long double
using namespace std; 
 
const int N = 3e5 + 10000, OO = 1e9, M = 1e9 + 7, P = 6151, SQ = 1000, lg = 12;
typedef pair <int, int> pii;
int a[N];

void solve() {	
	int n, x = 0, s = 0;
	cin >> n;
	for(int i = 0; i < n; i++) {
		cin >> a[i];
		x = x ^ a[i];
		s += a[i];
	}
	cout << 2 << endl;
	cout << x << " " << (s + x) << endl;
	return;
}

int32_t main() {
    use_fast;
    int t;
    cin >> t;
    while(t--) 
    	solve();
	return 0; 
}
