#include <bits/stdc++.h>
using namespace std;
int n, a[10] = {2, 7, 2, 3, 3, 4, 2, 5, 1, 2};

int init();
int solve();

int main()
{
    init();
    solve();
}

int init()
{
    scanf("%d", &n);
}

int solve()
{
    int x = n % 10, y = n / 10;

    printf("%d", a[x] * a[y]);
}
