#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
ll gcd(ll a, ll b)
{
  return b?gcd(b, a%b):a;
}
int main()
{
  ll a, b; scanf("%lld %lld", &a, &b);
    a = min(a, b);
  ll x = 1, y = 1;
  for (ll i = 1; i <= a; ++i)
    {
      x *= i;
    }
  printf("%lld\n", x);
}
