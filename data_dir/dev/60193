#include <bits/stdc++.h>

using namespace std; 	
typedef long long ll;
typedef unsigned long long ull;
const int N = 1e5+50;
const int oo = 1e9;
const int mod = 1e9+7;

int f[N];
int main(){
	int n;
	cin>>n;
	for(int i=0;i<n;i++){
		int a;
		cin>>a;
		f[a]++;
	}
	for(int i=0;i<N;i++){
		if(f[i]%2){
			puts("Conan");
			return 0;
		}
	}
	puts("Agasa");
}