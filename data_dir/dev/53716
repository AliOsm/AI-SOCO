#include<bits/stdc++.h>
using namespace std;
long long a,b,c,d,i,e,f,g,n,m,k,l,fix[200005];
int main()
{
           cin>>a>>b>>c;
           fix[a]++; fix[b]++; fix[c]++;
           if(fix[1]>=1) cout<<"YES";
           else if(fix[2]>=2) cout<<"YES";
           else if(fix[3]==3) cout<<"YES";
           else if(fix[2]==1 && fix[4]==2) cout<<"YES";
           else cout<<"NO";
}