#include <bits/stdc++.h>
#define MEM(dp,i) memset(dp,i,sizeof(dp))
#define F first
#define S second
#define pb push_back
#define R return
using namespace std;
typedef long long ll;
typedef pair<ll,ll> pll;
typedef pair<int,int> pii;
const int MX=2e5+9;
const ll inf=(1ll<<60);
const ll mod=1e9+7;
ll G1(ll a){
    ll AA[]={a,1,a+1,0};
    R AA[a%4];
}
ll GGG(ll a,ll b){
    R(G1(b)^(G1(a-1)));
}
string s,s1;
int main(){
    cin>>s>>s1;
    for(int i=0;i<s.size();i++){
        if(s[i]<s1[i]){
            puts("-1");
            R 0;
        }
    }
    cout<<s1<<endl;
}
