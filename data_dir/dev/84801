#include<bits/stdc++.h>

using namespace std;

int main(){
	int t;
	cin>>t;
	int p1=0,p2=0;
	for(int i=0;i<t;i++){
		int m,c;
		cin>>m>>c;
		if(m>c){
			p1++;
		}
		if(c>m){
			p2++;
		}
	}
	if(p1>p2){
		cout<<"Mishka";
	}
	else if(p2>p1){
		cout<<"Chris";
	}
	else if(p1==p2){
		cout<<"Friendship is magic!^^";
	}
}
