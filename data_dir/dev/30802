#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
#define ss second
#define ff first
ll n;
vector<ll> v;
int main(){
    cin>>n;
    for(int i=1;i<=sqrt(n);i++)
    {
        if(n%i==0)
        {
            ll a=n/i,aa=a-1;
            ll b=i*(aa*a/2)+n/i;
            v.push_back(b);
            if(i*i!=n)
            {
            ll a1=n/(n/i),aa1=a1-1;
            ll b1=n/i*(aa1*a1/2)+n/(n/i);
            v.push_back(b1);
            }
        }
    }
    sort(v.begin(),v.end());
    for(int i=0;i<v.size();i++)
        cout<<v[i]<<' ';
}
/*
