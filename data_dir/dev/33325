#include <bits/stdc++.h>
#define INT long long int
#define MOD 1000000007
#define oo 978654321
#define OO 9999999987654321ll
#define PI (3.141592653589793238462643383279502884197)
using namespace std;
INT a, b, x, y, z, ans;
int main()
{
    ios::sync_with_stdio(false);
    cin>>a>>b>>x>>y>>z;
    x*=2, z*=3;
    x+=y, z+=y;
    if( x > a ) ans += x-a;
    if( z > b ) ans += z-b;
    cout<<ans;
    return 0;
}
