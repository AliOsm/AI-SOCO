#include <bits/stdc++.h>using namespace std;#define pb push_back#define FOR(i,begin,end) for (LL i=begin;i<=end;i++)#define rep(i,t) for (int i=0;i<t;i++)#define pii pair<int,int>#define pi pair<pii,int>#define X first#define Y secondtypedef long long LL;const LL maxN=1e2+7,N=13,MOD=1e6+3,Mod=1e9+7,INF=1e7+7;LL n,a[maxN];vector<int>q;set<int>ans;bool good;main(){   cin.tie(0);  cout.tie(0);  ios::sync_with_stdio(false);  FOR(i,1,100)  {    rep(j,16)      q.pb(j);    for(int j=14;j>0;j--)      q.pb(j);  }  cin>>n;  FOR(i,1,n)    cin>>a[i];  rep(i,q.size()-n-1)  {    good=true;    for(int j=1;j<=n;j++)    {      if(q[j+i-1]!=a[j])        good=false;    }    if(good)      ans.insert(q[i+n]);  }  if(ans.size()==1)  {    if(*ans.begin()>a[n])      return cout<<"UP\n",0;    return cout<<"DOWN\n",0;  }  return cout<<"-1\n",0;}   