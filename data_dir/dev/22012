#include<bits/stdc++.h>
#include<unordered_map>
using namespace std;
typedef double D;
typedef long long ll;
const ll mod=1e9+7;
const ll inf=(1ll<<61);
const int MX=3e5+9;
int n;
int main(){
    cin>>n;
    int ans=0;
    for(int i=1;i<=n;i++){
        if(n%i==0)ans++;
    }
    cout<<ans-1<<endl;
}
