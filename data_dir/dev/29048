#include<bits/stdc++.h>
using namespace std;
 
#define ll long long
 
ll arr[500005];
ll brr[500005];
 
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    ll a,b,i,j,n,m,t;
    cin>>a>>b;
    if(a==9&&b==1) cout<<99<<" "<<100<<endl;
    else if(abs(a-b)>=2) cout<<-1<<endl;
    else if(a>b) cout<<-1<<endl;
    else if(a==b) cout<<a*100+12<<" "<<b*100+13<<endl;
    else cout<<a*10+9<<" "<<b*10<<endl;
}