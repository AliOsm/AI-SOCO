#include<bits/stdc++.h>
using namespace std;
#define pb  push_back
#define ll  long long
#define vi  vector<ll >
#define vvi vector<vi >
#define all(x) x.begin(), x.end()

ll n, s;

int main() {
    ios_base::sync_with_stdio(false);
    cin >> n >> s;
    cout << (s + n - 1) / n;

    return 0;
}