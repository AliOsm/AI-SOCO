#include <bits/stdc++.h>
using namespace std;
#define eb emplace_back
#define emp emplace
#define fi first
#define se second
#define INF 0x3f3f3f3f
typedef long long ll;
typedef pair<int, int> ii;

int main(void) {
	ios_base::sync_with_stdio(false);

	string s; cin >> s;

	bool ok = 0;
	for (char c : s) {
		if (c == 'H') ok = 1;
		if (c == 'Q') ok = 1;
		if (c == '9') ok = 1;
	}

	if (ok) cout << "YES" << endl;
	else cout << "NO" << endl;

    
	return 0;
}

