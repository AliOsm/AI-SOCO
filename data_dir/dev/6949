#include <stdio.h>

#include<iostream>

#include<string>

#include<cstring>

using namespace std;

int main()
{
string a1;
string a2;
string a3;
string a4;
string a5;
string a6;
string a7;
string a8;
cin>>a1;
cin>>a2;
cin>>a3;
cin>>a4;
cin>>a5;
cin>>a6;
cin>>a7;
cin>>a8;
int s=0;
char mas[]="qrbnpQRBNP";
int mass[]={9,5,3,3,1,-9,-5,-3,-3,-1};

for(int a=0; a<8; a++)
{
    for(int aa=0; aa<strlen(mas); aa++)
    {
        if(a1[a]==mas[aa])
        s=s+mass[aa];
        
        if(a2[a]==mas[aa])
        s=s+mass[aa];
        
        if(a3[a]==mas[aa])
        s=s+mass[aa];
        
        if(a4[a]==mas[aa])
        s=s+mass[aa];
        
        if(a5[a]==mas[aa])
        s=s+mass[aa];
        
        if(a6[a]==mas[aa])
        s=s+mass[aa];
        
        if(a7[a]==mas[aa])
        s=s+mass[aa];
        
        if(a8[a]==mas[aa])
        s=s+mass[aa];
    }
}
    if(s<0)
    {
        cout<<"White";
    }
    else
    {
        if(s==0)
        {
            cout<<"Draw";
        }
        else
        {
            cout<<"Black";
        }
    }
}