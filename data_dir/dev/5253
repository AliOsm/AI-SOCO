#include <bits/stdc++.h>
    
#define ll long long
#define all(aaa) aaa.begin(), aaa.end()
 
using namespace std;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(0);

    int t;
    cin >> t;

    while (t--) {
        int a, b;
        cin >> a >> b;

        if (__gcd(a, b) == 1) {
            cout << "Finite\n";
        }
        else {
            cout << "Infinite\n";
        }
    }

    return 0;
}
