#include <bits/stdc++.h>
#define int long long
#define boost() ios_base :: sync_with_stdio(0); cin.tie(); cout.tie(); cout << fixed; cout << setprecision(15); srand(time(NULL))
#define all(x) x.begin(), x.end()
using namespace std;

int t;

signed main () {
  cin >> t;
  while (t--){
    int a, b, n;
    cin >> a >> b >> n;
    int f2 = a ^ b;
    n %= 3;
    if (n == 0) cout << a << endl;
    else if (n == 1) cout << b << endl;
    else cout << f2 << endl;
  }
  return 0;
}