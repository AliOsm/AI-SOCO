#include <bits/stdc++.h>

using namespace std;
 
int main() {
  string x;
  cin>>x;int y[4]={};
  
 for(int i=0;i<x.size();i++)
 {
     if(x[i]=='h')
     y[0]++;
     if(x[i]=='e'&&y[0]>0)
     y[1]++;
     if(x[i]=='l'&&y[1]>0)
     y[2]++;
     if(x[i]=='o'&&y[2]>1)
     y[3]++;
     
 }
 if(y[3]>0)
 cout<<"YES";
 else
 cout<<"NO";
}