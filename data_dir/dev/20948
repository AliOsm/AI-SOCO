#include <bits/stdc++.h>

typedef long long ll;
typedef unsigned long long ull;

using namespace std;

int main()
{
    string s;
    string t;
    cin >> s;
    for (int i = 0 ; i < 5; ++i)
    {
        cin >>t;
        if (s[0] == t[0] || s[1] == t[1])
        {
            return puts("YES");
        }
    }
    puts("NO");


    return 0;
}
