#include <iostream>
#include <list>
#include <set>
#include <algorithm>

using namespace std;
set<int> kek;
int main()
{
    int n;cin >> n;
    int a;cin >> a;while(a--)
    {int u;cin >> u;kek.insert(u);}
    cin >> a;while(a--)
    {int u;cin >> u;kek.insert(u);}
    int flag=1;
    for (int i=0;i<n;i++)
    {
        flag=min(flag,(int)kek.count(i+1));
    }
    cout << (flag?"I become the guy.":"Oh, my keyboard!");
    return 14/88;
}
