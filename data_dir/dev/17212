#include <bits/stdc++.h>
using namespace std;
long long  i, j, n, c[223456],a[1111234], b[1125678],d[123456], l,ll,r, s, t, k,x, y,y1,z,m;
long long p,q,qq,ma,mi;
pair <long long , long long> mm,u[223456];
int main() {
    cin >> n>>m>>mi>>ma;
    for (i = 0; i < m; i ++){
        cin >> a[i];
    }
    sort (a,a+m);
    if (a[0] < mi || a[m-1] > ma){
        cout << "Incorrect";
        return 0;
    }
    if (n-m>=2){
        cout << "Correct";
        return 0;
    }
    if (a[0] > mi && a[m-1] < ma){
        cout << "Incorrect";
    }
    else 
    cout <<"Correct";
    return 0;
}