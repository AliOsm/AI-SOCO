// In the name of Allah the Most Merciful.

#include<bits/stdc++.h>
using namespace std;

typedef long long ll;

bool check(char x)
{
    if(x=='1')return true;
    if(x=='3')return true;
    if(x=='5')return true;
    if(x=='7')return true;
    if(x=='9')return true;
    if(x=='a')return true;
    if(x=='e')return true;
    if(x=='i')return true;
    if(x=='o')return true;
    if(x=='u')return true;

    return false;
}

int main(void)
{
    string ar;
    cin >> ar;

    int counter = 0;

    for(int i=0;i<ar.size();i++){

        if(check(ar[i])==true)counter++;
    }

    cout << counter << endl;

    return 0;
}
