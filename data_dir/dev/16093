#include <bits/stdc++.h>
using namespace std;

const int N = int(2e5);

int n, m;
int arr[10];
int ans = 0;

int main (int argc, char *argv[])
{
	scanf("%d%d", &n, &m);
	for(int i = 0; i < n; ++i) {
		int a;
		scanf("%d", &a);
		arr[a - 1]++;
	}
	for(int i = 0; i < m; ++i) {
		n -= arr[i];
		ans += arr[i] * n;
	}
	printf("%d", ans); 
	
	return 0;
}

