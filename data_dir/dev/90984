#include<bits/stdc++.h>
using namespace std;

string s;

int main()
{
    int i,j,n,m,t;
    s="heidi";
    string a;
    cin>>a;
    int len=a.size();

    for(i=0,j=0;i<len;i++){
        if(s[j]==a[i]){
            j++;
        }
        if(j==5){
            cout<<"YES"<<endl;
            return 0;
        }
    }
    cout<<"NO"<<endl;
}