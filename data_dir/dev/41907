#include <bits/stdc++.h>

using namespace std;

const int N = 1005;

char s[N];
int n;

int main(){

    scanf("%d %s", &n, s);

    for(int i = 0; i + 1 < n; i++){
        if(s[i] != s[i+1]){
            printf("YES\n");
            printf("%c%c\n", s[i], s[i+1]);
            return 0;
        }
    }

    printf("NO\n");
}
