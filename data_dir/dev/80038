#include<iostream>
#include<algorithm>
using namespace std;
int a,s,d,f,g,h,j,k,l,i,n,m;
main(){
cin>>n>>m>>k>>f>>g>>h;
s=h%m;
for(i=0;i<m;i++){
if(i<s) cout<<h/m+1; else cout<<h/m;
cout<<" ";
}
if(n-m==0) return 0;
s=(g-h)%(n-m);
for(i=0;i<n-m;i++){
if(i<s) cout<<(g-h)/(n-m)+1; else cout<<(g-h)/(n-m);
cout<<" ";
}
//system("pause");
}
