# define ll long long
# define fi first
# define se second

using namespace std;

const ll N = 2e5 + 100;

int T, l, r;

int main()
{
	cin >> T;
	for (int i = 1; i <= T; i++){
		cin >> l >> r;
		cout << l << ' ' << 2 * l << endl;
	}
	



}
