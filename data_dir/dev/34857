#include<bits/stdc++.h>

using namespace std;
#define ll long long
#define dbug printf("I am here\n");
#define fast ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
const int maxn = 2e5+100;

const ll inf = 1e18;

int main()
{
    fast;
    int test;
    cin >> test;
    while(test--){
        int h  , m;
        cin >> h >> m;
        ll ans = 0;
        while(1){
            if(h == 24 and m == 0)break;
            m++;
            if(m == 60){
                h++;
                m = 0;
            }
            ans++;
        }
        cout << ans << endl;
    }
    return 0;
}
