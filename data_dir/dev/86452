#include<bits/stdc++.h>
#define MOD 1000000007
#define ll long long
#define mp make_pair
#define pb push_back 
using namespace std;
int main(){
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	int n;
	cin >> n;
	string s;
	cin >> s;
	int cta = 0, ctb = 0;
	for(int i = 0; i < s.length(); i++){
		if(s[i] == 'n')
			cta++;
		else if(s[i] == 'z')
			ctb++;
	}
	for(int i = 0; i < cta; i++)
		cout << 1 << " ";
	for(int i = 0; i < ctb; i++)
		cout << 0 << " ";
	cout << '\n';

}