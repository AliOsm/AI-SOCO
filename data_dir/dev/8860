//.cpp
#include <bits/stdc++.h>

using namespace std;

const int N = 1e5+9;
const long long inf = 1e18+7;

long long a, b, n;

int main() {
    ios::sync_with_stdio(false);
    cin>>a>>b;
    while(a<=b) {
        a *= 3;
        b *= 2;
        n++;
    }
    cout<<n;
    cout<<endl;
	return 0;
}