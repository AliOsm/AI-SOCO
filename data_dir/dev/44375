#include <bits/stdc++.h>
using namespace std;const int MAXN = 2e5 + 777;int a[MAXN];bool used[MAXN];int main() {int n;cin >> n;for (int i =1; i <= n; i++) {cin >> a[i];}int ans = 0;for (int i = n; i >= 1; i--) {if (used[i]) continue;int j = a[i];while(j > 0) {used[j] = 1;j = a[j];}ans++;}cout << ans;}