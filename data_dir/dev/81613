//In the name of God
#include<bits/stdc++.h>
#define F first
#define S second
#define pb push_back
#define ll long long
#define acm ios::sync_with_stdio(0),ios_base::sync_with_stdio(0),cin.tie(0),cout.tie(0);
#define mod 1000000007
using namespace std;
int main()
{
    acm;
    int n,cnt=0;
    cin>>n;
    for (int i=1;i<=n;++i)
    {
        if ((n-i)%i==0&&n-i>0) cnt++;
    }
    cout<<cnt<<"\n";
    return 0;
}