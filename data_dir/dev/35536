#include<bits/stdc++.h>
#define MAX 300005
using namespace std;
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
typedef long long i64;

int N;
char a[MAX];
bool dead[MAX];

int main()
{
    optimiza_io 
    cin >> N;
    for( int i = 1; i <= N; i ++ )
        cin >> a[i];
    int yo = ( N - 11 ) / 2;
    int el = yo;
    
    for( int i = 1; i <= N; i ++ )
        if( a[i] == '8' && el )
        {
            dead[i] = 1;
            el--;
        }
    for( int i = 1; i <= N; i ++ )
    {
        if( dead[i] )
            continue;
        if( a[i] == '8' )
        {
            cout << "YES\n";
            exit(0);
        }
        else if( yo )
            yo--;
        else
            break;
    }
    cout << "NO\n";
    return 0;
}

/*

13
8308888888888

*/