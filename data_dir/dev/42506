#include <bits/stdc++.h>
#define int long long 
using namespace std;


int32_t main() {
        ios_base :: sync_with_stdio(0); cin.tie(0); cout.tie(0);
        int test; cin >> test;
        while(test--) {
            int n, x; cin >> n >> x;
            cout << x * 2 << endl;
        }
}
