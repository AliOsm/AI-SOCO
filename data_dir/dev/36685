#include <bits/stdc++.h>
using namespace std;

#define OO (long long)1e18
#define mod (int)(1e9)+7

int main() {
	int tc;
	scanf("%d", &tc);
	while (tc--) {
		int n;
		scanf("%d", &n);
		int cnt = 0;
		for (int i = 0; i < n; i++) {
			int a;
			scanf("%d", &a);
			if (a % 2)
				cnt++;
		}
		if (cnt % 2 || (cnt && cnt < n))
			puts("YES");
		else
			puts("NO");
	}
}
