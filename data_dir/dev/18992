#include <bits/stdc++.h>

using namespace std;
#define ll long long
#define F first
#define pb push_back
#define S second
#define er erase
#define fi find
#define in insert
#define pi pair<int,int>
#define pii pair<pi,int>
#define go ios::sync_with_stdio(0)
int a,b,c,d,e,f;
int main()
{
    go;
    cin>>a>>b;
    cin>>c>>d;
    cin>>e>>f;
    for(int i=b;i>0;i--)
    {
        a+=i;
        if(d==i)
            a-=c;
        if(f==i)
            a-=e;
        if(a<0)
            a=0;
    }
    cout<<a;
    return 0;
}
