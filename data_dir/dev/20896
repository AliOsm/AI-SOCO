#include <bits/stdc++.h>

using namespace std;

#define rfile freopen("period.in", "r", stdin)
#define wfile freopen("period.out", "w", stdout)
#define files rfile; wfile
#define all(a) a.begin(), a.end()
#define pb push_back
#define fi first
#define se second
#define sws ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0)

typedef long long ll;
typedef long double ld;
typedef vector< int > vi;
typedef vector< char > vc;
typedef vector< vc > vvc;
typedef vector< vi > vvi;
typedef map< int, int > mapT;
typedef pair< int, int > pairT;
typedef priority_queue< ll > pq;
typedef vector< vector< pairT > > graph;

const int inf = (int)2e9;
const ll llinf = (ll)3e18;
const int N = (int)2e4 + 111;
const ld PI = (ld)acos(-1);

int cnt[N];

int main() {
	sws;
	// files;
	int n, mx = 0, cur = 0;
	cin >> n;
	vi a(n);
	for (int i = 0; i < n; i++) {
		cin >> a[i];
		mx += a[i];
	}
	// cout << mx << "\n";
	for (int i = 2; i <= n; i++)
		if (n % i == 0 && (n / i) >= 3) {
			for (int j = 0; j < i; j++)
				cnt[j] = 0;
			cur = mx;
			for (int j = 0; j < n; j++)
				cnt[j % i] += a[j];
			for (int j = 0; j < i; j++)
				cur = max(cur, cnt[j]);
			// cout << i << " " << cur << "\n";
			mx = max(mx, cur);
		}
	cout << mx;
	return 0;
}