#include <iostream>
#include <fstream>
#include <string>
#include <vector>
#include <map>
#include <unordered_map>
#include <list>
#include <set>
#include <algorithm>
#include <queue>
#include <iterator>
#include <functional>
#include <numeric>
#include <cmath>
#include <stack>
#include <limits>
#include <ctime>

using namespace std;

#define ll long long
#define sc(n) static_cast<ll>(n)
#define IO ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);

int main() {
	IO;
#ifdef _DEBUG
	ifstream cin("In.txt");
#endif	
	ll t;
	cin >> t;
	while (t--) {
		ll n, m;
		cin >> n;
		vector<ll>vc(n);
		for (auto& i : vc) {
			cin >> i;
		}
		sort(vc.rbegin(), vc.rend());
		for (auto& i : vc) {
			cout << i << " ";
		}
		cout << endl;
	}
	return 0;
}