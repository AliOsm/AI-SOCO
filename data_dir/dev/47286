#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef long double ld;
#define pb push_back
#define endl "\n"


int main(){
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

    ll x, y;
    cin >> x >> y;
    if (x == y)
        return cout << "=", 0;
    if (x == 1)
        return cout << "<", 0;
    if (y == 1)
        return cout << ">", 0;
    if (x + y == 6)
        return cout << "=", 0;
    if (x == 2 && y == 3)
        return cout << "<", 0;
    if (x == 3 && y == 2)
        return cout << ">", 0;
    if (x < y)
        cout << ">"; else
        cout << "<";

}
