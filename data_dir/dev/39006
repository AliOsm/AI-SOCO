#include <cstdio>

#include <cmath>

#include <cstring>

#include <queue>

#include <vector>

#include <iostream>

#include <algorithm>

#include <map>



using namespace std;



typedef long long LL;



int main()

{
    LL x;
    cin>>x;
    x%=360;
    x+=360;
    x%=360;
    x+=45;
    x%=360;
    x-=1;
    x/=90;
    cout<<x<<endl;

    return 0;

}