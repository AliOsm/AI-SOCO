#include<iostream>
#include<cstdio>
using namespace std;
int i, j, k, a, b, c, d, ans;
int main()
{
    cin >> a >> b >> c >> d;

    i = max(a * 3 / 10, a - (a * c / 250));

    j = max(b * 3 / 10, b - (b * d / 250));

    if(i > j) cout << "Misha";
    else if(j > i) cout << "Vasya";
    else cout << "Tie";
    return 0;
}
