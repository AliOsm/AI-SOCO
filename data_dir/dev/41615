#include <bits/stdc++.h>
using namespace std;
typedef long long int ll;

class ProblemCJotyAndChocolate {
public:
	ll gcd(ll a, ll b){
		return a%b==0?b:gcd(b, a%b);
	}
	ll lcm(ll a, ll b){
		return (a*b)/gcd(a, b);
	}
	void solve(std::istream& in, std::ostream& out) {
        ll n, a, b, p, q; in >> n >> a >> b >> p >> q;
		ll tp = (n/a);
		ll tq = (n/b);
		ll tboth = (n/lcm(a, b));
		tp -= tboth, tq -= tboth;
		ll res = (tp*p) + (tq*q) + (tboth)*(max(p, q));
		out<<res;
	}
};


int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
	ProblemCJotyAndChocolate solver;
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
	return 0;
}
