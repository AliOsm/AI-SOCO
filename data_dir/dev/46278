#include <stdlib.h>
#include <functional>
#include <algorithm>
using namespace std;
typedef long long ll;
int main()
{
	int q;
	cin >> q;
	while (q--) {
		ll k, x;
		cin >> k >> x;
		cout << k * 9 - (9 - x) << endl;
	}
	return 0;
}