#include<bits/stdc++.h>
#define pi 3.1415926535897932384626
using namespace std;
  
struct P{
  int x,y;
  bool operator <(const P &a) const{
    return x>a.x;
  };
};
  
int a,b,c,d,i,k,n,m,e,dx[10]={1,0,-1,0,1,1,-1,-1},dy[10]={0,1,0,-1,1,-1,1,-1};
int o[1];
int l[1];
int j[1];
long long x,y,z,mod=1000000007;
//string r;
char r[111111];
vector<int> v[1];
queue<int> q;
stack<int> s;
map<int,int> p;
P u[1];
  
bool as(P a,P b)
{
  if(a.x!=b.x)
    return a.x>b.x;
  return a.y>b.y;
}


int main()
{
  scanf("%d %s",&a,r+1);
  for(int t=1;t<=a;t++)
  if(r[t]=='L') n++;
  else m++;
  printf("%d",n+m+1);
}

