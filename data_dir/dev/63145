#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n;
	cin>>n;
	string s;
	cin>>s;
	int eights=0;
	int turns=s.length()-11;
	for(int i=s.length()-11;i>=0;i--)
	{
		if(s[i]=='8')
			eights++;
	}
	if(turns%2==0)
	{
		if(eights>turns/2)
			cout<<"YES";
		else
			cout<<"NO";
	}
	else
	{
		if(eights>turns/2)
			cout<<"YES";
		else
			cout<<"NO";

	}
}