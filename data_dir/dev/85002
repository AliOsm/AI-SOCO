#include <bits/stdc++.h>
using namespace std;

signed main(){
  ios::sync_with_stdio( 0 );
  vector< string > G( 3 );
  for( int i = 0; i < 3; ++i ){
    cin >> G[ i ];
  }
  if( G[ 0 ][ 0 ] != G[ 2 ][ 2 ] or G[ 0 ][ 1 ] != G[ 2 ][ 1 ]
   or G[ 0 ][ 2 ] != G[ 2 ][ 0 ] or G[ 1 ][ 0 ] != G[ 1 ][ 2 ] ){
    cout << "NO" << endl;
  } else{
    cout << "YES" << endl;
  }
  return 0;
}
