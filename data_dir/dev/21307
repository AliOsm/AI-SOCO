#include <bits/stdc++.h>
#define F first
#define S second
using namespace std;
typedef long long ll;
int n, m;
string s = "5", t = "5";
int main()
{
    ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    cin >> n >> m;
    ll sum1 = 5, sum2 = 5;
    while(sum1 < n || sum2 < n)
    {
        sum1 += 4, sum2 += 5;
        s += '4';
        t += '5';
    }
    reverse(s.begin(), s.end());
    reverse(t.begin(), t.end());
    cout << s << '\n' << t;
    return 0;
}
