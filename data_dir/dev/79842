#include<bits/stdc++.h>
#define r_0 return 0;
#define ll long long
#define ld  long double
#define pb push_back
#define fr first
#define se second
#define in freopen("input.txt","r",stdin);
#define out freopen("output.txt","w" stdout);
#define TIME ios_base::sync_with_stdio(NULL);
#define tcn cin.tie(NULL);
#define tct cout.tie(NULL);
using namespace std;
ll n,m,cnt,mx,ans,a , x , y , sum;
string s;

const ll N = 1e3 + 7;
set<int>st;
ll used[N];
vector<int>g[N];
int main(){
TIME tcn tct
 ll T;
   cin >> T;
   while( T -- ){
    cin >> n;
    sum = 0;
    ans = 0;
    cnt = 0;
    if( n <= 9 ){
        cout << n << endl;
        continue;
    }
    while( true ){
          if( n < 10 )
            break;
          cnt = n % 10;
          ans = n - cnt;
          sum += ans;
          n -= n - cnt;
          n += ( ans ) / 10;

    }
    cout << n + sum << endl;
   }
}
