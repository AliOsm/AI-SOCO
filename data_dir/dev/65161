#include "bits/stdc++.h"

using namespace std;
using LL = long long;

constexpr int N = 1e5 + 5;

int main() {
	ios::sync_with_stdio(false); cin.tie(nullptr);
	
	int a, b, c, d, e, f;
	cin >> a >> b >> c >> d >> e >> f;
	if (c == 0 and d) return cout << "Ron\n", 0;
	if (a == 0 and b and c && d) return cout << "Ron\n", 0;
	if (e == 0 && f && a && b && c && d) return cout << "Ron\n", 0;
	int up = b * d * f;
	int down = a * c * e;
	if (up > down) cout << "Ron\n";
	else cout << "Hermione\n";
}