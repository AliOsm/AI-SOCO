#include<bits/stdc++.h>
using namespace std;
int main()
{
	int x;
	cin >> x; int a[x];
	for(int i = 0; i < x; i++) cin >> a[i];
	if(a[x-1] == 0) cout << "UP\n";
	else if(a[x-1] == 15) cout << "DOWN\n";
	else if(x == 1) cout << "-1\n";
	else if(a[x-1] > a[x-2]) cout << "UP\n";
	else cout << "DOWN\n";
	return 0;
}
