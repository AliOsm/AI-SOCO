#include <bits/stdc++.h>
using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	    cin.tie(NULL);

	    int n;
	    cin >> n;
	    int sum1 = 0, sum2 = 0;
	    for (int i = 0; i < n; i++) {
	    	int x;
	    	cin >> x;
	    	if (x > 0)
	    		sum1 += x;
	    	else
	    		sum2 += x;
	    }
	    cout << sum1 - sum2;

	return 0;
}
