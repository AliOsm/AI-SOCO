#include<bits/stdc++.h>

using namespace std;

#define lli long long int

int main(){
	
	ios :: sync_with_stdio(false); cin.tie(nullptr); cout.tie(nullptr);
	
	int q;
	
	cin>>q;
	
	while(q--){
		
		int n;
		
		cin>>n;
		
		lli price = 0;
		
		for(int i=0; i<n; i++){
			
			lli x;
			
			cin>>x;
			
			price += x;
		}
		
		if(price%n == 0){
			
			cout<<price/n;
		}
		
		else{
			
			cout<<1+(price/n);
		}
		
		cout<<endl;
	}
}
