#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(void) {
    int r, x, y, x1, y1;
    int dx, dy;
    int res;

    scanf(" %d %d %d %d %d", &r, &x, &y, &x1, &y1);

    dx = x - x1;
    dy = y - y1;

    res = (int)ceil(sqrt(1LL * dx * dx + 1LL * dy * dy) /(1.0 * r + r));

    printf("%d\n", res);

    return 0;
}
