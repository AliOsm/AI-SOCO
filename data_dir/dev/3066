#include <bits/stdc++.h>
using namespace std;
typedef long long ll;


int n ;

int main(){

cin>>n;

for(int k=1;k<=n+10;k++){

    if( k*(k+1)/2 >n ){
        cout<<k-1<<endl;
        for(int i=1;i+1<k;i++){
                cout<<i<<" ";
        n-=i;
        }
        cout<<n<<endl;
        return 0;
    }

}

return 0;
}
