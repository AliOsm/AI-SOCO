{
	cout<<"YES";
}
void no()
{
	cout<<"NO";
}

void yes(bool res, bool small=false)
{
	if(!small)
	{
		if(res)
		cout<<"YES";
		else cout<<"NO";
	}
	else
	{
		if(res)
		cout<<"Yes";
		else cout<<"No";
	}
	
}
