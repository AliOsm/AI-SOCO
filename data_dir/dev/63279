inline void solution() 
{ 
	int n,m,cnt;
	cin>>n>>m>>cnt;
	ll a[n],b[m];
	read(a,n);
	read(b,m);
	lvector z,aa;
	ll k=(b[0]==1);
	rep(i,1,m)
	{
		if(b[i]==1)k++;
		else z.pb(k),k=0;
	}
	if(k>0)z.pb(k);
	k=0;
	sort(all(z));
	
	
	k=(a[0]==1);
	rep(i,1,n)
	{
		if(a[i]==1)k++;
		else aa.pb(k),k=0;
	}
	if(k>0)aa.pb(k);
	k=0;
	
	
	ll res=0;
	/*ll l=-1,r=-1;
	int j=0;
	while(j<n)
	{
		if(a[j]==1)
		{
			if(l==-1)
			{
				l=j,r=j;
			}
			else r++;
			
			ll dif=r-l+1;
			if(cnt%dif==0)
			{
				ll need=cnt/dif;
				auto lower = lower_bound(z.begin(), z.end(), need);
				if(lower!=z.end())
				{
					int pos=lower-z.begin();
					rep(i,pos,z.size())
					{
						ll h=z[i]-need+1;
						res+=h;
						debug(dif, need, h);
					}
				}
			}
		}
		else
		{
			l=-1;r=-1;
		}
		++j;
	}*/
	
	for(auto x:aa)
	{
		rep(dif,1,x+1)
		{
			//ll dif=r-l+1;
			if(cnt%dif==0)
			{
				ll need=cnt/dif;
				auto lower = lower_bound(z.begin(), z.end(), need);
				if(lower!=z.end())
				{
					int pos=lower-z.begin();
					rep(i,pos,z.size())
					{
						ll h=z[i]-need+1;
						h*=(x-dif+1);
						res+=h;
						//debug(dif, need, h);
					}
				}
			}
		}
	}
	
	res();
}
