#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    string s1;
    cin>>s1;
    string s2="";
    for(int i=s1.size()-1;i>=0;i--)
    {
        s2+=s1.at(i);
    }
    s1+=s2;
    cout<<s1<<endl;
    return 0;
}
