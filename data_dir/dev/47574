#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    string s;

    cin >> n >> s;

    int sum = 0;

    for(int i = 0; i < n - 1; i++){
        if(s[i] != s[i + 1]){
            cout << "YES" << endl;
            cout << s[i] << s[i + 1] << endl;
            return 0;
        }
    }

    cout << "NO" << endl;

    return 0;
}
