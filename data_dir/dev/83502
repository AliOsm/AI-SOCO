#include<iostream>
using namespace std;
int main(){

 long long n, m;
 cin>>n>>m;
 if(n<=m){
   if(n%2!=0)
    cout<<"Akshat"<<endl;
   else
    cout<<"Malvika"<<endl;


 }

 if(m<n){

    if(m%2!=0)
        cout<<"Akshat"<<endl;
    else
        cout<<"Malvika"<<endl;



 }











 return 0;
}
