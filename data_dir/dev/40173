const int mod = 1e9 + 7;
 
ll a[30];

void pre(){
	for(int i = 0 ; i < 30 ; i++ ){
		if(powl(2 , i) > 1e9)
			break;
		a[i] = powl(2,i);
	}
} 
 
 
int main()
{
    fast;
   // pre();
    ll t;
