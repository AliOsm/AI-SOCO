    	ll x , y;
    	cin >> x >> y ;
    	if(x >= y || x  >= 4){
    		cout << "YES\n";
    		continue;
    	}
    	if(x == 2 && y == 3){
    		cout << "YES\n";
    		continue;
    	}
    	cout << "NO\n";
    	
    }
    





















    
