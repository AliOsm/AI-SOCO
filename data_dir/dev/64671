#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
void sprint(string s) {for (int i = 0; i < s.size(); ++i)	printf("%c",s[i]); printf("\n");}
int main(){
	// ios_base::sync_with_stdio(0);
	int x=0;
	int y;
	for (int i = 0; i < 5; ++i)
	{
		cin>>y;
		x+=y;
	}
	if(x%5==0 and x/5>0){
		printf("%d\n", x/5);
	}
	else printf("-1\n");
	return 0;
}