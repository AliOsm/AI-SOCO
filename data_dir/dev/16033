//++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
//++--++--++--++--++--++--++--++--++--+--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
const int MAXN = 20088;
const ll inf = 1000000001;
const ll INF = 2e18;
const ld EPS = 1e-7;
const ll mod = inf + 6;
const ld PI = acos(-1.0);

signed main()
{
	fastio();
	int n;
	cin >> n;
	string s;
	cin >> s;
	rev(s);
	int cnt = 0;
	for (int i = 10; i < n; ++i)
	{
		if (s[i] == '8') cnt++;
	}
	int d = n - 11;
	if (cnt > d / 2)
	{
		cout << "YES";
	}
	else
	{
		cout << "NO";

	}
	//sp;
}