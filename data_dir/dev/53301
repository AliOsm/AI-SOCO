#include <bits/stdc++.h>
using namespace std;

int main( )
{
    int n, i, s = 0, b;
    cin >> n;
    for (i = 1; ; i++)
    {
        b = (i * (i + 1)) / 2;
        s += b;
        if (s > n)
        {
            i--;
            break;
        }
    }
    cout << i << "\n";
    return 0;
}
