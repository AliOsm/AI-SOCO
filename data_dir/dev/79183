#include <bits/stdc++.h>
using namespace std;

int T, arr[5];

int main(){
    scanf("%d", &T);
    while(T--){
        for(int i = 0; i < 3; i++){
            scanf("%d", &arr[i]);
        }
        sort(arr, arr + 3);
        printf("%s\n", arr[2] <= arr[0] + arr[1] + 1 ? "Yes" : "No");
    }
    return 0;
}
