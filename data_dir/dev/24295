typedef long long ll;
typedef long double ld;

const int Z = (int)1e5 + 111;
const int inf = (int)1e9 + 111;
const ll llinf = (ll)1e18 + 5;

int main() {
	//files;
	SWS;
	string s, t = "heidi";
	cin >> s;
	int num = 0;
	for (int i = 0; i < sz(s); ++i) {
		if (s[i] == t[num]) num++;
		if (num == 5) break;
	}
	cout << (num == 5 ? "YES" : "NO");
	return 0;
}