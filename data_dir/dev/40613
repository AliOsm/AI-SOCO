#include<bits/stdc++.h>
using namespace std;
#define FAST ios_base:: sync_with_stdio(false),cin.tie(0),cout.tie(0);
typedef long long ll;
int main()
{
    ll n;
    cin>>n;
    ll a[n],a1=0,a2=0;
    for(ll i=0;i<n;i++)
      {cin>>a[i];
        if(a[i]==0)
         a1++;
         if(a[i]==5)
         a2++;
      }
      if(a1==0)
        {
            cout<<"-1";
            return 0;
        }
      a2=a2/9;
      for(ll i=0;i<a2;i++)
       cout<<"555555555";
       if(a2==0)
         cout<<"0";
         else
         {
            for(ll i=0;i<a1;i++)
             cout<<"0";
         }     
     
    
}