#include <bits/stdc++.h>
using namespace std;
#define ll long long int
#define ld long double
#define ff first
#define ss second
#define pb emplace_back
#define mp make_pair
#define MOD 1000000007
#define FAST ios_base::sync_with_stdio(false), cin.tie(0), cout.tie(0);srand(time(NULL))
int main()
{
    FAST;
    ll a,b;
    cin >> a >> b;
    if(a==b)
    {
        cout<<a<<"1 "<<b<<"2";
    }
    else if(a+1==b)
    {
        cout<<a<<"9 "<<b<<"0";
    }
    else if(a==9 && b==1)
    {
        cout<<"99 100";
    }
    else 
    {
        cout<<"-1";
    }
    
}