#include <bits/stdc++.h>

using namespace std;

int main ()
{
    ios_base::sync_with_stdio(false);
    int x, y, z; cin >> x >> y >> z;
    if(x>y+z) cout << "+" << endl;
    else if(y>x+z) cout << "-" << endl;
    else if (z==0) cout << "0" << endl;
    else cout << "?" << endl;
    return 0;
}
