using namespace std;

const int N = 1e5 + 123;
const int M = 1e7 + 123;
const int inf = 1e9 + 123;
const ll INF = 1e18 + 123;
const ll MAXN = 1e10 + 123;
                     
int a[101][101], b[101][101];

int main ()
{
	speed;
	for (int i = 1; i <= 3; i ++)
		for (int j = 1; j <= 3; j ++)
			cin >> a[i][j];
	for (int i = 1; i <= 3; i ++)
	{
		for (int j = 1; j <= 3; j ++)
			cout << 1 - ((a[i][j] + a[i - 1][j] + a[i + 1][j] + a[i][j - 1] + a[i][j + 1]) % 2);
		cout << endl;
	}
}
