}
#define int ll
void solve()
{
    int hbit=0;
    int n;
    cin>>n;
    while(n>(1ll<<hbit)-1)  hbit+=2;
    int lbit=hbit-2,lnum=(1ll<<lbit);
    int rem=n-lnum;
    if(rem%3==0)
    {
        cout<<lnum+rem/3<<endl;
    }
    else if(rem%3==2)
    {
        int ans=3*lnum;
        rem-=2;
        rem/=3;
        int cur=1;
        while(rem)
        {
            int t=rem%4;
            if(t==1)
                ans|=3*cur;
            else if(t==2)
                ans|=1*cur;
            else if(t==3)
                ans|=2*cur;
            rem/=4;
            cur*=4;
        }
        cout<<ans<<endl;
    }
    else if(rem%3==1)
    {
        int ans1=lnum+rem/3;
        int ans3=3*lnum;
        rem-=1;
        rem/=3;
        int cur=1;
        while(rem)
        {
            int t=rem%4;
            if(t==1)
                ans3|=3*cur;
            else if(t==2)
                ans3|=1*cur;
            else if(t==3)
                ans3|=2*cur;
            rem/=4;
            cur*=4;
        }
        cout<<(ans1^ans3)<<endl;
    }
}