#include <bits/stdc++.h>

using namespace std;
const int mod = 1e9 + 7;
vector<int>level[40001];
int arr[200];
int k,n;
int main()
{
    int a,b,c;
    int t;
    cin>>t;
    while(t--){
        cin>>n;
        for(int i=0;i<n;i++)cin>>arr[i];
        sort(arr,arr+n);
        for(int i=n-1;i>=0;i--){
            cout<<arr[i]<<" ";
        }
        cout<<endl;




    }
    return 0;
}
