#include <bits/stdc++.h>
typedef long long ll ;
using namespace std;

int main()
{
    ios::sync_with_stdio(false) , cin.tie(0) , cout.tie(0) ;

    int t ; cin >> t ;

    while(t--){
         int a , b ;
         cin >> a >> b;

         cout << (__gcd(a, b) == 1 ? "Finite" : "Infinite" ) << endl;

    }


    return 0;
}
