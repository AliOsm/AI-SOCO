#include<bits/stdc++.h>
using namespace std;
#define Jewel ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
int main()
{
    Jewel;
    int n, t, i;
    cin>>n>>t;
    if(t==10)
    {   if(n==1)
            cout<<-1<<endl;
        else
        {
            cout<<'1';
            for(i=1; i<n; i++)
                cout<<'0';
        }
    }
    else
    {
        for(i=0; i<n; i++)
            cout<<t;
    }

    return 0;
}
