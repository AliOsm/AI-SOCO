int main(){
     //freopen("out.txt","w",stdout);
    //ios_base::sync_with_stdio(false);
   //cin.tie(0);


   string s;
   cin>>s;
   ll n=s.length();
   ll k=lxt();
    vector<int> v;
   f(i,0,s.length())
          if(s[i]=='5'||s[i]=='0') v.pb(i);
    ll ans=0;
    f(i,0,v.size()){
        ll x=v[i];
      ll ans1=bigmod((ll)2,x,(ll)M);
      ll ans2=bigmod((ll)2,n*k,(ll)M);
      ans2-=1;
      ll ans3=bigmod((ll)2,n,(ll)M);
        ans3-=1;
      ans3=bigmod(ans3,(ll)M-2,(ll)M);
        ans=ans%M+(((ans1*ans2)%M)*ans3)%M;
        if(ans<0) ans+=M;
    }
    cout<<ans%M<<endl;

return 0;
}
