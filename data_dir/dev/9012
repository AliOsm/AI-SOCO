#include <iostream>
#include <cmath>
#include <cstring>

using namespace std;

int main()
{
  int n;
  string s;
  cin >> s;
  n = s.size();
  int count  = 0;
  for(int i = 0; i < n; ++i){
    if(s[i] == 'a' || s[i] == 'e' || s[i] == 'i' || s[i] == 'o' || s[i] == 'u') ++count;
    else if(s[i] == '1' || s[i] == '3' || s[i] == '5' || s[i] == '7' || s[i] == '9') ++count;
  }
  cout << count << endl;
}