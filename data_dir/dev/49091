#include<iostream>
#include<cstdlib>
#include<vector>
#include<climits>
#include<cctype>
#include<map>
#include<list>
#include<cstdio>
#include<algorithm>
#include<memory.h>
#include<cmath>
#include<queue>
#include<fstream>
#include<sstream>
#include<set>
#include<iomanip>
#include<bitset>
#include<ctime>
#define L long long int
#define LD long double
#define pi 3.141592653589793238462643383
#define M 1000000007

using namespace std;

int main()
{
    L y,k,n,c,num,i;
    while(cin>>y)
    {
              cin>>k>>n;
              c=0;
              for(i=1;;i++)
              {
                           num=k*i;
                           if(num-y>=1&&num<=n)
                           {
                           cout<<num-y<<" ";
                           c++;
                           }
                           if(num>n)
                           break;
              }
              
              if(c==0)
              cout<<"-1\n";
              
    }
    return 0;
}
