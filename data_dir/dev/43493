#include<bits/stdc++.h>
using namespace std;
main()
{
   long n,d;
   while(cin>>n>>d)
   {
       long sum=0,i,a,rest,ans;
       for(i=1;i<=n;i++)
       {
           cin>>a;
           sum+=a;
       }
       rest=(n-1)*10;//proti gan er por 10 mint rest nei bole
       if(sum+rest>d)
       {
           cout<<-1<<endl;//somvob na
       }
       else
       {
           ans=d-sum;
           cout<<ans/5<<endl;
       }
   }
}
