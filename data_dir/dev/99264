/*ll fact[N];
ll invfact[N];
ll modInverse(ll a,ll m)
{
    int m0 = m;
    int y = 0, x = 1;

    if (m == 1)
      return 0;

    while (a > 1)
    {
        // q is quotient
        int q = a / m;
        int t = m;

        // m is remainder now, process same as
        // Euclid's algo
        m = a % m, a = t;
        t = y;

        // Update y and x
        y = x - q * y;
        x = t;
    }

    // Make x positive
    if (x < 0)
       x += m0;

    return x;
}
void initfact()
{
    fact[0]=1;
    for (int i=1;i<N;i++)
    {
        fact[i]=fact[i-1]*i;
        fact[i]%=M;
    }
    for (int i=0;i<N;i++)
    {
        invfact[i]=modInverse(fact[i],M)%M;

    }
}
ll npr(ll n,ll r)
{
    return (fact[n]*invfact[n-r])%M;
}*/
map<ll,ll>m;
ll M=998244353;
int main()
{

    //ofstream myf;
    //myf.open("file.txt");
    //freopen("fractions.in", "r", stdin);
    //freopen("output.txt", "w", stdout);
    time;
    int n;
    cin>>n;
    vector<ll>v;
    for (int i=0;i<n;i++)
    {

        ll x;
        cin>>x;
        v.push_back(x);
        m[x]=i;
    }
    ll maxi=m[v[0]];
    ll ans=1;
    for (int i=1;i<n;i++)
    {

        if (i>maxi)
        {
         ans+=ans;
         ans%=M;
        }
        maxi=max(maxi,m[v[i]]);
    }
    cout<<ans;
    return 0;

}
