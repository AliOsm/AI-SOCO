const int N=3e5+7;
const int inf=1e12;
const int mod=1e9+7;
const ld eps=1e-9;

main ()
{
    ios;
    int a,b;
    cin>>a>>b;
    if (a==9 && b==1){
        cout<<9<<' '<<10<<endl;
        exit(0);
    }
    if (a>b){
        cout<<-1<<endl;
        exit(0);
    }
    if (abs(a-b)>1){
        cout<<-1<<endl;
        exit(0);
    }
    if (a==b){
        cout<<a<<0<<' '<<b<<1<<endl;
    }
    else {
        cout<<a<<9<<' '<<b<<0<<endl;
    }
}
