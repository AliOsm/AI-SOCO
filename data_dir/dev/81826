#include <bits/stdc++.h>
typedef long long ll ;
#define pb push_back
#define f first
#define s second
#define all(v) v.begin(),v.end()
#define rall(v) v.rbegin(),v.rend()
#define SZ(a) (int)a.size()
#define Flush fflush(stdout);
using namespace std ;
int main()
{
    int t ;
    scanf ("%d" , &t );
    while (t--)
    {
        int a, b ;
        scanf ("%d%d" , &a , &b );
        int c = __gcd (a , b );
        if (c != 1 )printf ("Infinite\n");
        else printf ("Finite\n");
    }
}
