#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
typedef unsigned long long ull;
const int N = 1e5+50;
const ll oo = 1e18;
const int mod = 1e9+7;

map<int,int> mp;
int main(){
	int n;
	cin>>n;
	int ans = 0;
	for(int i=0;i<n;i++){
		int a;
		cin>>a;
		if(!mp[a] && a)ans++;
		mp[a] = 1;
	}
	cout << ans << endl;
}