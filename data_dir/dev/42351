#include<bits/stdc++.h>
#define r_0 return 0;
#define fr first
#define ll long long
#define se second
#define pb push_back
#define real_madrid ios_base::sync_with_stdio(NULL);
using namespace std;
ll s,v1,v2,t1,t2;
int main()
{
    real_madrid
  cin >>s >> v1 >> v2 >> t1 >> t2;
  if( 2*t1+v1*s>2*t2+v2*s )
    cout<<"Second";
  else if( 2 * t1 + v1 * s < 2 * t2 + v2 * s )
    cout<<"First";
  else
    cout<<"Friendship";
}

