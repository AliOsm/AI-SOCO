#include <bits/stdc++.h>

using namespace std;

int arr[5005];

int main(){

    int n;
    scanf("%d", &n);

    for(int i=1; i<=n; i++)cin>>arr[i];

    for(int i=1; i<=n; i++){
        int x=arr[i];
        int y=arr[x];
        if(arr[y]==i){
            cout<<"YES"<<endl;
            return 0;
        }
    }
    cout<<"NO";

}
