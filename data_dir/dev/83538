#include <bits/stdc++.h>
using namespace std;

int q, l1, l2, r1, r2;

int main(){
	cin >> q;
	while (q--){
		cin >> l1 >> r1 >> l2 >> r2;
		if (l1 == l2){
			if (r1 == r2) cout << l1 << " " << r1 << '\n';
			else cout << r1 << " " << r2 << '\n';
		}else cout << l1 << " " << l2 << '\n';
	}	
	return 0;
}