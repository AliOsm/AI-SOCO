#include <iostream>
#include <map>

#define FOR(i,n,x) for(int (i)=(x); (i)<=(n); ++(i))

using namespace std;

map <string,long> h;
long n,i;
string s;

int main ()
 {

  cin>>n;
  FOR(i,n,1)
   {
    cin>>s;
    if (h[s]==0) cout<<"OK"<<endl;
    else { 
          cout<<s<<h[s]<<endl;
         }
    h[s]++;     
   }
  
 }
