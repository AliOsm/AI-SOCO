#include <iostream>
using namespace std;

int main()
{
    long long a,xa,ya,xb,yb,xc,yc;
    cin>>xa>>ya>>xb>>yb>>xc>>yc;
    a=xa*(yb-yc)-ya*(xb-xc)+(xb*yc-xc*yb);
    if(!a)
        cout<<"TOWARDS"<<endl;
    else if(a>0)
        cout<<"LEFT"<<endl;
    else
        cout<<"RIGHT"<<endl;
    return 0;
}
