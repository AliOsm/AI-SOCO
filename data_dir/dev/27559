int main()
{
    CIN ;

    ll t, n, m, a, b, c, idx ;
    string s ;
    cin >> n >> m ;
    for( int i = 0 ; i < n ; i ++ )
    {
        cin >> s;
        v1.pb( s ) ;
    }

    for( int i = 0 ; i < m ; i ++ )
    {
        cin >> s;
        v2.pb( s ) ;
    }

    cin >> t ;
    while( t-- )
    {
        cin >> a ;
        a -- ;
        ll x = a % n ;
        ll y = a % m ;
        string p = v1[ x ] ;
        string q = v2[ y ] ;
        cout << p + q << endl ;
    }

    return 0 ;
}
