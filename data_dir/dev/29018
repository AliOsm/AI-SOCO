#include <iostream>

using namespace std;

int main()
{
    int n , sum = 0 , tmp , x;
    int arr[101] = {} ;
    cin >> n ;
    cin >> tmp ;
    for ( int i = 0 ; i < tmp ; i++ ) { cin >> x ; sum += ( !arr[x] ) ; arr[x] = 1 ; }
    cin >> tmp ;
    for ( int i = 0 ; i < tmp ; i++ ) { cin >> x ; sum += ( !arr[x] ) ; arr[x] = 1 ; }
    cout << ( sum == n ? "I become the guy." : "Oh, my keyboard!" ) ;
    return 0;
}
