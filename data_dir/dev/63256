#include <iostream>
#include <bits/stdc++.h>


using namespace std;

int main(){
    int a,b,c,d;
    cin>>a>>b>>c>>d;
    int first = max(3*a/10,a-a/250*c);
    int second = max(3*b/10,b-b/250*d);
    if(first > second)
        cout<<"Misha";
    else if(first < second)
        cout<<"Vasya";
    else
        cout<<"Tie";

}
