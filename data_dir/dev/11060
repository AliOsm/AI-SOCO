#include <iostream>
#include <cmath>
using namespace std;
int a,b;
int t1,t2,t3;
int main()
{
    ios::sync_with_stdio(false);
    cin>>a>>b;
    for (int i=1; i<=6; i++)
         if (abs(a-i)<abs(b-i)) t1++;
    else
        if ((abs(a-i)>abs(b-i))) t3++;
    else
        t2++;
    cout<<t1<<" "<<t2<<" "<<t3;
    return 0;
}
