using namespace std;

#define y1 roman_kaban
#define rank oryshych_konb
//const int mod = int(1e9) + 7;

const int MX = 200500;


int main()
{
    //freopen("in.txt","r", stdin);
    ios_base::sync_with_stdio(false);
    long long n;
    cin >> n;
    n = n
        - n / 2 - n / 3 - n / 5 - n / 7
        + n / 6 + n / 10 + n / 14
        + n / 15 + n / 21
        + n / 35
        - n / 30 - n / 42 - n / 70 - n / 105
        + n / 210;
    cout << n << endl;
    return 0;
}
