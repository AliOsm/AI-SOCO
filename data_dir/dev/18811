#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    if(n<=2){
        cout<<"No"<<endl;
        return 0;
    }
    cout<<"Yes"<<endl;
    cout<<n/2<<" ";
    for(int i=1;i<=n;i++)
        if(i%2==0)
            cout<<i<<" ";
    cout<<endl<<n-(n/2)<<" ";
    for(int i=1;i<=n;i++)
        if(i%2)
            cout<<i<<" ";
    cout<<endl;
    return 0;
}
