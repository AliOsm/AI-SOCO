const ld pie = 3.1415926535897932384626 ;
const ll mod = 1e9 + 7 ;

/// Tip : If a and b are positive integers ; we may say - ceil (a/b) = 1 + floor ( (a-1)/b ) .

void solve ( int test_case )
{
    int n , m ;
    cin >> n >> m ;

    vvi v ( n , vi ( m ) ) ;

    int maxx = 0 ;
    int pos = -1 ;

    for ( int i=0 ; i < n ; i ++ )
    {
        int row_minn = 1e9 + 100 ;
        for ( int j = 0 ; j < m ; j ++ )
        {
            int t ;
            cin >> t ;

            v[i][j] = t ;

            row_minn = min ( row_minn , t ) ;
        }

        if ( row_minn > maxx )
        {
            pos = i ;
            maxx = row_minn ;
        }
    }

    int i = pos ;

    int ans = 1e9 + 10 ;
    for ( int j = 0 ; j < m ; j ++ )
        ans = min ( ans , v[i][j] ) ;

    cout << ans ;
    line ;
}

int main()
{fast
    int t = 1;
    // cin >> t;

    for ( int i=1 ; i <= t ; i ++ ) solve ( i ) ;
    return 0;
}
