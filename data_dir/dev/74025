#include <bits/stdc++.h>
#define pb() push_back()

using namespace std;

const int N = 200005;
typedef long long ll;

ll z, a[6], sum;
int main(){
  for (int i = 0; i < 6; ++i){
      cin >> a[i];
      sum += a[i];
  }

  if (sum % 2 == 1){
      cout << "NO";
      return 0;
  }

  for (int i = 0; i < 6; ++i){
      for (int j = i + 1; j < 6; ++j){
          for (int k = j + 1; k < 6; ++k){
              if (a[i] + a[j] + a[k] == sum / 2){
                  cout << "YES";
                  return 0;
              }
          }
      }
  }

  cout << "NO";


}













