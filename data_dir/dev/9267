ll n,c,a[10010],b[10010],ans;

int main(){
	cin>>n>>c;
	c-=n;
	rep(i,n)cin>>a[i]>>b[i];
	rep2(x,1,10001){
		ll sum=0;
		rep(i,n){
			if(sum>c)break;
			sum+=a[i]*x/b[i];
		}
		if(sum==c)ans++;
	}
	cout<<ans;
}
