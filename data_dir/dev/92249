#include <bits/stdc++.h>
#define ll long long
#define ull unsigned ll
#define ld long double
#define mp make_pair
#define pb push_back
#define F first
#define S second

using namespace std;

const int MIN = 1e3 + 2;
const int MXN = 1e6 + 3;
const int INF = 1e9 + 7;
const ll LINF = 1e18 + 15;
const double EPS = 1e-9;
    
int a, b, c, d;    
    
int main(){
	cin >> a >> b >> c >> d;
	if (a == 1 && b == 0 && c == 0 && d == 0){               
		cout << 1;	                                         
		return 0;                                            
	}	                                                     
	if (a == 0 && b == 0 && c == 0 && d == 0){               
		cout << 0;
		return 0;
	}
	if (a == 0 && b == 1 && c == 1 && d == 0){
		cout << 0;
		return 0;
	}
	if (a == 0 && b == 1 && c == 0 && d == 0){
		cout << 0;
		return 0;
	}
	if (a == 1 && b == 1 && c == 0 && d == 0){
		cout << 1;
		return 0;
	}
	if (a == 0 && b == 0 && c == 1 && d == 0){
		cout << 0;
		return 0;
	}
	if (a == 1 && b == 0 && c == 1 && d == 0){
		cout << 0;
		return 0;
	}
	if (a == 1 && b == 1 && c == 1 && d == 0){
		cout << 1;
		return 0;
	}
	if (a == 0 && b == 0 && c == 0 && d == 1){
		cout << 1;
		return 0;
	}
	if (a == 1 && b == 0 && c == 0 && d == 1){
		cout << 1;
		return 0;
	}
	if (a == 0 && b == 1 && c == 0 && d == 1){
		cout << 0;
		return 0;
	}
	if (a == 1 && b == 1 && c == 0 && d == 1){
		cout << 0;
		return 0;
	}
	if (a == 0 && b == 0 && c == 1 && d == 1){
		cout << 1;
		return 0;
	}
	if (a == 1 && b == 0 && c == 1 && d == 1){
		cout << 1;
		return 0;
	}
	if (a == 0 && b == 1 && c == 1 && d == 1){
		cout << 0;
		return 0;
	}
	cout << 1;

   	return 0;
}