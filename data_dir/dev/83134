#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define ull unsigned long long
#define F first
#define S second
#define pb push_back
#define pi  acos(-1)
#define TIME 1.0*clock()/CLOCKS_PER_SEC
#define ld long double
#define endl '\n'

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
#ifdef LOCAL
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
#else
    //    freopen("input.txt", "r", stdin);
    //    freopen("output.txt", "w", stdout);
#endif
    int n , k;
    cin>>n>>k;
    for (int i = 0;i < n; ++i){
        for (int j = 0;j < n; ++j){
            if (i == j)cout<<k<<" ";
            else cout<<0<<" ";
        }
        cout<<endl;
    }
    return  0;
}