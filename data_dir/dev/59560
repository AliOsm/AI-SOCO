#include <bits/stdc++.h>
using namespace std;
int main () {
	int a;
	cin >> a;
	vector<int>v;
	while(a --){
		int x;
		cin >> x;
		v.push_back(x);
	}
	sort(v.begin(),v.end());
	for(int i = 0; i < v.size(); i++){
		if(v[i] != v[0]){
			cout << v[i];
			return 0;
		}
	}
	cout << "NO";
return 0;
}