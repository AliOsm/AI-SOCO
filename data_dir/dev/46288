#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
typedef unsigned long long ull;
const int N = 1e5+50;
const int oo = 1e9;
const int mod = 1e9+7;

int main(){
	int n;
	string s;
	cin>>n>>s;
	bool o=0;
	int z=0;
	for(int i=0;i<n;i++){
		if(s[i] == '1')o=1;
		else z++;
	}
	if(o){
		cout<<'1';
		for(int i=0;i<z;i++)cout << '0';
	}
	else puts("0");
}