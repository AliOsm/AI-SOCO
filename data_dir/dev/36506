#include <bits/stdc++.h>

using namespace std;

using ll = long long;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr);
	ll n;
	ll x, y;
	cin >> n >> x >> y;
	if (max(abs(1 - x), abs(1 - y)) <= max(abs(n - x), abs(n - y))) {
		cout << "White" << endl;
	} else {
		cout << "Black" << endl;
	}
}