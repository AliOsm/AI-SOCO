inline bool isPrime(LL n) {
    for (int i = 2; i * i <= n; ++i)if (n % i == 0)return false;
    return true;
}
inline string ekv(string a) {
    if (a.size() & 1)return a;
    string x = ekv(a.substr(0, a.size() / 2));
    string y = ekv(a.substr(a.size() / 2));
    return min(x + y, y + x);
}
inline LL cubic_root(LL x) {
    LL l = 0, r = MAX6;
    while (l != r) {
        LL m = (l + r + 1) / 2;
        if (m * m * m > x)
            r = m - 1;
        else
            l = m;
    }
    return l;
}
inline double FastInvSqrt(double x) {//   1/sqrt(X)	
  double xhalf = 0.5f * x;
  int i = *(int*)&x;  // ïðåäñòàâèì áèòû float â âèäå öåëîãî ÷èñëà
  i = 0x5f3759df - (i >> 1);  // êàêîãî ÷åðòà çäåñü ïðîèñõîäèò ?
  x = *(double*)&i;
  x = x*(1.5f-(xhalf*x*x));
  return x;
}
inline LL gcd(LL a,LL b)
{
	while(b ^= a ^= b ^= a = a % b);
	return a;
}
inline LL lcm(LL a, LL b)
{
  return a/gcd(a, b)*b;
}

inline void yes()
{
	cout<<"YES";
}
inline void no()
{
	cout<<"NO";
}

inline void yes(bool res, bool small=false)
{
	if(!small)
	{
		if(res)
		cout<<"YES";
		else cout<<"NO";
	}
	else
	{
		if(res)
		cout<<"Yes";
		else cout<<"No";
	}
	
}
