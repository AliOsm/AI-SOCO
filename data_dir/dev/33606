inline void solution() 
{ 
	int n;
	cin>>n;
	int a[n];
	read(a,n);
	rep(i,0,n)
	{
		if(a[i]==1)
		{
			cout<<"HARD";
			return;
		}
	}
	cout<<"EASY";
}
