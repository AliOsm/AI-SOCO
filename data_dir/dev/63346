# include <bits/stdc++.h>
# define speed ios_base::sync_with_stdio(0)
# define L_L long long

using namespace std;

const int N = 1e6+1;
const int MOD = 1e9+7;
const int M = 1e5+1;
const int P = 4e3+1;
int n,m,v;
string a[101],WB = "WB";
int main(){
    cin>>n>>m;
    for (int i=0;i<n;i++){
        cin>>a[i];
        }
    for (int i=0;i<n;i++){
        v = i%2;
        for (int j=0;j<m;j++){
            if (a[i][j]=='.'){
                a[i][j] = WB[v];
                }
            v = 1-v;
            }
        cout<<a[i]<<endl;
        }


    }

