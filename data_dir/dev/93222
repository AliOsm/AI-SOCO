#include <bits/stdc++.h>
using namespace std;

#define int long long
#define f(i,n) for(int i=0;i<n;i++)
#define rep(i,a,b) for(int i=a;i<=b;i++)

class ASnowball {
public:
	void solve(std::istream& in, std::ostream& out) {
        int w, h; in >> w >> h;
        int u1, d1, u2, d2; in >> u1 >> d1 >> u2 >> d2;
        for(int cur = h; cur > 0; --cur){
            w += cur;
            if(cur == d1){
                w -= u1;
                if(w < 0) w = 0;
            }
            else if(cur == d2){
                w -= u2;
                if(w < 0) w = 0;
            }
        }
        out << w;
	}
};


int32_t main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
	ASnowball solver;
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
	return 0;
}
