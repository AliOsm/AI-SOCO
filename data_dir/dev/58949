    cin>>n;
    for(i=1;i<=n;i++){
        cin>>a[i];
        dp[i]=1;
    }
    res=1;
    for(i=2;i<=n;i++){
        for(j=0;j<v[i].size();j++){
            x=v[i][j];
            if(a[x]<a[i]&&x<i){
                dp[i]=max(dp[i],dp[x]+1);
            }
        }
        res=max(res,dp[i]);
    }
    cout<<res<<'\n';
}
