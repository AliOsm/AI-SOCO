using namespace std;

inline bool isvowel (char c) {
	c = tolower(c);
    if (c == 'a' || c == 'e' || c == 'i' || c == 'y' || c == 'o' || c == 'u') return 1;
    return 0;
}

const double eps = 0.000001;
const ld pi = acos(-1);
const int maxn = 1e7 + 9;
const int mod = 1e9 + 7;
const ll MOD = 1e18 + 9;
const ll INF = 1e18 + 123;
const int inf = 2e9 + 11;
const int mxn = 1e6 + 9;
const int N = 6e5 + 123;                                          
const int M = 22;
const int pri = 997;
const int Magic = 2101;

const int dx[] = {-1, 0, 1, 0};
const int dy[] = {0, -1, 0, 1};
 
ll f (ll x) {
	ll c[5] = {0, 0};
	ll cur = 1;
	int nw = 0;
	while (x > 0) {
		ll mn = min(cur, x);
		c[nw % 2] += mn;
		x -= mn;
		nw ++;
		cur *= 2ll;
	}
	ll a = (c[0] % mod) * (c[0] % mod);
	a %= mod;
	ll b = (c[1] % mod) * ((c[1] + 1) % mod);
	b %= mod;
	return (a + b) % mod;
}

int main () {
	ll l, r;
	cin >> l >> r;
//	cout << f(r) << ' ' << f(l - 1) << '\n';
	cout << (mod + f(r) - f(l - 1)) % mod;
	return Accepted;
}

