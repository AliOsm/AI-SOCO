# include <bits/stdc++.h>

using namespace std;

typedef long double LD;
typedef long long int LL;
typedef pair <int,int> pii;

# define PB push_back
# define MP make_pair
# define L first
# define R second

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(0); cout.tie(0);
	string a, b;
	cin >> a >> b;
	if(a == b)
		cout << a << '\n';
	else
		cout << 1 << '\n';
	return 0;
}
