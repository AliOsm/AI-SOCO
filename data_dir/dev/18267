#include <bits/stdc++.h>

using namespace std;

int main() {
   int n;
   cin>>n;for(int i=0;i<min(2,n);i++)
   cout<<'O';
   int a=2,b=1;
   for(int i=3;i<=n;i++)
   {if(i==a+b)
   {
       cout<<'O';
       if(a<=b)
       a=i;
       else
       b=i;
   }else
   cout<<'o';
       
   }
    return 0;
}