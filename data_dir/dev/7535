#include <bits/stdc++.h>

using namespace std;


const int N = 5e5;

#define int long long


signed main()
{
	ios::sync_with_stdio(false);
	int n;
	cin >> n;
	for (int i = 0; i < n; i++) {
		int l1, r1, l, r;
		cin >> l >> r >> l1 >> r1;
		if (l == r1) cout << l << ' ' << l1 << "\n";
		else {
			cout << l << ' ' << r1 << "\n";
		}
	}
}
