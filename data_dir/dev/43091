#include <bits/stdc++.h>
using namespace std;
#define pb push_back
#define mp make_pair
#define x first
#define y second
typedef pair<int,int> pii;
typedef long long ll;

const int N = 200010;
const int oo = 1e9;
const int MOD = 1000000007;


ll x , y ,z , t1 ,t2 , t3;

int main(){
	cin >> x >> y >> z >> t1 >> t2 >> t3;
	ll res = (abs(x-z)*t2+t3*3+abs(x-y)*t2);
	ll f = abs(x-y)*t1;
	//cout << res << " " << f << endl;
	if(f < res){
		puts("NO");
	}else{
		puts("YES");
	}
	return 0;
}