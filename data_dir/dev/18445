#include <bits/stdc++.h>

using namespace std;

int main ()
{
    ios_base::sync_with_stdio(false);
    int t, a, b, c; cin >> t;
    while(t--) {
        cin >> a >> b >> c;
        if(a<=b+c+1 && b<=a+c+1 && c<=a+b+1) cout << "Yes" << endl;
        else cout << "No" << endl;
    }
    return 0;
}
