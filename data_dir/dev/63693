#include <bits/stdc++.h>
 
using namespace std;
 
#define pb push_back
#define mp make_pair
#define fir first
#define sec second
#define ll long long
#define ull unsigned long long
 
ll md=(1e9)*1e9+7;
int INF=1e9+7;

int main()
{
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    int n;
    string s;
    cin>>s;
    n=s.size();
    char mn='z';
    for (int i=0;i<n;i++)
    {
        if (mn<s[i]) cout<<"Ann"<<endl;
        else cout<<"Mike"<<endl;
        mn=min(mn,s[i]);
    }
    return 0;
}