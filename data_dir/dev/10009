#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,a,b,x,y;
    cin>>n;
    for(a=n/1234567;a>=0;a--)
        for(b=(n-a*1234567)/123456;b>=0;b--)
            if((n-a*1234567-b*123456)%1234==0){
                cout<<"YES";
                return 0;
            }
    cout<<"NO";
}
