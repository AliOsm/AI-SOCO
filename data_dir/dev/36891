#include<iostream>
#include<cstdio>
#include<cstring>
#include<cmath>
#include<cstdlib>
#include<algorithm>
#define LiangJiaJun main
using namespace std;
string biao[14]={"","","2","3","322","5","53","7","7222","7332"};
int n;char ch[24];string s;
int LiangJiaJun(){
    cin>>n;
    scanf("%s",ch+1);
    for(int i=1;i<=n;i++){
            int x=ch[i]-'0';
            s+=biao[x];
    }
    sort(s.begin(),s.end());
    for(int i=s.size()-1;i>=0;i--)cout<<s[i];cout<<endl;
    //system("pause");
    return 0;
}
