#include <bits/stdc++.h>
using namespace std;
#define ll long long

void  solve()
{
    ll h, m;
    cin>>h>>m;
    if (m == 0)
    {
        cout<<(24-h)*60<<endl;
    }
    else
    {
        cout<<(24-h-1)*60 + (60-m)<<endl;
    }
}
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    ll n;
    cin>>n;
    for(ll i=0; i<n; i++)
        solve();


    return 0;
}
