#include <bits/stdc++.h>

using namespace std;
using ll = long long;
using ii = pair<int, int>;
#define ff first
#define ss second
#define pb push_back

const int N = 100005;

int main(){
	int c[6];
	for(int i = 0; i < 4; i++){
		scanf("%d", c+i);
	}
	if(c[2]){
		if(c[0] && c[0] == c[3]){
			printf("1\n");
		}
		else
			printf("0\n");
	}
	else
		printf("%d\n", c[0] == c[3]);
	return 0;
}
