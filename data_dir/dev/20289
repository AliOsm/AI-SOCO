#include <bits/stdc++.h>
using namespace std;
#define int long long
typedef long long ll;

int32_t main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int b, k;
    cin >> b >> k;
    int sum = 0;
    for(int i = 0; i < k; ++i) {
        int digit;
        cin >> digit;
        sum *= b;
        sum += digit;
        sum %= 2;
    }
    cout << (sum==0?"even\n":"odd\n");
    return 0;
}

































