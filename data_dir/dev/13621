#include<bits/stdc++.h>
#define ll long long
#define ld long double
#define pb push_back
#define x first
#define y second
#define fastread ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
#define PI (atan(1)*4)
#define mp make_pair
using namespace std;

int main()
{
    fastread;
    ll n, m;
    cin >> n >> m;
    if (m == 0) {
        cout << n << " " << n;
        return 0;
    }
    cout << max(0LL, n - 2 * m) << " " << n-(long long)ceil((1.000 + sqrt(1.00 + 8 * m)) / 2);
    return 0;
}