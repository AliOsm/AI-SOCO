#include <bits/stdc++.h>
using namespace std;

#define pb push_back
#define mp make_pair
#define clr(i, j)   memset(i, j, sizeof i)
typedef long long ll;
typedef unsigned long long ull ;
typedef pair<int,int>   pii;

int main()
{
    int n, cnt = 0;
    cin >> n;
    for(int i=1; i<n; i++)
    {
        int x = n-i;
        if(x%i == 0)
            cnt++;
    }
    cout << cnt << endl;
}
