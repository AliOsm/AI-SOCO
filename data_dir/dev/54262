#include<bits/stdc++.h>
using namespace std;
int a[12345];

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL),cout.tie(NULL);

    int n,m;
    cin >> n >>m;
        while(m--){
        int l,r;
        cin >> l >> r;
    }
    for(int i = 0; i < n; i++){
        if(i % 2 == 0)cout<<"1";
        else cout<<"0";
    }
    return 0;

}
