#include <bits/stdc++.h>

using namespace std;

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0); cout.tie(0);

	int n;
	cin >> n;
	vector<int> a(n);
	set<int> s;
	for (int i = 0; i < n; ++i) {
		cin >> a[i];
		s.insert(a[i]);
	}
	if (s.size() == n) {
		cout << 0;
		return 0;
	}
	int ans = n;
	set<int> kek;
	for (int l = 0; l < n; ++l) {
		auto kek2 = kek;
		for (int r = n - 1; r >= l; --r) {
			if (kek2.size() == n - 1 - r + l) {
				ans = min(ans, r - l + 1);
			}
			else {
				break;
			}
			kek2.insert(a[r]);
		}
		kek.insert(a[l]);
	}
	cout << ans;

	return 0;
}