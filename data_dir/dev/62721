#include "bits/stdc++.h"
using namespace std;

#define F first
#define S second
#define mp make_pair
#define pb push_back
typedef long long int ll;
const ll N = 1e6,inf = 1e9+7;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);

    int tc;cin>>tc;
    while(tc--)
    {
        ll l,r;cin>>l>>r;
        cout<<l<< " "<<2*l<< "\n";
    }





    return 0;
}
