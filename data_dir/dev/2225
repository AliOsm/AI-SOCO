#include <bits/stdc++.h>
using namespace std;
#define pb  push_back
#define ll  long long
#define vi  vector<ll >
#define vvi vector<vi >
#define all(x) x.begin(), x.end()

int t, n;
vi  a;

int main() {
    ios_base::sync_with_stdio(false); cin.tie(0);
    cin >> t;
    while (t--) {
        cin >> n;
        a.resize(n);
        for (int i = 0; i < n; ++i)
            cin >> a[i];
        sort(all(a));
        reverse(all(a));
        for (auto x : a)
            cout << x << " ";
        cout << endl;
    }

	return 0;
}