#include <iostream>

using namespace std;

int main() {
	int n;
	cin >> n;
	while (n--) {
		int x;
		cin >> x;
		bool yes = false;
		if (x % 3 == 0)
			yes = true;
		if ((x >= 7) && (x - 7) % 3 == 0)
			yes = true;
		if ((x >= 14) && (x - 14) % 3 == 0)
			yes = true;
		cout << (yes ? "YES" : "NO") << endl;
	}
	return 0;
}
