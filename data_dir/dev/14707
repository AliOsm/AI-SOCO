#include<bits/stdc++.h>
using namespace std;
#define ll long long
void ans(ll x)
{
    cout<<x<<endl;
    return;
}
int main()
{
    ll a,b,c,d;
    cin>>a>>b>>c>>d;
    if(a!=d)
        ans(0);
    else if(a==0)
    {
        if(c==0)
            ans(1);
        else
            ans(0);
    }
    else
        ans(1);
}
