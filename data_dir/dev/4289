#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

signed main(){
  ios::sync_with_stdio( 0 );
  int N; cin >> N;
  for( int i = 1; i <= N; ++i ){
    ll cur = i == 1 ? 2LL : i - 1;
    ll nxt = 1LL * i * ( i + 1 ) * ( i + 1 );
    cout << ( nxt - cur ) << endl;
  }
  return 0;
}