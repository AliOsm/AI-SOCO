#include<bits/stdc++.h>
using namespace std;
int ara[110][110];
int bra[110][110];
int main()
{


    long long n,k;
    cin >>n>>k;
    int ara[n];
    for(int i=0;i<n;i++)
    {
        cin >>ara[i];
    }

    long long x=1;
    long long sum=0;
    int z=1;
    while(sum+z < k)
    {
         sum+=z;

        z++;

    }


    int det = abs(k-sum);



    cout <<ara[det-1] <<endl;
}
