#include <bits/stdc++.h>

using namespace std;

int main() {
    ios::sync_with_stdio(0) , cin.tie(0) , cout.tie(0);
#ifndef ONLINE_JUDGE
    freopen("input.in" , "r" , stdin);
#endif
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        cout << 1 << ' ' << n - 1<< '\n';
    }
}