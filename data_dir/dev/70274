#include<bits/stdc++.h>
#define ll long long
#define ld long double
#define pb push_back
#define x first
#define y second
#define fastread ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
#define PI (atan(1)*4)
#define mp make_pair
using namespace std;

int main()
{
	fastread;
	ll n,m;
	cin>>n>>m;
	if(n>=30){
		cout<<m;
		return 0;
	}
	ll val=(1LL)<<n;
	cout<<m%val;
	return 0;
}