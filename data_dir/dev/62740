#include<bits/stdc++.h>
using namespace std;
int n, m,x ,y, z;
string str[2000];
string xxx;

int main()
{
    cin >> n;

    for(int i = 1; i <= n; i++){
        cin >> str[i];
        bool flg = false;

        for(int j = 1; j < i; j++){
            if(str[i] == str[j]) flg = true;
        }

        if(flg) cout << "YES" << endl;
        else cout << "NO" << endl;
    }


    return 0;
}
