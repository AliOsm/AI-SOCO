// In The Name Of God

#include<bits/stdc++.h>

using namespace std;

int main()
{
	int n;
	cin>>n;
	int x,s=0;
	for(int i=1;i<=n;i++) cin>>x,s+=x;
	cin>>n;
	int y;
	for(int i=1;i<=n;i++)
	{
		cin>>x>>y;
		if(s<=y)
		{
			cout<<max(s,x);
			return 0;
		}
	}
	cout<<-1;
	
	return 0;
}
