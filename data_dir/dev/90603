lli modexp(lli a,lli n)
{
    lli r=1;
    while(n){
        if(n&1)
            r=(r*a)%mod;
        a=(a*a)%mod;
        n>>=1;
    }
    return r;
}

void solve()
{

    string a, b, s;
    cin>>a>>b;
    s = a[0];
    lli n = sz(a);
    string c;
    c = b[0];
    bool flag = 1;
    f(i, 1, n)
    {
        if(a[i] < c[0])
            s += a[i];
        else
        {
            s += c;
            flag = 0;
            break ;
        }
    }
    if(s == a && flag == 1)
        s+=c;
    cout<<s;
    return ;
}



int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);

    lli t;
//    cin>>t;
    t=1;
    while(t--)
    {
        solve();
    }
    return 0;
}
