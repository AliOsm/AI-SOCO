#define outl(n) cout<<n<<endl
#define line cout<<endl
#define bug(n) {outl(n);return;}
#define N 300005
ll n,m,a[N],q[N],s;
void solve(){
    cin>>n;
    rep(i,1,n+1)cin>>a[i],s+=a[i];
    sort(a+1,a+n+1);
    cin>>m;
    rep(i,1,m+1){
        cin>>q[i];
        outl(s-a[n-q[i]+1]);
    }
}
void prep(){

}
int main(){
    io;
    ll t=1;
    // cin>>t;
    prep();
    fix(12);
    while(t--)
        solve();
    return 0;
}
