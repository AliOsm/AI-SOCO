#include <stdio.h>
#include <iostream>
#include <vector>
#include <map>
#include <algorithm>
#include <assert.h>
#include <queue>
#include <string.h>
#include <string>
#include <set>
#include <memory.h>
#include <functional>
#include <bitset>
using namespace std;
#define ll long long


void solve() {
    int w,h;
    scanf("%d %d",&w,&h);
    int ret = 1;
    const int mod = 998244353 ;
    for(int i=0;i<w+h;++i) {
        ret = (ll)ret*2%mod;
    }
    cout<<ret<<endl;
}

int main() {
    //freopen("/Users/gotop/Documents/ps_practice/ps_practice/input.txt", "r", stdin);
    solve();
}
