#include <bits/stdc++.h>
using namespace std;

int main(){
	
    int t; cin >> t;
    while(t--){
        int n, k; cin >> n >> k;
        string res = "";
        int pos = 0;
        while(pos < n){
            for(int i = 0; i < k and pos < n; i++, pos++)
                res += ('a'+i);
        }
        cout << res << endl;
    }

	return 0;
}
