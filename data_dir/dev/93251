#include<bits/stdc++.h>
#define int long long

using namespace std;

signed main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);

    int T;
    cin >> T;
    while (T--)
    {
        int n, t, s = 0, x = 0;
        cin >> n;
        while (n--)
            cin >> t, s += t, x ^= t;

        cout << "2\n" << x << ' ' << s + x << '\n';
    }

    return 0;
}

