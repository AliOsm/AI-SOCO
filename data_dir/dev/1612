#include <bits/stdc++.h>
using namespace std;
int main()
{
	int n, k;
	cin>>n>>k;
	int D[n][n];
	for (int i=0; i<n; i++)
	{
		for (int j=0; j<n; j++)
		{
			D[i][j]=0;
		}
		D[i][i]=k;
	}
	for (int i=0; i<n; i++)
	{
		for (int j=0; j<n; j++)
		{
			cout<<D[i][j]<<" ";
		}
		cout<<endl;
	}
}