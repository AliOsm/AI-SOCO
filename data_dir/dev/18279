#include<bits/stdc++.h>
using namespace std;

#define scl(x) scanf("%lld",&x)
#define sc(x)  scanf("%d",&x)
#define ll long long
#define lop(i,n) for(int i=0;i<n;++i)
typedef pair<int, int> ii;
typedef pair<ll, ll> pll;

int main(){
#ifndef ONLINE_JUDGE
	freopen("i.txt","r",stdin);
#endif
	string table,hand;
	cin>>table;
	lop(i,5){
		cin>>hand;
		if(table[0]==hand[0])
			return puts("YES");
		if(table[1]==hand[1])
			return puts("YES");
	}
	puts("NO");

}