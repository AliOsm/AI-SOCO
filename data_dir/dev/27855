#include <bits/stdc++.h>
using namespace std;

typedef long long Long;

const int N = 1e5 + 5;

int q, l1, r1, l2, r2;

int main() {
#ifndef ONLINE_JUDGE
	freopen("in.txt", "r", stdin);
#endif

	cin >> q;
	while (q--) {
		cin >> l1 >> r1 >> l2 >> r2;

		if (l2 < l1) {
			cout << r1 << ' ' << l2 << endl;
		} else {
			cout << l1 << ' ' << r2 << endl;
		}
	}
	return 0;
}
