#include <bits/stdc++.h>
#define endl '\n'
using namespace std;
typedef long long ll;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll a, b;
    cin >> a >> b;
    while(true) {
        if(a == 0 || b == 0) break;
        if(a >= 2 * b) a %= 2 * b;
        else if(b >= 2 * a) b %= 2 * a;
        else break;
    }
    cout << a << " " << b << endl;
    return 0;
}
