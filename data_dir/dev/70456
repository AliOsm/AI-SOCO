#include <bits/stdc++.h>
 
using namespace std;
 
#define pb push_back
#define mp make_pair
#define fir first
#define sec second
#define ll long long
#define ull unsigned long long
 
//int md=998244353,inf=-1e9-7;
ll md=1e9+7;

int main()
{
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    int q;
    cin>>q;
    while (q--)
    {
        int l,r,d;
        cin>>l>>r>>d;
        int ans=r/d+1;
        if (d<l) cout<<d<<endl;
        else cout<<ans*d<<endl;
    }
}