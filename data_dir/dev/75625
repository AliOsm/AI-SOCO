#include<bits/stdc++.h>
using namespace std;
int main()
{
long double a,b,x,y,zz,qq;
cin>>a>>b;
if(a<b)
{
    cout<<-1;
}
else{
    x=(a/b-1)/2;

    x=floor(x);

    zz = a/((2*x)+1);

    qq = a/((2*x)+2) - (b*(2*x+1))/((2*x)+2);

    cout<<fixed<<setprecision(10)<<min(zz,qq+b);


}

}
