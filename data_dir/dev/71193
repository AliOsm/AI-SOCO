#define TIME ios_base::sync_with_stdio(NULL);
#define tcn cin.tie(NULL);
#define tct cout.tie(NULL);
using namespace std;
ll n , m , cnt , k, ans,mx,a,b, c, d, x, y;
string s,s1,s2,s3;
vector<string>v,v1;
int main(){
    TIME tcn tct
  cin >> n ;
  ll arr[n];
  for( int i=0;i < n;i ++ )
    cin >> arr[i];
    for( int i=0;i < n;i ++ ){
        cnt=0;
        for( int j=0;j < n;j ++ ){
            if( arr[i]<arr[j] )
                cnt++;
        }
        cout<<cnt+1<<" ";
    }
}
