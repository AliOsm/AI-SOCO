#include <bits/stdc++.h>
using namespace std;

#define all(x) begin(x), end(x)
#define sz(x) (int)(x).size()

using ll = long long;
using ld = long double;

ld sq(ld x) {
	return x * x;
}

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr);
	ld H, L;
	cin >> H >> L;
	cout << fixed << setprecision(20) << (sq(L)-sq(H))/(2*H) << endl;
}
