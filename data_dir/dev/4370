int main()
{
    //freopen("a.in", "r", stdin);
    //freopen("a.out", "w", stdout);
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.precision(20);
    //ll TL = 10.95 * CLOCKS_PER_SEC;
    //clock_t time = clock();
    int T;
    cin >> T;
    while (T--) {
        int n;
        cin >> n;
        ll sum = 0, x = 0;
        rep(i, 0, n) {
            int v;
            cin >> v;
            sum += v;
            x ^= v;
        }
        cout << 2 << "\n";
        cout << x << " " << sum + x << "\n";
    }
    return 0;
}