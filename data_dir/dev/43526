#include <bits/stdc++.h>
using namespace std;

using ll = long long;
const int sz = 1e5 + 10;

int main() {
    int n, pos;
    while( cin >> n ) {
        pos = 0;
        for( int i=1; i<n; i++ ) {
            pos += i;
            pos %= n;
            cout << ( pos + 1 ) << " ";
        }
        cout << "\n";
    }
    return 0;
}