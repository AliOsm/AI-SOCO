#include <iostream>
#include <cmath>
#include <map>
using namespace std;

int main() {
  int i,n,j;
  cin>>n;
  n*=2;
  i=1;
  while (1) {
      j=n-i*(i+1);
      j=int(sqrt(j));
      if (i>j) {cout<<"NO";return(0);}
      if (i*(i+1)+j*(j+1)==n) break; 
    //  cout<<i<<' '<<j<<endl;     
      i++;
      }
  cout<<"YES";
  //system("pause"); 
}
