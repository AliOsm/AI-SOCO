#include <bits/stdc++.h>

using namespace std;
set<long long>s;
long long n,m,i,l,k,j;
int main()
{
    cin>>n>>m;
    for(i=1;i<=n;i++){
        cin>>l;
        for(j=1;j<=l;j++){
            cin>>k;
            s.insert(k);
        }
    }
    if(s.size()==m){
        cout<<"YES"<<endl;
    }
    else{
        cout<<"NO"<<endl;
    }
    return 0;
}
