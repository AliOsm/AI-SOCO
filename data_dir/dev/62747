#include<iostream>
#include<math.h>
#include<algorithm>
#include<bits/stdc++.h>
using namespace std;


int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.precision(20);
    int n;
    cin>>n;
    int a[n],m=1000;
    for(int i=0;i<n;i++)
     {cin>>a[i];
     m=min(m,a[i]);}
     cout<<2+(a[2]^m);}
