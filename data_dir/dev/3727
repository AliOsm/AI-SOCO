#include <cstdio>
#include <cmath>
#include <vector>
#include <set>
#include <algorithm>
#include <stack>
#include <utility>

typedef long long ll;
typedef std::vector< int > vi;
  
int main()
{
    
#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
#endif 
    
    int n;
    scanf("%d", &n);
    
    int s = 0 ;
    for(int i = 0; i < n; ++i){
        int x;
        scanf("%d", &x ) ;
        
        s += x > 0 ? x : -x;
    }
    
    printf("%d\n", s ) ;
    
    return 0;
}