#include<bits/stdc++.h>
#define MAX 300005
#define INF 1e9
using namespace std;
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
typedef long long i64;

i64 a, b, ans, ansid;
i64 mcm( i64 a , i64 b ) { return a / __gcd( a , b ) * b; }

void check( i64 x )
{
    i64 k = x - ( a % x );
    i64 tmp = mcm( a + k , b + k );
    if( tmp < ans )
    {
        ans = tmp;
        ansid = k;
    }
}

int main()
{
    optimiza_io 
    cin >> a >> b;
    if( a > b )
        swap( a , b );
    ans = mcm( a , b );
    i64 x = b - a;
    for( i64 i = 1; i * i <= x; i ++ )
        if( x % i == 0 )
        {
            check( i );
            check( x / i );
        }
    cout << ansid;
    return 0;
}