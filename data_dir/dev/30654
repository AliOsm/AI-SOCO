#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
#define se second
#define fi first
#define pb push_back
const int N = 5e5;

int main() {

//	freopen("in.txt", "r", stdin);

	int n;
	cin >> n;
	long long ans = 0;
	string s;
	cin >> s;
	for (int i = 0; i < n; i++) {
		if (((s[i] - '0') & 1) == 0) {
			ans += i + 1;
		}
	}
	cout << ans << endl;

	return 0;
}
