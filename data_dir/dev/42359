#include <iostream>
#include <stdio.h>
#include<bits/stdc++.h>
#define ll long long
#define read freopen("input","r",stdin)
#define write freopen("output","w",stdout)
#define fr first
#define sc second
#define ii pair < int , int >
using namespace std;
int main()
{
 int n;
 cin>>n;
 int a = 0 , b = 0;
 string s;
 cin>>s;
 for(int i=0;i<s.length();i++)
 {
     if(s[i] == 'A')
        a++;
     else
        b++;
 }
 if(a > b)
    cout<<"Anton";
 else if(b > a)
 {
     cout<<"Danik";
 }
 else
    cout<<"Friendship";
}
