#include<bits/stdc++.h>
using namespace std;

typedef long long int ll;
typedef long double ld;

#define FAST ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define N 300010

int main(){
    
    FAST;
   
    ll n,count=0;
    cin>>n;
    
    for(int i=0;i<31;i++)
    {
        if(n & (1<<i) )
            count++;
    }
    
    cout<<count;
    
}