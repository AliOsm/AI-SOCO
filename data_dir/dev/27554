using namespace std;

typedef long long ll;

int main(int argc, char **argv) {
//	freopen("in.txt", "r", stdin);
	int n;
	scanf("%d", &n);
	string s = "";
	for (int i = 1; s.length() <= n; ++i) {
		s += to_string(i);
	}
	printf("%c\n", s[n - 1]);
	return 0;
}