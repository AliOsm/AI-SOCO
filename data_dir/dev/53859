#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

int main(){

int a,b;
scanf("%d:%d",&a,&b);
int i=0;
while( 1 ){

    if( a/10 == b%10 && a%10 == b/10 ){
        cout<<i<<endl;
        return 0;
    }
    b++;
    a+=b/60;
    b%=60;
    a%=24;
    i++;
}


return 0;
}
