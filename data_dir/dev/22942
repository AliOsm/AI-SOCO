#include <bits/stdc++.h>
#define MAX (ll)(2e5 + 7)
#define INF (ll)(1e18)
#define MOD (ll)(998244353)

using namespace std;
using ll = long long;

int main () {
    ll t;
    cin >> t;
    while (t--) {
        ll n, m;
        cin >> n >> m;
        if ((m - 1) * n + (n - 1) * m > n * m) {
            cout << "NO\n";
        }
        else {
            cout << "YES\n";
        }
    }
}
