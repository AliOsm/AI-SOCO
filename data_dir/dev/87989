#include <iostream>
#include <vector>
#include <string>
#include <cmath>
#include <algorithm>

using namespace std;

typedef long long ll;
typedef vector<int> vi;
typedef vector<ll> vl;
typedef pair<int, int> ii;

void sort_vi(vi & a) {
    sort(a.begin(), a.end());
}

void set_precision(int digits) {
    cout.precision(digits);
    cout.setf(ios::fixed);
}

template <class T = int>
T r() {
    T x;
    cin >> x;
    return x;
}

ll rl() {
    return r<ll>();
}

string rs() {
    return r<string>();
}

vi rvi(int n) {
    vi a(n, 0);
    for (int i = 0; i < n; ++i) a[i] = r();
    return a;
}

vi rsvi(int n) {
    vi a = rvi(n);
    sort_vi(a);
    return a;
}

void yn(bool can) {
    cout << (can ? "YES" : "NO") << endl;
}

int main() {
    string s = rs();
    int n = s.length();
    
    int answer = n * 25 + 26;
    cout << answer << endl;
    
    return 0;
}