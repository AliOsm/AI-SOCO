#include <utility>
#include <ctime>
using namespace std;
typedef long long LL;
typedef unsigned long long ULL;
#define MEM(a,b) memset((a),(b),sizeof(a))
const LL INF = 1e9 + 7;
const int N = 5e2 + 10;

int main()
{
	//freopen("input.txt", "r", stdin);
	//freopen("output.txt", "w", stdout);
	int x;
	map<int, int> mc;
	cin >> x;
	while (!mc.count(x))
	{
		mc[x] = 1;
		x++;
		while (x % 10 == 0) x /= 10;
	}
	cout << mc.size() << endl;
	return 0;
}