#include <bits/stdc++.h>
using namespace std;

void solve() {
    int n;
    scanf("%d", &n);
    string ans = "2";
    if(n==1) {
        printf("-1\n");
    } else {
        for(int i = 1; i < n; i++) {
            ans += "3";
        }
        for(int i = 0; i < n; i++) {
            printf("%c", ans[i]);
        }
        printf("\n");
    }
}

int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    // freopen("input.txt","r",stdin);
    // freopen("output.txt","w",stdout);
    int t;
    scanf("%d", &t);
    while (t--)
    {
        /* code */
        solve();
    }
    
    return 0;
}