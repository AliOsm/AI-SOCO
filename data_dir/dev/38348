#include<bits/stdc++.h>
#define ll long long
using namespace std;
const int N = 1e5 + 5;
int main()
{
    ios_base ::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
    string s;
    cin >> s;
    int n = s.length();
    cout << s;
    reverse(s.begin(), s.end());
    cout << s;
return 0;
}
