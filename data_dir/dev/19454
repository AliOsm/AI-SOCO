using namespace std;

#define alphabet_size 26
#define mod 1000000007
#define INF (ll)1e18
#define MAXN 100001

int main()
{
	ios_base::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	//freopen("In.txt", "r", stdin);

	int a, b, c, d;
	cin >> a >> b >> c >> d;

	int r1 = 0, r2 = 0;

	r1 = max(3 * a / 10, a - (c*a) / 250);
	r2 = max(3 * b / 10, b - (d*b) / 250);

	if (r1 == r2)
		cout << "Tie";
	else
		if (r1 > r2)
			cout << "Misha";
		else
			cout << "Vasya";

	return 0;
}