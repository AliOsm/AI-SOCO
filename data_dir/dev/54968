bool dp[20][1000];
int main(){
    cin>>n;
    dp[0][0]=1;
    rep(i,1,n){
        cin>>x;
        rep(j,0,359){
            dp[i][j] = dp[i-1][(j+x)%360] == 1 || dp[i-1][(j+360-x)%360] == 1; 
        }
    }
    if(dp[n][0])cout<<"YES";
    else cout<<"NO";
}