#include<bits/stdc++.h>
using namespace std;
#define ll  long long
#define endl '\n'
const ll mod = 1000000007;
const double PI = 3.141592653589793238463;
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
    int r1,r2,r3,c1,c2,c3,v1,v2,v3;
    cin>>r1>>r2>>r3>>c1>>c2>>c3>>v1>>v2>>v3;
    int sum=(r2+r3+c1+c3+v1+v2)/2;
    cout<<sum-r2-r3<<" "<<r2<<" "<<r3<<"\n"<<c1<<" "<<sum-c1-c3<<" "<<c3<<"\n"<<v1<<" "<<v2<<" "<<sum-v1-v2;
    return 0;
}
