#include <bits/stdc++.h>

using namespace std;

int main()
{
    int tc;
    cin >> tc;
    for (int j=0;j<tc;j++){
        int n,tut=0;
        cin >> n;
        int lim = n/7;
        for (int i=0;i<=lim;i++){
            int a=n-(7*i);
            if (a%3==0){
                tut=1;
                break;
            }
        }
        cout << (tut ? "YES":"NO") << endl;
    }

    return 0;
}
