#include<bits/stdc++.h>
using namespace std;
int a,s,d,f,g,h,j,k,l,i,n,m;
string x;
vector<int> v;
main(){
    cin>>n;
    while(k<n){
        a+=5;
        i=a;
        while(i%5==0){
            k++;
            i/=5;
        }
    }
    if(k==n) {cout<<5<<endl;
    for(i=0;i<5;i++){
        cout<<a+i<<" ";
    }} else cout<<0;
}
