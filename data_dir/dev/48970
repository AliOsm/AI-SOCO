const int infi = 1e9 + 7;
const ll infl = 1e18;
const long double eps = 1e-7;
const long double eps1 = 1e-10;
const int mod = (1e9 + 7);
const ll mod1 = 1e9 + 7;
const ll mod2 = 1e9 + 9;
const ll p1 = 29;
const ll p2 = 31;
const int ver1 = (62 * 62);

ll f[10];

int main() {
   // freopen("input.txt", "r", stdin); freopen("output.txt", "w", stdout);
    //freopen("vase.in", "r", stdin); freopen("vase.out", "w", stdout);
    int n, l, r;
    cin >> n;
    l = r = 0;
    for (int i = 0; i < n; ++i) {
        int a, b;
        cin >> a >> b;
        l += a, r += b;
    }
    l = min(l, n - l);
    r = min(r, n - r);
    cout << l + r;

    return 0;
}