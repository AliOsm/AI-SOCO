#include <bits/stdc++.h>
#include <stdio.h>

#define fi first
#define se second

using namespace std;

typedef long long ll;

const ll N = 1e6 + 7;
const ll INF = 1e9 + 7;

const int xi[] = {-1, +1, 0, 0};
const int yi[] = {0, 0, -1, +1};
const int p = 31;

int n, a[N];

int main(){

  for(int i = 1; i <= 4; i++){
    cin >> a[i];
  }

  if(a[3] && !a[1] && !a[4]){
    cout << 0;
    return 0;
  }

  if(a[1] != a[4]){
    cout << 0;
    return 0;
  }

  cout << 1;
}
