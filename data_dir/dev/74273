#include<iostream>
#include<math.h>
#include<algorithm>
#include<bits/stdc++.h>
using namespace std;
int main(){
  int n;
   cin>>n;
 while(n--)
{
  long long int l,r,d,p=0;
  cin>>l>>r>>d;
  if((l-1)/d==0){
     p=ceil((1.0*(r+1))/d)*d;
     cout<<p<<endl;}
  else
  {
     cout<<d<<endl;
  }}}