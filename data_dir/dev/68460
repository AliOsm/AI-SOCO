#include <iostream>

using namespace std;

int main() {
	int n, t;

	ios::sync_with_stdio(0);
	cin >> n >> t;
	if (t == 10) {
		if (n == 1)
			cout << "-1" << endl;
		else {
			for (int i = 1; i < n; i++)
				cout << "1";
			cout << "0" << endl;
		}
	}
	else {
		for (int i = 0; i < n; i++)
			cout << t;
		cout << endl;
	}
	
	return 0;
}