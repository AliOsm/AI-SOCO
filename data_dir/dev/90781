#include <bits/stdc++.h>
typedef long long ll ;
using namespace std;


int main() {
    ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);

    int a, b ;

    cin >> a >> b;

    if(a == 9 && b == 1){
        cout << a * 10 + 9 << " " << b * 100 << endl ;
    }
    else if(a <= b && b - a <= 1){
         if(a == b) cout << a * 10 << " " << b * 10 + 1 << endl ;
         else cout << a * 10 + 9 << " " << b * 10 << endl ;
    }
    else cout << -1 << endl ;

    return 0;
}
