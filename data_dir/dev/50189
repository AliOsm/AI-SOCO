#include <bits/stdc++.h>
using namespace std;
#define ll long long

void solve()
{
    string a, b, c;
    cin>>a>>b>>c;
    bool good = true;
    for(ll i=0; i<a.size(); i++)
    {
        bool cur = false;
        // swap a[i] and c[i]
        if (b[i] == c[i]) cur = true;
        if (a[i] == c[i]) cur = true;
        if (!cur)
        {
            cout<<"NO"<<endl;
            return;
        }

    }
    cout<<"YES"<<endl;



}

int main()
{


    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    ll n;
    cin>>n;
    while (n--)
        solve();
    return 0;
}
