#include<bits/stdc++.h>
#define ll long long int
#define r_0 return 0
#define fr first

#define sc second
#define pb push_back
#define sz() size()
#define TIME ios_base::sync_with_stdio(0)
using namespace std;
ll n,m,l,cnt,ans,one=1,mx,mn;
int sum;
string s,s1,s2;
char c;
int a[10000001];
float f,yak=1;
int main()
{
    TIME;
    cin >> n >> m;
    sum = n;
    for( int i = 0 ; n <= m ; i ++ , n *= sum )
    {
        if( n == m )
        {
            cout << "YES" << endl;
            cout << i;
            r_0;
        }
    }
    cout << "NO";
}















