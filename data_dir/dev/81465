#include <bits/stdc++.h>



using namespace std;



typedef long long LL;


int n,ans;
string s;

int main()

{
	cin>>n;
	ans=0;
	while (n--)
	{	
		cin>>s;
		if (s[0]=='T') ans+=4;
		else if (s[0]=='C') ans+=6;
		else if (s[0]=='O') ans+=8;
		else if (s[0]=='D') ans+=12;
		else if (s[0]=='I') ans+=20;
	}
	cout<<ans<<endl;

	return 0;

}