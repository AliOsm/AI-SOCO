#include<iostream>
using namespace std;
int main(){
    string x;
    char a,b,c;
    cin>>a>>x>>b>>x>>c>>x;
    if(a=='p' && b==c && c=='r')cout<<'F';
    else if(a=='r' && b==c && c=='s')cout<<'F';
    else if(a=='s' && b==c && c=='p')cout<<'F';
    else if(b=='r' && a==c && c=='s')cout<<'M';
    else if(b=='p' && a==c && c=='r')cout<<'M';
    else if(b=='s' && a==c && c=='p')cout<<'M';
    else if(c=='r' && a==b && b=='s')cout<<'S';
    else if(c=='p' && a==b && b=='r')cout<<'S';
    else if(c=='s' && a==b && b=='p')cout<<'S';
    else cout<<'?';
}