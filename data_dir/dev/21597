#include <bits/stdc++.h>

using namespace std;

int main()
{
    //freopen("in.txt", "r", stdin);
    //freopen("out.txt", "w", stdout);
    int a, b, c;
    scanf("%d %d %d", &a, &b, &c);
    int n;
    scanf("%d", &n);
    int res = 0;
    for(int i=0; i<n; i++){
        int x;
        scanf("%d", &x);
        if(x > b && x < c) res++;
    }
    printf("%d", res);
}
