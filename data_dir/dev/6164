#include <bits/stdc++.h>

#define Vanya Unstoppable

#define int long long

#define double long double

using namespace std;

signed main(){
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr); cout.tie(nullptr);

    string a;
    cin >> a;

    int pos = 0;
    for(int i = 0; i < a.size(); ++i){
        if(a[i] == '0'){
            pos = i; break;
        }
    }

    for(int i = 0; i < a.size(); ++i){
        if(i == pos) continue;
        cout << a[i];
    }
}
