// getline (std::cin,name);
ll MOD=1e9+7;
ll ceil1(ll n,ll x);
ll gcd(ll a,ll b){return __gcd(a,b);}
ll lcm(ll a,ll b){return (max(a,b)/gcd(a,b))*min(a,b);}
ll pow1(ll n,ll m,ll mod);
ll pow2(ll n,ll k);
ll modinv(ll n,ll mod){  return pow1(n,mod-2,mod);}
bool func(pair<ll,ll> &a,pair<ll,ll> &b ){
        if(a.ff != b.ff)return a.ff < b.ff;
        return a.ss > b.ss;
}

struct BIT{
        ll n;
        vll a;
        BIT(ll n1){
                n=n1;
                a.assign(n+1,0);
        }
        void update(ll i,ll val){
                for(;i<=n;i+=(i&(-i))){
                        a[i]+=val;
                }
        }
        ll getsum(ll i){
                ll sum=0;
                for(;i>0;i-=(i&(-i))){
                        sum+=a[i];
                }
                return sum;
        }
        ll sum(ll l,ll r){
                return getsum(r)-getsum(l-1);
        }
              
} ;
void solve(){
        ll i,j,k,l,n,m,x,y,z,r;
        
        cin>>n>>m;
        
        BIT BT(n);
        for(i=1;i<=n;i++){
                cin>>x;
                BT.update(x,1);
        }
        
        while(m--){
                cin>>x;
                if(x<0){
                        x=-x;
                        l=1,r=n;
                        ll mid;
                        while(l<=r){
                                ll mid=(l+r)/2;
                                if(BT.getsum(mid)>=x){
                                        r=mid-1;
                                }else{
                                        l=mid+1;
                                }
                        }
                        BT.update(l,-1);
                }else{
                        BT.update(x,1);
                }
        }
        
        for(i=1;i<=n;i++){
                if(BT.getsum(i)){
                        cout<<i<<endl;
                        return;
                }
        }
        cout<<0<<endl;
        return;
        
        
        
}
int main(){
        FS;
        ll i,j,k,n,m,x,y,z,q;
        q=1;
        // cin>>q;
        for(i=1;i<=q;i++){
                solve();
        }
        return 0;
}
ll ceil1(ll n,ll x){
        return (n-1)/x+(n>0);
}
ll pow1(ll n,ll m,ll mod){
        if(m==0)return 1;
        
        if(m%2==0)return pow1((n*n)%mod,m/2,mod);
    	
    	return (pow1((n*n)%mod,m/2,mod)*n)%mod;
}
ll pow2(ll n,ll k){
        ll ans=1;
            while(k>0){
                if(k%2==1)ans=ans*n;
                n=n*n;
                k/=2;
            }
            return ans;
}