#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef long double ld;
const int infint = 1e9;
const ll inf = 1e18;
const int MOD = (int)1e9 + 7;
const int MAXN = (int)1e5 + 7;
int main()
{
	ios::sync_with_stdio(false);
	cin.tie(0); cout.tie(0);
	ll n, k;
	cin >> n >> k;
	ll T1 = (n * 2) / k + ((n * 2) % k > 0);
	ll T2 = (n * 5) / k + ((n * 5) % k > 0);
	ll T3 = (n * 8) / k + ((n * 8) % k > 0);
	cout << T1 + T2 + T3;
}
