#include <iostream>
#include <vector>
#include <algorithm>
#include <map>
#include <set>
#include <cmath>

using namespace std;

int main()
{
    int n;
    cin >> n;
    int l=0;
    int r=0;
    while(n--)
    {
        int x,y;
        cin >> x >> y;
        if (x>0)
        ++l;
        else ++r;
    }
    if (l>1&&r>1)
    {
        cout << "No";
    }else cout << "Yes";
    return 0;
}
