#include<bits/stdc++.h>
using namespace std;

bool cck(int x, int y)
{
    if(x == 1) return y == 1;
    if(x <= 3) return y <= 3;
    return true;
}

int main()
{
    int t;
    scanf("%d", &t);

    while(t--)
    {
        int x, y;
        scanf("%d %d", &x, &y);

        printf("%s\n", cck(x, y)? "YES": "NO");
    }

    return 0;
}
