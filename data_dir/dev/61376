int main()
{
    //freopen("input.txt","r",stdin);
    //freopen("num.txt","w",stdout);
    int x,y,z;
    sc(x); sc(y); sc(z);
    if(z && x+z >= y && y+z >= x ) puts("?");
    else if(x==y) puts("0");
    else if(x>y) puts("+");
    else puts("-");


    return 0;
}
