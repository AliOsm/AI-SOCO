#include <bits/stdc++.h>
 
using namespace std;
 
#define pb push_back
#define mp make_pair
#define fir first
#define sec second
#define ll long long
#define ull unsigned long long
 
//int md=998244353,inf=-1e9-7;
ll md=998244353;

int main()
{
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    int a,b,c,n;
    cin>>a>>b>>c>>n;
    int x=n-(a+b-c);
    if (a>n||b>n||c>n||a+b-c<0||n-(a+b-c)<1||a+x>n||b+x>n||c+x>n) {cout<<-1; return 0;}
    cout<<n-(a+b-c);
    return 0;
}