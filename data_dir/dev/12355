#include <bits/stdc++.h>
#define ToyotaLandCruiser100 ios_base::sync_with_stdio; cin.tie(0); cout.tie(0);
#define tormoz return 0;

using namespace std;

typedef long long ll;
typedef long double ld;

const int N = 1e5 + 7;
const ld pi = 3.14159265358979323846;
const ll INF = 1e9 + 1;

int n;

string s1, s2;

int main(){
     cin >> n >> s1 >> s2;
     if(s2 == "week"){
          if(n == 1 || n == 7 || n == 2 || n == 3 || n == 4){
               cout << 52;
               return 0;
          }
          else if(n == 5 || n == 6){
               cout << 53;
               return 0;
          }
     }
     else{
          if(n <= 29){
               cout << 12;
               return 0;
          }
          else if(n == 30){
               cout << 11;
               return 0;
          }
     }
     cout << 7;
     return 0;
}