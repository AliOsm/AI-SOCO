#include <bits/stdc++.h>
using namespace std;
#define int long long
#define endl '\n'
typedef long long ll;

signed main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int b, g, n;
    cin >> b >> g >> n;
    int res = 0;
    for(int i = 0; i <= n; ++i) {
        if(i <= b && n-i <= g) ++res;
    }
    cout << res;
}
