#define int long long
#define f(i,n) for(int i=0;i<n;i++)
#define rep(i,a,b) for(int i=a;i<=b;i++)

class ASerejaAndMugs {
public:
	void solve(std::istream& in, std::ostream& out) {
        int n, s; in>>n>>s;
        int arr[n];
        f(i, n) in>>arr[i];
        sort(arr, arr+n);
        int res = 0;
        f(i, n-1) res += arr[i];
        if(res > s) out<<"NO";
        else out<<"YES";
	}
};


