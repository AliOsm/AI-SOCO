#include<bits/stdc++.h>
#define ll long long
#define f first
#define s second
#define mp make_pair
#define bp push_back
const int mx=1e5+10;
using namespace std;
ll fact(ll x){
	if(x==0||x==1)
	return 1;
	else return x*fact(x-1);
}
main(){
ios::sync_with_stdio(0);
cin.tie(0);
cout.tie(0);
ll a,b,tmp,ans;
cin>>a>>b;
tmp=min(a,b);
ans=fact(tmp);
cout<<ans;
}
