int res, cur, a[101010];
string s;

inline void Solve () {
	// easy
	int k1, k2, k3;
	cin >> k1 >> k2 >> k3;

	if (k1 == 1) res ++;
	else if (k1 == 2) cur ++;
	else if(k1 == 3) cnt ++;
	else if(k1 == 4) kk ++;

	if (k2 == 1) res ++;
	else if (k2 == 2) cur ++;
	else if(k2 == 3) cnt ++;
	else if(k2 == 4) kk ++;

	if (k3 == 1) res ++;
	else if (k3 == 2) cur ++;
	else if(k3 == 3) cnt ++;
	else if(k3 == 4) kk ++;

	if(res >= 1 || cur >= 2 || cnt >= 3) {
		cout << "YES";
		exit(0);
	}
	
	if(cur == 1 && kk == 2) {
	    cout << "YES";
		exit(0);
	}

	cout << "NO";
}

int main () {
	SpeedForce;
//	freopen(".in", "r", stdin);
//	freopen(".out", "w", stdout);	
//	cin >> tests;
	while(tests --) {
		Solve ();
		// Ee Zadrot
	}	
}