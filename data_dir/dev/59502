#pragma GCC optimize ("O3")
#include <bits/stdc++.h>
#define abdelrahman001 ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
typedef long long ll;
using namespace std;

int main()
{
	abdelrahman001
	ll t,h,m;
	cin >> t;
	while(t--)
	{
		cin >> h >> m;
		ll ans = 60 - m;
		h++;
		ans+=(24 - h)*60;
		cout << ans << '\n';
	}
	return 0;
}
