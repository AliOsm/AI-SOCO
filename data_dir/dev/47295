# include <stdio.h>
# include <math.h>
# include <string.h>

const int N = 111;

int n, m, k, i, a, b;
char s [N], si;

void Print ();

int main ()
{
   freopen ("input.txt", "rt", stdin);
   freopen ("output.txt", "wt", stdout);
# ifndef ONLINE_JUDGE
# endif

   scanf ("%s\n%d", &s, &n);

   if ( s [0] == 'f' && n == 1 )
      printf ("L");
   else if ( s [0] == 'f' && n == 2 )
      printf ("R");
   else if ( s [0] == 'b' && n == 2 )
      printf ("L");
   else
      printf ("R");

   return 0;
}

