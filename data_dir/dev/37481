#include <bits/stdc++.h>

    using namespace std;
    typedef long long ll;
    typedef vector<ll> vl ;

    #define mp make_pair
    #define pb push_back
    #define f first
    #define s second
    #define all(v) (v).begin(),(v).end()


    const int MOD = 1000000007;
    const int N = 1000005;
    const double PI =4*atan(1);
    const double eps = 1e-7;
    
    string ch;
    ll n;
    ll dp[30];

    int main(){
        ios::sync_with_stdio(0);
        //freopen("easy.txt","r",stdin);
        cin >> n;
        cin >> ch;
        ll x=n-11;
        ll u=0,v=0;
        for(int i=0;i<=x;i++){
            if(ch[i]=='8')u++;
            else v++;
        }
        if(u>v){
            cout << "YES";
        }else{
            cout << "NO";
        }
        return 0;
    }
        