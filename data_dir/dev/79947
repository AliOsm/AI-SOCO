#include <bits/stdc++.h>
#define fi first
#define se second
#define mp make_pair
#define inf 1000000000
#define md 1000000007
#define pb push_back
#define li 100005
#define int long long
using namespace std;
int n,t,a,b,c;
main(){
	scanf("%lld",&t);
	while(t--){
		scanf("%lld %lld %lld",&a,&b,&c);
		int mx=max(a,max(b,c));
		int kalan=a+b+c-mx;
		if(mx<=kalan+1){
			printf("Yes\n");
		}
		else printf("No\n");
	}
	return 0;
}


