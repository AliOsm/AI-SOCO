#include <bits/stdc++.h>

using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	string s;
	cin>>s;
	string ref = "heidi";
	int j = 0;
	for (int i = 0; i < s.length(); i++) {
		if (s[i] == ref[j]) j++;
		if (j == ref.length()) {
			cout<<"YES\n";
			return 0;
		}
	}
	cout<<"NO\n";
	return 0;
}