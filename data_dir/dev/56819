#include<bits/stdc++.h>
using namespace std;

#define scl(x) scanf("%lld",&x)
#define sc(x)  scanf("%d",&x)
#define ll long long
#define lop(i,n) for(int i=0;i<n;++i)
typedef pair<int, int> ii;
typedef pair<ll, ll> pll;

int n,m;

int main(){
#ifndef ONLINE_JUDGE
	freopen("i.txt","r",stdin);
#endif
	cin>>n>>m;
	deque<int> a,b;
	a.push_front(5);
	b.push_front(5);
	int rem=n-5;
	while(rem>0)rem-=4,a.push_front(4),b.push_front(5);
	lop(i,a.size())printf("%d",a[i]);
	puts("");
	lop(i,b.size())printf("%d",b[i]);
	puts("");
}
