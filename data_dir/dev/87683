#include<bits/stdc++.h>
using namespace std;
main()
{
    int a,b;
    cin>>a>>b;
    if(a==9 && b==1)
    {
        cout<<"9 10"<<endl;
        return 0;
    }
    else if(a==b)
    {
        cout<<a<<"8"<<" "<<b<<"9"<<endl;
        return 0;
    }
    else if((b-a)==1)
    {
        cout<<a<<"9 "<<b<<"0"<<endl;
        return 0;
    }
    cout<<"-1"<<endl;
    return 0;
}
