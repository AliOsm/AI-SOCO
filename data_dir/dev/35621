#include <bits/stdc++.h>
using namespace std;

int n, k, ans = 0;
vector<vector<int> > arr;
vector<int> dp;

int main (int argc, char *argv[])
{
	scanf("%d%d", &n, &k);
	arr.resize(k);
	dp.resize(n);
	for(int i = 0; i < k; ++i) {
		arr[i].resize(n);
		for(int j = 0; j < n; ++j) {
			scanf("%d", &arr[i][j]);
			--arr[i][j];
		}
	}
	for(int i = 0; i < n; ++i) {
		int cnt[n],
			cur_ans = 0;
		memset(cnt, 0, sizeof(cnt));
		for(int j = 1; j < k; ++j) {
			for(int q = 0; arr[0][i] != arr[j][q]; ++q) {
				if(++cnt[arr[j][q]] == k - 1) {
					cur_ans = max(cur_ans, dp[arr[j][q]]);
				}
			}
		}
		dp[arr[0][i]] = cur_ans + 1;
		ans = max(cur_ans + 1, ans);
	}
	printf("%d", ans);

	return 0;
}

