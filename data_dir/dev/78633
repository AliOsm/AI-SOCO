#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

int a[105];

int main()
{
    ios_base::sync_with_stdio(0),cin.tie(0),cout.tie(0);
    int t;
    cin >>t;
    while(t--){
        int n;
        cin >> n;
        for(int i=0;i<n;i++)cin>>a[i];
        sort(a,a+n,greater<int>());
        for(int i=0;i<n;i++)cout<<a[i]<<" ";
        cout<<endl;
    }

    return 0;
}
