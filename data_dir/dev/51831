#include <bits/stdc++.h>
using namespace std;

signed main() {
  ios::sync_with_stdio(false);
  int n;
  cin >> n;
  vector<int> a(n);
  for (size_t i = 0; i < n; ++i) {
    cin >> a[i];
    if (a[i] & 1) {
      cout << "First" << endl;
      exit(0);
    }
  }
  cout << "Second" << endl;
  return 0;
}
