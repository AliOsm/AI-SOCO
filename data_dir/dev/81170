#include<bits/stdc++.h>
#define ll long long
#define f first
#define s second
#define mp make_pair
#define bp push_back
const int mx=1e5+10;
int dx[]={1,0,-1,0,-1,1,-1,1};
int dy[]={0,1,0,-1,-1,1,1,-1};
using namespace std;
int a[mx],b[mx];
main(){
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    int t;
    cin>>t;
    while(t--){
        int n,l,r,v;
        cin>>n>>v>>l>>r;
        ll ans=(n/v)-(r/v)+((l-1)/v);
        cout<<ans<<endl;
    }
}
