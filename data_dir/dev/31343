int main(){
	ll test;
	scanf("%lld",&test);
	while(test--){
		scanf("%lld%lld%lld",&a,&b,&n);
		if(n % 3 == 0){
			printf("%lld",a);
		}
		if(n % 3 == 1){
			printf("%lld",b);
		}
		if(n % 3 == 2){
			printf("%lld",a^b);
		}
		puts("");
	}
}