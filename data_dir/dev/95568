#include <cstdio>
#include <iostream>
#include <vector>
#include <algorithm>
#include <functional>
#include <set>
#include <map>
#include <string>
#include <queue>
#include <stack>
#include <cstring>
#include <iterator>
#include <cmath>
#include <unordered_set>
#include <unordered_map>
#include <bitset>
using namespace std;
typedef int mt;
#define rep(a, b, i) for(register mt i = a; i < b; ++i)
#define repe(a, b, i) for(register mt i = a; i <= b; ++i)
#define repd(a, b, i) for(register mt i = a; i > b; --i)
#define repde(a, b, i) for(register mt i = a; i >= b; --i)

#define N 200100
#define K 50
#define M ((1 << 14) + 1)
#define INF 10000000000000007// 1e16 + 7
#define MOD 1000000009  //1000000007//1e9 + 7
#define EPS 1e-9
#define MAX 1000000000000000001
//#pragma comment(linker, "/STACK:1000000000")

mt n, a, t = 1;
bool cnt[N];
mt answ;
int main(void)
{
#ifdef _DEBUG	
	freopen("in.txt", "r", stdin);
#endif
	scanf("%d", &n);
	while (n--) {
		scanf("%d", &a);
		if (cnt[a]) cnt[a] = false;
		else ++answ;
		cnt[t++] = true;
	}
	printf("%d\n", answ);
	return 0;
}