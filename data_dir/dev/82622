#include <stdio.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>
#include <string.h>

typedef long long ll;
typedef double dl;
typedef long double ldl;

#define fl for(i=0; i<n; i++)

#define fl1(a,b,i) for(i=a; i<b; i++)
#define fl2(a,b,i) for(i=a; i>=b; i--)

#define in(a) scanf("%d",&a)

#define inl(a) scanf("%lld",&a)

#define indl(a) scanf("%lf",&a)

#define ins(s) scanf("%s",s)

#define inc(a) scanf(" %c",&a)

#define gt getchar()

#define eps1 1e-9

#define eps2 1e-6

#define pi acos(-1)

#define ceel(n,m) (n+m-1)/m

#define outd(a) printf("%d\n",a)

#define outd2(a) printf("%d ",a)

#define outdcs(a) printf("Case %d: %d\n",i,a)

#define outl(a) printf("%lld\n",a)

#define outl2(a) printf("%lld ",a)

#define outlcs(a) printf("Case %lld: %d\n",i,a)

#define outdl(a) printf("%f\n",a)

#define outdl2(a) printf("%f ",a)

#define outdlcs(a) printf("Case %f: %d\n",i,a)

#define outs(s) printf("%s\n",s)

#define max(a,b) (a>b)?a:b

#define min(a,b) (a<b)?a:b

#define swap(a,b) a^=b; \
                                   b^=a; \
                                   a^=b \

void bubble(int *ara,int n)
{
    int i,j;
    fl1(0,n-1,i)
    {
        fl2(i+1,n,j)
        {
            if(ara[i]>ara[j])
            {
                swap(ara[i],ara[j]);
            }
        }
    }
    return ;
}

int main()
{
    ll n;
    inl(n);
    if(n)
    {
        switch(n%4)
        {
        case 0:
            outs("6");
            break;
        case 1:
            outs("8");
            break;
        case 2:
            outs("4");
            break;
        case 3:
            outs("2");
            break;
        }
    }
    else
    {
        outs("1");
    }
    return 0;
}



