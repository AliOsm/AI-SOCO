//Solution by Tima
#include <bits/stdc++.h>
 
#define f first
#define s second
#define ll long long
#define ull unsigned long long
#define mp make_pair
#define pb push_back
#define vi vector <int>
#define ld long double
#define pii pair<int, int>
#define y1 sda
 
using namespace std;    
const int N = int(3e5) + 12, mod = int(1e9)  + 7; 

int t, a[N], b[N],n, x,y;

int main () {
	cin >> t;
	while(t--){
		cin >> n >> x >> y;
		for(int i = 0; i < x; i++) cin >> a[i];
		for(int i = 0; i < y; i++) cin >> b[i];
		sort(a, a + x);
		sort(b, b + y);
		if(a[x - 1] > b[y - 1]) cout << "YES" << endl;
		else cout << "NO" << endl;
	}
 
return 0;
}