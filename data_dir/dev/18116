//double eps =1e-9;
void comparing_doubles(double f,double s)
{
    if (f-s>eps)
    {
        cout<<">"<<endl;
    }
    else if (f-s<-eps)
    {
        cout<<"<"<<endl;
    }
    else
    {
        cout<<"=";
    }
}
