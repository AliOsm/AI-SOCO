//ll gcd(ll a,ll b){while(b!=0){ll a2=a;a=b;b=a2%b;}return a;}

using namespace std;
ll n;
ll solve(vector<ll>m)
{
    ll cnt=0;
    for (int i=0;i<n;i++)
    {
        ll save=-1;

             for (int j=i+1;j<n;j++)
            {
                 if (m[i]==m[j])
                 {
                     save=j;break;
                 }
            }
         int index=i;
         if (save==-1){continue;}
          for (int j=save;j>i+1;j--)
          {
              swap(m[j],m[j-1]);
           cnt++;
          }

    }
    return cnt;
    }
 vector<ll>m;
int main()
{
    time;

    cin>>n;
    n*=2;
   
    for (int i=0;i<n;i++)
    {

        ll x;
        cin>>x;
        m.push_back(x);
    }
    ll mini=1e18;
    mini=min(mini,solve(m));
    reverse(m.begin(),m.end());
    mini=min(mini,solve(m));
    cout<<mini;
    return 0;
}

