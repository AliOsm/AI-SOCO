#include <bits/stdc++.h>

using namespace std;

int main()
{
    string s;
    cin >> s;
    int len = s.length(), cnt = 0, cnto = 0;
    for (int i = 0; i < len; i++)
    {
        if (s[i] == '-')
        {
            cnt++;
        }
        else
        {
            cnto++;
        }
    }
    if (cnto == 0 || cnt % cnto == 0)
    {
        printf("YES");
    }
    else
    {
        printf("NO");
    }
    return 0;
}
