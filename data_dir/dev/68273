#include <iostream>
using namespace std;


void W(int n)
{
	if(n == 5 || n == 6)
		cout<< "53\n";
	else
		cout<< "52\n";
	return;
}	

void M(int n)
{
	if(n <= 29)
		cout<< "12\n";
	if(n == 30)
		cout<< "11\n";
	if(n == 31)
		cout<< "7\n";
	if(n > 31)
		cout<< "0\n";
	return;
}
int main()
{
	int n;
	string s, t;
	cin>> n >> s >> t;
	
	if(t == "week")
		W(n);
	else
		M(n);
}