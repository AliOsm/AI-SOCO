using namespace std;
//using namespace __gnu_cxx;


int32_t main() {
    kchay;
    int t;
    cin >> t;
    while (t--) {
        int a, b, c;
        
        cin >> a >> b >> c;
        if (a > b) {
            swap(a, b);
        }
        if (a > c) {
            swap(a, c);
        }
        if (b > c) {
            swap(b, c);
        }
        
        if ((a - 1) > b + c) {
            cout << "No" << endl;
            continue;
        }
        if ((b - 1) > a + c) {
            cout << "No" << endl;
            continue;
        }
        if ((c - 1) > a + b) {
            cout << "No" << endl;
            continue;
        }
        cout << "Yes" << endl;
    }
    return 0;
}
