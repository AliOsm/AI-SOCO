#include <stack>
#include <deque>
#include <cassert>
 
  
  
using namespace std;
  


#define ll long long




int main() {
	int n;
	scanf("%d", &n);
	vector<int> a(n);
	for (int i = 0; i < n; ++i) {
		scanf("%d", &a[i]);
	}
	sort(a.begin(), a.end());
	cout << (a[0] + a[n - 1]) / 2 << endl;
	return 0;
}







