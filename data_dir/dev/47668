//let's do it
 #include <iostream>
#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define inf 1000000000000
#define mod 1000000007
#define pb push_back
//#define mp make_pair
#define all(v) v.begin(),v.end()
#define S second
#define F first
#define boost1 ios::sync_with_stdio(false);
#define boost2 cin.tie(0);
#define mem(a,val) memset(a,val,sizeof a)
#define endl "\n"
#define maxn 100001

map<string,string>mp;
int main()
{
    boost1;boost2;
    int n;
    string s;
    
    mp["purple"]="Power";
    mp["green"]="Time";
    mp["blue"]="Space";
    mp["orange"]="Soul";
    mp["red"]="Reality";
    mp["yellow"]="Mind";
    cin>>n;
    while(n--)
    {
        cin>>s;
        mp.erase(s);
    }
    cout<<mp.size()<<endl;
    for(auto it:mp)
    cout<<it.second<<endl;
    return 0;
}
