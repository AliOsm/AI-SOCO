/*jai mata di
  let's rock */
#include <bits/stdc++.h>
using namespace std;
int main()
{
	int t;
	scanf("%d",&t);
	while(t--)
	{
		int n;
		scanf("%d",&n);
		int cnt=0;
		int i,j;
		for(i=1;i<10;i++)
		{
			long long val=0;
			for(j=1;j<=10;j++)
			{
				val=val*10+i;
				if(val<=n) cnt++;
			}
		}
		printf("%d\n",cnt);
	}
	return 0;
}
