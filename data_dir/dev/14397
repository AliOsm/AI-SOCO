#include "bits/stdc++.h"
using namespace std;

int main(){

	int tc;
	cin >> tc;

	while(tc--){

		long long  n;
		cin >> n;

		long long a = 0 , b = 0;

		while(n--){

			long long aa;
			cin >> aa;

			if(aa & 1){
				b++;
			}else{
				a++;
			}
		}

		cin >> n;

		long long c = 0 ,d = 0;

		while(n--){

			long long aa;
			cin >> aa;

			if(aa & 1){
				d++;
			}else{
				c++;
			}
		}
		cout << a*c + b*d << endl;
	}
}