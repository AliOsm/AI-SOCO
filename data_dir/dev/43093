# define FILE
using namespace std;

const int mod = 998244353;
const int N = 2e5 + 100;


int main(){
    # ifdef FILEs
        freopen( "input.txt", "r", stdin );
        freopen( "output.txt", "w", stdout );
    # endif
    int t;
    cin >> t;
    while( t -- ){
        int n, m;
        cin >> n >> m;
        if( n % m == 0 ){
            cout <<"YES" << endl;
        }else{
            cout << "NO" << endl;
        }
    }   
    
    return 0;
}