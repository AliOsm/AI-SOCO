#include<iostream>
#include<string>
#include<math.h>
#include<queue>
#include<stack>
#include<stdio.h>
#include<map>
#include<set>
#include<memory.h>
#include<algorithm>
#include<vector>
using namespace std;
int Abs(int x){
    if(x<0)return -x;
    return x;
}
int main(){
    string x,y;
    cin>>x>>y;
    for(int i=0;i<x.size();++i){
        if(x[i]!=y[i])cout<<1;
        else cout<<0;
    }
}