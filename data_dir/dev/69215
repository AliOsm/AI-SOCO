#include <bits/stdc++.h>
using namespace std;
long long a,b,d;
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cin >> a >> b;
    d = a + b + 1;
    for(int i = b + 1; i <= d; ++i){
        cout << i << " ";
    }
    for(int i = b; i > 0; --i){
        cout << i << " ";
    }
}