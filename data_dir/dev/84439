#include<bits/stdc++.h>

using namespace std;

int main(){
	
	double x1,x2,x3,y1,y2,y3;
	cin >> x1 >> y1 >> x2 >> y2 >> x3 >> y3;
	
	int dx,dy;
	
	cout << 3 << endl;
	
	cout << ((x1 + x2)/2 - x3)*2 + x3 << " " << ((y1 + y2)/2 - y3)*2 + y3 << endl;
	cout << ((x1 + x3)/2 - x2)*2 + x2 << " " << ((y1 + y3)/2 - y2)*2 + y2 << endl;
	cout << ((x3 + x2)/2 - x1)*2 + x1 << " " << ((y3 + y2)/2 - y1)*2 + y1 << endl;
}