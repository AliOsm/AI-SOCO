#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
typedef pair <int , int> pii;

const int N = 2e5+10 , mod = 1e9+7;

int32_t main(){
   ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
   int t;
   cin >> t;
   while(t--){
      int n;
      cin >> n;
      set <int> s;
      for(int i = 0 ; i < n ; i++){
         int x;
         cin >> x;
         s.insert(x);
      }
      cout << s.size() << "\n";
   }
   return 0;
}
