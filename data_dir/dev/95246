#include <bits/stdc++.h>

using namespace std;

int n, m, ans, v[300002], viz[300002], nr;
vector<int>g[300002];

int main()
{
  cin >> n >> m;
  for (int i = 1; i <= n; i++) {
    cin >> v[i];
  }
  for (int i = 1; i <= m; i++) {
    int x, y;
    cin >> x >> y;
    g[x].push_back(y);
  }
  viz[v[n]] = 1;
  nr = 1;
  for (int i = n - 1; i >= 1; i--) {
    int aux = 0;
    for (auto it : g[v[i]]) {
      if (viz[it])
        aux++;
    }
    if (nr == aux) {
      ans++;
    }
    else {
      nr++;
      viz[v[i]] = 1;
    }
  }
  cout << ans;
  return 0;
}
