#include <bits/stdc++.h>
#include <math.h>
#define fr first
#define sc scanf
#define pf printf
#define se second
#define dl double
#define ll long long
#define pb push_back
#define mk make_pair
#define pr pair<int,int>
#define piip pair< pair<ll,ll>,ll >
#define fin(s) freopen( s, "r", stdin )
#define fout(s) freopen( s, "w", stdout )
#define TIME ios_base::sync_with_stdio(0)
using namespace std;

const ll INF = 4e18;
const int N = 2e5 + 1;
const ll mod = 1e9 + 7;
const long double eps = 1E-7;

ll n, m, k = 1;
int main()
{
    TIME;
    cin >> n >> m;
    for( int i = 1; i <= n; i ++ )
    {
        k *= 2;
        if( k > m )
            break;
    }
    cout << m % k << endl;
}
