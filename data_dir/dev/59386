#include <bits/stdc++.h>
using namespace std;
#define eb emplace_back
#define emp emplace
#define fi first
#define se second
typedef long long ll;
typedef pair<int, int> ii;
mt19937 rng(chrono::steady_clock::now().time_since_epoch().count());

int main(void) {
	ios_base::sync_with_stdio(false); cin.tie(0);

	int q; cin >> q;

	while (q--) {
		int l, r, d; cin >> l >> r >> d;
		if (d < l) cout << d << endl;
		else cout << (r+d)-(r+d)%d << endl;
	}

	return 0;
}

