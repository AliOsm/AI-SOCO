#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n, d, i, x, y;
    x=0;
    cin>>n>>d;
    int D[n];
    for (i=0; i<n; i=i+1)
    {
        cin>>D[i];
        x=x+D[i];
    }
    if (x+(n-1)*10>d)
    {
        cout<<-1;
        return 0;
    }
    y=(d-x)/5;
    cout<<y;
    return 0;
}