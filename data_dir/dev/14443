//++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
//++--++--++--++--++--++--++--++--++--+--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
const int MAXN = 200008;
const ll inf = 1000000001;
const ll INF = 2e18;
const ld EPS = 1e-7;
const ll mod = inf + 6;
const int moff = 998244353;

const int mod13 = 1000003;
const ld PI = acos(-1.0);
signed main()
{
	fastio();
	vector<string> kek{"ten", "eleven", "twelve", "thirteen", "fourteen", "fifteen", "sixteen", "seventeen", "eighteen", "nineteen"};
	int n;
	cin >> n;
	vector<string> kek2{ "zero","one","two","three","four","five","six","seven","eight","nine" };
	vector<string> lol{ "twenty","thirty","forty","fifty","sixty","seventy","eighty","ninety" };
	if (n < 10)
	{
		cout << kek2[n];
	}
	else if (n < 20)
	{
		cout << kek[n - 10];
	}
	else
	{
		if (n % 10 == 0)
		{
			cout << lol[n / 10 - 2];
		}
		else
		{
			cout << lol[n / 10 - 2] << '-' << kek2[n % 10];
		}
	}
	//sp;
}