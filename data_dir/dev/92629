#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
#define rep(i,a,b) for(int i = (a); i < (b); i++)
#define iter(it,c) for(auto it = (c).begin(); it != (c).end();++it)
#define pb push_back
#define fs first
#define sc second
typedef pair<int,int> ii;
typedef vector<int> vi;
typedef vector<ii> vii;
const int INF = ~(1<<31);
const double pi = acos(-1);

int main() {
    cin.sync_with_stdio(false);
	ll n,x;
	cin >> n >> x;
	ll dasize = n-1;
	rep(i,0,n) {
		ll num;
		cin >> num;
		dasize += num;
	}
	if(dasize == x) cout << "YES" << endl;
	else cout << "NO" << endl;
    return 0;
}


