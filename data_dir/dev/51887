#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

int l[1005];
int r[1005];

int main()
{
    ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
    int n,m;
    cin >> n >> m;
    for(int i=0;i<m;i++){
        cin >> l[i] >> r[i];
    }
    for(int i=0;i<n;i++)cout<<i%2;

    return 0;
}
