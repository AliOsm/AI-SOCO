#include  <bits/stdc++.h>
using namespace std;
#define int long long
int32_t main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
    string s;cin>>s;
    for(int i=0;i<5;i++)
    {
        string p;cin>>p;
        if(s[0]==p[0] || s[1]==p[1])
            return cout<<"YES",0;
    }
    cout<<"NO";

}