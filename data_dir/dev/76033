#include <bits/stdc++.h>

using namespace std;

const int N = 100010;



int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int tc;
    cin >> tc;
    while (tc--) {
        int l, r;
        cin >> l >> r;
        cout << l << " " << l * 2 << endl;
    }
    return 0;
}