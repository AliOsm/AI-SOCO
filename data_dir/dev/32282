#define ll long long
#define clr(x) memset(x,0,sizeof(x))
#define _clr(x) memset(x,-1,sizeof(x))
#define fr(i,a,b) for(int i = a; i < b; ++i)
#define frr(i,a,b) for(int i = a; i > b; --i)
#define pb push_back
#define sf scanf

#define pf printf
#define mp make_pair

using namespace std;

int p[40],a[1000000];
int main() {
	int t;
	sf("%d",&t);
	while(t--){
		ll l,r;
		sf("%lld%lld",&l,&r);
		printf("%lld %lld\n",l,2*l);
	}
}