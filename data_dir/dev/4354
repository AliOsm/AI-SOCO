#include <bits/stdc++.h>
using namespace std ;

int n ;

int main()
{

    string s = "ROYGBIV" ;

    cin >> n ;

    cout << s ;

    n -= 7 ;
    int k = n / 4 ;
    n = n%4 ;
    s = "GBIV" ;
    for( int i = 0 ; i < k ; i++ )cout << s ;
    if( n == 1 )cout << "G" ;
    else if( n == 2 )cout << "GB" ;
    else if( n == 3 )cout << "GBI" ;



    return 0 ;
}
