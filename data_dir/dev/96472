#include <bits/stdc++.h>

using namespace std;

typedef long long ll ;

int a[6][3] =
{
    {0, 1, 2},
    {1, 0, 2},
    {1, 2, 0},
    {2, 1, 0},
    {2, 0,1},
    {0, 2, 1}
};

int main()
{
    int n ;
    cin >>  n ;
    int x;
    cin >> x ;
    n %= 6 ;
    cout << a[n][x] ;

    return 0;
}
