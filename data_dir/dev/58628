#include <bits/stdc++.h>
using namespace std;
#define pf push_front
#define pb push_back
#define FOR(i,begin,end) for (ulli i=begin;i<=end;i++)
#define rep(i,t) for (LL i=0;i<t;i++)
#define pii pair<LL,LL>
#define pi pair<pii,pii>
#define X first
#define Y second
typedef long long LL;
typedef unsigned long long int ulli;
const LL maxN=6407,MOD=1e9+7,inf=1e9+7,N=1e4+7;
LL n,h,x,ans;
int main()
{
  cin>>n>>h;
  FOR(i,1,n)
    {
      cin>>x;
      ans+=(x<=h) ? 1 : 2;
    }
    return cout<<ans<<"\n",0;
} 