using namespace std;
using ll = long long;
const int oo = numeric_limits<int>::max() / 2;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie();
	ll n, k;
	cin >> n >> k;
	cout << (k + n - 1) / n << endl;
	return 0;
}
