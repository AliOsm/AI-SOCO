#include <iostream>
#include <vector>
#include <cstring>

using namespace std;
typedef long long Long;



int main() {

    int x,y,z, a, b, c;
    cin >> x >> y >> z >> a >> b >> c;
    int t1 = (abs(z-x) + abs(x - y)) * b + 3*c;
    int t2 = abs(x - y) * a;
    cout << (t1 <= t2 ? "YES" : "NO") << endl;


}

