#include<bits/stdc++.h>

#define FI(i,a,b) for(int i=(a);i<=(b);i++)
#define FD(i,a,b) for(int i=(a);i>=(b);i--)

#define LL long long

using namespace std;

LL n;
LL c(int a,int b){
    LL res=1;
    FI(i,1,b) res=res*(a+1-i)/i;
    return res;
}

int main(){
    scanf("%lld\n",&n);
    printf("%d\n",2-n%2);
    return 0;
}