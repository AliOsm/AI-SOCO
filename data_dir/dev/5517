#include <bits/stdc++.h>
#define mk make_pair
#define fs first
#define sc second
using namespace std;
typedef long long ll;
typedef long double ld;
int a[1010];
int main(){
  int n;
  while(cin>>n){
    int b=0, c=0;
    for(int i=0; i<n; ++i){
      cin>>a[i];
      if(a[i]>0)
      b+=a[i];
      else
      c+=a[i];
    }
    int ans=b-c;
    cout<<ans<<endl;
  }
  return 0;
}


