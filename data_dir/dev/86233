#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define MAX_INT 100000
#define ld long double
int main()
{ 
    ll n,i,j,k,b,m,l,r;
    cin>>n>>k;
    ll a[n];
    for(i=0;i<n;i++)cin>>a[i];
    sort(a,a+n);ll sum=0;
    for(i=0;i<k;i++)sum+=a[i];
    cout<<sum<<endl;
    return 0;
}