#include <iostream>
#include <set>
#include <string>
#include <vector>

using namespace std;


const int MAX_N = 123;

string s[MAX_N];
string s2[MAX_N];
int n, m;

void hal() {
    cout << "No\n";
    exit(0);
}

void check() {
    set<string> st;
    for (int i = 1; i <= n; i++) {
        st.insert(s[i]);
    }
    vector<int> used(m);
    for (const auto& s : st) {
        for (int i = 0; i < m; i++) {
            if (s[i] == '#') {
                if (used[i]) {
                    hal();
                }
                used[i] = true;
            }
        }
    }
}

void solve() {
    cin >> n >> m;
    for (int i = 1; i <= m; i++) {
        for (int j = 0; j < n; j++) {
            s2[i] += '.';
        }
    }
    for (int i = 1; i <= n; i++) {
        cin >> s[i];
        for (int j = 0; j < m; j++) {
            s2[j + 1][i - 1] = s[i][j];
        }
    }
    check();
    for (int i = 1; i <= m; i++) {
        s[i] = s2[i];
    }
    swap(n, m);
    check();
    cout << "Yes\n";
}

int main(int argc, const char * argv[]) {
    ios_base::sync_with_stdio(false);
    solve();
    return 0;
}
