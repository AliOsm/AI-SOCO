#include<bits/stdc++.h>
#define ll long long int
#define r_0 return 0
#define fr first
#define sc second
#define pb push_back
#define TIME ios_base::sync_with_stdio(0)
using namespace std;
int main()
{
    int n,c=0,cnt = 0;
    cin >> n;
    int a[n];
    for(int i =1 ;i <= n ; i ++ )
    {
        cin >> a[i];
    }
    for(int i =1 ;i <= n ; i ++ )
    {
        if( a [i] == 0 )
        {
            c ++;
        }
        else
        {
            cnt ++ ;
        }
    }
    if( cnt == 0 )
    {
        cout << "EASY";
    }
else
    {
        cout << "HARD";
    }
}















