#include <iostream>
using namespace std;
int main()
{
    long long a,b,c,d;
    cin >> a >> b >> c >> d;
    if(d - c + d - b + d - a == d)
        cout << d - c << " " << d - a << " " << d - b << endl;
    else if(c - d + c - b + c - a == c)
        cout << c - d << " " << c - a << " " << c - b << endl;
    else if(b - d + b - c + b - a == b)
        cout << b - c << " " << b - a << " " << b - d << endl;
    else if(a - c + a - b + a - d == a)
        cout << a - d << " " << a - c << " " << a - b << endl;
    return 0;
}
