#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    ll n;
    cin>>n;
    ll i;
    map <string, ll> a;
    for(i=0; i<n; i++)
    {
        string q;
        cin>>q;
        if (a[q] == 0)
        {
            cout<<"NO"<<endl;
        }
        else
            cout<<"YES"<<endl;
        a[q] = 1;
    }



    return 0;
}
