	ll power(ll x,ll y, ll p) 
	{ 
	    ll res = 1; 
	    x = x % p;
	    while (y > 0) 
	    {  // If y is odd, multiply x with result 
	        if (y & 1) 
	            res = (res*x) % p;
	            
	        y = y>>1; 
	        x = (x*x) % p;   
	    } 
	    return res; 
	} 
	ll isPrime(ll n)
	        {
	            
	           if(n==0 || n==1)
	           return 0;
	            if (n%2 == 0) 
	                return 2;
	            
	            if(n%3==0)
	                return 3;
	         
	            for (ll i=5; i*i<=n; i=i+6)
	            {
	                if (n%i == 0)
	                   return i;
	                if (n%(i+2) == 0)
	                   return i+2;
	            }
	            
	            return n;
	        }
	
	vector<vector<ll> >  v(10010);
	
	bool bfs(ll n,ll m)
	{
	    
	    queue<ll> q;
	    q.push(n);
	    map<ll,ll> fr,level;
	    level[n]=0;
	    ll fl=0;
	    fr[1]=1;
	    map<pair<ll,ll>,ll> vis;
	    vector<ll> parent(10010);
	    while(q.size()>0)
	    {    //cout<<"t";
	        ll x=q.front();
	        q.pop();
	        ll co=0;
	        for(ll i=0;i<v[x].size();i++)
	        {    
	            if(fr[v[x][i]]==0)
	            {
	                q.push(v[x][i]);
	                fr[v[x][i]]=1;
	                parent[v[x][i]]=x;
	            }
	            else if(co==0)
	            {
	                co++;
	            }
	            else
	            {    
	                if(parent[x]==parent[v[x][i]])
	                {    //cout<<"Z";
	                    ll a=parent[x],b=x,c=v[x][i];
	                    if(vis[mp(a,b)]==1 || vis[mp(b,c)]==1 || vis[mp(a,c)]==1)
	                    {
	                        if(vis[mp(a,b)]==0 || vis[mp(b,c)]==0 || vis[mp(a,c)]==0)
	                        return false;
	                        
	                    }
	                    else
	                    {    //cout<<"AAA ";
	                        vis[mp(a,b)]=vis[mp(b,a)]=vis[mp(a,c)]=vis[mp(c,a)]=vis[mp(b,c)]=vis[mp(c,b)]=1;
	                        fl++;
	                        
	                    }
	                }
	                else
	                {
	                    //cout<<x<<"    "<<v[x][i]<<endl;
	                    return false;
	                }
	                
	            }
	            level[v[x][i]]=level[x]+1;
	        }
	    }
	    //cout<<fl<<"x ";
	    if(fl==m/3)
	    {    
	        return true;
	    }
	    else
	    {
	        //cout<<"X";
	        return false;
	    }
	}
	
	int main()
	{
	    ll n,i,q;
	    cin>>n;
	    ll a[n];
	    for(i=0;i<n;i++)
	    cin>>a[i];
	    sort(a,a+n+1);
	    ll sum=0;
	    
		for(i=0;i<n;i++)
	    {
	        
	       sum+=a[i];
	        
	    }
	    
	    cin>>q;
	    while(q--)
	    {
	    	ll x;
	    	cin>>x;
	    	//cout<<sum[n]<<" "<<n-x+1<<endl;
	    	ll ans=sum-a[n-x];
	    	cout<<ans<<endl;
		}
	    
	}