#include<bits/stdc++.h>
using namespace std;

typedef long long int ll;
typedef long double ld;
typedef vector<ll> vec;
typedef pair<ll,ll> pair;

#define ff first
#define ss second
#define pb push_back
#define mp make_pair
#define M 1
#define LM LLONG_MAX

int main(){

    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    
    ll n,f_0=0,f_5=0,temp;
    cin>>n;
    
    for(int i=0;i<n;i++)
    {
        cin>>temp;
        if(temp==0)     f_0++;
        else        f_5++;
    }
    
    if(f_0>0)
    {
        temp=f_5/9;
        while(temp--)
        {
            cout<<"555555555";
        }
        if(f_5/9)
        {
            while(f_0-- )
            {
                cout<<0;
            }
        }
        else
        {
            cout<<0;
        }
    }
    else
    {
        cout<<-1;
    }
    
}