#include <bits/stdc++.h>

using namespace std;

int n, cnt, ans;

string c;

string str;

int main(){
    cin >> n;
    for(int i = 1; i <= n; i++){
        cin >> c;
        str += c;
    }
    string s = str;
    reverse(s.begin(), s.end());
    if(s == str){
        cout << "YES";
    }
    else {
        cout << "NO";
    }
}
