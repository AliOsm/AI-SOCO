#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

char s[500005];

int main()
{
	ignore = scanf("%s", s);
	int n = strlen(s);
	
	char mn = 'z';
	
	for (int i = 0; i < n; i++) {
		if (s[i] > mn) {
			printf("Ann\n");
		}
		else {
			printf("Mike\n");
		}
		mn = min(mn, s[i]);
	}
}
