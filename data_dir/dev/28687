#include <bits/stdc++.h>
using namespace std;
#define forn(i, n) for(int i = 0; i < (int) (n); i++)

int n, a, b, c, A, B, C;

int main(){
	scanf("%d", &n);
	forn(i, n){
		scanf("%d %d %d", &a, &b, &c);
		A += a;
		B += b;
		C += c;
	}
	printf("%s\n", (A or B or C) ? "NO" : "YES");
	return 0;
}
