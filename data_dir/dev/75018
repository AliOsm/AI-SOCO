///*BISMILLAHIR RAHMANIR RAHIM*///
#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
map<int,int>mp;
int main(){
    int n,x,i,res=0;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&x);
        mp[x]++;
        res=max(res,mp[x]);
    }
    printf("%d\n",res);
    return 0;
}

