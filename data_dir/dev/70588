#include <bits/stdc++.h>
using namespace std;

#define elif else if
#define rep(i,a,n) for (ll i=a;i<n;i++)
#define per(i,a,n) for (ll i=a-1;i>=n;i--)
#define repp(i,a,n) for (ll i=a;i<=n;i++)
#define pper(i,a,n) for (ll i=a;i>=n;i--)
#define pf(x) printf("%.15f\n", x)
#define fix(x,y) cout<<fixed<<setprecision(y)<<x;
#define pb push_back
#define IOS ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define files freopen("input.in","r",stdin);freopen("output.out","w",stdout)
#define bs(s,n) binary_search(s.begin(),s.end(),n)
#define be(s) s.begin(),s.end()
#define sz(s) s.size()
#define pii pair<long long,long long>
#define in insert
#define er erase
#define lb lower_bound
#define ub upper_bound
#define fi first
#define se second

typedef long long ll;
typedef unsigned long long ull;
typedef long double ld;
const ld pi = 3.1415926535897932384626433;
const ll M = 998244353;

ll n,m,x,k;
set<ll> v;
map<ll,ll> a;

int main()
{
    IOS;
    cin>>n>>m;
    rep(i,0,m){
        cin>>x;
        a[x]++;
            v.in(x);
        if(sz(v)==n){
            cout<<1;
            k++;
            v.clear();
            for(auto i:a){
                if(i.se>k)
                    v.in(i.fi);
            }
        }
        else
            cout<<0;
    }
}
