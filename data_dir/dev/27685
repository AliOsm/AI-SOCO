//++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
//++--++--++--++--++--++--++--++--++--+--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
const int MAXN = 5001;
const int inf = 1000000000;
const ld INF = 1.0 / 0.0;
const ld EPS = 1e-7;
const int mod = inf + 7;
string s, t;
vector<int> a;
bool check(int m)
{
	vector<int> used(s.size());
	for (int i = 0; i < m; ++i)
	{
		used[a[i] - 1] = 1;
	}
	string ans = "";
	for (int i = 0; i < s.size(); ++i)
	{
		if (!used[i])
		{
			ans.pbc(s[i]);
		}
	}
	int x = 0;
	for (int i = 0; i < ans.size() && x < t.size(); ++i)
	{
		if (ans[i] == t[x])
		{
			x++;
			continue;
		}
	}
	return x == t.size();
}
signed main()
{
	fastio();
	cin >> s >> t;
	a.resize(s.size());
	vin(a);
	int l = 0, r = s.size();
	while (r - l > 1)
	{
		int m = (l + r) / 2;
		if (!check(m)) r = m;
		else l = m;
	}
	if (!check(r)) r--;
	if (r < s.size() && check(r + 1))r++;
	cout << r;
	//sp;
}