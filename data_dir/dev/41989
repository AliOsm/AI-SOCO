#include <bits/stdc++.h>
#define pb push_back
#define pf push_front
#define FAST ios::sync_with_stdio(0), ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
#define ll long long
#define rw freopen("output.txt","w",stdout);
#define mod 998244353
#define F first
#define S second
#define f(x) return cout << x << "\n", 0
#define lp(i,a,x) for (int i=a;i<=x;++i)
#define lp2(i,x,a) for (int i=x;i>=a;--i)
#define all(x) x.begin(),x.end()
using namespace std;
const int N=1e5+5;
int main()
{
    FAST
    int n,p,b,x,ans=0;
    cin>>n>>b>>p;
    lp(i,1,n)
    {
        cin>>x;
        if (x==1)
        {
            if (b)
                b--;
            else ++ans;
        }
        else
        {
            if (p)
                --p;
            else if (b)
                b--;
            else
                ans++;
        }
    }
    cout<<ans<<"\n";
    return 0;
}