#include <cstdio>
#include <iostream>
#include <algorithm>
#include <map>
#include <set>
#include <vector>
#include <deque>
#include <string>
#include <cmath>
#include <cctype>
#include <locale>
#include <queue>
#include <utility>

#define forn(n) for(int i = 0; i < n; i++)
#define fornm(n, m) for(int i = 0; i < n; i++) for(int j = 0; j < m; j++)
#define pb push_back
#define pf push_front
#define ll long long
#define ld long double
#define pi 3.141592653589793238462643383

using namespace std;

int main() 
{
   int n, k; cin >> n >> k;
   for(int i = 0; i < n; i++)
   {
      if(i != 0) cout << endl;
      for(int j = 0; j < n; j++)
      {  
         if(j != 0) cout << ' ';
         if(i == j) cout << k;
         else cout << 0;
      }
   }
   return 0;
}
