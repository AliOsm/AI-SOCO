#include<bits/stdc++.h>
using namespace std;

int main()
{
    int x;
    char s[111];

    cin >> x;
    gets(s);

    if(strlen(s) == 8){
        if(x == 5 || x == 6) cout << 53 << endl;
        else cout << 52 << endl;
    }
    else{
        if(x < 30) cout << 12 << endl;
        else if(x == 30) cout << 11 << endl;
        else cout << 7 << endl;
    }

    return 0;
}
