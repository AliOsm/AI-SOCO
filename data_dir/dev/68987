#include<bits/stdc++.h>
 using namespace std;
 int main()
 {
    /*freopen("input.txt","r",stdin);
    freopen("output.txt","w",stdout);*/
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int n;
    cin >> n;
    if(n<=10 || n>=22)
    {
       cout << 0;
       return 0;
    }
    n-=10;
    if(n==10)
    {
       cout << 15;
    }
    else
    {
       cout << 4;
    }
    return 0;
 }
