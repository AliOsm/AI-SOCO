        freopen("output.txt", "w", stdout);
    #endif
    int n;
    cin >> n;
    set<ll>s;
    REP(i,1,n)
    {
    	ll x;
    	cin >> x;
    	s.insert(x);
    }
    ll val=(*s.rbegin())-(*s.begin())+1;
    val-=s.size();
    cout << val;
}