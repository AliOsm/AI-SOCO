#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
#include <cstdio>
#include <map>
#include <queue>
#include <cstring>
#include <cstdlib>
using namespace std;
#define INF 2001001001
#define MOD 1000000007

int main()
{
  //ios_base::sync_with_stdio(0);cin.tie(0);
  int n,zero=0;
  string s;
  cin>>n>>s;
  if (s=="0"){
    cout<<0<<endl;
    return 0;
  }
  for (int i=0;i<n;i++){
    if (s[i]=='0')
      zero++;
  }
  cout<<1;
  for (int i=0;i<zero;i++)
    cout<<0;
  cout<<endl;
  return 0;
}
