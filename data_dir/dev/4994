#include<bits/stdc++.h>
using namespace std;

#define fi first
#define se second
#define long long long
typedef pair<int,int> ii;

int main()
{
	// freopen("input.in", "r", stdin);

	int t; scanf("%d", &t);
	while(t--)
	{
		int h, m; scanf("%d %d", &h, &m);
		printf("%d\n", 60*(24-h)-m);
	}
}