#include<bits/stdc++.h>

#define ll long long
#define dl double

using namespace std;

int main()
{
    int n, m;
    cin >> n >> m;

    int x = n / m, y = m - n % m;
    for(int i = 1; i <= m; i++){
        if(y){
            cout << x << " ";
            y--;
        }
        else cout << x + 1 << " ";
    }
    return 0;
}
