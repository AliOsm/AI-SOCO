int main(){
	int n; string s; int b, a;
	cin>>n;
	bool good = false;
	FOR(i,0,n){
		cin>>s>>b>>a;
		if(b >= 2400 && a > b) good = true;
	}
	if(good) cout <<"YES\n";
	else cout << "NO\n";
	
