#include <bits/stdc++.h>
using namespace std;

#define elif else if
#define rep(i,a,n) for (ll i=a;i<n;i++)
#define pf(x) printf("%.8f\n", x)
#define pb(x) push_back(x)
#define ios ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define rs(s) s.begin(),s.end()
#define sz(s) s.size()
typedef long long ll;
const int M = 2e9;

int main(){
    ios;
    ll t,n,k,s=0,i,x,q,j;
    cin>>t;
    while(t){
        t--;
        cin>>n;
        int l[n+1],r[n+1];
        rep(i,1,n+1)
        cin>>l[i]>>r[i];
        l[0]==0;
        x=1;
        bool q=false;
        for(i=1;;i++){
                while(l[x]<=i && r[x]<i){
            cout<<0<<" "; if(x==n){q=true; break;} x++;}
            if(q)break;
            if(l[x]<=i){cout<<i<<" "; if(x==n)break; x++;}
        }
        cout<<endl;
    }
    }
