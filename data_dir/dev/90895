#include <bits/stdc++.h>
using namespace std;
#define pb push_back
#define mp make_pair
#define all(x) x.begin(), x.end()
#define boost() ios_base :: sync_with_stdio(0); cin.tie(); cout.tie(); cout << fixed; cout << setprecision(15); srand(time(NULL))
#define endl '\n'
typedef long long ll;
const int _N = 3e5 + 11;
const ll INF = 1e18;
const ll MOD = 1e9 + 7;

string x, y, z;

int main() {
	boost();
//	freopen("in", "r", stdin);
	cin >> x >> y; int n = x.size();
	for (int i = 0; i < n; i++) {
		if (x[i] < y[i]) { cout << -1 << endl; return 0; }
	}
	for (int i = 0; i < n; i++) cout << min(x[i], y[i]); cout << endl;
	return 0;
}
