// TAKEN FROM GEEKSFORGEEKS
#include "bits/stdc++.h"
#define rep(i , n) for(int i = 0; i < n; ++i)
#define reps(i , x , y) for(int i = x; i <= y; ++i)
using namespace std;
const int N = 1e5 + 5;
const int LGN = 20;
int n;
int u;
int v;
int w;
int x;
int main() {
    scanf("%d" , &n);
    if(!n) {
        printf("1\n");
        return 0;
    }
    int x = n % 4;
    if(x == 0) {
        printf("6\n");
        return 0;
    }
    if(x == 1) {
        printf("8\n");
    } else if(x == 2) {
        printf("4\n");
    } else {
        printf("2\n");
    }
    return 0;
}
