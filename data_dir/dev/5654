#include <bits/stdc++.h>
using namespace std;

#define ll long long
#define F first
#define S second
#define ii pair < int , int >
#define ever (;;)

int t,a,b;

int main()
{
    scanf("%d",&t);
    while(t--)
    {
        scanf("%d%d",&a,&b);

        int l = max( max(a,b) , min(a,b)*2 );

        printf("%d\n",l*l);
    }
}
