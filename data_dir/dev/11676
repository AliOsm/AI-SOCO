#include <bits/stdc++.h>
using namespace std;

typedef long double ld;

int main()
{
    int n;
    cin >> n;

    int a[n];
    for (int i = 0; i < n; ++i)
        cin >> a[i];

    sort(a, a + n);
    for (int i = 0; i < n; ++i)
    {
        if (i) cout << " ";
        cout << a[i];
    }
    cout << endl;
}
