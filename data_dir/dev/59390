#include<stdio.h>
#include<string.h>
#include<algorithm>
#include<map>
#include<string>
#include<math.h>
#include<queue>
#include<stack>
#include<iostream>
using namespace std;
#define LL long long
#define mod 1000000007
int main(void)
{
	LL n, k, i;
	scanf("%lld%lld", &n, &k);
	for(i=1;i<=k;i++)
	{
		if(n%i!=i-1)
		{
			printf("NO\n");
			break;
		}
	}
	if(i==k+1)
		printf("YES\n");
	return 0;
}