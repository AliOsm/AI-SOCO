#include <bits/stdc++.h>
using namespace std;

int A[100002];

int main() {
    ios_base::sync_with_stdio(false);
    int n, x;
    cin >> n >> x;
    int sum = x;
    for(int i = 0; i < n; i++) {
        cin >> A[i];
        sum -= A[i];
    }
    if(sum == n - 1) {
        cout << "YES" << endl;
    }
    else cout << "NO" << endl;

}
