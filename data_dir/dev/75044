#include <bits/stdc++.h>
using namespace std;

signed main(){
  ios::sync_with_stdio( 0 );
  int N, K; cin >> N >> K;
  vector< int > A( N );
  for( int i = 0; i < N; ++i ){
    cin >> A[ i ];
  }
  sort( A.begin(), A.end(), greater< int >() );
  cout << A[ K - 1 ] << endl;
  return 0;
}
