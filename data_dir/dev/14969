#include<bits/stdc++.h>
using namespace std;
#define int long long
typedef long long ll;

int32_t main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int v;
    cin >> v;
    if(v == 2) {
        cout << 2 << '\n';
    } else cout << 1 << '\n';
    return 0;
}



























