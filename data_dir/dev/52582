#include <iostream>

using namespace std;

int main()
{
    int n;
    cin >> n ;
    string s;
    cin >> s;
    int a=0;
    int b=0;
    for (int i=0;i<n-1;i++)
    {
        if (s[i]=='S'&&s[i+1]=='F'){a++;}
        if (s[i]=='F'&&s[i+1]=='S'){b++;}
    }
    if (a>b){cout << "YES";}else{cout << "NO";}
    return 0;
}
