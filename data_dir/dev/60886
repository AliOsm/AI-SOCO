#include <bits/stdc++.h>
using namespace std;
#define ll long long
ifstream in("file.in");
ofstream out("file.out");
#define nibab return
#define INF 1000000005
#define LINF 1000000000000000005

///________CODE_STARTS_HERE________\\\

int n,a,b,s;

int main(){
    ios_base :: sync_with_stdio(0);
    cin >> n;
    s=sqrt(n);
    for(int i=1;i<=s;i++){
        if(!(n%i)) a=i, b=n/i;
    }
    cout << a << ' ' << b;
}


