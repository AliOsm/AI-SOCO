#include <bits/stdc++.h>
#define ll long long

using namespace std;

int main ()
{
    ios_base::sync_with_stdio(false);
    int t; cin >> t;
    while(t--) {
        int n; string s; cin >> n >> s;
        bool x = false;
        for(int i=0;i<n-10;i++)
            if(s[i]=='8') x = true;
        cout << ( x ? "YES" : "NO" ) << endl;
    }
    return 0;
}
