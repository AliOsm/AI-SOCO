#include<bits/stdc++.h>
#define pb push_back
#define adelkhah main
#define int long long
using namespace std;
typedef long long ll;
typedef long double ld;
typedef pair<int,int> pii;
int32_t adelkhah()
{
    int n,s; cin >> n >> s;
    if(s % n == 0) return cout << s/n,0;
    return cout << s/n + 1,0;
}
