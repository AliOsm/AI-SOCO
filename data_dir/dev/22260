void f(){
    int data[3];
    for(int i = 0;i<3;i++)cin>>data[i];
    sort(data, data+3);
    
    if(data[2] > data[0]+data[1]+1){
        cout<<"No\n";
    }else{
        cout<<"Yes\n";
    }
}


int main(int argc, char *argv[]){
    //freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout);
    
    
    int n;cin>>n;
    
    for(int i = 0;i<n;i++)f();
	return 0;
}
 