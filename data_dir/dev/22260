//ceil,floor
 
/*
long long MOD = 1e9 + 7;
long long POWER(long long a, long long power){
    if(power == 0){
        return 1;
    }
    
    long long answer = POWER(a, power/2);
    if(power % 2 == 0){
        return answer * answer % MOD;
    }else{
         return answer * answer % MOD * a % MOD;
    }
}
