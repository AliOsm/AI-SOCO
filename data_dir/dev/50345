std::mt19937_64 gen(gen_seed);

#define all(c) (c).begin(),(c).end()
#define forn(i, a, b) for(int i = a; i < b; i++)

#define pb push_back
#define mp make_pair

int main()
{
#ifndef ONLINE_JUDGE
    freopen("input.txt", "rt", stdin);
    freopen("output.txt", "wt", stdout);
#endif
    vi a;
    forn(i,0,1000) a.pb(i*(i-1)/2);
    int k;
    cin>>k;
    if(k==0) {
        cout<<'a';return 0;
    }
    int cur = 0;
    string ans;
    for(int i = 999; i>=2; i--) {
        while(k>=a[i]) {
            k-=a[i];
            ans = ans + string(i, 'a' + cur);
            cur++;
        }
    }
    printf("%s", ans.c_str());
    
}


