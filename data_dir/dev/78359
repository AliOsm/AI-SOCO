class TaskA {
public:
    void solve(istream &inp, ostream &outp) {
        Input in(inp);
        Output out(outp);

        string s = in.readString();
        vi pos;
        for (int i = 0; i < s.size(); i++) {
            if (i + 5 <= s.size() && s.substr(i, 5) == "twone") {
                pos.push_back(i + 3);
                i += 4;
            } else if (i + 2 <= s.size()) {
                string sub = s.substr(i, 3);
                if (sub == "one" || sub == "two") {
                    pos.push_back(i + 2);
                    i += 2;
                }
            }
        }
        out.printLine(pos.size());
        out.printLine(pos);
    }
};


int main() {
    std::ios::sync_with_stdio(false);
    std::cin.tie(0);
    TaskA solver;
    std::istream &in(std::cin);
    std::ostream &out(std::cout);
    int n;
    in >> n;
    for (int i = 0; i < n; ++i) {
        solver.solve(in, out);
    }

    return 0;
}
