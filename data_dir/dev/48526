#include <bits/stdc++.h>

using namespace std;

main()
{
    ios_base::sync_with_stdio(false);
    int n;
    string s;
    cin >> n >> s;
    for(int i = 1; i <= n; i++){
        if(n % i == 0){
            reverse(s.begin(), s.begin() + i);
        }
    }
    cout << s << endl;
}
