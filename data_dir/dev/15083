#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    ll t;
    cin>>t;
    while(t--)
    {
        ll n;
        cin>>n;
        string s;
        cin>>s;
        ll f=0;
        for(ll i=0;i<n;i++)
        {
            if(s[i]=='8')
            {
                if(n-i>=11)
                    cout<<"YES"<<endl;
                else
                    cout<<"NO"<<endl;
                f=1;
                break;
            }
        }
        if(f==0)
            cout<<"NO"<<endl;
    }
}
