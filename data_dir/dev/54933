#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;
typedef long double ld;
typedef pair<ll,ll> p2;
void solve(){
  ll n,k;
  cin>>n>>k;
  for(ll i=1;i<=min((ll)10000,k);i++)
    if(n%i!=i-1){
      cout<<"No";
      exit(0);
    }
  cout<<"Yes";
}
int main(){
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  cout.tie(NULL);
  ll qc=1;
  for(ll i=1;i<=qc;i++)
    solve();
}