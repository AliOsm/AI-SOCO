#include <iostream>
#include <cstdio>
#include <algorithm>
#include <vector>
#include <list>
#define rank krevedka
#define ran rank
using namespace std;

int a[100];

int main()
{
    long long p,d,r;
    cin >> p >>d;
    r = p;
    int k = 0;
    while(true)
    {
        if(p%10==9) { k++; p/=10; r = p; d/=10; continue;}
        if(p==0 || d==0) break;
        p--; d--;
    }
    if(r)cout << r;
    while(k--)
            cout <<9;

    return 0;
}
