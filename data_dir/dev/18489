#include <iostream>
#include <string>
#include <cmath>
#include <algorithm>
#include <vector>
#include <set>
#include <map>
#include <sstream>
#include <cstring>
#include <cstdio>
#include <cstdlib>
#include <iomanip>
using namespace std;
int main()
{
    int n, m, i = 0;
    cin >> n >> m;
    while (1)
    {
        i++;
        if (m >= i)
            m -= i;
        else
            break;
        if (i == n)
            i = 0;
    }
    cout << m;
}