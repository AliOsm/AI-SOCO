#include <bits/stdc++.h>
using namespace std;

string S;

int main() {
	
	int N;
	cin >> N;
	
	int flag = 0;
	
	while(N--)
	{
	    cin >> S;
	    
	    int x, y;
	    cin >> x >> y;
	    
	    if(x>=2400 && y>x)
	        flag = 1;
	}
	
	if(flag)
	    cout << "YES\n";
	else
	    cout << "NO\n";
	
	return 0;
}
