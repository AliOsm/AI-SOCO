#include<bits/stdc++.h>
using namespace std;

const int MOD=1e9+7, MAXN=1e5+5;

int N;
long long M, P, C, X;

int main () {
	ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
	
	cin>>N>>M;
	
	for (int i=0;i<N;i++) {
		cin>>X;
		P+=X;
		cout<<P/M-C<<' ';
		C=P/M;
	}
	
	cout<<'\n';
	return 0;
}
