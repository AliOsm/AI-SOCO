#include <bits/stdc++.h>
using namespace std;
long long i,j,n,c[212345],d1[5555],d2[5678],l,r,s,t,k,b[250555],m,xh,x,x1,y,yy;
long long a[1223556];
vector<long long> p[12345],w,q[123456];
int main() {
    cin >> n;
    for (i = 0; i < n; i ++){
        scanf("%lld", &a[i]);
    }
    for (i = 0; i <n; i ++){
        if (a[i] != a[n-1]){
            break;
        }
    }
    x = i;
    for (i = n-1; i >=0; i --){
        if (a[i] != a[0]){
            break;
        }
    }
    y = i;
    s = max (n-1-x,y);
    cout << s;

    return 0;
} 
