#include <bits/stdc++.h>
#define ll long long
#define OO LONG_LONG_MAX
#define oo 1e9
#define yalla ios::sync_with_stdio(0),cin.tie(0),cout.tie(0);
#define sz  100005
#define re return
#define mod  1000000007
#define pi acos(-1)
using namespace std;
string str;
ll nb,ns,nc,pb,ps,pc,b,s,c,total,rim,r1,r2,r3,sum,r,fi,se,th;
int main()
{yalla;
  cin>>str>>nb>>ns>>nc>>pb>>ps>>pc>>r;

  for(int i=0;i<str.size();i++)
    if(str[i]=='B')b++;
    else if(str[i]=='S')s++;
    else c++;


if(!b)fi=OO,nb=0; else fi=nb/b;
if(!c)se=OO,nc=0; else se=nc/c;
if(!s)th=OO,ns=0; else th=ns/s;

total=min(fi,min(se,th) );
nb-=(total*b); nc-=(total*c); ns-=(total*s);

while(nb || nc ||ns)
{
 r1=b-nb; if(r1<0)r1=0,nb-=b; else nb=0;
 r2=c-nc; if(r2<0)r2=0,nc-=c; else nc=0;
 r3=s-ns; if(r3<0)r3=0,ns-=s; else ns=0;
 ///
 r-=(r1*pb+r2*pc+r3*ps);
 if(r<=0) break;
 total++;
}
if(r>0)
{
    total+=r/(pb*b+pc*c+ps*s);
}
else if(r==0)
total++;

cout<<total<<endl;
    return 0;
}
