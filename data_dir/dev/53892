#include <bits/stdc++.h>
#define ll long long
using namespace std;
ll a,l,r,a1,b1,m,c1,b,c,d,p[100000],k,n;
string s,t;
int main()
{ ios_base::sync_with_stdio(0);
 cin.tie(NULL);
 cout.tie(NULL);
 cin>>s;
 k=1;
for(int i=1;i<s.size();i++)
{
    if (s[i]==s[i-1]) k++; else k=1;
    if (k==7) return cout<<"YES",0;
}
cout<<"NO";
}