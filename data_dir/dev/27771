const int MOD = 1e9 + 7;

map<int, string> answ;
map<int, string> segunda;
map<int, string> primera;

int abs(int x){
	if( x < 0) return -x;
	return x;
}
int main(){
	
	
	int n; cin >> n;
	string w; cin >> w;
	

	int ones = 0;
	
	for(int i = 0; i < w.size(); i++){
		if(w[i] == '1') ones++;
		else ones--;
	}
	
	cout << abs(ones) << endl;
	
	return 0;
}
