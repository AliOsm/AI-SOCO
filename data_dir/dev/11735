#include <bits/stdc++.h>
using namespace std;
#define finish(x) return cout << x << endl, 0;
#define ll long long

string a, b;

int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(NULL);
    cout.tie(NULL);
    cin >> a >> b;
    cout << a[0];
    for(int i = 1 ; i < a.size() ; i++){
        if(a[i] >= b[0]) break;
        cout << a[i];
    }
    cout << b[0] << endl;
    return 0;
}
