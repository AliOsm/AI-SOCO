#include <bits/stdc++.h>
#define fast ios::sync_with_stdio(0); cin.tie(0); cout.tie(0)
#define pb push_back
#define endl '\n'
#define MOD 1000000007

using namespace std;
typedef long long int ll;

int main(void)
{
	#ifndef ONLINE_JUDGE
	freopen("in.txt", "r", stdin);
	#endif
	fast;
	ll a, b, x, y; cin >> a >> b >> x >> y;
	ll d = __gcd(x, y);
	ll X = x/d, Y = y/d;
	ll m = min((a/X), (b/Y));
	cout << m << '\n';

}