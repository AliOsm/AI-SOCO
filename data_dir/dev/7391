#include <bits/stdc++.h>

using namespace std;
typedef long long ll;

int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    /*
    freopen("input.txt","r",stdin);
    freopen("output.txt","w",stdout);
    */

    int n;
    cin >> n;
    int s=0;
    int m=n;
    while(n--){
        int x;
        cin >> x;
        s+=x;
    }
    if(s%m==0)
        cout << m;
    else
        cout << m-1;

}
