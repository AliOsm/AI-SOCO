#include <iostream>
#include <vector>

using namespace std;

int main ()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	cout.tie (0);
	string s;
	cin >> s;
	for (int i = 1; i <= s.size (); i ++)
		if (s[i - 1] == 'r')
			cout << i << "\n";
	for (int i = s.size (); i >= 1; i --)
		if (s[i - 1] == 'l')
			cout << i << "\n";
}
