#include<bits/stdc++.h>
#define f first
#define s second 
#define pb push_back
#define mp make_pair
#define ll long long
#define u unsigned
using namespace std;
int ans;
bool b;
string s;
main()
{
	ios_base::sync_with_stdio(false);
	int n,i,j;
	cin>>n;
	for(i=0;i<n/2;++i)
	{
		if(i%2)s+="aa";
		else s+="bb";
	}
	if(n%2)
	{
		if(s[s.size()-1]=='a')s+='b';
		else s+='a';
	}
	cout<<s;
}