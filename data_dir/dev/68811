#include <iostream>
#include <cstdio>
#include <algorithm>
#include <cmath>
#include <string>
#include <vector>
#include <stack>
#include <queue>
#include <set>
#include <cstring>
#include <map>
#include <cstdlib>
#include <ctime>
#include <cassert>
#include <bitset>
#define f first
#define s second
#define ll long long
#define ull unsigned long long
#define mp make_pair
#define pb push_back
#define vi vector <int>
#define ld long double
#define pii pair<int, int>
#define y1 sda
using namespace std;    
const int N = int(3e5), mod = int(1e9)  + 7; 

int n, c[N], x, k;

int main () {
	scanf("%d%d",&n,&k);
	ll ans = 0;
	for(int i = 1; i <= n; i++){
		scanf("%d",&x);
		ans += c[x ^ k];
		c[x]++;
	}
	cout << ans;


return 0;
}