#include<bits/stdc++.h>
using namespace std;

int m, n, x, y, z, p, q;

int main()
{
    cin >> n;

    while(n--){
        cin >> x >> y;

        if(x > y) p++;
        else if(y > x) q++;
    }

    if(p == q) cout << "Friendship is magic!^^" << endl;
    else if(p > q) cout << "Mishka" << endl;
    else cout << "Chris" << endl;


    return 0;
}

