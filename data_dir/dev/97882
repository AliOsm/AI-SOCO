#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a1 ,a2 ,a3 ,a4;
    cin>>a1>>a2>>a3>>a4;
    string s;
    cin>>s;
    int k =0 ;
    for (int i=0;i<s.size();i++)
    {
        if (s[i] == '1')
        {
            k+= a1;
        }
        else if (s[i] == '2')
        {
            k+=a2;
        }
        else if (s[i] == '3')
        {
            k+=a3;
        }
        else k+=a4;
    }
    cout<<k;
}