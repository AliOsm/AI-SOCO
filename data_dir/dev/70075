// start + (end - start) / 2;

#define OO ll(1e12)
#define MOD ll(1000000007)
#define BASE 128LL
#define BASEINV (ll)570312504
using namespace std;

int main() {
	int n;
	cin >> n;
	set<int>st;
	map<int, int>mp;
	int c = 0, b = 0;
	for (int i = 0; i < n; i++) {
		int a;
		cin >> a;
		mp[a]++;
		if (!c)c = a;
		else if (a != c) b = a;
	}
	if (mp.size() == 2 && mp[c] == mp[b])puts("YES"), cout << c << " " << b << "\n";
	else puts("NO");
}