const lli INF = 0xFFFFFFFFFFFFFFFL;

lli seed;
mt19937 rng(seed=chrono::steady_clock::now().time_since_epoch().count());
inline lli rnd(lli l=0,lli r=INF)
{return uniform_int_distribution<lli>(l,r)(rng);}

class CMP
{public:
bool operator()(ii a , ii b) //For min priority_queue .
{    return ! ( a.X < b.X || a.X==b.X && a.Y <= b.Y);   }};

void add( map<lli,lli> &m, lli x,lli cnt=1)
{
    auto jt=m.find(x);
    if(jt==m.end())         m.insert({x,cnt});
    else                    jt->Y+=cnt;
}

void del( map<lli,lli> &m, lli x,lli cnt=1)
{
    auto jt=m.find(x);
    if(jt->Y<=cnt)            m.erase(jt);
    else                      jt->Y-=cnt;
}

bool cmp(const ii &a,const ii &b)
{
    return a.X<b.X||(a.X==b.X&&a.Y<b.Y);
}

const lli mod = 1000000007L;
const lli N = 1000L;

    lli T,n,i,j,k,in,cnt,l,r,u,v,x,y;
    lli m;
    string s;
    bool a[N+5][2*N+5];
    //priority_queue < ii , vector < ii > , CMP > pq;// min priority_queue .

void add(lli pos)
{
    lli cnt=0;
    if(s[pos]=='W')
        cnt=1;
    if(s[pos]=='L')
        cnt=-1;

    lli i;
    // dbg(cnt,s[pos]);
    repA(i,N-k+1,N+k-1)
        a[pos][i]|=a[pos-1][i-cnt];

    i=N+k;
    if(pos==n)
        a[pos][i]|=a[pos-1][i-cnt];

    i=N-k;
    if(pos==n)
        a[pos][i]|=a[pos-1][i-cnt];
}

void solve(lli pos,lli v)
{
    dbg(pos,v);
    assert(a[pos][v+N]);
    if(pos==0)
    {
        assert(v==0);
        return;
    }

    if(s[pos]!='?')
    {
        if(s[pos]=='W')
            solve(pos-1,v-1);
        else if(s[pos]=='L')
            solve(pos-1,v+1);
        else
            solve(pos-1,v);
        return;
    }

    if(v==k-1)
    {
        if(a[pos-1][v-1+N])
        {
            s[pos]='W';
            solve(pos-1,v-1);
        }
        else
        {
            s[pos]='D';
            solve(pos-1,v);
        }
    }
    else if(v==-k+1)
    {
        if(a[pos-1][v+1+N])
        {
            s[pos]='L';
            solve(pos-1,v+1);
        }
        else
        {
            s[pos]='D';
            solve(pos-1,v);
        }
    }
    else
    {
        if(a[pos-1][v+N])
        {
            s[pos]='D';
            solve(pos-1,v);
        }
        else if(a[pos-1][v+1+N])
        {
            s[pos]='L';
            solve(pos-1,v+1);
        }
        else
        {
            s[pos]='W';
            solve(pos-1,v-1);
        }
    }
}

int main(void) {
    ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
    // freopen("txt.in", "r", stdin);
    // freopen("txt.out", "w", stdout);
// cout<<std::fixed<<std::setprecision(35);
// cin>>T;while(T--)
{

    cin>>n>>k;
    cin>>s;
    s="0"+s;
    a[0][N]=true;
    repA(i,1,n)
    {
        if(s[i]=='?')
        {
            s[i]='W';
            add(i);
            s[i]='L';
            add(i);
            s[i]='D';
            add(i);
            s[i]='?';
        }
        else
            add(i);
    }

    dbg(a[n][N+k],a[n][N-k]);
    dbg(a[1][N-1]);
    dbg(a[2][N-2],a[2][N]);
    dbg(a[3][N-1],a[2][N+1]);

    if(a[n][N+k]&&a[n][N-k])
    {
        if(s[n]=='?')
            s[n]='W';
        if(s[n]=='W')
            solve(n-1,k-1);
        else if(s[n]=='L')
            solve(n-1,-k+1);
        else
            s="0NO";
    }
    else if(a[n][N+k])
    {
        s[n]='W';
        solve(n-1,+k-1);
    }
    else if(a[n][N-k])
    {
        s[n]='L';
        solve(n-1,-k+1);
    }
    else
        s="0NO";

    cout<<s.substr(1)<<endl;

}   aryanc403();
    return 0;
}