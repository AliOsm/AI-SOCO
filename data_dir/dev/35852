#include<bits/stdc++.h>
using namespace std;

int main() {
    int a, b, c, d, e, f;
    scanf("%d%d%d%d%d%d", &a, &b, &c, &d, &e, &f);
    if(a == 0 && b == 0 && c) {puts("0"); return 0;}
    if(d == 0 && e == 0 && f) {puts("0"); return 0;}
    if(a == 0 && b == 0) {puts("-1"); return 0;}
    if(d == 0 && e == 0) {puts("-1"); return 0;}
    if(a * e == b * d) {
        if(a * f == c * d && b * f == c * e) puts("-1");
        else puts("0");
    }
    else puts("1");
}
