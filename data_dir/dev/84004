/*************************************************/
ll n,m,i,j,a[1000005],b[1000005],f[1000005],t[1000005],rev;
void solve(ll x,ll y)
{
	if (rev)
	{
	cout<<f[y]-f[x]<<endl;
	forup(i,f[x]+1,f[y]) cout<<i<<' ';cout<<endl;
	}
	cout<<y-x<<endl;
	forup(i,x+1,y) cout<<i<<' ';cout<<endl;
	if (!rev)
	{
	cout<<f[y]-f[x]<<endl;
	forup(i,f[x]+1,f[y]) cout<<i<<' ';cout<<endl;
	}
	exit(0);
}
int main()
{
	read(n);
	rep(i,n)
	{
		read(a[i]);
		a[i]+=a[i-1];
	}
	rep(i,n)
	{
		read(b[i]);
		b[i]+=b[i-1];
	}
	if (a[n]>b[n])
	{
		rep(i,n)
		{
			swap(a[i],b[i]);
		}
		rev=1;
	}
	f[0]=0;
	rep(i,n)
	{
		f[i]=f[i-1];
		while (b[f[i]+1]<=a[i]) f[i]++;
	}
	memset(t,-1,sizeof(t));
	t[0]=0;
	rep(i,n)
	{
		if (t[a[i]-b[f[i]]]!=-1) solve(t[a[i]-b[f[i]]],i);
		t[a[i]-b[f[i]]]=i;
	}
	return -1;
}