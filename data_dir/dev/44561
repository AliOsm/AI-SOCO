#include <iostream>
#include <algorithm>
#include <vector>
#include <set>
#include <stack>
#include <map>
#include <iomanip>
#include <cmath>
using namespace std;
int main() {

  ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

  int t = 0;

  cin >> t;

  while(t--) {
    int a = 0, b = 0, n = 0;
    cin >> a >> b >> n;
    int f[3] = { a, b, a ^ b };
    cout << f[n % 3] << endl;
  }

  return 0;
}
