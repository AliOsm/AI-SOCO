int get(int i, int j){
	cout << "? " << i << " " << j << endl;
	cout.flush();
	int ans;
	cin >> ans;
	return ans;
}
int main() {
	sync;
	srand(time(NULL));
	cout.precision(10);
	cout << fixed;
#ifdef LOCAL
	//input;
#else

#endif

	int n;
	cin >> n;

	int ar[n + 1];
	int x = get(1, 2);
	int y = get(2, 3);
	int z = get(1, 3);

	ar[2] = (x + y - z) / 2;
	ar[1] = (x - ar[2]);
	ar[3] = (y - ar[2]);

	for(int i = 4; i <= n; i++){
		ar[i] = get(1, i) - ar[1];
	}

	cout << "!";
	for(int i = 1; i <= n; i++)
		cout << " " << ar[i];
	cout << endl;
	cout.flush();

}
