#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
#include <set>
#include <queue>
#include <deque>
#include <map>
#include <stack>
#include<bitset>
#include<list>
#include<cassert>
#include<numeric>
 
using namespace std ;


int main()
{
	int a, b, c, d;
	cin >> a >> b >> c >> d;
	int misha = max(3 * a / 10, a - a / 250 * c);
	int vasya = max(3 * b / 10, b - b / 250 * d);
	if (misha > vasya)
		cout << "Misha";
	if (vasya == misha)
		cout << "Tie";
	if (misha < vasya)
		cout << "Vasya"; 
}