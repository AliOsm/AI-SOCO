//++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
//++--++--++--++--++--++--++--++--++--+--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
const int MAXN = 5001;
const int inf = 1000000000;
const ld INF = 1.0 / 0.0;
const ld EPS = 1e-7;
const int mod = inf + 7;
signed main()
{
	fastio();
	int q;
	cin >> q;
	while (q--)
	{

		int l1, r1, l2, r2;
		cin >> l1 >> r1 >> l2 >> r2;
		set<int> a;
		set<int> b;
		a.insert(l1); a.insert(r1);
		b.insert(l2); b.insert(r2);
		b.insert(l2 + 1);
		b.insert(r2 - 1);
		a.insert(l1 + 1);
		a.insert(r1 - 1);
		for (int i : a)
		{
			bool f = 0;
			for (int j : b)
			{
				if (i != j)
				{
					cout << i << ' ' << j << endl;
					f = 1;
					break;
				}


			}
			if (f) break;
		}
	}
}