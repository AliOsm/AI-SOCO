typedef vector<int> vi;
typedef long long ll;
typedef long double ld;
typedef double D;
typedef pair<int, int> ii;
typedef vector<ii> vii;
typedef vector<string> vs;
typedef vector<vi> vvi;

template<class T> T abs(T x) { re x > 0 ? x : -x; }

const string pok[8] = {"vaporeon", "jolteon", "flareon", "espeon", "umbreon", "leafeon", "glaceon", "sylveon"};

int n;
int m;
string s;

int main () {
    cin >> n >> s;
    for (int i = 0; i < 8; i++)
        if (sz (pok[i]) == n) {
            int ok = 1;
            for (int j = 0; j < n; j++)
                if (!(s[j] == '.' || s[j] == pok[i][j]))
                    ok = 0;
            if (ok) cout << pok[i] << endl;
        }
    return 0;
}