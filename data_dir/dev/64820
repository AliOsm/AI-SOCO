#include <bits/stdc++.h>
#define ll long long
using namespace std;
ll a,l,r,a1,b1,m,c1,b,c,d,p[100000],k,n;
string s,t;
int main()
{ 
 cin>>n;
    for(int i=1;i<=n;i++)
    {   
        cin>>a1>>b1>>c1;
        a+=a1;b+=b1;c+=c1;
    }
    if (a==0 && b==0 && c==0) cout<<"YES"; else cout<<"NO";
}