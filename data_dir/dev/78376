#include <bits/stdc++.h>
using namespace std;

string S;

int main()
{
    ios::sync_with_stdio(0);
    cin.tie(NULL), cout.tie(NULL);

    int N;
    cin >> N >> S;

    int x = 0, y = 0;

    for(auto s : S)
    {
        if(s == 'L')
            x++;
        else
            y++;
    }

    cout << x + y + 1 << "\n";
    return 0;
}
