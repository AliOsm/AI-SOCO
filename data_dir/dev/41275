#include<bits/stdc++.h>
#define r_0 return 0;
#define ld long  double
#define ll long long int
#define pb push_back
#define TIME ios_base::sync_with_stdio(NULL);
#define tcn cin.tie(NULL);
#define tct cout.tie(NULL);
using namespace std;
ll n,x,y,z,a,b,c,cnt,mx=1e9,mn=1e9,ans;
string s,s2,s1[6]={"Danil","Olya","Slava","Ann","Nikita"};
const ll N=1e7+7;
vector<int>v;
ll used[N];
main(){
cin >> s;
for( int i=0;i < s.size();i ++ ){
        if( s[i]=='D' and s[i+1]=='a' and s[i+2]=='n' and s[i+3]=='i' and s[i+4]=='l' )
            cnt++;
        if( s[i]=='O' and s[i+1]=='l' and s[i+2]=='y' and s[i+3]=='a' )
            cnt++;
        if( s[i]=='S' and s[i+1]=='l' and s[i+2]=='a' and s[i+3]=='v' and s[i+4]=='a' )
            cnt++;
        if( s[i]=='A' and s[i+1]=='n' and s[i+2]=='n' )
        cnt++;
        if( s[i]=='N' and s[i+1]=='i' and s[i+2]=='k' and s[i+3]=='i' and s[i+4]=='t' and s[i+5]=='a' )
            cnt++;
}
if( cnt==1 )
    cout<<"YES";
else
    cout<<"NO";
}
