#include <bits/stdc++.h>
using namespace std;
#define pb  push_back
#define ll  long long
#define vi  vector<ll >
#define vvi vector<vi >
#define all(x) x.begin(), x.end()

ll b, g, n;
set<ll> s;

int main() {
    ios_base::sync_with_stdio(false);
    cin >> b >> g >> n;
    for (ll i = 0; i <= b; ++i)
        if (0 <= n - i && n - i <= g)
            s.insert(i);
    cout << s.size();

    return 0;
}
