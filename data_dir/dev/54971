int main()
{
    int n,t;cin>>n>>t;
    
    if(n==1){
        if(t==10){
            cout<<-1;
        }
        else{
            cout<<t;
        }
        return 0;
    }
    
    if(t==10){
        cout<<1;
        for(int i=1;i<n;i++)cout<<0;
    }
    else{
        cout<<t;
        for(int i=1;i<n;i++)cout<<0;
    }
            

    return 0;
}
