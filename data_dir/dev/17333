#include <bits/stdc++.h>
using namespace std;
#define ll long long

bool prime(ll x)
{
    for(ll i = 2; i<x; i++)
    {
        if (x%i==0) return false;
    }
    return true;
}
vector <ll> P;
int main()
{
  ll n,k,i;
  cin>>n>>k;
  for(i=0; i<k; i++)
  {
      if (n%10==0) n/=10;
      else
        n--;
  }
  cout<<n;




    return 0;
}
