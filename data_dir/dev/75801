#include <cstdio>
#include <cstring>
#include <algorithm>
#include <cmath>
using LL = long long;

int main() {
    int n = 0;
    int k = 0;
    scanf("%d%d", &n, &k);
    for (int i = 0; i < k; ++i) {
        if (n % 10 == 0) {
            n /= 10;
        } else {
            --n;
        }
    }
    printf("%d\n", n);
    return 0;
}
