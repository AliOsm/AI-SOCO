#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef pair <int, int> pii;

const int N = 1e5 + 5;

int main() {
	ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);	
	int tc;
	cin >> tc;
	while(tc--) {
		int n, m;
		cin >> n >> m;
		if(n % m == 0)
			cout << "YES\n";
		else
			cout << "NO\n";
	}
}
