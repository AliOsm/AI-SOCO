#include <bits/stdc++.h>


using namespace std;

const int N = 2e3+5 , mod = 1000000007;

int t , a , b , c;


int main(){
    cin>>t;
    while(t-- && cin>>a>>b>>c)
    {
        if(c%3==0)
        {
            cout<<a<<"\n";
        }
        else if(c%3==1)
        {
            cout<<b<<"\n";

        }
        else
        {
            cout<<(a^b)<<"\n";
        }
    }
    return 0;
}

