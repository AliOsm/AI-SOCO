//              +-- -- --++-- +-In the name of ALLAH-+ --++-- -- --+              \\

#include <bits/stdc++.h>

using namespace std ;

int const N = 2e5 + 20 ;
int n , ans ;
bool mark[N] ;

int main(){
	ios::sync_with_stdio(false) , cin.tie(0) , cout.tie(0) ;

	cin >> n ;

	mark[0] = 1 ;

	while (n --) {
		int x ;
		cin >> x ;
		if (!mark[x]) mark[x] = 1 ;
		else ans ++ ;
	}
	
	cout << ans << '\n' ;
}
