#include<bits/stdc++.h>
#define ll long long
#define ld long double
#define pii pair<int,int>
using namespace std;
int main(){
    int a,b,c; cin>>a>>b>>c;
    int x,y,z; cin>>x>>y>>z;
    int s1,s2,s3;
    if(a>=x)
        s1=(a-x)/2;
    else s1=a-x;
    if(b>=y)
        s2=(b-y)/2;
    else
        s2=b-y;
    if(c>=z)
        s3=(c-z)/2;
    else
        s3=c-z;
    if(s1+s2+s3>=0){cout<<"Yes";}else cout<<"No";
}
