#include <bits/stdc++.h>

#define f first
#define s second

#define pb push_back
#define ppb pop_back
#define mp make_pair

#define ld long double
#define ll long long
#define ull unsigned ll
#define pii pair <int, int>
#define pll pair <ll, ll>

#define bit(x) __builtin_popcountll(x)
#define all(x) x.begin(), x.end()
#define sqr(x) ((x) * 1ll * (x))
#define sz(x) (int)x.size()

#define purple ios_base :: sync_with_stdio(0), cin.tie(0), cout.tie(0);
#define rep(_i, _from, _to) for (int _i = _from; _i <= _to; ++_i)
#define per(_i, _from, _to) for (int _i = _from; _i >= _to; --_i)

#define nl '\n'
#define ioi exit(0);

#define _27day ""

using namespace std;

const int N = 1e6 + 7, mod = 1e9 + 7, inf = 1e7 + 7;
const ll linf = (ll)1e18 + 7;
const ld eps = 1e-15, pi = 3.141592;
const int dx[] = {-1, 0, 1, 0, 1, -1, -1, 1}, dy[] = {0, 1, 0, -1, 1, -1, 1, -1};

  int n, m, z, ans;
  int cnt[N];
int main() {
  #ifndef _27day
    freopen (_27day".in", "r", stdin);
    freopen (_27day".out", "w", stdout);
  #endif

  cin >> n >> m >> z;
  for (int i = n; i <= z; i += n)
    cnt[i]++;

  for (int i = m; i <= z; i += m)
    if (cnt[i]) ans++;

  cout << ans;
  ioi
}
