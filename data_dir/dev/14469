#include<bits/stdc++.h>
#define ll long long
using namespace std;
ll A,B,ans;
int w33ha(){
    cin>>A>>B;
    ll g=9;
    ans=0;
    while(g<=B){
        ans+=A;
        g=g*10+9;
    }
    cout<<ans<<endl;
    return 0;
}
int main(){
    int T;cin>>T;
    while(T--)w33ha();
    return 0;
}
