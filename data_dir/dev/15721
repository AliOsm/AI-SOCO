int64 mask;
int64 n;
int64 ans = 0;
int mix[] = {0, 2, 4, 3, 1};
string st, blend;
void solve()
{
	cin >> n;
	n %= 360;
	int alfa = min(abs(n), 360 - n);
	n = (360 + n) % 360;
	int ans = n / 90;
	n -= ans * 90;
	if (90 < 2 * n)
	{
		n -= 90;
		ans++;
	}
	if (alfa <= abs(n))
		cout << 0;
	else
		cout << ans;
}

int main()
{
    //ios_base::sync_with_stdio(false); cin.tie(0);
#ifdef _MY_DEBUG
    freopen("input.txt", "rt", stdin); freopen("output.txt", "wt", stdout);
#else
    //freopen(TASK ".in", "rt", stdin); freopen(TASK ".out", "wt", stdout);
#endif
    //stresstest(); return 0;
    srand(1313);
	
	solve();
	
    return 0;
}