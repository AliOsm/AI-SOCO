const lli INF = 0xFFFFFFFFFFFFFFFL;

lli seed;
mt19937 rng(seed=chrono::steady_clock::now().time_since_epoch().count());
inline lli rnd(lli l=0,lli r=INF)
{return uniform_int_distribution<lli>(l,r)(rng);}

class CMP
{public:
bool operator()(ii a , ii b) //For min priority_queue .
{    return ! ( a.X < b.X || a.X==b.X && a.Y <= b.Y);   }};

void add( map<lli,lli> &m, lli x,lli cnt=1)
{
    auto jt=m.find(x);
    if(jt==m.end())         m.insert({x,cnt});
    else                    jt->Y+=cnt;
}

void del( map<lli,lli> &m, lli x,lli cnt=1)
{
    auto jt=m.find(x);
    if(jt->Y<=cnt)            m.erase(jt);
    else                      jt->Y-=cnt;
}

bool cmp(const ii &a,const ii &b)
{
    return a.X<b.X||(a.X==b.X&&a.Y<b.Y);
}

const lli mod = 1000000007L;
const lli maxN = 30000L;

    lli T,n,i,j,k,in,cnt,l,r,u,v,x,y,N;
    lli m;
    string s;
    vii a;
    vi b;
    map<ii,vi> flag;
    //priority_queue < ii , vector < ii > , CMP > pq;// min priority_queue .

void add(ii x,ii y)
{
    lli u,v,w,g;
    v=y.X-x.X;
    u=-y.Y+x.Y;

    if(u==0)
    {
        v=1;
    }
    else if(v==0)
    {
        w=1;
    }
    else
    {
        g=__gcd(abs(u),abs(v));
        u/=g;v/=g;
    }

    w=-u*x.X-v*x.Y;
    // dbg(x,y,u,v,w,"Pre");
    if(u==0)
    {
        if(w==0)
        {
            v=1;
        }
        else if(v==0)
        {
            w=1;
        }
        else
        {
            g=__gcd(abs(w),abs(v));
            w/=g;v/=g;
            if(v<0)
            {
                v*=-1;
                w*=-1;
            }
        }
    }
    else if(v==0)
    {
        if(w==0)
        {
            u=1;
        }
        else if(u==0)
        {
            w=1;
        }
        else
        {
            g=__gcd(abs(w),abs(u));
            u/=g;w/=g;
            if(u<0)
            {
                u*=-1;
                w*=-1;
            }
        }
    }
    else if(u<0)
    {
        u*=-1;v*=-1;w*=-1;
    }
    dbg(u,v,w);
    flag[{u,v}].pb(w);
}

lli find(vi &a)
{
    lli n=(lli)a.size(),ans=0,cnt=0;
    lli i=0,j=0;
    if(n==0)
        return 0;

    sort(a.begin(),a.end());
    while(i<n)
    {
        j=i;
        while(j<n&&a[i]==a[j])
            j++;
        cnt++;
        N++;
        i=j;
    }
    ans=cnt*(cnt-1)/2;
    dbg(a,ans);
    return ans;
}

int main(void) {
    ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
    // freopen("txt.in", "r", stdin);
    // freopen("txt.out", "w", stdout);
// cout<<std::fixed<<std::setprecision(35);
// cin>>T;while(T--)
{

    cin>>n;
    a.clear();a.reserve(n);

    fo(i,n)
    {
        cin>>x>>y;
        a.pb({x,y});
    }

    fo(i,n)
    repA(j,i+1,n-1)
        add(a[i],a[j]);
    N=0;
    lli ans=0,bb=0;

    for(auto &x:flag)
        ans-=find(x.Y);

    ans+=N*(N-1)/2;
    cout<<ans<<endl;
}   aryanc403();
    return 0;
}