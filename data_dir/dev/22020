//----------------------------------------------------------
int n, m;
int k, dis[300], p[300], c = 0, b = -1;

void init()
{   
    cin >> n;
    string st;
    for (int i = 0; i < n; ++i)
    {
        vi tr;
        cin >> st;
        int x = 0;
        for(int j = 1; j <= 12; ++j)
        {
            if (12 % j)
                continue;
            int k = 12 / j; 
            
            for (int jj = 0; jj < 12 / j ; ++jj)
            {
                x = 0;
                for (int ii = jj; ii < 12; ii += k)
                {
                    if (st[ii] == 'X')
                        x++;
                }
                if (x == j)
                {
                    tr.push_back(j);
                    break;
                }
            }
        }
        cout << tr.size() << ' ';
        for (int j = 0; j < tr.size(); ++j)
        {
            cout << tr[j] << 'x' << 12 / tr[j] << ' '; 
        }
        cout << endl;
    }
  
}

