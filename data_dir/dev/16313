#include<bits/stdc++.h>
using namespace std;
#define int long long int
main()
{
	int t;
	cin>>t;
	while(t--)
	{int n,a,b,c,d;
	cin>>n>>a>>b>>c>>d;
	bool flag=false;
	int w1=n*(a+b);
	int w2=n*(a-b);
	int ww1=c+d;
	int ww2=c-d;
	if(ww2<=w1&&ww1>=w2)
		flag=true;
	if(flag)
		cout<<"Yes\n";
	else
		cout<<"No\n";}


}