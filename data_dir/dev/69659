#include <bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(0);
    int tcs;
    string s;
    cin>>tcs;
    while(tcs--){
        cin>>s;
        sort(s.begin(),s.end());
        if(s[0]==s.back())
            cout<<-1<<endl;
        else
            cout<<s<<endl;
    }
    return 0;
}
