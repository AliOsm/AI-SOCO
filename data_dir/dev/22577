int abv(int a){
    
}


int main(){
    int x,y;cin>>x>>y;
    int n;cin>>n;
    double answer=1000.0;
    int a,b,v;
    double cur;
    double one=1.;
    for(int i=0;i<n;i++){
        cin>>a>>b>>v;
        
        cur=sqrt( (((x-a)*(x-a)+(y-b)*(y-b))*one))/v;
        if(cur<answer)answer=cur;
    }
    printf("%.7f",answer);


    return 0;
}