#include <stdio.h>

int n , k , i, t,c ,r,d ;
char s ;

int main()
{
    scanf("%d %d" , &n, &k) ;
    if ( k == 1)
    {
        if( n== 1)
        {
            printf("a") ;

        }
        else{
            printf("-1") ;

        }
    }
   else  if ( n < k )
    {
        printf("-1" ) ;
        return 0 ;

    }
    else
    {
        r = k - 2 ;
        d = n - r ;
        c = d/ 2 ;
        for ( i = 0 ; i < c ; i++)
        {
            printf("ab" ) ;
        }
        if( d %2 == 1)
        {
            printf("a" ) ;
        }
        for( i = 0 ; i < r ; i++)
        {
            s = 'a' + 2 + i ;
            printf("%c" , s) ;
        }
    }
 return 0 ;
 }
