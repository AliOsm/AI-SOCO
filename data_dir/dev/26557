#include <bits/stdc++.h>
using namespace std;
long long  i, j, n, c[123456],a[1111234], b[1125678],d[123456], l,ll,r, s, t, k, m, xh, x, x1, y,z;
string p,q;
int main() {
    cin >> p;
    n = p.size();
    for (i = 1; i< n-1; i ++){
        if (p[i]!='.'&&p[i-1]!='.'&&p[i+1]!='.'&&p[i] != p[i-1] && p[i]!=p[i+1]&&p[i-1]!=p[i+1]){
            cout << "Yes";
            return 0;
        }
    }
    cout << "No";
    return 0;
}