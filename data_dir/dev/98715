#include <bits/stdc++.h>
#define	ss second
#define ff first
#define use_fast ios::sync_with_stdio(false), cin.tie(0), cout.tie(0)
#define ret(n) return cout << n, 0
#define se(n) cout << setprecision(n) << fixed
#define pb push_back
#define int long long
#define ld long double
using namespace std; 
 
const int N = 3e5 + 10000, OO = 1e9, M = 1e9 + 7, P = 6151, SQ = 1000, lg = 12;
typedef pair <int, int> pii;

void solve() {
	int n, k1, k2;
	cin >> n >> k1 >> k2;
	bool u = false;
	for(int i = 1; i <= k1; i++) {
		int x;
		cin >> x;
		if(x == n)
			u = true;
	}
	for(int i = 1; i <= k2; i++) {
		int x;
		cin >> x;
		if(x == n)
			u = false;
	}
	if(u)
		cout << "YES" << endl;
	else
		cout << "NO" << endl;
}

int32_t main() {
    use_fast;
    int t;
    cin >> t;
    while(t--) 
    	solve();
	return 0; 
}
