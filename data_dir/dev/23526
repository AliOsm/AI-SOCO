#include <iostream>

using namespace std;

int main()
{
    string s;
    cin >> s;
    char mn='z'+1;
    for (auto c:s)
    {
        if (c<=mn)
        {
            cout << "Mike" << endl;
        } else {
            cout << "Ann" << endl;
        }
        mn=min(mn,c);
    }
    return 0;
}
