#include <algorithm>
#include <iostream>
#include <cmath>
#include <fstream>
#include <vector>
#include <iomanip>
#include <string>
#include <queue>
#include <map>
#include <stack>
#include <set>

using namespace std;

typedef long long ll;
typedef long double ld;

template <typename T>
T gcd(T a, T b) {
	while (a > 0 && b > 0) {
		if (a > b) a %= b; else b %= a;
	}
	return (a + b);
}

int main() {
	//freopen("input.txt", "r", stdin);
	int n, m;
	cin >> n >> m;
	bool ok = true;
	for (int i = 1; i <= n * m; ++i) {
		char ch;
		cin >> ch;
		if (!(ch == 'B' || ch == 'W' || ch == 'G')) ok = false;
	}
	if (ok) {
		cout << "#Black&White" << endl;
	}
	else {
		cout << "#Color" << endl;
	}
	return 0;
}