#include<bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL),cout.tie(NULL);

    long long n,k;
    cin >> n >> k;
    if(k % n == 0){
        cout<<(k/n)<<endl;
    }
    else{
        cout<<(k/n)+1<<endl;
    }
    return 0;
}
