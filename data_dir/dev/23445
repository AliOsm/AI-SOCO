#include <bits/stdc++.h>
using namespace std;
int main(){
	int n, k = 0;
	cin >> n;
	string a[n];
	for(int i = 0; i < n; i ++){
		cin >> a[i];
		if(a[i] == "Tetrahedron"){
			k += 4;
		}
		if(a[i] == "Cube"){
			k += 6;
		}
		if(a[i] == "Octahedron"){
			k += 8;
		}
		if(a[i] == "Dodecahedron"){
			k += 12;
		}
		if(a[i] == "Icosahedron"){
			k += 20;
		}
	}
	cout << k;
}

