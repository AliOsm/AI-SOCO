#include <bits/stdc++.h>
#define endl '\n'
#define debug(X) cout << #X << " = " << X << endl

using namespace std;

int main() {
	ios_base::sync_with_stdio(false); cin.tie(0);
	#ifdef UTP
	freopen("input.txt", "r", stdin);
	#endif
	long long H, L; cin >> H >> L;
	double ans = (L * L - H * H) * 1.0 / (2.0 * H);
	cout << fixed << setprecision(8) << ans << endl;
	return 0;
}
