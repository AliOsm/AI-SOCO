#include<bits/stdc++.h>
#define ll long long
#define ld long double
#define mp make_pair
#define fi first
#define se second
#define pb push_back
#define y1 hvkusfdk
using namespace std;
int n, a[5005];
int main(){
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    //freopen("password.in", "r", stdin);
    //freopen("password.out", "w", stdout);
    cin >>n;
    for (int i = 1; i <= n; ++i){
        cin >>a[i];
    }
    for (int i = 1; i <= n; ++i){
        if (a[a[a[i]]] == i)return cout <<"YES", 0;
    }
    cout <<"NO";
}
