#include <bits/stdc++.h>

typedef long long ll;
using namespace std;

int main(){
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    int x;
    for(int i=0;i<5;i++)
        for(int j=0;j<5;j++){
            cin >> x;
            if(x)
                cout << abs(i-2)+abs(j-2) << endl;
        }

    return 0;
}
