const int MX=40000, SZ=500000;
bool p[MX];

bool ok(int x){
	if(x==2)return 1;
	if(x==1||x%4!=1)return 0;
	for(int i=2;i*i<=x;i++)if(!p[i]&&x%i==0)return 0;
	return 1;
}

void run()
{
	for(int i=2;i*i<MX;i++)if(!p[i])for(int j=i*i;j<MX;j+=i)p[j]=1;
	
	int l,r,ans=0; cin>>l>>r;
	for(;l<=r&&l%SZ;l++)if(ok(l))ans++;
	for(;l<=r&&(r+1)%SZ;r--)if(ok(r))ans++;
	
	if(l<=r)for(int i=l/SZ;i<=r/SZ;i++)ans+=prime[i];
	cout<<ans<<endl;
}

int main()
{
#ifndef ONLINE_JUDGE
	int cs=2;rep(i,cs)
#endif
	run();
}
