#include<bits/stdc++.h>
using namespace std;

typedef long long ll;

int main(void)
{
    ios::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);

    int n;
    cin >> n;

    if(n==2)cout << 2 << endl;
    else cout << 1 << endl;

    return 0;
}
