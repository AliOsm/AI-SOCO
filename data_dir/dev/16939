#include<bits/stdc++.h>
using namespace std;
#define forr(i,a,n)	for(long long int i=a; i<n; i++)
#define loop(i,a,n)	for(long long int i=a; i>=n; i--)

int main() 
{	
	ios_base :: sync_with_stdio(false),cin.tie(NULL),cout.tie(0);
	
	long long int a,b,c,d,e;
	cin>>a>>b>>c>>d>>e;
	a=max(a,c),b=min(b,d);
	if(b<c)
		cout<<0;
	else
	{
	
		if(e>=a and e<=b)
			b--;
		cout<<max(0ll,b-a+1);
	}
	
	return 0;	
}