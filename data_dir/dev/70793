#pragma GCC omptimize("unroll-loops")
#pragma optimize("SEX_ON_THE_BEACH")
#pragma GCC optimize("no-stack-protector")
#pragma comment(linker, "/STACK:1000000000")
#include<bits/stdc++.h>

using namespace std;

//#define need_tree

#ifdef need_tree
    #include<ext/pb_ds/assoc_container.hpp>
    using namespace __gnu_pbds;
    template<class T> using ordered_set = tree<T, null_type, less<T>, rb_tree_tag, tree_order_statistics_node_update>;
#endif // need_tree

using ll = long long int;
using ull = unsigned long long int;
using dd = long double;
using ldd = long double;
using si = short int;
using pii = pair<int, int>;
using pll = pair<ll, ll>;

ll get_seed(string s)
{
    ll ans = 0;
    for (int i = 0; i < s.size(); ++i)
        ans += s[i];
    return ans;
}

int popcount(int x)
{
    return __builtin_popcount(x);
}

ll popcount(ll x)
{
    return __builtin_popcount(x);
}

#define all(x) (x).begin(), (x).end()
#define ff first
#define ss second


// debug defines
#ifdef HOME
    int jjj;
    #define PO cout << "Pomelo" << endl;
    #define OL cout << "Oliva" << endl;
    #define debug(x) cout << #x << " " << x << endl;
    #define debug_p(x) cout << #x << " " << x.ff << " " << x.ss << endl;
    #define debug_v(x) cout << #x << " "; for (auto iii : x) cout << iii << " "; cout << endl;
    #define debug_vp(x) cout << #x << " "; for (auto ii : x) cout << '[' << ii.ff << " " << ii.ss << ']'; cout << endl;
    #define wait() cin >> jjj;
    #define say(x) cout << x << endl;
#else
    #define PO 0;
    #define OL 0;
    #define debug(x) 0;
    #define debug_p(x) 0;
    #define debug_vp(x) 0;
    #define debug_v(x) 0;
    #define debug_vp(x) 0;
    #define wait() 0;
    #define say(x) 0;
#endif // HOME
// end of debug defines

vector<int> rh(3e6);
vector<int> prime;

void build()
{
    for (int i = 2; i < rh.size(); ++i)
    {
        if (rh[i])
            continue;
        prime.push_back(i);
        for (ll j = (ll)(i) * (ll)(i); j < rh.size(); j += i){
            if (rh[j] == 0)
                rh[j] = i;
        }
    }
}



void solve(int i)
{
    int n;
    cin >> n;
    vector<int> arr(2 * n);
    for (int i = 0; i < n * 2; ++i)
        cin >> arr[i];
    sort(all(arr));
    build();
    vector<int> a;
    reverse(all(arr));
    multiset<int> dest;
    vector<int> noneed;
    debug_v(arr);
    for (int i : arr)
    {
        if (dest.find(i) != dest.end())
        {
            dest.erase(dest.find(i));
            continue;
        }
        if (rh[i] == 0)
        {
            noneed.push_back(i);
            continue;
        }
        a.push_back(i);
        dest.insert(i / rh[i]);
    }
    debug_v(a);
    sort(all(noneed));
    debug_v(noneed);
    debug_v(dest);
    for (int i : noneed)
    {
        if (dest.find(i) != dest.end()){
            dest.erase(dest.find(i));
            continue;
        }
        a.push_back(i);
        dest.insert(prime[i - 1]);
        debug_v(dest);
    }
    for (int i : a)
        cout << i << " ";
}

int main()
{
    ios_base::sync_with_stdio(0);
    cout.tie(0);
    cin.tie(0);

    int t;
    t = 1;
    //cin >> t;
    for (int i = 0; i < t; ++i)
        solve(i);

    return 0;
}
