#include <bits/stdc++.h>
#define ll long long int
#define mp make_pair
#define pb push_back
#define pii pair<int, int>
#define pll pair<ll, ll>
#pragma GCC optimize ("O3")
using namespace std;
int main () {
    ios::sync_with_stdio(false);
    cin.tie (0);
    int n, m; cin>>n>>m;
    vector <int> num (m, n/m);
    for (int i=0;i<n-n/m*m;++i) num[i]++;
    for (int i : num) cout<<i<<' ';
    return 0;
}