#include <bits/stdc++.h>

using namespace std;
typedef long long ll;
const int maxn = 2e5 + 17;
int n, l, r;

int main(){
	ios::sync_with_stdio(0), cin.tie(0);
	cin >> n;
	while(n--){
		cin >> l >> r;
		cout << l << ' ' << l * 2 << '\n';
	}
}
