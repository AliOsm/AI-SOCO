#include<iostream>
#include <bits/stdc++.h>
#define ll long long
using namespace std;
int main()
{
    ll a , b ;
    cin>>a>>b;
    while(true)
    {
        if(a==0||b==0)
            break ;
        if(a>=2*b)
         a%=(2*b);
        else if(b>=2*a)
            b%=(2*a);
        else
            break ;

    }
    cout<<a<<" "<<b;
    return 0;
}
