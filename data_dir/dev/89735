#include <bits/stdc++.h>
using namespace std;
#define ll long long 
const long long N=1e6+5;
#define fast ios_base::sync_with_stdio(0);cin.tie(NULL);cout.tie(NULL) 
int main() {
       fast;
   long double x,y;
   cin>>x>>y;
  // cout<<log(x);
  long double s=y*log(x);
  long double t=x*log(y);
  // cout<<s<<" "<<t<<" ";
    if(s<t)
   cout<<"<";
   else if(s>t)
   cout<<">";
   else
   cout<<"=";
	return 0;
}