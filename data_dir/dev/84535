#include <iostream>

using namespace std;

int main()
{
    int n, i, pr, nr;
    string a;
    bool t = false;
    cin >> n;
    for (i = 0; i < n; i++)
    {
        cin >> a >> pr >> nr;
        if (pr >= 2400 && nr > pr) t = true;
    }
    if (t == true) cout << "YES";
    else cout << "NO";
    return 0;
}
