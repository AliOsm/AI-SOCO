#include<bits/stdc++.h>
using namespace std;
long long a,b,c,d,i,e,f,g,n,m,k,l,ans,A[200005];
int main()
{
           cin>>n;
           for(long long i=1;i<=2*n;i++)
           {
                      cin>>A[i];
                      if(i!=1 && A[i]!=A[i-1]) k=1;
           }
           if(k==0) { cout<<-1; return 0; }
           sort(A+1,A+1+2*n);
           for(long long i=1;i<=2*n;i++)
           cout<<A[i]<<" ";
}