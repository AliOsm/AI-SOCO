#include <bits/stdc++.h>
using namespace std;

int main()
{
    string arr="qwertyuiopasdfghjkl;zxcvbnm,./";
    char c;
    cin>>c;
    if(c=='R')
    {
        string s;
        cin>>s;
        for(int i=0; i<s.size(); i++)
        {

            char x=s[i];
            int num=arr.find(x);
            cout<<arr[num-1];
        }
    }
    else
    {
        string s;
        cin>>s;
        for(int i=0; i<s.size(); i++)
        {
            char x=s[i];
            int num=arr.find(x);
            cout<<arr[num+1];
        }
    }
    return 0;
}
