#include "cstdio"
using namespace std;
int n, sc, sm;
int main() {
    int c, m;
    scanf("%d", &n);
    for (int i=0; i<n; i++) {
        scanf("%d%d", &m, &c);
        if (c>m) sc++;
        if (m>c) sm++;
    }
    if (sm>sc)
        printf("Mishka");
    else if (sm<sc)
        printf("Chris");
    else
        printf("Friendship is magic!^^");
    return 0;
}