#include <bits/stdc++.h>
#define int long long
using namespace std;
signed main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int n;
    cin>>n;
    int x,s=0;
    for(int i=0;i<n;i++)
    {
        cin>>x;
        if(x>0)
            s+=x;
        else
            s-=x;
    }
    cout<<s;
}