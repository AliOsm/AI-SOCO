#include<bits/stdc++.h>
using namespace std;

#define x first
#define y second
#define pb push_back
#define mp make_pair

#define all(a) (a).begin(), (a).end()

#define int long long

int32_t main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    int n;
    cin >> n;
    cout << 2 << endl;
    for (int i = 2; i <= n; i++) cout << i * i * i + 2 * i * i + 1 << endl;
    return 0;
}
