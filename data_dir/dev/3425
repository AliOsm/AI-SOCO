#include <bits/stdc++.h>
#define ll long long
#define ld long double
#define pb push_back
#define f first
#define s second
#define y1 bbp
#define sz size()
using namespace std;
ll const N=1e6+7,inf=1e9+7;
ll n,m,ans,x,a[N],b[N],y,k,qq,q,kk;
vector<ll> v;
bool us[N];
char ch;
string s,t;
main()
{
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    cin>>n;
    for (int i=1;i<=n;i++)
    {
        cin>>x>>y;
        if (__gcd(x,y)==1) cout<<"Finite"; else cout<<"Infinite";
        cout<<endl;
    }
    return 0;
}
