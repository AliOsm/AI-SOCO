#include<iostream>
#include <vector>
#include <algorithm>
#include <cmath>
#include <set>
#include <queue>
#include <deque>
#include <map>
#include<unordered_map>
#include <stack>
#include<bitset>
#include<list>
#include<cassert>
#include<numeric>
 
using namespace std;

 
using namespace std;

string s;
char mini = 'z';

int main()
{
	cin >> s;
	for (int i = 0; i < s.size(); i++)
	{
		if (mini > s[i])
		{
			mini = s[i];
		}
		if (mini < s[i])
		{
			cout << "Ann" << endl;
		}
		else
		{
			cout << "Mike" << endl;
		}
	}


}
