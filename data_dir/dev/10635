#include <iostream>
#include <cmath>
#include <cstdio>
#include <algorithm>

using namespace std;

int main(){
	string w = "qwertyuiopasdfghjkl;zxcvbnm,./";
	string s; char p; 
	cin >> p >> s;
	for(int i=0; i<s.size(); i++){
		for(int j=0; j<w.size(); j++){
			if(s[i]==w[j]){
				if(p=='R'){
					cout << w[j-1];
			  	}else{
			  		cout << w[j+1];
			  	}
		  	}
	  	}
  	}
  	return 0;
}

