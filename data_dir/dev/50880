#include <bits/stdc++.h>
#define INT long long int
using namespace std;
string s;
int ans;
int main()
{
    ios::sync_with_stdio(false);
    cin>>s;
    for(int i = 0 ; i < s.size() ; i++ )
        if( s[i] == 'a' or s[i] == 'e' or s[i] == 'i' or s[i] == 'o' or s[i] == 'u'
           or s[i] == '1' or s[i] == '3' or s[i] =='5' or s[i] == '7' or s[i] == '9' ) ans++;
    cout<<ans;
    return 0;
}
