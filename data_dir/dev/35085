#include <iostream>
using namespace std;
typedef long long ll;
int n, m;
int main()
{
    char a[111111];
    cin>>n>>m;
    for(int i=1; i<=n*m; i++)
    {
        cin>>a[i];
    }
    for(int i=1; i<=n*m; i++)
    {
        if(a[i]=='C'  || a[i]=='M' || a[i]=='Y' )
        {
            cout<<"#Color";
            return 0;
        }
    }
    cout<<"#Black&White";
    return 0;

}
