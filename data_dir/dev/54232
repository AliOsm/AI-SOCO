#include <bits/stdc++.h>
using namespace std;

int v[] = { 0, 4, 10, 20, 35, 56, 83, 116, 155, 198, 244, 292, 341, 390, 439, 488, 537, 586, 635, 684, 733 };


int main()
{
    int n;
    cin >> n;


    if (n <= 14) {
        cout << v[n] << '\n';
        return 0;
    }

    cout << 1LL * (n - 13) * 49 + v[13];

    return 0;
}