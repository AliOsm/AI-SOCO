#include <iostream>
typedef long long ll;
using namespace std;
int main() {
  ll n;
  cin >> n;
  ll x = 0, y = 0, z = 0;
  ll xi, yi, zi;
  for (ll i = 0; i < n; ++i) {
    cin >> xi >> yi >> zi;
    x += xi; y += yi; z += zi;
  }
  cout << ((x == 0 && y == 0 && z == 0) ? "YES\n" : "NO\n");
  return 0;
}