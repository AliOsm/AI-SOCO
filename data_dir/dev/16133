#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define pb push_back
#define mp make_pair
#define F first
#define S second
int main() {
	ll t, ang, i;
	cin >> t;
	while(t--) {
		cin >> ang;
		for(i = 3;; i++) {
			if(!((ang * i) % 180) && (i - 2) * 180 >= ang * i) {
				cout << i << "\n";
				break;
			}
		}
	}
	return 0;
}
