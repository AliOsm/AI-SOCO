#include <bits/stdc++.h>
using namespace std;

signed main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int t;
    cin>>t;
    while (t--)
    {
        int n;
        cin>>n;
        int a[n];
        int s=0;
        for (int i = 0; i <n ; ++i) {
            cin>>a[i];
            s+=a[i];
        }
        if(s%n==0)
        {
            cout<<s/n<<"\n";
        }
        else
        {
            cout<<(s/n)+1<<"\n";
        }
    }
}