#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,ans,f=0,ff=0;
    cin>>n;
    int l[n],r[n];
    for(int i=0;i<n;i++)
    {
        cin>>l[i]>>r[i];
    }
    int k;
    cin>>k;
    for(int i=0;i<n;i++)
    {
        if(k<=r[i] && k>=l[i])
        {
             ans=i;
             
        }
    }
    cout<<n-ans;
}