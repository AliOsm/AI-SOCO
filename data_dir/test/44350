#include<bits/stdc++.h>
using namespace std;
#define Jewel ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
int a, b, n, aa[24][60], i, j, mx;
int main()
{
    Jewel;
    cin>>n;
    while(n--)
    {
        cin>>a>>b;
        aa[a][b]++;
        mx = max(mx, aa[a][b]);
    }
    cout<<mx<<endl;
    return 0;
}
