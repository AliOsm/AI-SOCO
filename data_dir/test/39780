ll MOD(ll a, ll b)
{
    if(a>b)
        return a-b;
    else
        return b-a;
}
ll max3(ll a,ll b, ll c)
{
    return max(c,max(a,b));
}
ll min3(ll a,ll b,ll c)
{
    return min(a,min(b,c));
}
ll power(ll x,ll y )
{
    ll res = 1;
    while (y > 0)
    {
        if (y & 1)
            res = (res*x);//%mod;
        y = y>>1;
        x = (x*x);//%mod;
    }
    return res;
}
ll logg(ll a)
{
    ll x=0;
    while(a>1)
    {
        x++;
        a/=2;
    }
    return x;
}
ll gcd(ll a,ll  b)
{
    if (a == 0)
        return b;
    return gcd(b % a, a);
}
int main()
{
    ll n,m;
    cin>>n>>m;
    ll sum=0;ll ans1=n;ll ans2;
    for(ll i=0;i<=n;i++)
    {
        sum=(i*(i-1))/2;
        if(sum>=m)
        {
            ans1=n-i;
            break;
        }
    }
    if(m>n/2)
    {
        ans2=0;
    }
    else
    {
        ans2=n-2*m;
    }
    cout<<ans2<<' '<<ans1;
}
