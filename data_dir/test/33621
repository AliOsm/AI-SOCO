#include <bits/stdc++.h>
#include <math.h>
#define fr first
#define sc scanf
#define pf printf
#define se second
#define dl double
#define ll long long
#define pb push_back
#define mk make_pair
#define pr pair<int,int>
#define piip pair< pair<ll,ll>,ll >
#define fin(s) freopen( s, "r", stdin )
#define fout(s) freopen( s, "w", stdout )
#define TIME ios_base::sync_with_stdio(0)
using namespace std;

const ll INF = 1e9;
const ll N = 1e2 + 1;
const ll mod = 1e9 + 7;
const long double eps = 1E-7;

int ans;
int g, mx;
int n, a[N];
int main()
{
    TIME;
    cin >> n;
    for( int i = 1; i <= n; i ++ )
    {
        cin >> a[i];
        g = __gcd( g, a[i] );
        mx = max( mx, a[i] );
    }
    for( int i = 1; i <= n; i ++ )
        ans -= a[i] % g == 0;
    ans += mx / g;
    if( ans % 2 == 0 )
        cout << "Bob";
    else
        cout << "Alice";
}
