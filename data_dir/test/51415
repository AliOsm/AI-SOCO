#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
const ll N=2e5+5;
const ll MOD=1e9+7;

int main(){
    ios_base:: sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);
    ll n;
    cin>>n;
    ll a[2*n];
    for(ll i=0;i<2*n;i++)cin>>a[i];
    sort(a,a+2*n);
    if(a[n-1]<a[n])cout<<"YES";
    else cout<<"NO";
}
