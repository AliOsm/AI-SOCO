#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

const int maxn = 1e5 + 17;

int main(){
	ios::sync_with_stdio(0), cin.tie(0);
	int q;
	cin >> q;
	while(q--){
		int n;
		cin >> n;
		ll s = 0;
		while(n--){
			int x;
			cin >> x;
			if(x <= 2048)
				s += x;
		}
		cout << (s >= 2048 ? "YES" : "NO") << '\n';
	}
}

