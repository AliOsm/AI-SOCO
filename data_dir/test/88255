// Author: πα

#include <bits/stdc++.h>
using namespace std;
#define long int64_t

int main()
{
    int a, b, c, d;
    cin >> a >> b >> c >> d;
    int x = max(3*a/10, a - a*c/250);
    int y = max(3*b/10, b - b*d/250);
    if(x == y)
        return cout << "Tie", 0;
    cout << (x > y ? "Misha" : "Vasya");
    return 0;
}
