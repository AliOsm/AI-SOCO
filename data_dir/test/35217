#include <iostream>

using namespace std;

int main()
{
    ios_base::sync_with_stdio(0);
    int n, i, s1, s2, v;
    s1 = s2 = 0;
    cin >> n;
    for (i = 1; i <= n; i++)
    {
        cin >> v;
        if (v >= 0) s1 += v;
        else s2 += v;
    }
    cout << s1 - s2;
    return 0;
}
