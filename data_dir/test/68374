#include <bits/stdc++.h>
using namespace std;
#define pb  push_back
#define ll  long long
#define vi  vector<int>
#define vvi vector<vi >
#define all(x) x.begin(), x.end()

ll n, x, y, xx, yy;

int main() {
    ios_base::sync_with_stdio(false);
    cin >> n;
    x = y = 0;
    for (int i = 0; i < n + n; ++i) {
        cin >> xx >> yy;
        x += xx;
        y += yy;
    }
    cout << x / n << " " << y / n;

    return 0;
}