#include <stdio.h>
#include <math.h>
#include <string.h>
#include <iostream>
#include <algorithm>
#include <vector>
#include <map>
#include <utility>
#include <stack>
#include <queue>
#include <set>
#include <list>
#include <bitset>

using namespace std;

#define fi first
#define se second
#define long long long
typedef pair<int,int> ii;
typedef pair<int,ii> iii;

int main()
{
	//ios_base::sync_with_stdio(); cin.tie(0); cout.tie(0);
	// freopen("input.in", "r", stdin);

	int a, b, c, n;
	scanf("%d %d %d %d", &a, &b, &c, &n);
	if(c > a || c > b) printf("-1\n");
	else if(a+b-c >= n) printf("-1\n");
	else printf("%d\n", n-(a+b-c));
}










