#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int a[n],b[n],i,p=0,q=0;
    for(i=0;i<n;i++){
        scanf("%d %d",&a[i],&b[i]);
        if(a[i]==b[i]) continue;
        else if(a[i]>b[i]) p++;
        else q++;
    }
    if(p==q) printf("Friendship is magic!^^");
    else if(p>q) printf("Mishka");
    else printf("Chris");
    return 0;
}
