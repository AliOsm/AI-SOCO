#include <iostream>
#include <string>
#include <algorithm>
#include <vector>
#include <set>
#include <utility>
#include <map>

int main() {
    int64_t n, m;
    std::cin >> n >> m;
    n += m;
    int64_t mod = 998244353;
    int64_t ans = 1;
    while (n--) {
        ans = (ans * 2) % mod;
    }
    std::cout << ans;
    return 0;
}
