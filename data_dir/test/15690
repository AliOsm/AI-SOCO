int n, m, k;

void solve(int) {
  cin >> n >> k;
  map<int, int> mp;
  for (int i = 0; i < k + 1; ++i) {
    cout << '?';
    for (int j = 0; j < k + 1; ++j) {
      if (i != j) {
        cout << ' ' << j + 1;
      }
    }
    cout << endl;
    int pos, val;
    cin >> pos >> val;
    ++mp[val];
  }
  cout << "! " << k + 1 - mp.begin()->second << endl;
}

void precalc() {
}

int main() {
  ios::sync_with_stdio(0);
  cin.tie(0);
  precalc();
  int test = 1;
  //cin >> test;
  //auto start = chrono::high_resolution_clock::now();
  for (int i = 1; i <= test; ++i) {
    solve(i);
  }
  //cerr << chrono::duration_cast<chrono::milliseconds>(chrono::high_resolution_clock::now() - start).count() << '\n';
  return 0;
}
