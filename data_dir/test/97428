#include<iostream>
#include<cstdlib>
#include<vector>
#include<climits>
#include<cctype>
#include<map>
#include<list>
#include<cstdio>
#include<algorithm>
#include<memory.h>
#include<cmath>
#include<queue>
#include<fstream>
#include<sstream>
#include<set>
#define L long long int
#define LD long double
#define pi 3.141592653589793238462643383
#define M 1000000007

using namespace std;

int main()
{
    int t=1,n,m;             
    
    while(t--)
    {
              cin>>n>>m;
              if(n==0&&m==0)
              {
              cout<<"0 0";
              return 0;
              }
              if(n==0)
              cout<<"Impossible"<<"\n";
              
              
              else
              {
                  if(n>=m)
                  cout<<n<<" ";
                  else
                  cout<<m<<" ";
                  if(m>0)
                  cout<<n+m-1<<"\n";
                  else
                  cout<<n<<"\n";
              }
              
              
    }
    return 0;
}
