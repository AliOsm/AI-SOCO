#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    vector <int> v;
    cin >> n;
    v.resize(2 * n);

    for (int i(0); i < 2 * n; i++)
        cin >> v[i];

    sort(v.begin(), v.end());

    if (v[n - 1] != v[n])
        cout << "YES";
    else
        cout << "NO";
    return 0;
}
