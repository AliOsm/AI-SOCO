void solve(){
	int n,j=0;
	cin>>n;
	int a[n],b[n];
	bool flag[n+1];
	for(int i=0;i<n;i++){
		cin>>a[i];
	}
	for(int i=0;i<n;i++){
		cin>>b[i];
		flag[i]=1;
	}
	int ans[n]={0};
	flag[n]=1;
	for(int i=0;i<n;i++){
		for(;j<n&&flag[b[i]];j++){
			if(b[i]==a[j]){
				j++;
				ans[i]++;
				break;
			}
			else{
				flag[a[j]]=0;
			}
			ans[i]++;
		}
	}
	for(int i=0;i<n;i++){
		cout<<ans[i]<<" ";
	}
}
signed main(){   
    optimize();
	int t=1;
//	P();
   // cout<<setprecision(18)<<endl;
    //cin>>t;
	while(t--){
		solve();
	}
}