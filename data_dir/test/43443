#include <bits/stdc++.h>
#define mk make_pair
#define fs first
#define sc second
using namespace std;
typedef long long ll;
typedef long double ld;

int main(){
  int l, r;
  while(cin>>l>>r){
    if(l==r){
      cout<<l<<endl;
    }
    else{
      cout<<2<<endl;
    }
  }
  return 0;
}


