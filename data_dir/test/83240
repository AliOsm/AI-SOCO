#include <bits/stdc++.h>
using namespace std;
#define pb push_back
#define f first
#define s second
typedef long long ll;
typedef long double ld;


int main(){

	int t;
	scanf("%d", &t);
	while(t--){
		int n, k; scanf("%d%d", &n, &k);
		for(int i = 0, j = 0; i < n; i++, j++){
			if(j == k) j = 0;
			printf("%c", 'a' + j);
		}
		printf("\n");
	}

	return 0;
}