#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

int n, c, a;

main()
{
	cin >> n;
	for (int i=1;i<=n;i++)
	{
		cin >> a;
		if (a==0)
			c++;
	}
	if (n==1)
	{
		if (c)
			cout << "NO";
		else
			cout << "YES";
	}
	else
	{
		if (c==1)
			cout << "YES";
		else
			cout << "NO";
	}
}
