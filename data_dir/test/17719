//http://codeforces.com/problemset/problem/355/A

#include <iostream>
#include <string>
#include <cmath>
#include <vector>
#include <algorithm>
#include <iterator>
#include <cstdlib>
#include <cstdio>
#include <map>
#include <set>
#include <queue>
#include <deque>
#include <stack>
#include <list>

using namespace std;

int size_k, size_d;

int main()
{
	int ii;

	cin >> size_k >> size_d;

	if (size_d == 0)
	{
		if (size_k == 1)
			cout << "0";
		else
			cout << "No solution";
	}
	else
	{
		cout << size_d;

		for (ii = 1; ii < size_k; ii++)
			cout << "0";
	}

	return 0;
}