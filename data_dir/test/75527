#include <bits/stdc++.h>
#define ll long long

using namespace std;

int a[110];

int main()
{
    int t;  cin >> t;
    while(t--){
        int n;  cin >> n;
        int o = 0 , e = 0;
        for(int i = 0;i<n;i++)  {
            cin >> a[i];
            if(a[i] & 1)  o++;
            else  e++;
        }
        cout << (o == n || e == n ? "YES" : "NO") << '\n';

    }
    return 0;
}
