#include <algorithm>
#include <iostream>
#include <vector>
#include <set>
#include <map>
#include <fstream>

using namespace std;

#define ll long long

int main()
{
    ios_base::sync_with_stdio(false);
    int n, a;
    vector <int> s;
    cin >> n;
    for (int i = 0; i < n; ++i) {
        cin >> a;
        s.push_back(a);
    }
    sort (s.begin(), s.end());
    cout << s[(n - 1) / 2] << endl;
    return 0;
}