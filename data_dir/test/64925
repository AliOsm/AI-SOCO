#include <stdio.h>

int main()
{
	int n, i, num[1000] = { 0 };

	scanf ("%d", &n);
	for (i = 0; i < n; i++) {
		int a;
		scanf ("%d", &a);
		num[a - 1]++;
	}

	for (i = 0; i < 1000; i++)
		if (num[i] > n / 2 + (n % 2 == 1))
			break;

	puts (i < 1000 ? "NO" : "YES");

	return 0;
}
