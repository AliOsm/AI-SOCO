#include <bits/stdc++.h>
using namespace std;
typedef long long intt;
#define pb push_back
#define sc second
#define fr first
#define mk make_pair

int main()
{
    intt n;
    cin>>n;
    intt k;
    cin>>k;
    intt res=0;
    while(k%2==0)
    {
        k/=2;
        res++;
    }
    cout<<res+1;



return 0;
}
