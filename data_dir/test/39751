#include<bits/stdc++.h>

using namespace std;

int ans, s, s1, win;

int main () {
	cin >> s >> s1;
	for (int i = 1; ; i ++) {
		if(i % 2) {
			if(s < i) {
				cout << "Vladik"; 
				return 0; 
			}
			s -= i;
		} else {
			if(s1 < i) {
				cout << "Valera"; 
				return 0;
			}
			s1 -= i;
		}
	}
}
