#include<bits/stdc++.h>

using namespace std;

int main()
{
    string str;
    set<char> n;
    cin>>str;
    n.insert(str.begin(),str.end());
    if(n.find('H')!=n.end() || n.find('Q')!=n.end() || n.find('9')!=n.end())
        printf("YES\n");
    else printf("NO\n");
    return 0;
}
