#include <bits/stdc++.h>
using namespace std;

int main() {
	string s;
	for(int i = 0; i < 10; i++) {
		printf("%d\n", i);
		getline(cin, s);
		if(s == "cool" || s == "great" || s == "not bad"
			|| s == "dont think so" || s == "dont touch me!") {
			cout << "normal" << endl;
			break;
		}
		else if(s == "dont even" || s == "terrible" || s == "worse"
			|| s == "are you serious" || s == "go die in a hole" || s == "no way"){
			cout << "grumpy" << endl;
			break;
		}
	}
	return 0;
}