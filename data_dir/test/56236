��� s1 = t1, s2 = t2, ..., si-1 = ti-1, � si < ti.


*/

using namespace std;

const int N = 100100;

char s [N];
int n, a, d;
bool ok;

int main ()
{
# ifndef ONLINE_JUDGE
    freopen ("C.TXT", "r", stdin);
    freopen ("C.OUT", "w", stdout);
# endif

    cin >> s;

    n = strlen (s);

    for ( int i = 0; i < n; i++ )
        if ( s [i] == 'a' )
            ;
        else
        {
            ok = true;
            for ( ; i < n && s [i] != 'a'; i++ )
                s [i]--;
            break;
        }

    if ( ! ok )
        s [n-1] = 'z';

	 cout << s;

    return 0;
}
