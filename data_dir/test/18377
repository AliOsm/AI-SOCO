#include <bits/stdc++.h>
using namespace std;

const int MAX = 1e5 + 5;

#define dbg(a) cout << "-> " << #a << " = " << a << endl

int main() {
    int T;
    scanf("%d", &T);
    for (int t = 0; t < T; t++) {
        int n;
        scanf("%d", &n);
        long long sum = 0;
        for (int i = 0; i < n; i++) {
            int a;
            scanf("%d", &a);
            sum += a;
        }
        printf("%lld\n", (sum + n - 1) / n);
    }
    return 0;
}
