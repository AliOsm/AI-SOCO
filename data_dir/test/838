#include<bits/stdc++.h>
#define si(x) scanf("%d",&x)
#define sii(x,y) scanf("%d %d",&x,&y)
#define siii(x,y,z) scanf("%d %d %d",&x,&y,&z)
#define su(x) scanf("%u",&x)
#define sl(x) scanf("%ld",&x)
#define sli(x) scanf("%lld",&x)
#define slii(x,y) scanf("%lld %lld",&x,&y)
#define sliii(x,y,z) scanf("%lld %lld %lld",&x,&y,&z)
#define slu(x) scanf("%llu",&x)
#define sf(x) scanf("%f",&x)
#define slf(x) scanf("%lf",&x)
#define sc(x) scanf("%c",&x)
#define ss(x) scanf("%s",x)
#define pf printf
#define fpf fprintf
#define pb push_back
#define f(i,x,y,z) for(int i=x;i<y;i=i+z)
#define f1(i,x,y,z) for(int i=x;i>y;i=i-z)
#define MAX 1000000
#define pi 3.1415926535
#define mp make_pair
#define M 1000000007
#define xx first
#define yy second
#define N 100005
#define E 100005
#define vc vector
#define pq priority_queue
#define Q queue


using namespace std;

typedef long long int ll;
typedef unsigned long long int lu;

int main()
{
    string s;
    cin>>s;
    int n, c = 0;
    f(i,0,s.length(),1)
    {
        if(s[i]>='0' && s[i]<='9'){
            n = (s[i]-'0');
            if((n&1)==1) c++;
        }
        else if(s[i]=='a' || s[i]=='e' || s[i]=='o' || s[i]=='u' || s[i]=='i') c++;
    }
    pf("%d\n",c);
    return 0;
}
