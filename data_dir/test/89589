#include<iostream>
#include<map>
using namespace std;
int a,b,c,d,i,e,f,g,n,k,l;
string s;
map <string,string> m;
int main()
{
           cin>>n;
           m["purple"]="Power";
           m["green"]="Time";
           m["blue"]="Space";
           m["orange"]="Soul";
           m["red"]="Reality";
           m["yellow"]="Mind";
           for(i=1;i<=n;i++)
           {
           cin>>s;
           m[s]="Fuck";
           }
           cout<<6-n<<endl;
           if(m["purple"]!="Fuck") cout<<"Power"<<endl;
           if(m["green"]!="Fuck") cout<<"Time"<<endl;
           if(m["blue"]!="Fuck") cout<<"Space"<<endl;
           if(m["orange"]!="Fuck") cout<<"Soul"<<endl;
           if(m["red"]!="Fuck") cout<<"Reality"<<endl;
           if(m["yellow"]!="Fuck") cout<<"Mind"<<endl;
}