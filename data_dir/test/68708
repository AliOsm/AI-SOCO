#include <bits/stdc++.h>
using namespace std;
#define mod 998244353
const int N = 100010;
string s;
int n;
map < char , int > frq;
int main() {
	cin >> n;
	cin >> s;
	for(int i=0;i<n - 1;i++){
		if(s[i] != s[i + 1]){
			cout << "YES" << endl;
			cout << s[i] << s[i + 1] << endl;
			return 0;
		}

	}
	puts("NO");
	return 0;
}