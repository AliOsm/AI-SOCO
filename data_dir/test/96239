#define _CRT_SECURE_NO_WARNINGS

#include <iostream>
#include <conio.h>
#include <cstdlib>
#include <ctime>
#include <cmath>
#include <cstring>
#include <string>
#include <vector>
#include <stdio.h>
#include <climits>
#include <Windows.h>
#include <set>
#include <iterator>

using namespace std;

typedef unsigned long long ull;
typedef long long ll;

void bla()
{
    string s;

    set <char> name;

    cin >> s;

    for (int i(0); i < s.size(); i++)
        name.insert(s[i]);

    cout << ((name.size() % 2) ? "IGNORE HIM!" : "CHAT WITH HER!");


}


int main()
{
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);


    bla();


    //_getch();
    return 0;
}