#define endl "\n"
#define c0 ios_base :: sync_with_stdio(0); cin.tie (0);
#define s second
#define f first
#define ll long long
#define ull unsigned ll
using namespace std;

const int MaxN = 5e5 + 17;
//const ll INF = 1e18 + 17;
//const int MOD = 1e9 + 7;
ll int n, a[MaxN];
int main ()
{
    c0;
    // freopen (".in", "r", stdin);
    // freopen (".out", "w", stdout);
    cin >> n;
    for (int i = 1; i <= n; ++ i)
        cin >> a[i];
    sort (a + 1, a + n + 1);
    int x = n / 2ll + (n % 2 != 0);
    cout << a[x];
    return 0;
}
