#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef long double ld;



int main(){
    ios_base::sync_with_stdio(0);cin.tie(0);
    ll h1, m1, h2, m2;
    char c;
    cin >> h1 >> c >> m1 >> h2 >> c >> m2;
    m1 += h1*60;
    m2 += h2*60;
    ll mid = (m1+m2)/2;
    cout << setw(2) << setfill('0') << mid/60 << ':' << setw(2) << setfill('0') << mid%60;

}
