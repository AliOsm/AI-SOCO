#include <iostream>

using namespace std;
char a[105][105];

int main()
{
    int n = 0 , m = 0 , b = 0  ;
    cin>>n>>m;
    for(int i = 0 ; i < n ; i++){
         for(int j = 0 ; j < m ; j++){
             cin>>a[i][j];
          if(a[i][j] == 'C' || a[i][j] == 'Y' || a[i][j] == 'M'){
            b = 1;
            break;
          }

         }
    }
   if(b)
    cout<<"#Color";
    else
        cout<<"#Black&White";

    return 0;
}
