#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
typedef long double ld;

int main() {
    ios_base::sync_with_stdio(false);
    int test;
    cin >> test;
    for (int tst = 1; tst <= test; ++tst) {
        string s;
        cin >> s;
        sort(s.begin(), s.end());
        if (s[0] == s.back()) cout << -1 << endl; else cout << s << endl;
    }
    return 0;
}
