//In the name of God

#include <bits/stdc++.h>
#define lol long long
using namespace std;

int main()
{
  ios::sync_with_stdio(false);
  int n,m;
  cin>>n>>m;
  string s;
  cin>>s;
  for(int i=0;i<m;i++)
    {
      int l,r;
      char c1,c2;
      cin>>l>>r>>c1>>c2;
      l--;r--;
      for(int j=l;j<=r;j++)
	if(s[j]==c1)
	  s[j]=c2;
    }
  cout<<s<<endl;
  return 0;
}