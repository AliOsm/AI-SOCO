#include <bits/stdc++.h>
using namespace std;

int main()
{
	int t;
	cin >> t;
	while(t--)
	{
		int n;
		cin >> n;
		int s = 0, I = 0, P = 0;
		for(int i = 0; i < n; i++)
		{
			int x;
			cin >> x;
			s += x;
			if(x % 2) I++;
			else P++;
		}
		if(P and I or s % 2) puts("YES");
		else puts("NO");
	}

	return 0;
}