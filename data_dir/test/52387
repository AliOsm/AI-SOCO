#include<bits/stdc++.h>

//let's start with easiest ladders

using namespace std;

int main()
{
    int n;

    cin>>n;

    double sum = 0;

    for (int i=1;i<=n;i++)
    {
        int x;
        cin>>x;
        sum+=x/(1.00*n);
    }

    printf("%.10f",sum);
    return 0;
}
