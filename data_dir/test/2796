#include<bits/stdc++.h>
using namespace std;
int l,r,ll,rr,n,arr[200009];
int main()
{
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
        if(arr[i])rr++;
        else ll++;
    }
    if(rr==0||ll==0)
    {
        cout<<0;
        return 0;
    }
    for(int i=0;i<n;i++)
    {
        if(arr[i])
            r++;
        else l++;
        if(r==rr||l==ll)
        {
            cout<<i+1;
            break;
        }
    }
}
