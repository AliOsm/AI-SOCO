#include <cstdio>
#include <iostream>
#include <algorithm>
#include <cmath>
using namespace std;

int n,m,i,j,t,k;
int a[5555];
int ans[222];



int main ()
{

cin>>n>>m;
for (i=0;i<m;i++) cin>>a[i];
sort(a,a+m);
for (i=0;i<m;i++)
 if (a[i+2]-a[i]==2 || a[i]==n || a[i]==1) {cout<<"NO";return 0;}
cout<<"YES";
}


