#include<bits/stdc++.h>
#define ll long long
#define mp make_pair
#define fi first
#define se second
#define pb push_back
#define ld long double
using namespace std;

const int inf = 2e9;
const int N = 1e5 + 5;
const int md = 1e9 + 7;

ll n, m;

int main(){
    ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
    cin >>n>>m;
    while (1){
        if (n == 0 || m == 0)break;
        if (n >= 2 * m)n %= 2 * m;else if (m >= 2 * n)m %= 2 * n;else break;
    }
    cout <<n<<" "<<m;
}
