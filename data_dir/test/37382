#include<bits/stdc++.h>
using namespace std;


int main(){

   int t;
   cin>>t;

   long long r,g,b;

   while(t--){
       cin>>r>>g>>b;

       vector<long long> vc;
       vc.push_back(r);
       vc.push_back(g);
       vc.push_back(b);

       sort(vc.begin(),vc.end());

       if(vc[0]+vc[1]>=(vc[2]-1)){
           cout<<"Yes"<<endl;
       }
       else{
           cout<<"No"<<endl;
       }

   }
}