#include<iostream>
#include <bits/stdc++.h>
#define ll long long
using namespace std;
int x1,x2,y1,y2,a,b,x,y;
int main()
{
    cin>>x1>>y1>>x2>>y2>>a>>b;
    x=abs(x1-x2) , y=abs(y1-y2);
    if(x%a||y%b||(abs(x/a-y/b))%2!=0)
        cout<<"NO";
    else
        cout<<"YES";
    return 0;
}
