#include <iostream>
#include <queue>
#include <algorithm>
#include <cstring>

using namespace std;



int main()
{
 int n,s1=0,s2=0;
 cin>>n;
 for(int i=0;i<n;i++)
{
    int x;
    cin>>x;
    if (x==1) s1++;else s2++;
}
if(s2>0) {cout<<2<<' ';s2--;}
if (s1>0) {cout<<1<<' ';s1--;}
for(int i=1;i<=s2;i++) cout<<2<<' ';
for(int i=1;i<=s1;i++) cout<<1<<' ';


}
