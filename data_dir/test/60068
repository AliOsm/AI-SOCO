#include<bits/stdc++.h>
using namespace std;
vector<bool>bl(2009,true);
vector<long long int>vec,ans;
int main()
{
    long long int a,b,c,d,i,j,k,l,n,m,x,y;
    a=2000;
    b=sqrt(a);
    vec.push_back(2);
    for(i=3;i<=b;i+=2)
    {
        if(bl[i])
        {
            for(j=i*i;j<=a;j+=i*2)
            {
                bl[j]=false;
            }
        }
    }
    for(i=3;i<=a;i+=2)
    {
        if(bl[i])
        {
            vec.push_back(i);
        }
    }
    l=vec.size();
    cin>>n;
    x=1;
    for(i=0;i<l;i++)
    {
        if(vec[i]*vec[i]>n)
        {
            break;
        }
        if(n%vec[i]==0)
        {
            b=0;
            while(n%vec[i]==0)
            {
                b++;
                n=n/vec[i];
            }
            for(j=0;j<b;j++)
            {
                ans.push_back(vec[i]-1);
            }
        }
    }
    if(n!=1)
    {
        ans.push_back(n-1);
    }
    y=ans.size();
    a=-1;
     if(y>=3)
     {
         if(ans[y-1]==1 && ans[y-2]==1 && ans[y-3]==1)
         {
             ans[y-2]=3;
             y--;
         }
     }
    for(i=y-1;i>=0;i--)
    {
       a++;
       c=powl(vec[a],ans[i]);
       x=x*c;

    }
    cout<<x<<endl;
}
