#include<bits/stdc++.h>

using namespace std;
#define pb push_back
const int maxi=1e6+2;
const long long mo=1e9+7;
vector<int> v[maxi];
int n;
string s,t;
int a[maxi];
int cnt[maxi];
int main()
{
   cin>>n;
   cin>>s;

   for (int i=1;i<=n;i++)
    cnt[s[i-1]]++;

    for (int i='a';i<='z';i++)
        for (int j=1;j<=cnt[i];j++)
        t+=i;

    cout<<t<<endl;
}
