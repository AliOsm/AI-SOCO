#include <cstdio>
#include <iostream>
#include <cstring>
#include <algorithm>
#include <vector>
#include <set>
#include <map>
#include <queue>
#include <cassert>
#include <cmath>
#include <sstream>
#include <complex>
#include <stack>
#include <unordered_map>
using namespace std;

typedef long long ll;
typedef pair<int, int> ii;
typedef pair<unsigned, unsigned> uu;
typedef pair<ll, ll> l4;


#define pb push_back
#define mp make_pair


const int maxn = 1e2+1;
int a[maxn<<1];
int n;

int main()
{
  scanf("%d", &n);
  for (int i = 0; i < 2*n; ++i)
    {
      scanf("%d", a+i);
    }
  sort(a, a+2*n);
  puts(a[n]==a[n-1]?"NO":"YES");
}
