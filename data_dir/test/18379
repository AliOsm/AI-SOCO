#include <stdio.h>

#include<iostream>

#include<string>

#include<cstring>

using namespace std;

int zero(int aa)
{
    for(int a=0; a<aa; a++)
    {
    cout<<'0';    
    }
}

int main()
{

int n;
int t;
cin>>n;
cin>>t;

if(t==10)
{
    if(n==1)
    cout<<"-1";
    else
    {
        cout<<t;
        zero(n-2);
    }
}
else
{
    cout<<t;
    zero(n-1);
}

}