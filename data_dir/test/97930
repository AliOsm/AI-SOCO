//In the name of Allah

#include<bits/stdc++.h>

using namespace std;

int a1, b1;

int main()
{
    int a, n;
    cin >> n;
    for(int i = 0; i < n; i ++)
    {
        cin >> a;
        a1 |= a;
    }
    for(int i = 0; i < n; i ++)
    {
        cin >> a;
        b1 |= a;
    }
    cout << a1 + b1 << endl;
    return 0;
}
