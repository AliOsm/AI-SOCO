#include <bits/stdc++.h>
#define pb push_back
#define mp make_pair
#define fi first
#define se second

using namespace std;

typedef long long ll;
typedef pair<int, int> ii;

int main()
{
	int n, k, t;
	cin >> n >> k >> t;
	int ans;
	if (t <= k)
		ans = t;
	else if (t <= n)
		ans = k;
	else
		ans = k - (t-n);
	cout << ans << '\n';
	return 0;
}


