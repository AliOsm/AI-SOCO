#include<bits/stdc++.h>

#define ll long long
#define dl double

using namespace std;

int main()
{
    int Y, W;
    cin >> Y >> W;

    int B = max(Y, W);
    int S = 0;
    for(int i = B; i <= 6; i++) S++;

    int d = __gcd(6, S);

    cout << S / d << "/" << 6 / d << endl;
    return 0;
}
