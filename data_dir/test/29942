#include <bits/stdc++.h>
#define ll long long
#define ld long double
#define ull unsigned ll
#define F first
#define S second
#define mp make_pair
#define pb push_back

using namespace std;

const int MIN = 1e3 + 2;
const int MXN = 1e5 + 3;
const int INF = 1e9 + 7;
const ll LINF = 1e18 + 15;
const double EPS = 1e-9;

int k, d;

int main(){
    cin >> k >> d;
    if (d == 0 && k > 1){
        cout << "No solution";
        return 0;
    }
    else
        cout << d;
    for (int i = 1; i < k; i++){
        cout << 0;
    }
	return 0;
}