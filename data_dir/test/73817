#include <bits/stdc++.h>
using namespace std;

const int maxn = 1e5;
int c[maxn];
vector<int> t;
vector<int> g[maxn];

void dfs(int v) {
	if(c[v]==2)
		return;
	c[v]=1;
	for (int u : g[v]) {
		if(c[u]==1) {
			cout<<-1<<"\n"; exit(0);
		}
		dfs(u);
	}
	t.push_back(v);
	c[v]=2;
}

int main(){
	ios::sync_with_stdio(0); cin.tie(0);
	int n,k; cin>>n>>k;
	vector<int> q;
	while(k--) {
		int v; cin>>v; v--; q.push_back(v);
	}
	for (int v=0; v<n; v++) {
		int m; cin>>m;
		while(m--) {
			int u; cin>>u; u--;
			g[v].push_back(u);
		}
	}
	for (int v : q)
		dfs(v);
	cout<<t.size()<<"\n";
	for (int v : t)
		cout<<v+1<<" ";
	cout<<"\n";
	return 0;
}
