#include<bits/stdc++.h>
using namespace std;
#define Jewel ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

int main()
{
    Jewel;
    int x, y, a, b, i, j, c=0;
    cin>>x>>y>>a>>b;
    for(i=a; i<=x; i++)
    {
        for(j=b; j<=y; j++)
        {
            if(i>j)
                c++;
        }
    }
    cout<<c<<endl;
    for(i=a; i<=x; i++)
    {
        for(j=b; j<=y; j++)
        {
            if(i>j)
                cout<<i<<" "<<j<<endl;
        }
    }
    return 0;
}


