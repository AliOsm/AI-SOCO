#include <bits/stdc++.h>
#pragma GCC optimization ("unroll-loops")
#pragma GCC optimization ("O2")
#pragma GCC target("popcnt")
#pragma GCC target ("avx2")
using namespace std;

int main(){
	string a,b;
	cin >> a >> b;
	if(a == b){
		cout << -1 << endl;
	}else{
		cout << max(a.size(),b.size()) << endl;
	}
}