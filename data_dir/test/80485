int gcd(int x,int y)
{
    if(y==0)
        return x;
    else
        return gcd(y,x%y);
}
int expo(int n,int m,int p)   //MODULAR EXPONENTIATION
{
    int r=1;
    n=n%p;
    while(m>0)
    {
        if(m%2)
            r=(r*n)%p;
        n=(n*n)%p;
        m=m/2;
    }
    return r%p;
}
int32_t main()
{
 
   IOS;
   #ifndef ONLINE_JUDGE
   freopen("input.txt","r",stdin); 
   freopen("output.txt","w",stdout);
   #endif
   /********************* Code starts here ************************/
   int a,b,x,y,z;
   cin>>a>>b>>x>>y>>z;
   int ans=0;
   ans=max(0ll,2*x-a);
   if(a>2*x)
   	a-=2*x;
   else
   	a=0;
   if(b>=y)
   {
   	if(a<y)
   		ans=ans+y-a;
   }
   else
   {
   	ans=ans+y-b;
   	if(a<y)
   		ans=ans+y-a;
   }
   if(b>=y)
   	b-=y;
   else
   	b=0;
   if(3*z>b)
   	ans=ans+3*z-b;
   cout<<ans;
} 