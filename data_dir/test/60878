const double PI = 2 * acos(0);
const int MOD = 1000000007;
const int HASH = 3137;

void solve() {
  char buf[5];
  gets(buf);
  bool row = buf[0] == 'a' || buf[0] == 'h';
  bool col = buf[1] == '1' || buf[1] == '8';
  if(row && col) printf("3\n");
  else if(row || col) printf("5\n");
  else printf("8\n");
}

int main() {
  /*
  freopen("integral.in","r",stdin);
  freopen("integral.out","w",stdout);
  */
  int qq = 1;
  //int qq = 1e9;
  //int qq; scanf("%d\n", &qq);
  for(int casenum = 1; casenum <= qq; casenum++) {
    solve();
  }
  return 0;
}
