#include<iostream>
#include<cstring>
#include<cmath>
#include<cstdio>
using namespace std;

int main(){
	int a = 0, b = 0, c;
	scanf("%d", &c);
	char s[1000000];
	cin>>s;
	for (int i = 0; i < strlen(s); ++i) 
		if (s[i] == 'A') ++a;
		else ++b;
	if (a > b) printf("Anton");
	else if (b > a) printf("Danik");
	else printf("Friendship");
	
}
