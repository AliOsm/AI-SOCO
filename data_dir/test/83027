#include <bits/stdc++.h>
#define ff first
#define ss second
using namespace std;
#define MP make_pair
#define PB push_back
#define ll long long
#define int long long
#define f(i,x,n) for(int i=x;i<n;i++)
#define mod 1000000007
int32_t main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int x,y,a;
    f(i,1,6)
    {
        f(j,1,6)
        {
            cin>>a;
            if(a==1)
            {
                x=i,y=j;
            }
        }
    }
    cout<<abs(3-x)+abs(3-y)<<endl;
    return 0;
}