#include<bits/stdc++.h>

#define LL long long
#define Dl double
#define Pb push_back
#define Po pop_back
#define Mp make_pair
#define In insert

using namespace std;

typedef vector<LL> Vl;
typedef pair<LL, LL> Pll;
typedef vector<Pll> Vpl;
typedef queue<LL> Ql;
typedef stack<LL> Sl;
#define X first
#define Y second

int main()
{
    LL n;
    cin >> n;

    LL a = n % 10, b = n / 10;

    LL d[11];

    d[0] = 2, d[1] = 7, d[2] = 2, d[3] = 3, d[4] = 3;
    d[5] = 4, d[6] = 2, d[7] = 5, d[8] = 1, d[9] = 2;

    cout << d[a] * d[b] << '\n';
    return 0;
}
