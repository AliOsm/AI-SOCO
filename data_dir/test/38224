#include <cstdio>
#include <iostream>
#include <fstream>
#include <cstring>
#include <algorithm>
#include <map>
#include <cmath>
using namespace std;

int sx,sy,ii,n,m,i,j,k,x,xx,y,yy,z;
map <int,int> mp;
int t,tp,tmp;
int a[555][555];

int main ()
{
int s1,s2;
cin>>n>>k>>x>>y>>s1>>s2;
t=s2/k;
for (i=1;i<=s2%k;i++) cout<<t+1<<' ';
for (i=s2%k+1;i<=k;i++) cout<<t<<' ';

s2=s1-s2;
k=n-k;
if (k==0) return 0;
t=s2/k;
for (i=1;i<=s2%k;i++) cout<<t+1<<' ';
for (i=s2%k+1;i<=k;i++) cout<<t<<' ';

}
