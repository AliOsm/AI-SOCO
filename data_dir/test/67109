#pragma GCC optimize ("O3")
#include <bits/stdc++.h>
#define ll long long
#define ld long double
#define f first
#define s second
#define IO ios_base::sync_with_stdio(0),cin.tie(0),cout.tie(0);
using namespace std;
const int N = 1e4 + 5, M = 2 * N + 5, NN = 1e2 + 5;

int a, b;
int main(){
    scanf("%d%d", &a, &b);
    if(a == b)printf("%d0 %d1", a, b);
    else if(a + 1 == b)printf("%d9 %d0", a, b);
    else if(a == 9 && b == 1)puts("9 10");
    else puts("-1");
}