/*************************************************/
int n,i,dep[3005],vis[3005],incir[3005];
vi e[3005];
int dfs1(int x,int fa)
{
	int t=0;vis[x]=1;
	fe(it,e[x]){
		if(*it==fa)continue;
		if(!vis[*it]){
			int a=dfs1(*it,x);
			if(a==x)incir[x]=1;
			if(a&&a!=x){
				incir[x]=1;t=a;
			}
		}else if(vis[*it]==1){
			incir[x]=1;t=*it;
		}
	}vis[x]=2;
	return t;
}
void dfs2(int x,int fa)
{
	fe(it,e[x]){
		if(incir[*it]||*it==fa)continue;
		dep[*it]=dep[x]+1;dfs2(*it,x);
	}
}
int main()
{
	scanf("%d",&n);
	rep(i,n){
		int x,y;read(x);read(y);
		e[x].pb(y);e[y].pb(x);
	}
	dfs1(1,0);rep(i,n)if(incir[i])dfs2(i,0);
	rep(i,n)printf("%d ",dep[i]);
	return 0;
}