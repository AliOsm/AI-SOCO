#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n ;
    cin>>n;
    long long  arr[n] ;
    for(int i = 0 ; i < n ; ++i)
        cin>>arr[i] ;
    sort(arr , arr + n) ;
    long long ans = 0 ;
    for(int i = 0 ; i < n/2 ; ++i)
        ans += (arr[i] + arr[n-1-i]) * (arr[i] + arr[n-1-i]) ;
    return cout<<ans<<"\n" , 0 ;
}
