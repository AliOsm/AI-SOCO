#include<bits/stdc++.h>

using namespace std;

int main(){
	
	ios :: sync_with_stdio(false); cin.tie(nullptr); cout.tie(nullptr);
	
	int n;
	
	cin>>n;
	
	int M,C;
	
	M = C = 0;
	
	while(n--){
		
		int m,c;
		
		cin>>m>>c;
		
		if(m>c){
			
			M++;
		}
		
		else if(c>m){
			
			C++;
		}
	}
	
	if(M>C){
		
		cout<<"Mishka";
	}
	
	else if(C>M){
		
		cout<<"Chris";
	}
	
	else{
		
		cout<<"Friendship is magic!^^";
	}
}
