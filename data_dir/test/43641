#include <bits/stdc++.h>
using namespace std;

#define F first
#define S second
typedef long long ll;
typedef long double ld;
typedef pair <int,int> ii;

const int N=100100;

int main()
{
    ll n,x,y;
    scanf("%lld%lld%lld",&n,&x,&y);

    ll dist1 = max ( abs ( x-1 ) , abs ( y-1 ) );
    ll dist2 = max ( abs ( x-n ) , abs ( y-n ) );

    if(dist1<=dist2)
        printf("White");
    else
        printf("Black");
}
