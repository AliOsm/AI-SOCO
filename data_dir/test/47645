#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define sz size()
#define sc second
#define fr first
#define r_0 return 0
#define TIME ios_base::sync_with_stdio(0);
using namespace std;
vector < ll > v;
ll n , m , k , l ,cnt , ans , x , y;
string s;
int main()
{
TIME
ll a[1001];
    for ( ll i = 1 ; i <= 4 ; i ++ ){
        cin >> a[ i ];
    }
    cin >> s;
    for ( ll i = 0 ; i < s.sz ; i ++ )
    {
        m = s[i] - '0';
        cnt += a[m];
    }
    cout << cnt ;
}




