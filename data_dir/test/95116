#define re return(0);
#define ce continue;
typedef long long ll;
typedef long double ld;
#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
#include <string>
#include <cstring>
#include <set>
#include <iomanip>
#include <fstream>
#include <stack>
#include <map>
using namespace std;
double const pi=3.1415926536;
int main()
{
	int n;
	cin>>n;
	int a[n];
	for(int i=0;i<n;i++)
	cin>>a[i];
	sort(a,a+n);
	int s=0;
	for(int i=0;i<n;i++)
	{
		s++;
		if(a[i]<s)
		{
			s--;
		}
	}
	cout<<s+1;
	
}


