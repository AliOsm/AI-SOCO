#include<bits/stdc++.h>
#define ll long long int
#define ull unsigned long long int
#define getFaster ios_base::sync_with_stdio(false),cin.tie(NULL),cout.tie(NULL)
#define rep(i,init,n) for(int i=init;i<n;i++)
#define repl(i,init,n) for(ll i=init;i<n;i++)
#define rev(i,n,init) for(int i=n;i>=init;i--)
#define MAXN 1000005
#define MOD 998244353
#define endl '\n'
#define f first
#define s second
using namespace std;
const double PI = atan(1.0)*4;
const ll INF=1e7+3;
vector<int> v;
int main(){
       getFaster;
        int c=0,n;
        string str;
        cin>>n>>str;
        v.push_back(-1);
        rep(i,0,str.length()) if(str[i]=='8'){c++;v.push_back(i+1);}
        int l=str.length();
        int x=(l-11)/2;
        if(c<=x || l-v[x+1]+1<11)
        {
          cout<<"NO"<<endl;
          return 0;
        }
        cout<<"YES"<<endl;
        return 0;
}