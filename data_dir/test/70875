#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define sz size()
#define sc second
#define fr first
#define r_0 return 0
#define TIME ios_base::sync_with_stdio(0);
using namespace std;
string s , s1 , s2;
int main()
{TIME
    cin >> s >> s1 >> s2;
    if ( s == "paper" && s1 == "rock" && s2 == "rock" || s == "rock" && s1 == "scissors" && s2 == "scissors" || s == "scissors" && s1 == "paper" && s2 == "paper" )
    cout << "F";
   else if ( s1 == "paper" && s == "rock" && s2 == "rock" || s1 == "rock" && s == "scissors" && s2 == "scissors" || s1 == "scissors" && s == "paper" && s2 == "paper" )
    cout << "M";
    else if ( s2 == "paper" && s == "rock" && s1 == "rock" || s2 == "rock" && s == "scissors" && s1 == "scissors" || s2 == "scissors" && s == "paper" && s1 == "paper" )
    cout << "S";
    else
    cout << "?";
}
