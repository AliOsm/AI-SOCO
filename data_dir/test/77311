#include <bits/stdc++.h>
using namespace std;

const int N = int(1e5);

int n, m;
int arr[N];

void solve() {
	bool cnt[N + 1];
	int last = 0;
	memset(cnt, false, sizeof(cnt));
	for(int i = n - 1; i >= 0; --i) {
		if(!cnt[arr[i]]) {
			last++;
			cnt[arr[i]] = true;
		}
		arr[i] = last;
	}
}

int main (int argc, char *argv[])
{
	scanf("%d%d", &n, &m);
	for(int i = 0; i < n; ++i) {
		scanf("%d", &arr[i]);
	}
	solve();
	for(int i = 0; i < m; ++i) {
		int l;
		scanf("%d", &l);
		l--;
		printf("%d\n", arr[l]);
	}

	return 0;
}

