#include <bits/stdc++.h>

using namespace std;
typedef long long ll;


int main()
{
    ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
    int t;
    cin>>t;
    while(t--){
        ll n;
        ll a[3];
        cin >> a[0] >> a[1] >> n;
        a[2] = a[0] ^ a[1];
        cout << a[n%3]<<endl;
    }


    return 0;
}
