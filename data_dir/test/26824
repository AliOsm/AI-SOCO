#include <bits/stdc++.h>
#include <iostream>
#include <cmath>
#include <algorithm>
#include <iomanip>
#include <cstdio>
#include <map>
#include <string>
#include <set>
#include <vector>
#include <stack>
#include <deque>
#include <queue>
#include <ctime>
#include <list>
#include <iterator>
#define ll long long
#define ss second
#define ff first
#define pb push_back
#define faster ios::sync_with_stdio(0), cin.tie(0), cout.tie(0)
using namespace std;
const ll maxn=300100;
ll n,m,x,y,z,w,k,t,ans;
int main(){
    cin>>n>>m;
    for(ll i=1;i<=m;i++){
        for(ll j=1;j<=m;j++){
            if((i*i+j*j)%m==z){
                if(n%m>=i)x=n/m+1;
                else x=n/m;
                if(n%m>=j)y=n/m+1;
                else y=n/m;
                ans+=x*y;
            }
        }
    }
    cout<<ans;
    return 0;
}
