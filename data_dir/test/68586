# define fast ios_base::sync_with_stdio(false) ; cin.tie(0) ; cout.tie(0);
# define dist(a,b,p,q) sqrt((p-a)*(p-a) + (q-b)*(q-b))

# define pp(n) printf("%.10Lf",n);
# define line cout<<"\n";

const ld pie = 3.14159265358979 ;
const ll mod = 1e9 + 7 ;

string vow = "aeiou";

vi v ;
int n ;

bool f ( int indx , int val )
{
    if ( indx == -1 )
    {
        val %= 360 ;
        if ( val == 0 )
            return true ;
        else
            return false ;
    }

    bool q1 = f ( indx-1 , val + v[indx] ) ; // we take this angle clockwise
    bool q2 = f ( indx-1 , val - v[indx] ) ; // anticlockwise

    if ( q1 || q2 ) return true ;
    return false ;
}

void solve ( int test_case )
{
    cin >> n ;
    v.resize ( n ) ;
    for ( int i=0 ; i < n ; i++ ) cin >> v[i] ;

    if ( f ( n-1 , 0 ) ) cout << "YES" ;
    else cout << "NO" ;
}

int main()
{
    int t = 1;
    //cin >> t;

    for ( int i=0 ; i<t ; i++ ) solve(i);
    return 0;
}
