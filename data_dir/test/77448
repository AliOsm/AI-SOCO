const int MOD=1e9+7;
int main()
{
  int n;
  sc(n);
  int v[n];
  for(int i=0;i<n;i++) sc(v[i]);
  sort(v,v+n);
  cout<<v[(n-1)/2]<<endl;
  return 0;
}
