#include<iostream>
#include<algorithm>
using namespace std;
int a,b,c,d,i,e,f,g,n,m,k,l,A[100005];
int main()
{
           cin>>n;
           for(i=1;i<=n;i++)
           cin>>A[i];
           sort(A+1,A+n+1);
           k=1;
           for(i=1;i<=n;i++)
           if(A[i]>=k) k++;
           cout<<k;
}