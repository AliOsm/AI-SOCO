#include <iostream>
#include <vector>
#include <string>

using namespace std;

int max(int a, int b) {
    return a > b ? a : b;
}

int min(int a, int b) {
    return a < b ? a : b;
}

int main() {
    long long n;
    cin >> n;
    
    if (n == 0) cout << 1;
    else {
        vector<int> answers = { 6, 8, 4, 2 };
        cout << answers[n % 4];
    }    
    cout << endl;
    
    return 0;
}