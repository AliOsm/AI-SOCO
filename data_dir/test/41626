#include <iostream>
#include <cstdio>
#include <fstream>
#include <algorithm>
#include <queue>
#include <cmath>
#include <cstring>
using namespace std;

#define pii pair<int,int>
#define piii pair<pair<int,int>,int >
#define mp make_pair
//int i,j,x,y,z,n,m,len,k,tp;
int ans=0;
int i,j,n,m,k,z,x,y,t,mi,ma;
int a[111];
char st[11];

int main()
{
//freopen("ladder.in","r",stdin);
//freopen("ladder.out","w",stdout);
scanf("%d%d%d%d",&n,&m,&mi,&ma);
for (i=0;i<m;i++) scanf("%d",&a[i]);
sort(a,a+m);
if (a[0]<mi || a[m-1]>ma) {printf("Incorrect");return 0;}
if (a[0]!=mi) x=1;
if (a[m-1]!=ma) x++;
if (m+x>n) printf("Incorrect"); else printf("Correct");

}
