#include <bits/stdc++.h>
#define lli long long int
#define pii pair<int,int>
#define plli pair<lli,lli>
#define X first
#define Y second
using namespace std;

int main()
{
	ios_base::sync_with_stdio(false);
	
	int a,b;
	cin>>a>>b;
	
	int s = min(a,b);
	int r = max(a,b) - s;
	
	cout<<s<<" "<<r/2;
	
	return 0;
}
