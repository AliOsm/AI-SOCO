#include <bits/stdc++.h>
using namespace std;
int main(){
	int t;
	cin>>t;
	while (t > 0){
		int n;
		cin>>n;
		string s, f;
		cin>>s>>f;
		int ok = 0;
		for (int i=0; i<n; i++){
			if (s[i] == f[i]){
				ok++;
			}
		}
		if (ok == n - 2){
			set<char> st, ft;
			for (int i=0; i<n; i++){
				if (s[i] != f[i]){
					st.insert(s[i]);
					ft.insert(f[i]);
				}
			}
			if (st.size() == 1 && ft.size() == 1){
				cout<<"Yes"<<endl;
			}
			else{
				cout<<"No"<<endl;
			}
		}
		else if (ok == n){
			cout<<"Yes"<<endl;
		}
		else{
			cout<<"No"<<endl;
		}
		t--;
	}
}