#include <iostream>
#include <cstdio>
#include <vector>
#include <map>
#include <set>
#include <string>
#include <cmath>
#include <algorithm>
#include <assert.h>
#include <memory.h>
#include <string.h>
#include <complex>
#include <queue>
#include <cstdlib>
#include <ctime>
using namespace std;

#define ll long long
#define pb push_back
#define mp make_pair
#define sz(x) (int)(x).size()

char tmpstr[44];
map<int, int> st;

int get_hash(string &s) {
	int res = 0;
	for(int i = 0; i < sz(s); i++) {
		res *= 31;
		res += (s[i] - 'a' + 1);
	}
	return res;
}


int main() {
	//freopen("input.txt", "rt", stdin);
	//freopen("output.txt", "wt", stdout);
	int n;
	scanf("%d\n", &n);
    //st.reserve(n*5);
	for(int i = 0; i < n; i++) {
		int p = 0;
		while(true) {
		    tmpstr[p] = getchar();
		    if (tmpstr[p]=='\n') {
		        tmpstr[p] = '\0';
		        break;
		    }
		    p++;
		}
		string s(tmpstr);
		int h = get_hash(s);
		map<int, int>::iterator it = st.find(h);
		if (it != st.end()) {
			//printf("%s%d\n", tmpstr, it->second);
			for(int j = 0; j < p; j++) putchar(tmpstr[j]);
			printf("%d", it->second);
			it->second++;
		}
		else {
		    putchar('O');
		    putchar('K');
		    st[h] = 1;
		}
		putchar('\n');
	}
	return 0;
}