#include<stdlib.h>
#include<stdio.h>
#include<math.h>
#include<cstring>
#include<iostream>
#include<algorithm>
using namespace std;
int n,c,b;string s;
int main()
{
	scanf("%d",&n);cin>>s;
	for(int i=0;i<n;i++)c+=s[i]=='0',b+=s[i]=='1';
	if(!b)return printf("0\n"),0;printf("1");for(int i=0;i<c;i++)printf("0");
	return printf("\n"),0;
}