//In the name of Allah
#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef pair <int , int> pii;

const int N = 1e5+10 , MOD = 1e9+7;

int32_t main()
{
    ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
    int n;
    cin>>n;
    if(n==1){
        cout<<"a";
        return 0;
    }
    cout<<"aa";
    for(int i=2 ; i<n ; i++){
        if((i%4) == 2 || (i%4) == 3)
            cout<<"b";
        else
            cout<<"a";
    }
    return 0;
}
