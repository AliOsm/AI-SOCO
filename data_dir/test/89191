
// In the name of God
#include <bits/stdc++.h>

using namespace std;
#define Size(x) ((int)(x).size())
#define pb push_back
typedef long long ll;
typedef long double ld;
typedef pair<int,int>pii;
const int INF = 1e9 + 10;
const int MN = 55;

int hor[MN] , ver[MN];
int n;

int main()
{
	ios_base :: sync_with_stdio(false) ,cin.tie(0) , cout.tie(0);
	cin >> n;
	for(int i=0;i<n*n;++i){
		int x,y;cin>>x>>y;
		if(!hor[x] && !ver[y]) hor[x] = ver[y] =true , cout <<i+1 << ' ';
	}	
	cout << endl;
	return 0;
}

