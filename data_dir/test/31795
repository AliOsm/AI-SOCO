using namespace std;

#define FTB(i,a,b) for(LL i=a;i<=b;i++)
#define FT(i,a,b) for(LL i=a;i<b;i++)
#define FGB(i,a,b) for(LL i=a;i>=b;i--)
#define FG(i,a,b) for(LL i=a;i>b;i--)
#define endl "\n"
#define PB push_back
#define mp make_pair
#define fi first
#define se second
#define debug cout<<"VL\n";
#define PI 3.1415926535897
#define lbnd(dau,cuoi,giatri) lower_bound(dau,cuoi,giatri)
#define ubnd(dau,cuoi,giatri) upper_bound(dau,cuoi,giatri)
typedef double DB;
typedef long long LL;
typedef unsigned long long UL;
typedef long double LD;
typedef pair<LL,LL> II;
typedef vector<LL> VI;
typedef vector<bool> VB;



LL n;

struct DATA{
	LL a,b;
};
DATA s[5003];
bool cmp(DATA x,DATA y){
	return x.a<y.a||(x.a==y.a&&x.b<y.b);
}

int main(){
    ios::sync_with_stdio(false);cin.tie(0);
//	freopen("input.txt","r",stdin);
//  freopen("output.txt","w",stdout);
	cin>>n;
	FTB(i,1,n) cin>>s[i].a>>s[i].b;
	sort(s+1,s+n+1,cmp);
	LL tmp=s[1].b;
	FTB(i,2,n){
		if(tmp<=s[i].b) tmp=s[i].b;
		else tmp=s[i].a;
	}

	cout<<tmp;




	return 0;
}



