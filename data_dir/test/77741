using namespace std;

#define REP(i,n) for((i)=0;(i)<(int)(n);(i)++)
#define snuke(c,itr) for(__typeof((c).begin()) itr=(c).begin();itr!=(c).end();itr++)

int main(void){
    int N,K,i;
    
    cin >> N >> K;
    if(K >= N*(N-1)/2){
        cout << "no solution" << endl;
    } else {
        REP(i,N) cout << 0 << ' ' << i << endl;
    }
    
    return 0;
}
