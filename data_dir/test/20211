int main(){
	//read;
	int n;
	cin>>n;
	if(n==1 || n==2)cout<<-1;
	else {
		cout<<n-1 <<" "<<n ;
		rep(i,n-2)cout<<" "<<n-i-2;
	}
	return 0;
}