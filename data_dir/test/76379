#include<bits/stdc++.h>
#define ll long long
#define OO 2e18
#define oo 1e9
#define yalla ios::sync_with_stdio(0),cin.tie(0),cout.tie(0);
#define sz  100005
#define re return
#define mod  1000000007
#define e9  1000000000
#define pi acos(-1)
using namespace std;
int n,m,cnt;
int main()
{cin>>n;  int l[n],r[n];
 for(int i=0;i<n;i++)
 {cin>>l[i]>>r[i];

 }
cin>>m;
for(int i=0;i<n;i++)
{
 if(m>=l[i]&&m<=r[i] )
        break;
 cnt++;
}
cout<<n-cnt<<endl;
    return 0;
}

