#include <bits/stdc++.h>
using namespace std;

const int N = int(2e5) + 5;

char s[N];
int n;

int main (int argc, char *argv[])
{
	scanf("%s", &s);
	n = int(strlen(s));
	for(int i = 0; s[i]; ) {	
		int j = i;
		for(; s[j] && s[i] == s[j]; ++j);
		int q = j - i;
		if(q % 2) {
			char c = 'a';
			if(s[i] == c) {
				c = 'b';
			}
			for(int k = i + 1; k < j; k += 2) {
				s[k] = c;
			}
		} else {
			char a = s[i],
				b,
				c = 'a';
			if(j != n) {
				b = s[j];
			}
			while(c == a || c == b) {
				++c;
			}
			for(int k = i + 1; k < j; k += 2) {
				s[k] = c;
			}
		}
		i = j; 
	}
	printf("%s", s);

	return 0;
}

