#include<bits/stdc++.h>

using namespace std;

int main(){
	int n,m;
	cin >> n;
	
	int a = 0,b = 0;
	
	for(int i = 1;i <= n;i++){
		int x;
		cin >> x;
		a = max(a,x);
	}
	
	cin >> m;
	
	for(int i = 1;i <= m;i++){
		int x;
		cin >> x;
		b = max(b,x);
	}
	
	cout << a << " " << b << endl;
}