#include <bits/stdc++.h>
#define ff first
#define ss second
using namespace std;
#define MP make_pair
#define PB push_back
#define ll long long
#define int long long
#define f(i,x,n) for(int i=x;i<n;i++)
#define ld long double
#define mod 1000000007
int32_t main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int n,ans=0;
	cin>>n;
	string a,b;
	cin>>a>>b;
	f(i,0,n)
	{
		ans+=min( abs(a[i]-b[i]) , 10-abs(a[i]-b[i]) );
	}
	cout<<ans<<endl;
	return 0;
}