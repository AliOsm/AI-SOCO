#include <bits/stdc++.h>
using namespace std;
#define si(n) scanf("%d", &n)
#define sl(n) scanf("%lld", &n)
#define pi(n) printf("%d\n", n)
#define pl(n) printf("%lld\n", n)
#define rep(i, l, r, p) for(i = l; i <= r; i += p)
#define mt make_tuple
#define ll long long
#define pb push_back
#define mk make_pair
#define F first
#define S second
int movex[4] = {-1, 0, 1, 0};
int movey[4] = {0, 1, 0, -1};
int movex1[8] = {-1, -1, 0, 1, 1, 1, 0, -1};
int movey1[8] = {0, 1, 1, 1, 0, -1, -1, -1};
int main() {
	int n, i;
	scanf("%d", &n);
	printf("%d ", n);
	for(i = 1; i < n; i++)
		printf("%d ", i);
    return 0;
}
