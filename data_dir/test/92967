mt19937 rng(chrono::steady_clock::now().time_since_epoch().count());
typedef long long ll;
typedef long double ld;
#define INF 2001001001
#define MOD 1000000007

int T;
int main()
{
  //ios_base::sync_with_stdio(0);cin.tie(0);
  //freopen (".in","r",stdin);
  //freopen (".out","w",stdout);
  cin>>T;
  while (T--){
    int A,B,N;
    cin>>A>>B>>N;
    N=N%3;
    if (N==0)
      cout<<A<<endl;
    else if (N==1)
      cout<<B<<endl;
    else
      cout<<(A^B)<<endl;
  }


  return 0;
}
