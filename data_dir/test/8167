#include<bits/stdc++.h>
using namespace std;
main()
{
    long long n,m;
    while(cin>>n)
    {
        for(long long i=1;i<=n;i++)
        {
            if(i%4==2||i%4==1)
            {
                cout<<"b";
            }
            else
                cout<<"a";
        }
        cout<<endl;
    }
}
