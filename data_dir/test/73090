///*BISMILLAHIR RAHMANIR RAHIM*///
#include <bits/stdc++.h>
using namespace std;
 
int main(){
    int n, blue, red, ans, i;
    scanf("%d %d %d", &blue, &red, &n);
    ans = 0;
    for ( i = 0; i <= blue; i++ ) {
        if ( i <= n && n - i <= red ) {
            ans ++;
        }
    }
    printf("%d\n", ans);
    return 0;
}