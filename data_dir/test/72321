#include <bits/stdc++.h>
using namespace std;

const int MAXN = 1001 * 1001 , MOD = 1e9 + 7 ;

int dp [ MAXN ] = {} , ps [ MAXN ] = {};


 main()
{
   ios::sync_with_stdio(0);
   cin.tie(0);
   int a , b , c;
   cin >> a >> b >> c ;
   for(int i = 0 ; i < 1000*1000 ; i ++)
   {
       if((c-i*a)%b==0&&c-i*a>=0)
            return cout <<"YES",0;
   }
   cout<<"NO";




}

