const int pri = 997;

int tests = 1;
int n, m, k, cnt[31], ans;
string s;

bool isit(char type) {
	if(type == 'a' || type == 'e' || type == 'i' || type == 'o' || type == 'u')
		return 1;
	return 0;
}

inline void Solve () {
	cin >> s;
	char pr = '1';
	for (int i = 0; i < s.size(); i ++) {
		
		if(isit(s[i])) {
			for (char c = 'a'; c <= 'z'; c ++)
				cnt[c - 'a'] = 0;
			cout << s[i];
			continue;
		}
		cnt[s[i] - 'a'] ++;	
		int mx = 0, Ss = 0;
		for (char c = 'a'; c <= 'z'; c ++) {
				mx = max(mx, cnt[c - 'a']);
				Ss += cnt[c - 'a'];
		}
		if(mx != Ss && Ss >= 3) {
			cout << " ";
			for (char c = 'a'; c <= 'z'; c ++)
				cnt[c - 'a'] = 0;
			cnt[s[i] - 'a'] ++;
		}
		cout << s[i]; 
	}
}

int main () {
	SpeedForce;
//	freopen(".in", "r", stdin);
//	freopen(".out", "w", stdout);	
	// cin >> tests;
	while(tests --) {
		Solve ();
		// Ee Zadrot
	}	
}