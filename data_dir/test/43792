#include <iostream>
#include <algorithm>
#include <time.h>
#include <stack>
#include <string>
#include <cstring>
#include <cassert>
#include <cstdio>
#include <cmath>
#include <vector>
#include <queue>
#include <map>
#include <set>

using namespace std;

const int N = 100005;
const int inf = 1000 * 1000 * 1000;
const int mod = 1000 * 1000 * 1000 + 7;

int n;
int a[N];
map <int,bool> dp;

int main(){
    cin>>n;
    int sum = 0;
    for(int i=1;i<=n;i++){
        scanf("%d",&a[i]);
        sum += abs(a[i] - 0);
    }
    cout<<sum<<endl;
    return 0;
}
