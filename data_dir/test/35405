#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n , sum = 0;
    vector <int> v;
    for(int i = 0 ; i < 4 ; i++){
        int a ; cin>>a;
        v.push_back(a);}
    string s ;cin>>s;
    for(int i = 0 ; i < s.size() ; i++){
        int num = s[i] - '0';
        sum += v[num-1];}
    cout<<sum<<endl;


    return 0;
}
