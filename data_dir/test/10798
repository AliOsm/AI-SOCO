#include <iostream>
#include <algorithm>
#include <map>
#include <bitset>
#include <set>
#include <vector>
#include <queue>
#include <deque>
#include <stack>
#include <string>
#include <string.h>
#include <cstring>
#include <iomanip>
#include <stdio.h>
#include <sstream>
#include <bitset>
#include <locale>
#include <iostream>
#include <cmath>
#include <iomanip>
#include <math.h>
#include <bitset>
#include <functional>
using namespace std;

#define ll  long long
#define ld long double

int main()
{
	ios::sync_with_stdio(false);
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr), cout.tie(nullptr);
	string input;
	cin >> input;
	for (ll i = 0; i < input.size(); i++)
		if (input[i] == 'r')
			cout << i + 1 << "\n";

	for (ll i = input.size() - 1; i >= 0; i--)
		if (input[i] == 'l')
			cout << i + 1 << "\n";
	return 0;
}