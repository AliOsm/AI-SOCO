#include <bits/stdc++.h>
#define ll long long
using namespace std;
int main()
{
    //ifstream cin("in.inn");
    ll vsk = 0;
    bool ir = false;
    string v;
    cin >> v >> v;
    
    for(ll i = 0;i < v.size();i++)
         if(v[i] == '1') {ir = true;break;}
    if(!ir){cout << 0;return 0;}
    cout << 1;
    for(ll i = 0;i < v.size();i++)
        if(v[i] != '1')cout << 0;
    return 0;
}
