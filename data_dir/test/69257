#include <iostream>
#include <stdio.h>
#include <algorithm>
#include <cmath>
#include <cstring>
#include <cstdlib>
#include <vector>
#include <utility>
#include <iomanip>
#include <iterator>
#include <queue>
#include <ctime>
#include <set>
#include <map>

using namespace std;

#define pb push_back
#define mp make_pair
#define F first
#define S second

int n, k;
int l, r;
vector< pair<int, int> > v, res;

int main() {
    scanf("%d%d", &n, &k);
    for (int i = 1; i <= n; i++) {
        scanf("%d%d", &l, &r);
        v.pb(mp(l, -1));
        v.pb(mp(r, 1));
    }
    sort(v.begin(), v.end());
    int cur = 0;
    bool is = false;
    int was;
    for (size_t i = 0; i < v.size(); i++) {
        if (v[i].S == -1) {
            ++cur;
            if (cur >= k && !is) {
                is = true;
                was = v[i].F;
            }
        } else if (v[i].S == 1) {
            --cur;
            if (cur < k && is) {
                is = false;
                res.pb(mp(was, v[i].F));
            }
        }
    }
    printf("%d\n", (int)res.size());
    for (size_t i = 0; i < res.size(); i++) printf("%d %d\n", res[i].F, res[i].S);
    return 0;
}