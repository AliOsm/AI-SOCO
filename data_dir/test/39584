#define ll long long
#define clr(x) memset(x,0,sizeof(x))
#define _clr(x) memset(x,-1,sizeof(x))
#define fr(i,a,b) for(int i = a; i < b; ++i)
#define frr(i,a,b) for(int i = a; i > b; --i)
#define pb push_back
#define sf scanf
 
#define pf printf
#define mp make_pair
#define N 300000
 
const int mod = 998244353;
 
using namespace std;
 
string s[N],t[N];
 
int main() {
	int n,m;
	cin>>n>>m;
	fr(i,0,n) {
		cin>>s[i];
	}
	fr(i,0,m) {
		cin>>t[i];
	}
	int q;
	cin>>q;
	while(q--) {
		int y;
		cin>>y;
		y--;
		cout<<s[y%n]+t[y%m]<<endl;
	}
}
