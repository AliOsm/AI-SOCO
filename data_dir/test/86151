#include <bits/stdc++.h>
using namespace std;
long long  i, j, n, c[123456],a[1111234], b[1125678],d[123456], l,ll,r, s, t, k, m, xh, x, x1, y,z;
string p[34567],q;
map<long long,long long> mm;
int main() {
    cin >> n;
    for (i = 0; i< n; i ++){
        cin >> k>>m>>x;
        if (x < k)        
        cout <<x<<endl;
        else {
            s = m/x*x;
            if (s <= m)
                s += x;
            cout <<s<<endl;
        }
    }
 
    return 0;
}