#include <bits/stdc++.h>
#define ll long long
#define ld long double
using namespace std;
ll t, r, l, v, d, ans;
bool bol;
int main()
{
    cin >> t;
    while(t--){
        cin >> d >> v >> r >> l;
        ans = (r-1)/v + d/v - l/v;
        cout << ans << '\n';
    }
    return 0;
}
