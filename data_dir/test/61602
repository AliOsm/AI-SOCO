#pragma GCC optimize("-O3")
#include <bits/stdc++.h>
#define ll long long
#define f first
#define s second
#define ios ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define y1 ibibibibbibibibiibigbabypanfewewf
using namespace std;
ll const N=5*1e5+5;
ll x1,r,x2,c,q,y1,l,y2,d,ans,a,aa[N],aaa[N],b,y3,k,pref[5005][4];
ll m,n;
string t,s;
pair <ll,ll> p[N];
set <ll> st;
map <ll,string> mp;
int  main()
{
    ios
cin>>n>>s;
mp[2]="2";
mp[3]="3";
mp[4]="322";
mp[5]="5";
mp[6]="53";
mp[7]="7";
mp[8]="7222";
mp[9]="7332";
for (int i=0;i<s.size();i++)
{
    t+=mp[s[i]-'0'];
}
sort(t.rbegin(),t.rend());
cout<<t;
}