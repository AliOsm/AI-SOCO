/* In The Name Of God */
#include <bits/stdc++.h>

# define xx first
# define yy second
# define pb push_back
# define pp pop_back
# define eps 1e-9

using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
typedef vector<int> vint;
string s[] = {"Sheldon","Leonard","Penny","Rajesh","Howard"};
int main(){
	ios_base::sync_with_stdio (0);
	int n;cin>>n;
	int i,tmp=1;
	while(true){
		for(i=0 ; i<5 ; i++){
			if(n>tmp)
				n-=tmp;
			else
				goto hell;
		}
		tmp*=2;
	}
	hell:
	cout<<s[i]<<endl;
	return 0;
}

