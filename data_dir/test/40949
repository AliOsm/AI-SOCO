#include <iostream>
int arr[3][3];
using namespace std;
int main()
{
    for (int i = 0; i < 3; i++)
        for (int j = 0; j < 3; j++)
            cin >> arr[i][j];
    cout << (arr[1][2] + arr[2][1]) / 2 << " " << arr[0][1] << " " << arr[0][2] << endl;
    cout << arr[1][0] << " " << (arr[0][2] + arr[2][0]) / 2 << " " << arr[1][2] << endl;
    cout << arr[2][0] << " " << arr[2][1] << " " << (arr[0][1] + arr[1][0]) / 2 << endl;
}
