int main()
{
    abdelrahman001
    string s;
    cin >> s;
    for(int i = 0;i<s.sz();i++)
        if(s[i] == 'r')
            printf("%d\n",i+1);
    for(int i = s.sz()-1;i>=0;i--)
        if(s[i] == 'l')
            printf("%d\n",i+1);
    return 0;
}
