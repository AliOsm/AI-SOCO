#include<iostream>
#include<math.h>
#include<algorithm>
#include<string>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.precision(20);
    int c1,c2,c3,c4;
    cin>>c1>>c2>>c3>>c4;
    if(c1!=c4)
     cout<<0;
    else
    {
        if(c3>0&&c1==0)
         cout<<0;
        else
        cout<<1;
    }}