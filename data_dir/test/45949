#define ll long long
#define ss second
#define ff first
using namespace std;
ll n,m,x,y,q,z;
string s;
int main(){
    cin>>s;
    for(int i=0;i<s.size();i++){
        if(s[i]=='a' && q)break;
        if(s[i]!='a' && q==0){
            q=1;
        }
        if(q==1)s[i]-=1;
    }
    if(q==0)s[s.size()-1]='z';
    cout<<s;
    return 0;
}
