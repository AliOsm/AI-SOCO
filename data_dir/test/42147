#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
#include <string>
#include <set>
using namespace std;
int main()
{
	string s,b;
	cin>>s;
	int t=0;
	if(s.length()>6)
	{
	for(int i=0;i<s.length()-6;i++)
	{
		if(s[i]==s[i+1] && s[i]==s[i+2] && s[i]==s[i+3] && s[i]==s[i+4]&&s[i]==s[i+5]&&s[i]==s[i+6])
		t=1;
	}
	if(t==0)
	cout<<"NO";
	else
	cout<<"YES";
    }
    else
    cout<<"NO";
}


