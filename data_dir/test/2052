#include<bits/stdc++.h>
using namespace std;

int main() {

  int s,v1,v2,t1,t2;

  scanf("%d %d %d %d %d",&s,&v1,&v2,&t1,&t2);
  if(s*v1+2*t1<s*v2+2*t2) {
    printf("First");
  }
  else if(s*v1+2*t1>s*v2+2*t2) {
    printf("Second");
  }
  else {
    printf("Friendship");
  }

}
