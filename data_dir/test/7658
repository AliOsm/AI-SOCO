#include<bits/stdc++.h>
#include<ext/pb_ds/assoc_container.hpp>
#include<ext/pb_ds/tree_policy.hpp>
#define MAX ((int)(1e7))
#define pi (2*acos(0))
#define mp make_pair
#define M 1000000007
#define xx first
#define yy second
#define NN ((int)(5e5+2))
#define N ((int)(5e5+2))
#define pq priority_queue
///#define Q queue
#define eps 0
#define log 20
#define pb push_back
#define MOD 998244353


using namespace std;
using namespace __gnu_pbds;

typedef long long ll;
typedef unsigned long long int lu;

typedef tree < int,  null_type,  less < int >,  rb_tree_tag,  tree_order_statistics_node_update > o_set;

int x[5], y[5];

int main()
{
    int n, i;
    scanf("%d",&n);
    for(i = 0; i<3; i++) scanf("%d %d",&x[i],&y[i]);
    if((x[1]-x[0])*(x[2]-x[0])>0 && (y[1]-y[0])*(y[2]-y[0])>0) printf("YES\n");
    else printf("NO\n");
    return 0;
}
