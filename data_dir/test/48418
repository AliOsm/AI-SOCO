#include <bits/stdc++.h>
#define LL long long
#define pb push_back
#define mp make_pair
#define fi first
#define sc second
using namespace std;

int main() {
	int n,m,k,x,y;
	cin>>n>>m>>k;
	x = (k+m*2-1)/(m*2);
	y = k%(m*2);
	if(y==0) y= m*2;
	y = (y+1)/2;
	cout<<x<<" "<<y<<" ";
	if(k%2==0) cout<<"R"<<endl;
	else cout<<"L"<<endl;
	return 0;
}