#include <iostream>
#include <map>
#include <cstdio>
#include <algorithm>
#include <vector>
using namespace std;
vector <int> v[55555];
int t,n,m,i,j,x1,x2,k,i1,i2,a[55555],p[55555],d[55555];
int po(int  n)
{
m=0;
while (n)
{
 m=max(m,n%10);
 n/=10;
}
return m;
}
int main()
{
  cin>>n;
 while (n)
 {
  n-=po(n);
  k++;
 }
 cout<<k;
}
