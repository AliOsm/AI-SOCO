#include "bits/stdc++.h"
using namespace std;

void solve() {
	int t; cin >> t;
	while(t--) {
		int a, b, n;
		cin >> a >> b >> n;
		int rem = n % 3;
		if(rem == 0) {
			cout << a << endl;
		} else if(rem == 1) {
			cout << b << endl;
		} else {
			cout << (a ^ b) << endl;
		}
	}
}

int main() {
	solve();
	return 0;  
}