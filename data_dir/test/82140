#include<bits/stdc++.h>
using namespace std;
#define forr(i,a,n) for(int i=a; i<n; i++)
#define loop(i,a,n)	for(int i=a; i>=n; i--)
#define conti continue
#define brk break
#define pb push_back
#define SPEED ios_base :: sync_with_stdio(false), cin.tie(NULL), cout.tie(0)
#define in cin>>
#define out cout<<
#define endl <<"\n"
#define ll long long 
#define remove(a,b) a.erase(std::remove(a.begin(),a.end(),b),a.end())
#define runn(it,a) for(auto it = a.begin(); it!= a.end(); it++) 
#define fi first
#define se second

int main() 
{	
	int a; in a;
	ll int b[a],c=0,d=0; 
	forr(i,0,a)	{in b[i]; c+=b[i];}
	sort(b,b+a);
	forr(i,0,a/2)	{d+=b[i];} ll int e=c-d;
	out d*d+(e*e);
	return 0;	
}