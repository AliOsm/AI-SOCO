# include <bits/stdc++.h>

# define speed ios_base::sync_with_stdio(0)
# define L_L long long
# define pr pair <int,int>

const int mod = 1e9 + 7;
const int N = 1e5 + 1e3;

using namespace std;




L_L a,b;

int main()
{
       cin>>a>>b;
       while (a!=0 && b!=0)
       {

              if (a >= 2*b)
              {
                     a = a % (2*b);
              }
              else if (b >= 2*a)
              {
                     b = b % (2 * a);
              }
              else
              {
                     break;
              }
       }
       cout<<a<<' '<<b;
}
