#include<bits/stdc++.h>
using namespace std;

int n,i;

int main()
{
    scanf("%d",&n);
    for(i=0;i+1<n;i++)
    {
        if(i%2==0) printf("I hate that ");
        else printf("I love that ");
    }
    if(i%2==0) printf("I hate it");
    else printf("I love it");
    return 0;
}
