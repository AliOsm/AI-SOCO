#include <bits/stdc++.h>
#define SZ(x) (int)x.size()
typedef long long ll;
#define X first
#define Y second
const int SZ=1e5+5;
const int mod=1e9+7;
const ll oo=(1LL<<62);
const double PI=acos(-1);
using namespace std;

int n,m;

int main()
{
	cin>>n>>m;
	if(n>m) swap(n,m);

	ll res=0;
	for(int i=1;i<=n;i++)
		res+=(m+i)/5-i/5;

	cout<<res;
}
/*
6 12
11 14
1 5
3 8
5 7
21 21


14
31
1
5
7
88
*/