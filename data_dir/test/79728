#include<bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;


int main(){
	ios::sync_with_stdio(0);
	cin.tie(0); cout.tie(0);
	int n; cin>>n;
	string s; cin>>s;
	if (s=="0") cout<<0;
	else{
		int cnt=0; 
		for(auto a: s) cnt+=(a=='0');
		cout<<1;
		while(cnt--) cout<<0;
	}
}