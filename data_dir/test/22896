#include <bits/stdc++.h>
using namespace std;


int main(){
    #ifdef LOCAL_MACHINE
        freopen("in.txt","r",stdin);
        freopen("out.txt","w",stdout);
    #endif
    
    int tc;
    scanf("%d",&tc);

    while(tc--){
        int h,m;
        scanf("%d %d\n",&h,&m);

        printf("%d\n",24*60-(h*60+m));
    }    
}
