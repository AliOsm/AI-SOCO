#include<bits/stdc++.h>
using namespace std;

#define F first
#define S second
#define ii pair < int , int >
#define LS pair < ll , string >
typedef long long ll;
const int inf = 1<<30;
const int N =100100;
int n , a[N] , q ;

int main ( ){
  int a , b , c , n ;
  scanf("%d %d %d %d",&a,&b,&c,&n);
  int all = a+b- c ;
  if (a < c || b < c || all > n-1  ) {
    printf("-1\n");
    return 0 ;
  }
  printf("%d",n-all);
return 0 ;
}
