#include <bits/stdc++.h>
#define re return(0);
#define ce continue;
typedef long long ll;
typedef long double ld;
using namespace std;
double const pi=3.1415926536;
int main()
{ 
   int cnt[100005];
   for(int i=0;i<=100000;i++)
   cnt[i]=0;
   int n;
   cin>>n;
   int a[n];
   for(int i=0;i<n;i++)
   {
   cin>>a[i];
   cnt[a[i]]++;
   }
   for(int i=0;i<=100000;i++)
   {
   	if(cnt[i]%2==1)
    {
    	cout<<"Conan";
    	re;
    }
   }
   cout<<"Agasa";
}


