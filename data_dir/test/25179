#include<bits/stdc++.h>
using namespace std;

int main(){
long x;
cin>>x;
double g;long h;
bool o=false;
for(int i=0;i<x;i++)
{cin>>g;
for(double i=0;i<10000;i++)
{
    long double yr=180*i;
    if(yr/(i+2)==g)
       {

        o=true;break;}

}
if(o)
   {

    cout<<"YES"<<endl;o=false;}
else cout<<"NO"<<endl;
}

}
