#include<bits/stdc++.h>
#define hell 1000000000
#define ll long long
using namespace std;
int main()
{
 int m,n,x,y;
 cin>>n>>m;
    int a[n];
    while(m--)
        cin>>x>>y;
    for(int i=0;i<n;i++)
    {
        if(i%2==0)
            a[i]=0;
        else
            a[i]=1;
    }
    for(int i=0;i<n;i++)
        cout<<a[i];
}
