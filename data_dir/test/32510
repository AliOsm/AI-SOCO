#include <iostream>
#include <stdio.h>

using namespace std;

long long n,x,i,X,Y,a;

int main()
 {
  cin>>n>>x;
  for (i=1;i<=n;i++)
   cin>>a,X=X*x+a;
  cin>>n>>x;
  for (i=1;i<=n;i++)
   cin>>a,Y=Y*x+a;
  if (X<Y) cout<<"<"; else
  if (X>Y) cout<<">"; else
           cout<<"=";
 }
