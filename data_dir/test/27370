#include <bits/stdc++.h>

using namespace std;

class TaskA {
public:
    void solve(std::istream& in, std::ostream& out) {
        int n;
        in >> n;
        string s;
        in >> s;
        for (int i = 0; i < n; i++) {
            for (int j = 1; j <= n; j++) {
                if (i + 4 * j < n && s[i] == '*' && s[i + j] == '*' && s[i + 2 * j] == '*' && s[i + 3 * j] == '*' && s[i + 4 * j] == '*') {
                    out << "yes";
                    return;
                }
            }
        }
        out << "no";
    }
};


int main() {
    TaskA solver;
    std::istream& in(std::cin);
    std::ostream& out(std::cout);
    solver.solve(in, out);
    return 0;
}
