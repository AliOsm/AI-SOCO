#include<bits/stdc++.h>
# define PI 3.14159265358979323846
using namespace std;

int main()
{
  int n;
  cin>>n;
  int k;
  cin>>k;
  for(int i=0;i<n;i++)
  {
    for(int j=0;j<n;j++)
     {
      if(i==j)
      cout<<k<<" ";
      else
      cout<<0<<" ";  
     }
     cout<<endl; 
  }  
  return 0;
}