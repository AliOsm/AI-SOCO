#include <bits/stdc++.h>
using namespace std;
long long  i, j, n, c[223456],d[123456], l,r,h, s,t, k,x, y,z,a[1434568],m,b[445678];
string p,q,qq;
int main() {
    cin >> n;
    t = 0;
    while (s < n){
        t ++;
        x = t;
        while (x%5 == 0){
            x/=5;
            s ++;
        }
    }
    if (s == n){
        cout << 5<<endl;
        for (i = 0; i < 5; i ++)
            cout << t+i<<' ';
            return 0;
    }
    cout <<0;
    return 0;   
}