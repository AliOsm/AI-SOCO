#include<bits/stdc++.h>

using namespace std;

#define lli long long int

int main(){
	
	ios :: sync_with_stdio(false); cin.tie(nullptr); cout.tie(nullptr);
	
	int n,t;
	
	cin>>n>>t;
	
	for(int i=1; i<=n; i++){
		
		int a;
		
		cin>>a;
		
		t -= (86400-a);
		
		if(t <= 0){
			
			cout<<i;
			
			break;
		}
	}
}
