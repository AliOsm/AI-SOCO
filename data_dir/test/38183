#include <bits/stdc++.h>
#define int  long long
#define ld long double
using namespace std;

const int MAXN = 1001 * 1001 ;


main()
{
    ios::sync_with_stdio(0);cin.tie(0);
    int n;
    cin >> n ;
    for(int i = 1 ; i < n ; i+=2)
        cout<<i<<' ';
    int x  = n-1;
    if(x%2==0)
        x--;
    if(x!=n-1)cout<<n <<' ';
    for(int j = x ; j > 0 ; j-=2)
        cout << j<<' ';
    for(int i = 2 ; i < n ; i+=2)
        cout<<i<<' ';
     x  = n-1;
     if(x%2==1)
        x--;
    if(x!=n-1)cout<<n<<' ';
    
     for(int j = x ; j > 0 ; j-=2)
        cout << j<<' ';
    cout<<n<<' ';
}
