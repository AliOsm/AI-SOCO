#include<bits/stdc++.h>
using namespace std;
 
typedef long double ld;
typedef long long ll;
 
typedef vector<ll> vec; 
#define INF LLONG_MAX
 
#define PP pair<int,int>
#define pb push_back
 
#define ff first
#define ss second
 
#define P pair<ll,ll>
#define FAST ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
 
#define N 1000005
#define mod 1000000007
 
signed main()
{
      FAST;
      
      ll t;
      cin>>t;
      
      while(t--)
      {
            ll a,b;
            cin>>a>>b;
            string p=to_string(b);
            
            ll tt=p.size();
            
            for(auto X:p)
            {
                  if(X!='9')
                  {
                        tt--;
                        break;
                  }
            }
            
            cout<<a*tt<<"\n";
            
            
      }
      
      
}


