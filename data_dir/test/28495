void brainfuck()
{
    cin>>n;
    cin>>s;
    set < char > st;
    for ( i = 0 ; i < n; i++ )st.insert( s [ i ] );
    a = n - st.size();
    if ( 26 - st.size() < a )cout<<"-1";
    else cout<<a;
