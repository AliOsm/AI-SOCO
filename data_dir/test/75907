///*********************************************************//

using namespace std;
///         0123456789
#define MX  102
#define MOD 1000000007
#define INF (1<<28)
#define eps 1e-9

/// ==========================================////

int b[MX][MX],a[MX][MX],n,m,c[MX][MX];

int main()
{
    cin>>n>>m;
    F(i,1,n+1)
    {
        F(j,1,m+1)
        {
            cin>>b[i][j];
            a[i][j]=1;
            c[i][j]=b[i][j];
        }
    }

    F(i,1,n+1)
    {
        F(j,1,m+1)
        {
            if(b[i][j]==0)
            {
                for(int k=1; k<=m; k++) a[i][k]=0;
                for(int k=1; k<=n; k++) a[k][j]=0;
            }
        }
    }

    F(i,1,n+1)
    {
        F(j,1,m+1)
        {
            c[i][j]=0;
            for(int k=1; k<=m; k++) c[i][j]|=a[i][k];
            for(int k=1; k<=n; k++) c[i][j]|=a[k][j];
        }
    }

    int f=0;

    F(i,1,n+1)
    {
        F(j,1,m+1)
        {
            if(b[i][j]!=c[i][j])
            {
                f=1;
            }
        }
    }
    if(f)
    {
        printf("NO\n");
        return 0;
    }
    printf("YES\n");
    F(i,1,n+1)
    {
        F(j,1,m+1)
        {
            cout<<a[i][j]<< " ";
        }
        NL;
    }
    return 0;
}
