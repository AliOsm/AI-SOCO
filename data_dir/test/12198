#include <iostream>

using namespace std;

int main()
{
    int n;
    cin >> n;
    int e = 0;
    while(n--)
    {
        int a;
        cin >> a;
        if (a % 2 == 0)
            e++;
        if (e % 2 == 0)
            cout << "2\n";
        else
            cout << "1\n";
    }
    return 0;
}
