#define lb lower_bound

using namespace std;

typedef long long ll;
typedef unsigned long long ull;
typedef long double ld;
typedef pair<int,int> pii;
typedef pair<ll,ll> pll;

inline void boost(){
    ios_base::sync_with_stdio();
    cin.tie(0);
    cout.tie(0);
}

const ll maxn=1e5+123;
const ll inf=1e9+123;
const ll mod=1e9+7;
const double pi=acos(-1);

int main(){
    boost();
    //freopen("input.txt","r",stdin);
    //freopen("output.txt","w",stdout);
    int m,d;
    cin>>m>>d;
    if(m==1 || m==3 || m==5 || m==7 || m==8 || m==10 || m==12){
        if((31-(7-d+1))%7){
            cout<<(31-(7-d+1))/7+2;
            return 0;
        }
        cout<<(31-(7-d+1))/7+1;
        return 0;
    }
    if(m==4 || m==6 || m==9 || m==11){
        if((30-(7-d+1))%7){
            cout<<(30-(7-d+1))/7+2;
            return 0;
        }
        cout<<(30-(7-d+1))/7+1;
        return 0;
    }
    if(m==2){
        if((28-(7-d+1))%7){
            cout<<(28-(7-d+1))/7+2;
            return 0;
        }
        cout<<(28-(7-d+1))/7+1;
        return 0;
    }
	return 0;
}
