//In the name of Allah

#include<bits/stdc++.h>

using namespace std;

const int Maxn = 100 + 5;
int n, t;
bool flag;

int main()
{
    cin >> n >> t;
    int a, b;
    for(int i = 0; i < n; i ++)
    {
        cin >> a >> b;
        if(a == t || a == 7 - t || b == t || b == 7 - t)
            flag = true;
    }
    if(flag)
        cout << "NO" << endl;
    else
        cout << "YES" << endl;
    return 0;
}
