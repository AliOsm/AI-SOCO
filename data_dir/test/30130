#include <bits/stdc++.h>
using namespace std;

int main() {
	int n;
	cin >> n;
	vector <int> w(n), h(n);
	for (int i = 0; i < n; ++i) {
		cin >> w[i] >> h[i];
	}
	int ans = (int)1e9 + 100;
	for (int H = 1; H <= 1000; ++H) {
		vector <int> a;
		int cnt = 0;
		int sum = 0;
		for (int i = 0; i < n; ++i) {
			if (w[i] > H && h[i] > H) {
				cnt += 1000;
				break;
			}
			if (h[i] <= H && w[i] > H) {
				sum += w[i];
				continue;
			}
			if (h[i] > H && w[i] <= H) {
				cnt++;
				sum += h[i];
				continue;
			}
			if (h[i] >= w[i]) {
				sum += w[i];
				continue;
			}
			else {
				sum += w[i];
				a.push_back(w[i] - h[i]);
			}
		}
		if (cnt > n / 2) {
			continue;
		}
		sort(a.begin(), a.end());
		while (cnt + 1 <= n / 2 && !a.empty()) {
			sum -= a.back();
			a.pop_back();
			++cnt;
		}
		ans = min(ans, sum * H);
	}
	cout << ans << endl;
	return 0;
}