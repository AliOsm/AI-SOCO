#include <bits/stdc++.h>
using namespace std;

#define pb push_back
#define mk make_pair
#define fi first
#define se second
#define eb emplace_back

typedef long long ll;
typedef pair<int,int> ii;
typedef vector< pair<int,int> > vii;
const int INF = 0x3f3f3f3f;

int main() {
    ios::sync_with_stdio(false);
    int rest[5] = {6,8,4,2};
    ll n; cin >> n;
    if(n == 0) { cout << 1 << endl; return 0; }
    n = n%4;
    cout << rest[n] << endl;

    return 0;
}

