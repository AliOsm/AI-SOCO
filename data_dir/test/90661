#define lowbit(x) ((-(x))&(x))  
#define vi vector<int>  
using namespace std;  
const int N = 55;
int k,n,a[N];
int main(){
    cin>>n>>k;
    for(int i=0;i<n;i++)
        cin>>a[i];
    if(k>n) puts("-1");
    else{
        sort(a,a+n);
        cout<<a[n-k]<<" "<<a[n-k]<<endl;
    }
    return 0;
}