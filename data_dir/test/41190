#include <iostream>
#include <stdio.h>

using namespace std;
int na,nb,m,a[100005],b[100005],k;
int main()
{
scanf("%d %d",&na,&nb);
scanf("%d %d",&k,&m);

for(int i=0;i<na;i++){
    scanf("%d",&a[i]);

}

for(int i=0;i<nb;i++){
    scanf("%d",&b[i]);

}

if(a[k-1]<b[nb-m]){printf("YES\n");}else{printf("NO\n");}

    return 0;
}
