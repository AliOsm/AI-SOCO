#pragma optimize("O3")
#include <bits/stdc++.h>
#define ll long long
#define s second
#define f first
#define pb push_back
#define ios ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define y1 ibibibibbibibibiib
using namespace std;
ll const N=1e5+2;
ll k,b,d,q,w,x;
ll m,n,a[N],suf[N],predf[N];
bool us[N];
char ch;
string t;
int main()
{
cin>>n>>m;
if (m%n!=0) return cout<<-1,0;
if (m%3==0){
    while(m%(n*3)==0)
{
 n*=3;
 q++;
}
}
if (m%2==0){
    while(m%(n*2)==0)
{
 n*=2;
 q++;
}
}
if (n!=m) cout<<-1; else
cout<<q;
}

