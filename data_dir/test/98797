#include<iostream>
#include<math.h>
#include<algorithm>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.precision(20);
    int n,a,b,x;
    cin>>n>>a>>b;
    int c[n+1]={0};
    for(int i=0;i<a;i++)
    {
        cin>>x;
        c[x]=1;
    }
    for(int i=0;i<n;i++)
    {
        if(c[i+1]==1)
         cout<<1;
        else
        cout<<2;
        cout<<" ";
    }}
    