#define f(x) return cout << x << "\n", 0
#define lp(i,a,x) for (int i=a;i<=x;++i)
#define lp2(i,x,a) for (int i=x;i>=a;--i)
#define all(x) x.begin(),x.end()
#define P(x)        cout<<#x<<" = { "<<x<<" }\n"
using namespace std;
const int N=1e5+5;
inline bool odd(int x)
{
    return (x&1);
}
int main()
{
#ifdef ONLINE_JUDGE
    FAST;
#endif
    int n;
    map<int,int>mp;
    cin>>n;
    lp(i,1,n)
    {
        int x;
        cin>>x;
        mp[x]++;
    }
    int mx=INT_MIN;
    for (auto u:mp)
    {
        mx=max(mx,u.S);
    }
    cout<<mx;
    return 0;
}