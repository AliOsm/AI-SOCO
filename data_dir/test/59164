#include "bits/stdc++.h"
using namespace std;

#define  F  first
#define  S  second
#define  pb push_back
#define  mp make_pair
typedef long long int ll;
const int N = 1e6+2,inf = 1e9+7;
int vis[N];
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);

    int n;cin>>n;
    int mx = 0;
    while(n--)
    {
        int x;cin>>x;
        vis[x]++;
        mx=max(mx,vis[x]);
    }
    cout<<mx<< "\n";







    return 0;
}

