#include <bits/stdc++.h>
#define pb push_back
#define F first
#define S second
#define all(x) x.begin(), x.end()
using namespace std;
typedef long long ll ; 
typedef long double ld ;
typedef pair<ll , ll> pll ;

const ll N = 2e2 + 30 , Mod = 1e9 + 7 ;
const ll SQ = 330 ;


int main () {
    ios::sync_with_stdio(0), cin.tie(0) ;
    ll n ;
    cin >> n ;
    ll g = 0  ,mn = 1e18;
    for (int i =0  ; i < n ; i ++) {
    	ll x ;
    	cin >> x ;
    	mn = min(mn , x) ;
    	g = __gcd(g , x) ;
    	
	}
	cout << n * g;
    return (0) ;
}
