#include <iostream>
#include <cmath>
#include <algorithm>
#include <iomanip>
#include <cstdio>
#include <map>
#include <string>
#include <set>
#include <vector>
#include <stack>
#include <deque>
#define ll long long
#define ss second
#define ff first
using namespace std;
ll n,m,x,y,z;
string f,s[200];
int main(){
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>f;
        z=0;
        for(int j=m-1;j>=0;j--){
            if(s[j]==f){cout<<"YES";z=1;break;}
        }
        if(z==0){
            s[m]=f;
            m++;
            cout<<"NO";
        }
        cout<<endl;
    }
    return 0;
}