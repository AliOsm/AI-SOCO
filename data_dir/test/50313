#include <bits/stdc++.h>

using namespace std ;
typedef unsigned long long ll ;


int main ()
{
   int c , sum = 0 ;

   for ( int  i = 0 ; i < 5 ; i++)
   {
       cin>>c;

       sum+=c;

   }

if ( sum == 0 || sum % 5 )
    cout<<"-1";
else
    cout<< sum / 5 ;

    return 0 ;
}



