#include <stdio.h>
#include <algorithm>
using namespace std;

int main(void) {
    int k;
    int x;
    int maxi = 0;

    scanf(" %d", &k);
    for (int i = 0; i < k; i++) {
        scanf(" %d", &x);
        maxi = max(maxi, x);
    }
    int res = max(0, maxi - 25);
    printf("%d\n", res);


    return 0;
}
