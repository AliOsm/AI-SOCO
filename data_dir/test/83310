#include<bits/stdc++.h>
using namespace std;
#define ll  long long
#define endl '\n'
const ll mod = 1000000007;
const double PI = 3.141592653589793238463;
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
    int q;
    cin>>q;
    while (q--){
        int a,b,c,d,k;
        cin>>a>>b>>c>>d>>k;
        if (a/c+(a%c!=0)+b/d+(b%d!=0)<=k)
            cout<<a/c+(a%c!=0)<<" "<<b/d+(b%d!=0)<<endl;
        else
            cout<<-1<<endl;
    }
    return 0;
}
