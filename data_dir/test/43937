#include<bits/stdc++.h>
using namespace std;
int main()
{


  int n;
  double r,d;
  cin >>  r>>d;
   cin >>n;
   int cnt=0;
  while(n--)
  {
       double x,y,m;
       cin >>x>>y>>m;
    double det1 = sqrt( (x*x)+(y*y)) ;

    if(  det1-m>=r-d    &&    det1+m <= r ) cnt++;
  }
  cout  <<cnt <<endl;

}