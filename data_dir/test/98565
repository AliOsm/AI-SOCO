#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define f first
#define s second
#define bp push_back
#define mp make_pair
const int mx=3e5+10;
const double eps=1e-9;
const int inf=1e9;
int a[222],b[220];
main(){
    int n,m;
    scanf("%d",&n);
    for(int i=0;i<n;i++)scanf("%d",a+i);
    scanf("%d",&m);
    for(int i=0;i<m;i++)scanf("%d",b+i);
    printf("%d %d",*max_element(a,a+n),*max_element(b,b+m));

}
