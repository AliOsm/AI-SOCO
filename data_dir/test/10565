#include<bits/stdc++.h>
#define MOD 1000000007
using namespace std;
typedef long long int ll;
double PI=4.00*(atan(1));

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    ll n;
    cin>>n;
    if(n%2==1)
    {
        cout<<"black";
    }
    else
    {
        cout<<"white"<<endl;
        cout<<"1 2";
    }
    return 0;
}