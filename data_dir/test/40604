#include<bits/stdc++.h>
#define ll long long
using namespace std;
const int N = 1e5 + 5;
int main()
{
    ios_base ::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
    int n;
    cin>>n;
    string s;
    map<string , int> m;
    for(int i=0;i<n;i++)
    {
        cin>>s;
        m[s]++;
        if(m[s]==1)cout<<"OK\n";
        else cout<<s<<m[s]-1<<endl;
    }
return 0;
}
