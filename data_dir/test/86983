  while(t--)
  {
      int a[4],r,g,b;
      cin>>a[0]>>a[1]>>a[2];
      sort(a,a+3);
      r=a[2];
      b=a[1];
      g=a[0];
      r=r-g;
      b=b-g;
      if(r==0 && b==0)
      {
          cout<<"YES"<<endl;
          continue;
      }
      r=r-b;
      if(r<=g+1)
      {
          cout<<"YES"<<endl;
      }
      else cout<<"NO"<<endl;

  }


















































