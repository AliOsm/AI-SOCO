#include <algorithm>
#include <iostream>
#include <cmath>
#include <fstream>
#include <vector>
#include <iomanip>
#include <string>
#include <queue>
#include <map>
#include <stack>
#include <set>

using namespace std;

typedef long long ll;
typedef long double ld;

template <typename T>
T gcd(T a, T b) {
	while (a > 0 && b > 0) {
		if (a > b) a %= b; else b %= a;
	}
	return (a + b);
}

int main() {
	//freopen("input.txt", "r", stdin);
	string a, b;
	cin >> a >> b;
	if (a.size() != b.size()) {
		cout << max(a.size(), b.size()) << endl;
		return 0;
	}
	if (a == b) {
		cout << -1 << endl;
		return 0;
	}
	cout << a.size() << endl;
	return 0;
}