#include <bits/stdc++.h>
using namespace std;

int main(){
  int t, a, b, c, d, k;
  cin >> t; 
  
  while(t--){
    cin >> a >> b >> c >> d >> k;
    int x = a/c, y = b/d;
    if(a%c > 0){x++;}
    if(b%d > 0){y++;}
    if(x + y <= k){cout << x << " " << y << "\n";}
    else{cout << "-1\n";}
  }
}