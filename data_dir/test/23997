#include <bits/stdc++.h>

using namespace std;
typedef long long ll;
typedef pair<int, int> pi;


int main() {
  ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
  int k, r;
  cin >> k >> r;
  for (int i = 1; i <= 10; i++)
    if (i*k >= r and ( (i*k-r)%10==0  or i*k%10 == 0)) {
      cout << i << endl;
      return 0;
    }
}

