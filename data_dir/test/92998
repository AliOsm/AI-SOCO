#include<bits/stdc++.h>
#define FIO ios_base::sync_with_stdio(false),cin.tie(0),cout.tie(0);
using namespace std;    typedef long long ll;    typedef long double ld;

const int inf = 1e9;
const ll INF = 1e18;
int n, sum;
int main(){
    FIO;
    cin >> n;
    for(int i = 0; i < n; i++){
        int x;
        cin >> x;
        sum += abs(x);
    }
    cout << sum;
    return 0;
}
