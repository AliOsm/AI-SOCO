#include <cstdio>
#include <algorithm>
#include <vector>
#include <cmath>
#include <cstring>
#include <string>
#include <iostream>
#include <stack>
#include <queue>
#include <deque>
#include <set>
#include <map>

using namespace std;

typedef unsigned long long LL;
typedef long double LD;
typedef pair<int, int> PII;

LL n;

void solve()
{
	scanf("%llu", &n);
	puts("2");
	for(LL k = 2; k <= n; k++)
	{
		printf("%llu\n", k * k * k + k * k * 2 + 1);
	}
}

int main()
{
	solve();
	return 0;
}
