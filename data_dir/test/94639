#include <iomanip>
using namespace std;


# define ll long long
# define PI 3.1415926535

int a[5005],b[5005],c[5005];

int main() 
{
	ios::sync_with_stdio(0), cin.tie(0);
    int n;
    cin >> n;
    int p,q;
    cin >> p;
    int a[p];
    set <int> ans;
    for(int i=0; i<p; i++)
    {
    	cin >> a[i];
    	ans.insert(a[i]);
    }
    cin >> q;
    int b[q];
    for(int i=0; i<q; i++)
    {
    	cin >> b[i];
    	ans.insert(b[i]);
    }
     if(ans.size()==n)
     	cout << "I become the guy." << endl;
     else
     	cout << "Oh, my keyboard!" << endl;
    return 0;
}

