const ll N = 4000 + 7;
typedef pair<ll,ll>   pii;
typedef vector<pii>   vpii;
typedef vector<ll>    vll;
const string alpha = "abcdefghijklmnopqrstuvwxyz";
ll power(ll x,ll y){ll res=1;x=x;while(y>0){if(y&1)res=(res*x);y=y>>1;x=(x*x);}return res;}
template<typename T> T gcd(T a,T b){ if(a==0) return b; return gcd(b%a,a); }
template<typename T> T powm(T a,T b, ll m){T cnt=1; while(b>0){ if(b%2==1) cnt=(cnt*a)%m; b/=2; a=(a*a)%m; }return cnt%m;}
ll ncr(ll n,ll r){ll res=1;if(r>n)return 0;if(r>n-r)r=n-r;for(ll i=0;i<r;i++){res*=(n-i);res/=(i+1);}return res;}
ll sqr(ll x) { return x * x;}
// always use 1ll*ans to avoid signed integer overflow; 
void prec(){
	
}
ll n,m,u[N],v[N],ans=INT_MAX,flag=0,a,b,c,d,deg[N];
bool mp[N][N];
void solve(){
	cin>>n>>m;
	rep(i,1,m){
		cin>>u[i]>>v[i];
		deg[u[i]]++;
		deg[v[i]]++;
		mp[v[i]][u[i]]=1;
		mp[u[i]][v[i]]=1;
	}
	rep(i,1,m){
		rep(j,i+1,m){
			a=min(u[i],v[i]);
			b=max(u[i],v[i]);
			c=min(u[j],v[j]);
			d=max(u[j],v[j]);
			if((a==c && mp[b][d])){
				flag|=1;
				ans=min(ans,deg[a]+deg[b]+deg[d]-6ll);
			}
			if(b==d && mp[a][c]){
				flag|=1;
				ans=min(ans,deg[a]+deg[b]+deg[c]-6ll);
			}
		}
	}
	if(flag) cout<<ans;
	else cout<<-1;
}
int main()
{
    cin.sync_with_stdio(0); cin.tie(0);
	cin.exceptions(cin.failbit);
    int tc=1;
    //cin>>tc;
    prec();
    while(tc--)
    	solve();
    return 0; 
}