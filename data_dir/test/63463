int main() {
	int X, HH, MM;
	scanf("%d %d %d", &X, &HH, &MM);

	int t = HH * 60 + MM;
	for (int y = 0;; y++) {
		int nt = (t - X*y) % 1440;
		nt = (nt + 1440) % 1440;

		int t1 = nt / 60, t2 = nt % 60;
		if (t1 % 10 == 7 || t1 / 10 == 7 || t2 % 10 == 7 || t2 / 10 == 7) return !printf("%d\n", y);
	}
	return 0;
}
