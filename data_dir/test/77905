#include <iostream>
#include <string>
#include <sstream>
#include <vector>
#include <cmath>
#include <cstdio>
#include <cstdlib>
using namespace std;

int gcd(int a, int b) {
    return b == 0 ? a : gcd(b, a % b);
}

int main(void) {
    int n, m;
    cin >> n >> m;
    int *a = new int[n + 1];
    for (int i = 0; i <= n; ++i) {
        cin >> a[i];
    }
    int *b = new int[m + 1];
    for (int i = 0; i <= m; ++i) {
        cin >> b[i];
    }
    if (n > m) {
        if (a[0] * b[0] > 0) {
            puts("Infinity"); 
        } else {
            puts("-Infinity");
        }
    }
    else if (n < m) {
        puts("0/1");
    } else {
        int p = a[0];
        int q = b[0];
        int g = gcd(abs(p), abs(q));
        p /= g;
        q /= g;
        if(q < 0) {
            p = -p;
            q = -q;
        }
        cout << p << '/' << q << endl;
    }
    
}