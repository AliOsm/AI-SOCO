using namespace std;
class TaskD {
public:
	void solve(std::istream& in, std::ostream& out) {
        make(int, n);
        map<int, int> count;
        set<pair<int, int>> mem;
        int sametwo = 0;
        forn(i, 2 * n){
            make(int, temp);
            count[temp] ++;
            auto pos = make_pair(i % n, temp);
            if (mem.count(pos) > 0) sametwo++;
            mem.insert(pos);
        }
        make(ll, mod);
        ll res = 1;
        for(auto key : count){
//            out << key.first << el;
//            res *= (ll) key.second;
//            res %= mod;
            forn(i, key.second){
                res *= (i + 1);
                while (res % 2 == 0 && sametwo > 0){
                    res /= 2;
                    sametwo --;
                }
                res %= mod;
            }
        }
        while (res % 2 == 0 && sametwo > 0){
            res /= 2;
            sametwo --;
        }
        out << res;
	}
};


