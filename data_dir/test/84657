#include <bits/stdc++.h>

using namespace std;

const int N = 1e3 + 5;

int main()
{
  int n;
  int m;
  cin>>n>>m;
  string s;
  cin>>s;
  for(int i=0;i<m;i++){
        int l,r;
        char c1,c2;
        cin>>l>>r>>c1>>c2;
        for(int i=l-1;i<r;i++){
            if(s[i]==c1){
                s[i]=c2;
            }
        }

  }
  cout<<s<<endl;
}
