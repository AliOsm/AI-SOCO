int main()
{
	string s = "qwertyuiopasdfghjkl;zxcvbnm,./";

	string a, b;
	cin >> a >> b;

	if(a == "R")
	{
		for(int i = 0; i < b.size(); i++)
			cout << s[s.find(b[i]) - 1];
	}
	else
	{
		for(int i = 0; i < b.size(); i++)
			cout << s[s.find(b[i]) + 1];
	}

	//pp;
}