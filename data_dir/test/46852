#include <bits/stdc++.h>
#define MOD 1000000007
#define INT long long int
using namespace std;
INT c,n,m,a[1010];

int main()
{
    ios::sync_with_stdio(false);
    cin>>c;
    while(c--)
    {
        INT s = 0;
        cin>>n>>m;
        for(int i = 0 ; i < n ; i++ ) cin>>a[i], s+=a[i];
        if( s >= m ) cout<<m<<endl;
        else cout<<s<<endl;
    }
}
