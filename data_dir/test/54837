#include <bits/stdc++.h>
using namespace std;

int main( )
{
    int t, i, j, k, a, b, n;
    cin >> t;
    while (t--) {
        cin >> a >> b >> n;
        n = n % 3;
        if (n == 0) {
            cout << a << '\n';
        }
        else if (n == 1) {
            cout << b << '\n';
        }
        else {
            cout << (a ^ b) << '\n';
        }
    }

    return 0;
}
