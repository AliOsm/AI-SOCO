#include <bits/stdc++.h>
using namespace std;
#define pb push_back
#define mp make_pair
#define mod 1000000007
#define pi 3.1415926535897932384626
#define LMAX 9223372036854775807
#define ll long long
#define fi first
#define sec second
#define pii pair<int, int>
ll n, m, k, x=2, y, sum, ans;
int main () {
	scanf("%lld", &n); 
	printf("2\n");
	for (ll i=2; i<=n; i++) {
		ll y=i*(i+1)*(i+1)-i+1;
		printf("%lld\n", y);
	}
	return 0;
}