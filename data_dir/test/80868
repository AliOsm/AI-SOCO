long long n,m,k,i,j,p,p2;
string s,t;
long long get(long long x)
{
	long long ans=x,i,y=x;
	for (i=2;i*i<=y;i++)
	{
		if (x%i==0)
		{
			ans=ans/i*(i-1);
			while (x%i==0) x/=i; 
		}
	}
	if (x>1) ans=ans/x*(x-1);
	return ans;
}
long long qp(long long x,long long y)
{
	if (y==0) return 1;
	if (y==1) return x;
	long long z=qp(x,y/2);
	z=z*z%p;
	if (y&1) z=z*x%p;
	return z;
}
int main()
{
	cin>>s>>t>>p;
	p2=get(p);
	for (i=0;i<s.size();i++)
	{
		n=(n*10+s[i]-'0');
		n%=p;
	}
	m=(n-1+p)%p;
	if (t=="1")
	{
		k=0;
	}
	else
	{
		for (i=0;i<t.size();i++)
		{
			k=(k*10+t[i]-'0');
			k%=p2;
		}
		k=(k-1+p2)%p2+p2;
	}
	long long ans=m*qp(n,k)%p;
	if (ans==0) ans=p;
	cout<<ans;
	return 0;
}