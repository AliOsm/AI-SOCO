#include<bits/stdc++.h>

using namespace std;

#define pb push_back
const int maxi = 1e6+10;
vector<int> v[maxi];
int n;
string s;
int a,b;

int main()
{
    int t;
    cin>>t;

    while(t--)
    {
        cin>>a>>b>>n;
        if (n%3==0) printf("%d\n",a); else
            if (n%3==1) printf("%d\n",b); else
            printf("%d\n",a^b);
    }
    return 0;
}
