#include <iostream>
#include <cmath>
#include <algorithm>
#include <iomanip>
#include <cstdio>
#include <map>
#include <string>
#include <set>
#include <vector>
using namespace std;
long long n;
int main(){
cin>>n;
for(int i=0;i<n;i++){
    if(i%4==0 || i%4==1)cout<<'a';
    else cout<<'b';
}
    return 0;
}
