#include <bits/stdc++.h>

using namespace std;

int n,x,y;

int main() {
    cin>>n;
    cout<<"YES"<<endl;
    while (n--) {
        scanf("%d%d%d%d",&x,&y,&x,&y);
        if (x%2 && y%2) printf("4\n"); else
        if (x%2) printf("3\n"); else
        if (y%2) printf("2\n"); else
        printf("1\n");
    }
}
