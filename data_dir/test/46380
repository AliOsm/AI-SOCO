#include<bits/stdc++.h>
using namespace std;
int main ()
{

    int  tc;
   cin >>tc;

    while(tc--)
    {
        int l,r,d;

        cin >> l >>r >>d;
        if(l>r)
            swap(r,l);
        if(l>d)
        {
            cout << d <<endl;
        }
        else
        {

              int ans1 = r/d;

           // cout <<ans1<<endl;
              ans1++;
              cout << ans1*d<<endl;


        }
    }

}
