// ITNOG

#include <iostream>
#include <algorithm>
#include <set>
using namespace std;

int a, b, n;
vector<int> ans;

int main()
{
      cin >> a >> b;
      n = a+b+1;
      for(int i = 1; i <= n; ++i)
            ans.push_back(i);
      reverse(ans.begin()+a, ans.end());
	for(int i = 0; i < n; ++i)
            cout << ans[i] << ' ';
      cout << endl;
      return 0;
}
