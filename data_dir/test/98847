#include <iostream>
#include <cstdlib>
#include <cstring>
#include <algorithm>
#include <map>
#include <set>
#include <vector>
#include <cstdio>

using namespace std;

int main()
{
    int n, i;
    cin >> n;
    if (n<=2) cout << -1 << endl; else
    {
              cout << n-1 << " " << n << " ";
              for (i = 1;i<=n-2;i++)
              {
                  cout << i << endl;
                  if (i<n-2) cout << " "; else cout << endl;
              }
    }
//    system("pause");
    return 0;
}

