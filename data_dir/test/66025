// Author: πα

#include <bits/stdc++.h>
using namespace std;
#define long int64_t

int main()
{
    ios::sync_with_stdio(false), cin.tie(nullptr);
    long l, r;
    cin >> l >> r;
    cout << "YES\n";
    for(long i = l; i <= r; ++i)
        cout << i << ' ' << ++i << '\n';
    return 0;
}
