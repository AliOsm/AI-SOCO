#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

multiset<ll> h_min, w_min, h_max, w_max;
multiset<ll, greater<ll> > h, w;

int main (int argc, char *argv[])
{
	ll ww, hh;
	int n;
	scanf("%lld%lld%d", &ww, &hh, &n);
	h.insert(hh);
	w.insert(ww);
	h_min.insert(0);
	h_min.insert(-hh);
	h_max.insert(0);
	h_max.insert(hh);
	w_min.insert(0);
	w_min.insert(-ww);
	w_max.insert(0);
	w_max.insert(ww);
	while(n--) {
		char c;
		scanf(" %c", &c);
		ll d;
		scanf("%lld", &d);
		if(c == 'H') {
			int q = - *h_min.upper_bound(-d),
				qq = *h_max.upper_bound(d);
			//cout << q << " " << qq << endl;
			h_min.insert(-d);
			h_max.insert(d);
			h.insert(d - q);
			h.insert(qq - d);
			h.erase(h.find(qq - q));
		} else {
			int q = - *w_min.upper_bound(-d),
				qq = *w_max.upper_bound(d);
			//cout << q << " " << qq << endl;
			w_min.insert(-d);
			w_max.insert(d);
			w.insert(d - q);
			w.insert(qq - d);
			w.erase(w.find(qq - q));
		}
		printf("%lld\n", *h.begin() * *w.begin());		
	}
	
	return 0;
}

