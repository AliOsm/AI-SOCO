#include <bits/stdc++.h>
#include<string>
    
using namespace std;
            
    #define ll long long
    #define db long double
    #define ff first
    #define ss second
    #define pb push_back
    #define mp make_pair
    #define inf LONG_LONG_MAX
	#define ninf LONG_LONG_MIN 
    #define MOD 1000000007
	#define N 1000010
    #define FAST ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

void solve()
{
	ll n,m,i,j,k;
	ll s;
	cin>>n>>s;
	if(n<=s/2)
	{
		cout<<"YES\n";
		for(i=0;i<n-1;i++)
			cout<<1<<" ";
		cout<<s-(n-1)<<"\n"<<s/2<<"\n";
	}
	else
		cout<<"NO\n";

}


int main()
{
    FAST
   	ll q=1;
   	//cin>>q;
   	while(q--)
   		solve();
  
 }