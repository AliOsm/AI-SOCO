#include <bits/stdc++.h>
using namespace std;

const int N = 1e5 + 5;

int n, m, a[N], b[N];

int main(){


    int q;
    cin >> q;
    while (q--){
        int n, m;
        cin >> n >> n >> m;
        for (int i = 1; i <= n; ++i) cin >> a[i];
        for (int i = 1; i <= m; ++i) cin >> b[i];
        sort (a + 1, a + n + 1);
        sort (b + 1, b + m + 1);
        if (a[n] > b[m]) cout << "YES\n";
        else cout << "NO\n";
    }

}
