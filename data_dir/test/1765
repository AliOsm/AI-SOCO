#include <iostream>
#include <string>
#include <cmath>
#include <algorithm>
#include <vector>
#include <set>
#include <map>
#include <sstream>
#include <cstdio>
#include <cstring>
using namespace std;
int main()
{
    int n , k;
    cin >> n >> k;
    for(int i=1 ; i < n-k ; i++)
        cout << i << " ";
    for(int i=n ; i >= n-k ; i--)
        cout << i << " ";
}