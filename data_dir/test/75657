//g++ -std=c++14 -g -O2 -o ./a ./A.cpp
#include <bits/stdc++.h>
using namespace std;
#define ff first
#define ss second
#define nl '\n'
typedef long long ll;
//////////////////////////////////////////////////////////////////////



int main(){
  ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
  
  ll aa,bb;cin>>aa>>bb;
  ll a=aa,b=bb;
  for(ll x=1;;x++){
    if(x%2)a-=x;
    else b-=x;
    //cerr << a << ',' << b << endl;
    if(a<0 or b<0){
      if(b<0)cout<<"Valera"<<nl;
      else cout<<"Vladik"<<nl;
      break;
    }
  }
  
  return 0;
}
