#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    ll n,i,j;
    cin>>n;
    vector<ll> a(n);
    for(i=0;i<n;i++)
        cin>>a[i];
    ll b=0,c=0;
    for(i=0;i<n;i++)
    {
        if(a[i]>=0)
            b+=a[i];
        else
            c+=a[i];
    }
    cout<<b-c<<endl;
}
