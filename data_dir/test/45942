#include <iomanip>
#include <iostream>
#include <vector>
#include <set>
#include <queue>
#include <cstdio>
#include <ctime>
#include <map>
#include <algorithm>
#include <string>

#define ll long long
#define mp make_pair
#define pb push_back

using namespace std;

const int N = 300010;

int n, m;
int p[N], pos[N];
vector <int> g[N];
bool isDeleted[N];
vector <pair<int, int> > vect;

int main() {
    scanf("%d%d", &n, &m);
    for (int i = 1; i <= n; i++) scanf("%d", &p[i]);
    for (int i = 1; i <= n; i++) pos[p[i]] = i;
    for (int i = 1; i <= m; i++) {
        int u, v;
        scanf("%d%d", &u, &v);
        if (pos[u] > pos[v]) continue;
        if (pos[u] > pos[v]) swap(u, v);
        g[u].pb(v);
        if (v == p[n]) vect.pb(mp(pos[u], u));
    }
    sort(vect.begin(), vect.end());
    reverse(vect.begin(), vect.end());
    int ans = 0;
    int cntDeleted = 0;
    for (auto x : vect) {
        int v = x.second;    
        int need = n - pos[v] - cntDeleted;
        for (int u : g[v]) {
            if (!isDeleted[u]) need--;
        }
        if (need == 0) {
            ans++;
            cntDeleted++;
            isDeleted[v] = true;
        }
    }
    cout << ans;
    return 0;
}