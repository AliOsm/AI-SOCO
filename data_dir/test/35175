#include <iostream>
using namespace std;

int main()
{
	int t;
	cin>> t;

	for(int o = 0; o < t; o ++)
	{
		int n, p;
		cin>> n >> p;

		int k = 2 * n + p;
		for(int i = 0; i < n; i ++)
			for(int j = i + 1; j < n; j ++)
				if(k > 0)
				{
					cout<< i + 1 << " " << j + 1 << endl;
					k --;
				}
				else
					break;
				
	}
}