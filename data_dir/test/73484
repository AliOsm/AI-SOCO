#include <iostream>
#include <cstdlib>
#include <cstdio>
#include <stdio.h>
using namespace std;
int main(){
  //freopen("input.txt","r",stdin);
  //freopen("output.txt","w",stdout);
  int q,w,e,t=0;
  char a[1001],s[1001];
  scanf("%d%s%s",&q,a,s);
  for(w=0;w<q;w++){
   e=abs(a[w]-s[w]);
   t+=min(e,10-e);}
  printf("%d",t);
  return 0;}
