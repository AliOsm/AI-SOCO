#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define pf push_front
#define fr first
#define sc second
#define r_0 return 0
#define ld long double
#define TIME ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
using namespace std;
    const ll N = 1e3 + 7;///

void solve(){
    char c;
    int x , y , ans = 0;
    cin >> c >> y;
    x = c - 'a' + 1;
    if(x > 1 && x < 8 && y > 1 && y < 8){
        cout << 8;
        return;
    }
    if((x == 1 || x == 8) && (y == 1 || y == 8)){
        cout << 3;
        return;
    }
    cout << 5;
}
int main(){
    TIME
    int T = 1;
    //cin >> T;
    while(T --)
        solve();
}
