#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    ll q;
    cin>>q;
    while(q--)
    {
        ll n,i,j;
        cin>>n;
        vector<ll> a(n);
        for(i=0; i<n; i++)
            cin>>a[i];
        ll sum=0;
        for(auto in:a)
            sum+=in;
        ll avg=((sum-1)/n)+1;
        cout<<avg<<endl;
    }
}

