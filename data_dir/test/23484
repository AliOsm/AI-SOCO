#include <bits/stdc++.h>
#define intt long long
#define pb push_back
#define mk make_pair
#define ii pair<intt,intt>
#define sc second
#define fr first
using namespace std;
int main()
{
    int n;
    cin>>n;
    string s;
    cin>>s;
    int l=0,r=0;
    int i;
    for(i=0;i<s.length();i++)
      l+=(s[i]=='L'),r+=(s[i]=='R');
    cout<<l+r+1;
    return 0;
}
