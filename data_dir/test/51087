#include <bits/stdc++.h>
using namespace std;
 
typedef pair<int,int> ii;
typedef long long ll;

int main(){

    int t; cin >> t;
    while(t--){
        int h, m; cin >> h >> m;
        int t1 = h*60 + m;
        int t2 = 24*60;
        cout << t2-t1 << endl;
    }
    
    return 0;
}
