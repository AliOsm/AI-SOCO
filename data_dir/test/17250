#include  <bits/stdc++.h>
using namespace std;
#define int long long
char c(char x)
{
	if(x=='e' || x=='a' || x=='i' || x=='o' || x=='u')
    		return '1';
    return '0';
}
int32_t main()  
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
    string s,t;cin>>s>>t;
    if(s.size()==t.size())
    for(int i=0;i<s.size();i++)
    {
    	s[i]=c(s[i]);
    	t[i]=c(t[i]);
    }
    if(s==t)
    	 cout<<"Yes";
    else
    	cout<<"No";
}