			return a;
		};

		int n, m, r; in >> n >> m >> r;
		vector<int> buys = ReadVec(n);
		vector<int> sells = ReadVec(m);
		int buy = *std::min_element(buys.begin(), buys.end());
		int sell = *std::max_element(sells.begin(), sells.end());
