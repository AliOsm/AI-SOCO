#include<stdio.h>
#include<stack>
#include<math.h>
#include<iostream>
#include<algorithm>
#include<string.h>
#include<string>
#include<memory.h>
#include<vector>
#include<map>
#include<queue>
using namespace std;
#define mp make_pair
#define pb push_back
#define pii pair<int,int>
#define pdd pair<double,double>
typedef long long li;
#define vint vector<int>
#define vvint vector<vint>
#pragma comment(linker, "/STACK:167177216")
#include<set>

int main()
{
	int n,k,tek;
	cin >> n >> k;
	tek=n;
	int i,j,m;
	for(i=1; i<=n-k-1; i++)
		cout << i << ' ';
	for(i=n-k; i<=n; i++)
	{
		cout << tek << ' ';
		tek--;
	}
	return 0;
}