#include <cstdio>
#include <iostream>
#include <string>
#include <map>
#include <cstring>
#include <set>
#include <unordered_set>
#include <algorithm>
#include <cmath>
#include <vector>

using namespace std;
int n;
int m;
int t;
int main() {
	cin >> n;
	while (n--) {
		cin >> m;
		t += abs(m);
	}
	cout << t << endl;

}
