#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

i64 N, A, B, C, D, ans;

int main()
{
    optimiza_io
    cin >> N >> A >> B >> C >> D;
    for( i64 i = 1; i <= N; i ++ ) {
        i64 sum = A + B + i;
        if( sum - A - C < 1 or sum - A - C > N ) continue;
        if( sum - C - D < 1 or sum - C - D > N ) continue;
        if( sum - B - D < 1 or sum - B - D > N ) continue;
        ans++;
    }
    
    cout << ans * N << "\n";
    return 0;
}