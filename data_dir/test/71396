/// يا رب قوتك
#include<bits/stdc++.h>
#include<cmath>
using namespace std;
#define ll long long
#define F first
#define S second
#define pb push_back
#define mp make_pair
const int MX=3*1e5 + 10;
const double PI = acos(-1.0), EPS = 1e-9;
int N, k;
int dx[]={0, 0, 1, -1};
int dy[]={1, -1, 0, 0};
ll __lcm(ll a, ll b)
{
    return (a * b) / __gcd(a, b);
}
int arr[MX];
int dp[MX];
int main()
{
    int n, s;
    cin>>n>>s;
    int ans=-1;
    for(int i=0;i<n;i++)
    {
        int x, y;
        cin>>x>>y;
        if(x < s)
        {
            int c=100-y
        ;
            if(c == 100) ans=max(ans, 0);
            else ans=max(ans, c);
        }
        else if(x == s && !y) ans=max(ans, 0);
    }
    cout<<ans;
    return 0;
}
