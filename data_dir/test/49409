#include<bits/stdc++.h>
using namespace std;
#define int long long int
#define double long double
void __solve(){
	int n,a,b,c,d;
	cin>>n>>a>>b>>c>>d;
	int l=(a-b);
	int r=(a+b);
	bool ok=false;
	if((a-b)*n<=(c+d) and (a+b)*n>=(c-d))ok=true;
	if(ok)cout<<"Yes"<<endl;
	else cout<<"No"<<endl;
}
int32_t main()
{
	ios_base::sync_with_stdio(0); cin.tie(0);cout.tie(0);
	int T;
	cin>>T;
	while(T--){
		__solve();
	}
}