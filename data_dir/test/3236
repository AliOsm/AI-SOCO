#include <bits/stdc++.h>
using namespace std;

#define ll long long 
#define pb push_back
#define pii pair<int, int>
#define vl vector<ll>

int main () {
    string s;
    cin>>s;
    for (int i=0; i<s.length(); i++) s[i] = (char) min ((int)s[i], (int)('9' - s[i]) + '0');
    if (s[0] == '0') s[0] = '9';
    cout<<s<<endl;
    return 0;
}