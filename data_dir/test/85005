#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef pair <int , int> pii;

const int N = 2e5+10 , mod = 1e9+7;

int32_t main(){
   ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
   int t;
   cin >> t;
   while(t--){
      int a , b , n;
      cin >> a >> b >> n;
      n = n % 3;
      if(n == 2)
         cout << (a ^ b) << "\n";
      else if(n == 0)
         cout << a << "\n";
      else
         cout << b << "\n";
   }
   return 0;
}
