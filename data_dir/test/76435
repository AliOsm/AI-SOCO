#define trace1(x)                cerr<<#x<<": "<<x<<endl
#define trace2(x, y)             cerr<<#x<<": "<<x<<" | "<<#y<<": "<<y<<endl
#define trace3(x, y, z)          cerr<<#x<<":" <<x<<" | "<<#y<<": "<<y<<" | "<<#z<<": "<<z<<endl
#define trace4(a, b, c, d)       cerr<<#a<<": "<<a<<" | "<<#b<<": "<<b<<" | "<<#c<<": "<<c<<" | "<<#d<<": "<<d<<endl
#define trace5(a, b, c, d, e)    cerr<<#a<<": "<<a<<" | "<<#b<<": "<<b<<" | "<<#c<<": "<<c<<" | "<<#d<<": "<<d<<" | "<<#e<< ": "<<e<<endl
#define trace6(a, b, c, d, e, f) cerr<<#a<<": "<<a<<" | "<<#b<<": "<<b<<" | "<<#c<<": "<<c<<" | "<<#d<<": "<<d<<" | "<<#e<< ": "<<e<<" | "<<#f<<": "<<f<<endl

const int N=55;
string arr[N];
int dp[N][2][2][2];


bool isdigit(char ch)
{
    if((ch-'0')>=0 and (ch-'0')<=9)
        return true;
    return false;
}

bool isspecial(char ch)
{
    if(ch=='#' or ch=='*' or ch=='&')
        return true;
    return false;
}

int build(int id,int m,bool dig,bool sp,bool al)
{
    if(!dig and !sp and !al)
        return 0;
    if(id==0)
        return 1e18;

    if(dp[id][dig][sp][al]!=-1)
        return dp[id][dig][sp][al];

    int ans=1e18;
    for(int i=0;i<m;i++)
    {
        int moves=min(i,m-i);
        if(isdigit(arr[id][i]))
            ans=min(ans,moves+build(id-1,m,0,sp,al));
        else if(isspecial(arr[id][i]))
            ans=min(ans,moves+build(id-1,m,dig,0,al));
        else
            ans=min(ans,moves+build(id-1,m,dig,sp,0));
    }
    return dp[id][dig][sp][al]=ans;
}

int32_t main()
{
    fast;
    int n,m;cin>>n>>m;
    for(int i=1;i<=n;i++)
        cin>>arr[i];

    MSET(dp,-1);
    bool dig=true,sp=true,al=true;
    cout<<build(n,m,dig,sp,al);

}