#include<bits/stdc++.h>

using namespace std;

#define pb push_back
const int maxi = 1e6+10;
int a[maxi];
string s;
vector<int> v[maxi];
int n;

void solve()
{
    cin>>n;
    if (n==1){
        printf("-1\n");
        return;
    }

    string s = "";

    s = "2";

    for (int i = 0; i<n-1;i++)
        s+='3';

    cout<<s<<endl;
}

int main()
{
    int  t;
    cin >>t;
    while(t--)
        solve();

    return 0;
}
