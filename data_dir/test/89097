#include<bits/stdc++.h>
using namespace std;

#define all(x) begin(x), end(x)
#define sz(x) (ll)(x).size()

using ll = long long;
using ld = long double;

constexpr ll inf = 0x3FFFFFFFFFFFFFFF;


int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    vector<ll> aaa = {0, 1, 1, 2, 2};
    ll t;
    cin >> t;
    for (ll i = 0; i < t; i++) {
        ll a, b;
        cin >> a >> b;
        ll d = abs(a - b);
        cout << d / 5 + (d%5+1)/2 << endl;
    }
}
