#define eps 1e-9
#define pi acos(-1.0)
#define o 1000000007
#define ll long long int
#define forn(i, n)  for (i=1; i<=(n); i++) 
#define pb push_back
#define pf push_front
#define mp make_pair
#define pii pair<int, int> 
#define sz size()
#define fors(w, s) for (set <ll> :: iterator w=s.begin(); w!=s.end(); w++)
#define sqr(a) a*a
#define sqrt(a) pow(a, 0.5)
/*#define x first
#define y second*/

int n, k, q, l, d, ans, p, h, j, i;
string s, a, b;

int main()
{
    //freopen ("input.txt","r",stdin); freopen ("output.txt","w",stdout);
    //freopen ("cross.in","r",stdin); freopen ("cross.out","w",stdout);
	getline (cin, a);
	getline (cin, b);
	if (a.sz!=b.sz)
	{
		printf ("NO");
		return 0;
	}
	if (a.sz==1 & a[0]!=b[0])
	{
		printf ("NO");
		return 0;
	}
	for (j=0; j<a.sz; j++)
	{
		if (a[j]=='1')
		{
			break;
		}
	}
	for (i=0; i<b.sz; i++)
	{
		if (b[i]=='1')
		{
			break;
		}
	}
	if (j<a.sz^i<b.sz)
	{
		printf ("NO");
		return 0;
	}
	printf ("YES");
    return 0;
}