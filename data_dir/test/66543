#include <bits/stdc++.h>
#define re return
#define ll long long
#define ull unsigned ll
#define ld long double
#define f first
#define s second
#define pi acos(-1)
#define oo (ll)1e9+5
#define OO 1e18
#define EPS 1e-6
#define FX(n) fixed<<setprecision(n)
#define mm(o,k) memset(o,k,sizeof o)
#define IO ios_base::sync_with_stdio(0),cin.tie(0),cout.tie(0);
using namespace std;
const int maxn=200005;

int main()
{
    IO;
    int q;
    cin >> q;
    while(q--){
        ll l,r,x;
        cin >> l >> r >> x;
        if(l>x){cout<<x<<endl;continue;}
        ll tmp=(r+1)/x;
        if(tmp*x>r)cout<<tmp*x<<endl;
        else cout<<(tmp+1)*x<<endl;
    }
    re 0;
}