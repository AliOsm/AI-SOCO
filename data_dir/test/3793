#include <iostream>
using namespace std;
int main()
{
	int n, x, y, a = 0, b = 0;
	cin >> n;
	for (int i = 0; i < n; ++i){
		cin >> x >> y;
		a += x > y ? 1 : 0;
		b += y > x ? 1 : 0;
	}
	if (a == b){
		cout << "Friendship is magic!^^";
	}
	if (a > b){
		cout << "Mishka";
	}
	if (b > a){
		cout << "Chris";
	}
	return 0;
}