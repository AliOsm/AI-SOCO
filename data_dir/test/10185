#include<bits/stdc++.h>
using namespace std;
typedef int ll;

template <typename T>
T readint() {
	T ans=0;
	char c;
	while((c=getchar()) && !(c>='0' && c<='9'));
	do {
		ans=ans*10+c-'0';
	} while((c=getchar()) && (c>='0' && c<='9'));
	
	return ans;
}

int n;
char t[1000001];

ll a,b;
ll suf[1000001];

ll hatv[1000001];

void calc(ll mod) {
	hatv[0]=1;
	for(int i=1;i<1000001;i++) {
		hatv[i]=(hatv[i-1]*10)%mod;
	}
}

int main() {
	int i=0;
	char c;
	while((c=getchar()) && c!='\n') {
		t[i++]=c;
	}
	
	n=strlen(t);
	a=readint<ll>();b=readint<ll>();

	calc(b);
		
	suf[n-1]=(t[n-1]-'0')%b;
	for(int i=n-2;i>=0;i--) {
		suf[i]=(suf[i+1]+hatv[n-1-i]*(t[i]-'0'))%b;
	}
	
	ll eddig=(t[0]-'0')%a;
	for(int i=1;i<n;++i) {
		if(eddig==0 && suf[i]==0 && t[i]!='0') {
			puts("YES");
			char tmp=t[i];
			t[i]='\0';
			puts(t);
			t[i]=tmp;
			puts(t+i);
			
			return 0;
		}
		
		eddig=(eddig*10+t[i]-'0')%a;
	}
	
	puts("NO");
	
	return 0;
	
}
