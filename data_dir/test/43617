#include <cstdio>

int main()
{
  int n;
  scanf("%d", &n);

  int sumX, sumY, sumZ;
  sumX = sumY = sumZ = 0;
  for(int i = 0; i < n; i++)
  {
    int x,y,z;
    scanf("%d%d%d", &x, &y, &z);
    sumX += x;
    sumY += y;
    sumZ += z;
  }
  
  puts((sumX == 0 && sumY == 0 && sumZ == 0) ? "YES" : "NO");

  return 0;
}