#include<bits/stdc++.h>
using namespace std;
int main(){
string s;
cin>>s;
s=s+'1';
if (s.length()<6){cout<<"NO";return 0;}
if (s=="oohell1"){cout<<"NO";return 0;}
while (s[0]!='h'){
    s.erase(0,1);
    if (s[0]=='1'){cout<<"NO";return 0;}
}
while (s[1]!='e'){
    s.erase(1,1);
    if (s[1]=='1'){cout<<"NO";return 0;}
}
while (s[2]!='l'){
    s.erase(2,1);
    if (s[2]=='1'){cout<<"NO";return 0;}
}
while (s[3]!='l'){
    s.erase(3,1);
    if (s[3]=='1'){cout<<"NO";return 0;}
}
while (s[4]!='o'){
    s.erase(4,1);
    if (s[4]=='1' ){cout<<"NO";return 0;}
}
if (s[0]=='h' && s[1]=='e' && s[2]=='l' && s[3]=='l' && s[4]=='o' ) {cout<<"YES"; return 0;}
cout<<"NO";
return 0;
}