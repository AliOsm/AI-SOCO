#include<bits/stdc++.h>
using namespace std;

int solve()
{
    string s,x;
    cin>>s;
    for(int i=0;i<5;++i)
    {
        cin>>x;
        if(x[0]==s[0] || x[1]==s[1])
            return cout<<"YES",0;
    }
    cout<<"NO";
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    solve();
}