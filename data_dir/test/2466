#include <bits/stdc++.h>

using namespace std;

#define pb push_back
#define ll long long
#define mp make_pair

int main()
{
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    int n,a[10005];
    cin>>n;
    for (int i=0;i<n;i++)
    cin>>a[i];
    sort(a,a+n);
    int k=n/2;
    if (!(n%2)) cout<<a[k-1];
    else cout<<a[k];
    return 0;
}