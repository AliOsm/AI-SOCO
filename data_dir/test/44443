#include <bits/stdc++.h>
using namespace std;

using ll = long long;

int main() {
    #ifdef LU_SERIOUS
        freopen( "in.txt", "r", stdin );
    #endif
    string s;
    int a, b, n;
    while( cin >> n >> a >> b >> s ) {
        --a; --b;
        if( s[a] != s[b] ) cout << "1\n";
        else cout << "0\n";
    }
    return 0;
}

