#include <bits/stdc++.h>

using namespace std;

int main ()
{
    ios_base::sync_with_stdio(false);
    int n; cin >> n;
    int c=0;
    for(int i=1;i<=n*3;i++) {
        c+=(i*(i+1)/2);
        if(c>n) {
            cout << i-1 << endl;
            return 0;
        }
    }
    return 0;
}
