#include <iostream>

using namespace std;

int main()
{
    ios_base::sync_with_stdio(0);
    int l1, l2;
    string a, b;
    cin >> a >> b;
    l1 = a.length();
    l2 = b.length();
    if (l1 != l2) cout << max(l1, l2);
    else
    {
        if (a == b) cout << -1;
        else cout << l1;
    }
    return 0;
}