#include <bits/stdc++.h>
#define mp make_pair
#define pb push_back
#define f first
#define s second
#define all(x) x.begin(), x.end()
using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
typedef pair<ll,ll> pll;
const int sz=1e2+9;
int n,a;
int f[sz];
int ans;

int main(){
	cin>>n;
	while(n--){
		cin>>a;
		f[a]++;
		ans=max(ans,f[a]);
	}	
	cout<<ans;
    return 0;
}