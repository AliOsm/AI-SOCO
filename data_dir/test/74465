#include<bits/stdc++.h>
 using namespace std;
 int main()
 {
    /*freopen("input.txt","r",stdin);
    freopen("output.txt","w",stdout);*/
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    string a;
    cin >> a;
    int cnt=0;
    for(int i=122;i>=97;i--)
    {
        for(int j=cnt;j<a.size();j++)
        {
           if(a[j]==(char)i)
           {
              cnt=j;
              cout << a[j];
           }
        }
    }
    return 0;
 }

