	static void solve(std::istream& in, std::ostream& out) {
		std::function<int64(int64, int64, int64)> interp = [&] (int64 one_delta, int64 times, int64 maximum) -> int64 {
			int64 hit = one_delta + times - 1;
			if (hit <= maximum) {
				return (hit + 1) * hit / 2 - (one_delta - 1) * one_delta / 2;
			} else {
				int64 actual_hit = maximum - one_delta + 1;
				int64 inverse = times - actual_hit;
				assert(inverse >= 0);
				return interp(one_delta, actual_hit, maximum) + inverse * maximum;
			}
		};

		int64 a, b, c, d; in >> a >> b >> c >> d;
		int64 res = 0;
		for (int64 i = a; i <= b; i++) {
			int64 j = max(b, c - i);
			int64 delta = interp(max(min(i + j, d + 1) - c, 0LL), c - j + 1, d - c + 1);
			res += delta;
		}
		out << res << endl;
	}
};


