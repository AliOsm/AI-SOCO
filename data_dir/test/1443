/* In The Name Of God */
#include <bits/stdc++.h>

# define xx first
# define yy second
# define pb push_back
# define pp pop_back
# define eps 1e-9

using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
typedef vector<int> vint;
set<double> s;
int main(){
	ios_base::sync_with_stdio (0);
	int t;cin>>t;
	for(int i=3 ; i<1000 ; i++)
		s.insert((i-2)*180.00 / i);
	while(t--){
		int tmp;cin>>tmp;
		if(s.find((double)tmp) != s.end())
			cout<<"YES\n";
		else
			cout<<"NO\n";
	}
	return 0;
}

