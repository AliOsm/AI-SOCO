#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
#define ss second
#define ff first
#define pb push_back
#define mp make_pair

int main() {
    ll n;
    cin>>n;
    map<ll,int> m;
    int sum=0;
    while(1)
    {
        m[n]++;
        if(m[n]==2)
            break;
        sum++;
        n++;
        while(n%10==0&&n>0)
            n/=10;


    }
    cout<<sum;
	return 0;
}
