#include <bits/stdc++.h>
using namespace std;
int main()
{
    int a, b, c;
    cin>>a>>b>>c;
    int ans = 999999999;
    for (int i=1;i<=100;++i)
    {
        if (abs(i-a) + abs(i-b) + abs(i-c) < ans)
        {
            ans = abs(i-a) + abs(i-b) + abs(i-c);
        }
    }
    cout<<ans<<'\n';
    return 0;
}
