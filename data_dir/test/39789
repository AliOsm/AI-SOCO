#include <bits/stdc++.h>
using namespace std;

#define ll long long
const long long N=1e5+5;
#define fast ios_base::sync_with_stdio(0);cin.tie(NULL);cout.tie(NULL) 
int main() {
       fast;
     ll n;
     cin>>n;
     ll a[n];
     for(int i=0;i<n;i++)
     cin>>a[i];
     sort(a,a+n);
     ll l=(n-1)/2;
     cout<<a[l];
	return 0;
}