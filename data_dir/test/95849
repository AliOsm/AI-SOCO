#include <bits/stdc++.h>
using namespace std;
int n,q,r;
string s="ROYGBIV",ss="GBIVGBI";

int main()
{
    //freopen("in.txt","r",stdin);
    //freopen("out.txt","w",stdout);
    cin>>n;
    q=n/7; r=n%7;
    while(q--) cout<<s;
    int st=0;
    while(r--){
        cout<<ss[st];
        st++;
    }
    return 0;
}
