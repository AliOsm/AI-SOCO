#include <bits/stdc++.h>
#define ToyotaLandCruiser100 ios_base::sync_with_stdio; cin.tie(0); cout.tie(0);
#define tormoz return 0;

using namespace std;

typedef long long ll;
typedef long double ld;

const int N = 1e5 + 7;
const ld pi = 3.14159;
const ll INF = 1e9 + 1;

string s;

int main(){
    getline(cin, s);
    for(int i = 0; i < s.size(); i++){
        if(isupper(s[i])){
            s[i] = tolower(s[i]);
        }
    }
    if(s.size() == 2){
        if(s[0] == 'a' || s[0] == 'e' || s[0] == 'i' || s[0] == 'o' || s[0] == 'u' || s[0] == 'y'){
            cout << "YES";
            return 0;
        }
        else{
            cout << "NO";
        }
        return 0;
    }
    for(int i = s.size() - 2; i > 0; i--){
        if(s[i] != ' '){
        if(s[i] == 'a' || s[i] == 'e' || s[i] == 'i' || s[i] == 'o' || s[i] == 'u' || s[i] == 'y'){
            cout << "YES";
            return 0;
        }
        else{
            cout << "NO";
            return 0;
        }
        }
    }
}
