#include <bits/stdc++.h>
#pragma GCC optimize("O3")

using namespace std;

typedef long long ll;

#define f first
#define s second
#define pb push_back

ll n, x, y, sumx, sumy;


int main()
{
  ios_base::sync_with_stdio(false);
  cin.tie(0); cout.tie(0);
  cin >> n;
  for (int i = 1; i <= 2 * n; i++) {
    cin >> x >> y;
    sumx += x;
    sumy += y;
  }
  cout << sumx / n << " " << sumy / n<< "\n";
  return 0;
}
