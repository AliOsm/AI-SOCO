    forn(akf,0,t) {
        int n,r;
        scanf("%d %d", &n,&r);
        int sum = 0;
        forn(i,0,n-1) {
            int x;
            scanf("%d", &x);
            sum+=x;
        }
        sum = sum%n;
        sum = (n+r-sum) % n;
        if(sum==0) sum=n;
        printf("%d\n", sum);
    }
