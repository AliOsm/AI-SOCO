#include <bits/stdc++.h>

using namespace std;

int main()
{
    string s , t ;
    cin >> s >> t ;
    int l1 = s.size() , l2 = t.size() , f1 = s.find("1") , f2 = t.find("1") ;
    if (l2 != l1) cout <<"NO";
    else if ( s == t ) cout << "YES";
    else
    {
        if (f1 == -1 || f2 == -1 ) cout << "NO" ;
        else cout << "YES";
    }
}
