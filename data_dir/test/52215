#include <bits/stdc++.h>
using namespace std;
typedef long long ll;


int main()
{
    ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
    ll n;
    cin >> n;
    vector<int> ans = {0, 4, 10, 20, 35, 56, 83, 116, 155, 198, 244, 292};
    if(n<12)cout<<ans[n];
    else cout << 292 + 49 * (n-11);

}
