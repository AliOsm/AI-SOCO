#include <bits/stdc++.h>
using namespace std;
long long i,j,n,h,g,c[112345],d[3][15555],l,r,ll,s,mm,t,k,b[1150555],m,xh,x,xa,xr,y,yr,yh,yd,z;
long long a[1223456];
string p,w,q;
int main() {
    cin >> n;
    for (i = 0; i <n; i ++){
        cin >> p;
        sort (p.begin (), p.end());
        t = 0;
        for (j = 1; j < p.size(); j ++){
            if (p[j-1] + 1 != p[j])
                t = 1;
        }
    if (t == 0)
        cout << "Yes";
    else cout << "No"; 
    cout <<endl;}
    return 0;
}  

