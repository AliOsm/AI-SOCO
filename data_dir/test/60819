#include <iostream>
#include <algorithm>
#include <cmath>
#include <vector>
#include <map>
#include <set>
#include <stdint.h>
#include <string>
#include <initializer_list>
#include <functional>
#include <future>
using namespace std;

int main(void)
{
	int n;
	cin >> n;
	set<int> q;
	int m;
	cin >> m;
	for (int i = 0; i < m; ++i)
	{
		int t;
		cin >> t;
		q.insert(t);
	}
	cin >> m;
	for (int i = 0; i < m; ++i)
	{
		int t;
		cin >> t;
		q.insert(t);
	}

	if (q.size() == n)
	{
		cout << "I become the guy.";
	}
	else
	{
		cout << "Oh, my keyboard!";
	}
	return 0;
}