/*****Enigma_27*****/
#include<bits/stdc++.h>
#define ll          long long
#define pb          push_back
#define mp          make_pair
#define pll         pair<ll,ll>
#define vll          vector<ll>
#define all(a)      (a).begin(),(a).end()
#define f           first
#define s           second
#define sz(x)       (int)x.size()
#define hell        1000000007
#define lbnd        lower_bound
#define ubnd        upper_bound
#define bs          binary_search
#define ios        	ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
using namespace std;
ll n,i,j,k,c,d,l=0,alice=0,bob=0,flag=1,m,q,a[200005],b[200],r;
int main()
{   ios
    map<string ,ll> mm;
    cin>>n;
    for(i=0;i<n;i++)
    {
        string s;
        cin>>s;
        if(mm[s]==1) cout<<"YES\n";
        else cout<<"NO\n";
        mm[s]=1;
    }

	return 0;
}
