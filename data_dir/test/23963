#include <iostream>
#include <fstream>

#include <bits/stdc++.h>

using namespace std;

class AFeedTheCat {
public:
    void solve(std::istream& in, std::ostream& out) {
        int hh, mm; in >> hh >> mm;
        int H, D, C, N;
        in >> H >> D >> C >> N;

        double cost = (H + N - 1) / N * C;

        if (hh >= 20) {
            cost *= .8;
        } else {
            int rem = 60 * (19 - hh) + 60 - mm;
            double x = ((rem * D + H + N - 1) / N) * C * .8;
            cost = min(cost, x);
        }

        out.precision(8);
        out << fixed << cost << endl;
    }
};


int main() {
	AFeedTheCat solver;
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
	return 0;
}
