#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

int main()
{
	ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
	int t;
	cin >> t;
	while(t--)
	{
		int a, b;
		cin >> a >> b;
		if(a == b)
			cout << 0 << '\n';
		else if(a < b)
		{
			if((b - a) % 2 == 1)
				cout << 1 << '\n';
			else
				cout << 2 << '\n';
		}
		else
		{
			if((a - b) % 2 == 0)
				cout << 1 << '\n';
			else
				cout << 2 << '\n';
		}
	}
	return 0;
}
