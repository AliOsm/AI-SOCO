//g++ -std=c++11 ./SC.cpp -o ./p
#include <bits/stdc++.h>
using namespace std;

#define pb push_back
#define ff first
#define ss second
#define rz resize
#define len length
#define nl '\n'

typedef long long ll;
typedef pair<int,int> pii;

int main(){
  ios::sync_with_stdio(0);
  cin.tie(0),cout.tie(0);

  int i,j,a,x,y;
  for(i=1;i<=5;i++)
    for(j=1;j<=5;j++){
      cin>>a;
      if(a==1)
	x=i,y=j;
    }

  cout<<(abs(x-3)+abs(y-3)) << nl;

  return 0;
};
