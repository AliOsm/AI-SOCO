///******************************************START******************************************
unsigned long long ar[N+5][N+5];
unsigned ll getRand(){

    unsigned ll res = 1ULL;
    for(int i = 1;i<=5;i++) {
        unsigned ll val =rand();
        val++;
        if(res*val!=0)
            res=res*val;
    }
    return res;
}
map<unsigned ll ,int> mp;
#define ull unsigned ll
unsigned ll tree[N+5][N+5];
int n,m;
void update(int x,int y,ull val) {
    for(int i =x;i<=n;i+=i&-i ) {
        for(int j = y;j<=m;j+=j&-j) {
            tree[i][j]^=val;
        }
    }
}
ull query(int x,int y) {
    ull sum = 0;
    for(int i = x;i>0;i-=i&-i) {
        for(int j = y;j>0;j-=j&-j) {
            sum^=tree[i][j];
        }
    }
    return sum;
}
int main(){
    #ifdef sayed
    //freopen("out.txt","w",stdout);
    // freopen("in.txt","r",stdin);
    #endif
    //ios_base::sync_with_stdio(false);
    //cin.tie(0);
    srand(time(0));
    for(int i = 1;i<N;i++) {
        for(int j = 1;j<N;j++) {
            ar[i][j]=getRand();
        }
    }

    n = nxt();
    m =nxt();
    int q= nxt();
    while(q--) {

        int t = nxt();
        if(t<3) {
            int x1 = nxt();
            int y1= nxt();
            int x2 = nxt();
            int y2 = nxt();
            ull val = ar[x1][y1]+ar[x2][y2];
            update(x1,y1,val);
            update(x2+1,y1,val);
            update(x1,y2+1,val);
            update(x2+1,y2+1,val);

        } else {
            int x1 = nxt();
            int y1= nxt();
            int x2 = nxt();
            int y2 = nxt();
            puts(query(x1,y1)==query(x2,y2)?"Yes":"No");

        }
    }
    return 0;
}