//ios::sync_with_stdio(false);
#include <bits/stdc++.h>
#define pb push_back
#define adelkhah main
#define left(x) ((x)*2)
#define right(x) (((x)*2)+1)
//#define int long long
using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
int32_t adelkhah()
{
    int k; cin >> k; string s; cin >> s;
    int sum = 0;
    for(int i = 0; i < s.size(); i++){
        sum += s[i] - '0';
    }
    sort(s.begin(),s.end());
    int ans = 0;
    for(int i = 0; sum < k; i++){
        sum += 9 - (s[i]-'0'); ans++;
    }
    cout << ans;

}
