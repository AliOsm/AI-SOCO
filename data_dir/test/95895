const ll N = 1e6 + 1;

ll n , m , l , cnt , ans , k , x , y , mn = N , mx ;
string s , t;
vector < ll > v , d;
map < ll , ll > mp;
pair < ll , ll > p [ N ];
bool b = false;
set < ll > st;
int main()
{
T
    cin >> n ;
    x = 1;
    for ( ll i = n * n ; i >= 1 ; i -- )
    {
        if ( i > x )
        cout << i << " " << x << " ";
        x ++;
    }
}











