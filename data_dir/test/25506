/*long long gcd(long long a, long long b)
{
if (b == 0)return a; return gcd(b, a%b);
}
long long lcm(long long a, long long b) {
return  a * b / gcd(a, b);
}
*/
vector<int>arr[30];
int main(){
	
	string s1, s2;
	cin >> s1 >> s2;
	int o = s2.size();
	int cnt = 0, ans=0, idx = -1;
	for (int i = 0; i < s1.size(); ++i)arr[s1[i] - 'a'].push_back(i);
	for (int i = 0; i < s2.size(); ++i) {
		if (arr[s2[i] - 'a'].size() == 0)return puts("-1");
	}

	while (cnt < o) {
		int l = 0, r = arr[s2[cnt] - 'a'].size()-1,mid;
		while (l < r) {
			mid = l + (r - l) / 2;
			if (arr[s2[cnt] - 'a'][mid] > idx)r = mid;
			else l = mid + 1;
		}
		if (l == (arr[s2[cnt] - 'a'].size() - 1) && arr[s2[cnt] - 'a'][l] <= idx) {
			++ans;
			idx = arr[s2[cnt] - 'a'][0];
		}
		else idx = arr[s2[cnt] - 'a'][l];
		++cnt;
	}
	pr(ans+1);
}
