#include <cstdio>
#include <iostream>
#include <algorithm>
#include <vector>
#include <set>
using namespace std;

typedef long long ll;

#define rep(i, n) for (int i = 0, _##i = (n); i < _##i; ++i)
#define repi(i, a) for (__typeof((a).begin()) i = (a).begin(), _##i=(a).end(); i != _##i; ++i)

int main() {
//  freopen("a.in","r",stdin); freopen("a.out","w",stdout);
    
    set<int> s;
    rep (i, 4) {
        int a;
        cin >> a;
        s.insert(a);
    }
    cout << 4 - s.size();
}
