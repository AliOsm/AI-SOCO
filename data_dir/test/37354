#include <bits/stdc++.h>
using namespace std;

set<int, greater<int> > st;
int n;
map<int, bool> mp;

int check(int num) {
	int mn = num;
	while(num / 2) {
		num /= 2;
		if(!mp[num]) {
			mp[num] = true;
			mp[mn] = false;
			return num;
		}
	}
	return mn;
}

int main (int argc, char *argv[])
{
	scanf("%d", &n);
	for(int i = 0; i < n; ++i) {
		int y;
		scanf("%d", &y);
		mp[y] = true;
		st.insert(y);
	}
	while(st.size()) {
		int y = *st.begin(),
			yy = check(y);
		st.erase(st.begin());
		if(y == yy) {
			printf("%d ", y);
		} else {
			st.insert(yy);
		}
	}
	
	return 0;
}

