#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define int long long
#define all(sdk) sdk.begin(),sdk.end()
#define mll map<int, int>
#define pb push_back
#define rep(i,a,b) for(int i=a;i<b;++i)
#define repd(i,a,b) for(int i=a;i>=b;--i)
#define mp make_pair
#define hell 1000000007
#define endl '\n'
#define vvll vector<vector<int> >
#define vll vector<int>
#define mint map<int,int> 
#define sz(x) (int)x.size()
#define sll set<int>
#define pll pair<int,int>
#define F first
#define S second
const long double PI = acos(-1.0);

int gcd(int a,int b){
    while(b>0){
        int t=a%b;
        a=b;
        b=t;
    }
    return a;
}

set<int> fac;
void solve(){  
    int n;
    cin>>n;
    for(int i=1;i*i<=n;++i){
        if(n%i==0){
            fac.insert(i);
            fac.insert(n/i);
        }
    }  
    fac.erase(1);
    int g=0;
    if(n==1){
        cout<<1<<endl;
        return;
    }
    for(int i:fac){
        g=gcd(i,g);
    }
    cout<<g<<endl;
}

signed main(){
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int t=1;
    // cin>>t;
    while(t--){
        solve();
    }
    return 0;
}