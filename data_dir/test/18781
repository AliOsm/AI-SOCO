#include<bits/stdc++.h>
using namespace std;

#define timesaver ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0)
#define ll long long int
#define nl "\n"
#define rep(i,a,b) for(int i=a;i<=b;i++)

const ll N=4e6+7;
set<ll> st;

int main(){
    timesaver;
    ll n=0,m=0,x=0,l=0,c=0;
    cin>>n;
    l=n-((n+1)/2);
    x=n;l=1;
    while(x){
        if(x<4){
            rep(i,1,x-1)cout<<l<<" ";
                cout<<x*l;break;}
        else
        rep(i,1,(x+1)/2)cout<<l<<" ";
        x/=2;
        l*=2;
    }



}
