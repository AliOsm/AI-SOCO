#include<bits/stdc++.h>
using namespace std;
#define ll long long
ll i,x,j,y,c[1000000],d[1000000],a,p,s,b;	
int main(){
	string s,s1;
	cin>>s>>s1;
	sort(s1.begin(),s1.end());
	reverse(s1.begin(),s1.end());
	for(i=0;i<s.size();i++){
		if(s[i]<s1[0]){
			cout<<s1[0];s1.erase(s1.begin());
		}
		else{
			cout<<s[i];
		}
	}
}