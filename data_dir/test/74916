#include<bits/stdc++.h>
#define ll long long
#define OO 2e18
#define oo 1e9
#define yalla ios::sync_with_stdio(0),cin.tie(0),cout.tie(0);
#define FILES freopen("input.txt", "r", stdin); freopen("output.txt", "w", stdout);
#define sz  100005
#define re return
#define mod  1000000007
#define pi acos(-1)
using namespace std;
int x1,y1,x2,y2,x,y;
int main()
{yalla;
cin>>x1>>y1>>x2>>y2;
cin>>x>>y;
int fi=abs(x1-x2);
int se=abs(y1-y2);
int th=fi/x;
int fo=se/y;
if(fi%x==0 &&se%y==0&& abs(th-fo)%2==0 )
 cout<<"YES"<<endl;
else
 cout<<"NO"<<endl;

 re 0;
}