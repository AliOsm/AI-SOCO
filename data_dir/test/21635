#include <vector>
#include <string>
#include <iostream>
#include <algorithm>
#include <cmath>
#include <cstdio>
#include <cstdlib>
#include <fstream>
#include <map>
#include <set>
#include <memory.h>
using namespace std;
#define sz(x) int((x).size())
#define FOR(i,a,b) for(ll (i) = (a); (i) <= (b); ++(i))
#define ROF(i,a,b) for(int (i) = (a); (i) >= (b); --(i))
#define rep(i,n) for (int (i) = 0; (i) < (n); ++(i))
#define fe(i,a) for (int (i) = 0; (i) < int((a).size()); ++(i))
#define C(a) memset((a),0,sizeof(a))
#define inf 1000000000
#define pb push_back
#define ppb pop_back
#define all(c) (c).begin(), (c).end()
#define pi 2 * acos(0.0)
#define sqr(a) (a)*(a)
#define mp(a,b) make_pair((a), (b))
#define X first
#define Y second
#define N 100005
#define mod 1000000009
//freopen("input.txt","r",stdin);
//freopen("output.txt","w",stdout);
using namespace std;
typedef vector<int> vint;
typedef long long ll;
typedef pair<int, int> pii;
using namespace std;
int n;
int a[200];
int gcd(int a, int b){
    return b ? gcd(b, a % b) : a;
}
int main(){
    cin>>n;
    rep(i, n) scanf("%d",&a[i]);
    int res = a[0];
    FOR(i, 1, n - 1){
        res = gcd(res,a[i]);
    }
    sort(a, a + n);
    int ans = a[n - 1] / res - n;
    if(ans % 2) cout << "Alice";
    else cout<<"Bob";
}
