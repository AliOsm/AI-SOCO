#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;
int main(){
    int t;
    cin>>t;
    while(t--){
        int  n;
        cin>>n;
        ll x=__gcd(n,180);
        x=180/x;
        ll y=(x-2)*180;
        y=y/x;
        if(y<n)cout<<2*x<<endl;
        else cout<<x<<endl;
        
    }
    return 0;
    
}