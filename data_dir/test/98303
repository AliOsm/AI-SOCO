#include <bits/stdc++.h>

#define ll long long
#define pb push_back
#define pii pair<int,int>
#define pll pair<ll,ll>
#define mp make_pair
#define ff first
#define ss second

using namespace std;

const double pi=acos(-1);
const int N=(int)1e6+7,mod=1000000007,M=2e9;

//scanf("%d",&);
int T,n,ans;

int main()
{
//    freopen(".txt","r",stdin);
    cin>>T;
    while(T--){
        cin>>n;
        ans=1;
        while(n){
            if(n%2)
                ans*=2;
            n/=2;
        }
        cout<<ans<<endl;
    }
    return 0;
}
