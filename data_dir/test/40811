#include <math.h>
#include <iostream>
#include <map>
#include <vector>
#include <set>
#include <cstring>
#include <algorithm>
#include <stack>
#include <queue>
#include <cstring>
#include <sstream>
#include <stdio.h>
using namespace std;
const int N=101;
int n;
int a,b;
int r,c;
int rr[N],cc[N];
int main(){
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        scanf("%d %d",&a,&b);
        if(!rr[a]){r++;rr[a]=1;}
        if(!cc[b]){cc[b]=1;c++;}
    
    }
    printf("%d\n",min(r,c));
   
    return 0;
}
