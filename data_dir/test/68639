#define use_fast ios::sync_with_stdio(false), cin.tie(0), cout.tie(0)
#define ret(n) return cout << n, 0
#define se(n) cout << setprecision(n) << fixed
#define ll long long
#define pb push_back
using namespace std;

const ll N = 2e6, OO = 1e9, M = 1e9 + 7;
typedef pair <int, int> pii;
ll a[N];

int main() {
	use_fast;
	ll n, m, k;
	cin >> n >> m >> k;
	m = min(k, m);
	if(m >= n)
		cout << "Yes" << endl;
	else
		cout << "No" << endl;
	return 0;
}
