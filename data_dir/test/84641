bool as(P a,P b)
{
    //if(a.x!=b.x)
    //   return a.x>b.x;
    //return a.y<b.y;
    return a.x+a.y>b.x+b.y;

}

int main()
{
    scanf("%d",&a);
    for(int t=1;t<=a;t++)
        scanf("%d",&o[t]);
    sort(o+1,o+a+1);
    for(int t=1;t<=a/2;t++)
    {
        x+=(long long)(o[t]+o[a-t+1])*(o[t]+o[a-t+1]);
    }
    printf("%lld",x);
}



