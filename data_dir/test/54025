#include <bits/stdc++.h>
using namespace std;
const int N = 100;
int a[N], b[N];
int main() {
	ios_base::sync_with_stdio (false), cin.tie(0), cout.tie(0);
	int t = 1;
//	cin >> t;
	while (t--) {
		for (int i = 0; i < 8; i++) {
			string s;
			cin >> s;
			bool z = false;
			for (int i = 1; i < 8; i++) {
				if (s[i] == s[i - 1]) {
					cout << "NO";
					return 0;
				}
			}
		}
		cout << "YES";
	}
	return 0;
}
