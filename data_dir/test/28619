using namespace std;
#include <bits/stdc++.h>

typedef long long i64;
typedef unsigned long long ui64;

int main()
{ios_base::sync_with_stdio(0);
 i64 n;
 cin>>n;
 i64 i,a[n];
 for(i=0;i<n;i++)
  cin>>a[i];
  sort(a,a+n);
  i64 k=1;
  for(i=0;i<n;i++)
  {
  	if(a[i]>=k)
  	{
  		a[i]=k;
  		k++;
	  }
	  
  }
 
  cout<<k;
  
  


 return 0;
}
