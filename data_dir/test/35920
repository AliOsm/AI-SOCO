/*************************************************/
int n,m,i,j,cnt[105][26];
map<string,int> mp;
string st;
char ans[105];
int main()
{
	cin>>n; 
	printf("? 1 %d\n",n);fflush(stdout);
	rep(i,n*(n+1)/2){
		cin>>st;
		sort(st.begin(),st.end());
		mp[st]++;
	}
	if(n>1){
		printf("? 2 %d\n",n);fflush(stdout);
		rep(i,n*(n-1)/2){
			cin>>st;
			sort(st.begin(),st.end());
			mp[st]--;
		}
	}
	fe(it,mp){
		if(it->second!=0){
			for(i=0;i<it->first.size();i++) cnt[it->first.size()][it->first[i]-'a']++;
		}
	}
	repd(i,n){
		forup(j,0,25) if(cnt[i][j]-=cnt[i-1][j]) ans[i]=j+'a';
	}
	printf("! ");
	rep(i,n) printf("%c",ans[i]);fflush(stdout);
	return 0;
}