const int mod = 1000000007;
const int N = 100005;
int a[N];
int main(){
    int n;
    cin >> n;
    int q;
    rep(i, n - 1){
        scanf("%d",&q);
        a[q] = 1;
    }
    for(int i = 1; i <= n; ++i){
        if(a[i] == 0){
            cout << i << endl;
            return 0;
        }
    }
}
