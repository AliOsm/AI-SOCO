#include <bits/stdc++.h>
using namespace std;

typedef long long ll; 
typedef pair<int,int> ii;
typedef tuple<int, int, int> i3;

int main(){
    
    int n, a, b; cin >> n >> a >> b;
    a--; b--;
    string s; cin >> s;
    if(s[a] != s[b]) cout << 1 << endl;
    else cout << 0 << endl;
        
    return 0;
}