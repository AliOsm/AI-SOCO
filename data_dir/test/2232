#include <iostream>
#include <string>
#include <vector>
#define SZ(x) ((int)(x).size())
#define FOR(it, c) for(__typeof((c).begin()) it = (c).begin(); it != (c).end(); ++it)
using namespace std;
typedef long long LL;

int main(void) {
  int a, m;
  cin >> a >> m;
  for(int i=0;i<m;i++) a = (a + a)%m;
  puts(a? "No":"Yes");
  return 0;
}
