#include <cmath>

using namespace std;

#define LL long long

int main()
{
	int n, k;
	cin >> n >> k;
	int ans = n - (k - n * 2);
	if (ans <= 0)
		cout << 0 << endl;
	else
		cout << ans << endl;
	return 0;
}