#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
	ll q;
	cin >> q;
	while(q--)
	{
		ll l1, r1, l2, r2;
		cin >> l1 >> r1 >> l2 >> r2;
		ll a = min(l1, r1);
		ll b = max(l2, r2);
		ll c = max(l1, r1);
		ll d = min(l2, r2);
		if (a != b)
			cout << a << " " << b << endl;
		else
			cout << c << " " << d << endl;
	}
	
}