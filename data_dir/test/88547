#include <bits/stdc++.h>
using namespace std;

int main()
{
    string st0, st1;

    // freopen("D:\\1.in", "r", stdin);
    // freopen("D:\\1.out", "w", stdout);

    cin >> st0 >> st1;
    for(int i = 0; i < st0.length(); i++)
    if(st0[i] == st1[i]) cout << '0';
    else cout << '1';

	return 0;
}
