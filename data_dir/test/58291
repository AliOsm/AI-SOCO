#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define fr(i,j,k) for(int i=j;i<k;i++)
#define f(n) fr(i,0,n)
#define f1(n) fr(i,1,n+1)
#define ms(i) memset(i,0,sizeof(i));
#define pb push_back

int main()
{
	int n,k;
	while(cin >> n >> k){
        int now = 0;
        int hold = 0;
        int cont[n+5] = {};
        f(n){
            int add;
            cin >> add;
            hold += add;
            int g=min(hold,8);
            hold -=g;
            now += g;
            if(now >= k){
                cout<<i+1<<endl;
                exit(0);
            }
        }
        cout<<-1<<endl;
	}
}
