#include <bits/stdc++.h>
using namespace std;

string s;

int ans;

int main(){



    cin >> s;


    for (int i = 0;i < s.size();i++)
        if (s[i] == 'a' || s[i] == 'e' || s[i] == 'i' || s[i] == 'o' || s[i] == 'u') ans++;
        else if (s[i] == '1' || s[i] == '3' || s[i] == '5' || s[i] == '7' || s[i] == '9') ans++;


    cout << ans;



}
