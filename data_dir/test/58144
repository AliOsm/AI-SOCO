░░░░░▌▒▒▒▒▀▀▀▒▒▒▒▒▒▒▐*/
//++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
//++--++--++--++--++--++--++--++--++--+--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
const int MAXN = 200001;
const int inf = 1000000001;
const ld INF = 1.0 / 0.0;
const ld EPS = 1e-7;
const int mod = inf + 7;
const ld PI = acos(-1.0);
signed main()
{
	fastio();
	int n;
	cin >> n;
	vector<int> a(2 * n);
	if (n % 2)
	{
		int nw = 1;
		int cnt = 0;
		while (nw < n)
		{
			a[cnt] = a[n - cnt - 1] = nw;
			nw += 2;
			cnt++;
		}
		nw = 2;
		cnt = 0;
		while (nw < n)
		{
			a[n + cnt] = a[2 * n - 2 - cnt] = nw;
			nw += 2;
			cnt++;
		}
		for (int i = 0; i < 2* n; ++i) if (!a[i]) a[i] = n;
		vout(a, ' ');
	}
	else
	{
		int nw = 1;
		int cnt = 1;
		while (nw < n)
		{
			a[cnt - 1] = a[n - cnt] = nw;
			nw += 2;
			cnt++;
		}
		nw = 2;
		cnt = 0;
		while (nw < n)
		{
			a[n + cnt] = a[2 * n - 2 - cnt] = nw;
			nw += 2;
			cnt++;
		}
		for (int i = 0; i < 2 * n; ++i) if (!a[i])a[i] = n;
		vout(a, ' ');
		
	}
}
