#include <bits/stdc++.h>

using namespace std;

#define debug(s) cout<< #s <<" = "<< s <<endl
#define all(v) (v).begin(), (v).end()
#define mem(a,val) memset(a,val,sizeof a)

#define int long long
#define ff first
#define ss second
#define pb push_back
#define endl '\n'

int32_t main()
{
  ios_base::sync_with_stdio(false);cin.tie(NULL);
  /*#ifndef ONLINE_JUDGE
    freopen("in", "r", stdin);
    freopen("out","w",stdout);
  #endif*/
  int n;
  cin >> n;
  int s1 = 0,s2 = 0;
  for(int i = 0; i < 2*n; ++i){
    int a,b;
    cin >> a >> b;
    s1 += a;
    s2 += b;
  }
  cout << s1/n << " " << s2/n << endl;
}

