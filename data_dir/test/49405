using namespace __gnu_cxx;

typedef long long ll;
typedef unsigned long long ull;

void fastInOut();

int main() {
#ifndef ONLINE_JUDGE
  freopen("input.txt", "r", stdin);
#endif
  fastInOut();
  string s, t;
  cin >> s;
  for (int i = 0; i < 5; ++i) {
    cin >> t;
    if (t[0] == s[0] || t[1] == s[1])
      return cout << "YES", 0;
  }
  cout << "NO";
  return 0;
}

