#include <bits/stdc++.h>
using namespace std;

int main()
{
    int f[26]= {0};
    string s;
    cin>>s>>s;
    for(auto c:s)f[c-'a']++;
    for(int i=0; i<26; i++)if( f[i]>1 || s.length()==1 )
        {
            cout<<"Yes"<<endl;
            return 0;
        }

    cout<<"No"<<endl;
    return 0;
}
