#include <bits/stdc++.h>
#define pb push_back
#define mp make_pair
using namespace std;

int main()
{
	int n,x1,x2;
	bool ok=0;
	scanf("%d",&n);
	string s;
	while(n--)
	{
		cin >> s;
		scanf("%d%d",&x1,&x2);
		if(x1 >= 2400 && x2 > x1)
			ok=1;
	}
	if(ok)
		printf("YES\n");
	else
		printf("NO\n");
}
