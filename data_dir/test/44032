#include<iostream>
#include<stdio.h>
#include<algorithm>
using namespace std;
int a,s,d,f,g,h,j,k,l,i,n,m,ans;
main(){
scanf("%d:%d",&n,&m);
scanf("%d:%d",&f,&g);
if(m<g) {m+=60;f++;}
if(n<f) {n+=24;}
if(n-f<10) cout<<"0"<<n-f; else cout<<n-f;
cout<<":";
if(m-g<10) cout<<"0"<<m-g; else cout<<m-g;
}
