string s,t;
int pre=1;

int main(){
	while(getline(cin,s)){
		int cur=0;
		while(cur<s.sz && s[cur]==' ')cur++;
		if(cur<s.sz && s[cur]=='#'){
			if(t.sz || pre==0)cout<<t<<endl,t="";
			cout<<s<<endl;
			pre=1;
		}else{
			rep(i,s.sz)if(s[i]!=' ')t+=s[i];
			pre=0;
		}
	}
	if(t.sz || pre==0)cout<<t<<endl;
}
