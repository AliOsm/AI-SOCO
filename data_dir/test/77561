#include <bits/stdc++.h>
using namespace std;

#define F first
#define S second
typedef long long ll;
typedef long double ld;
typedef pair <int,int> ii;

const int N=100100;

int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int l,r;
        scanf("%d%d",&l,&r);
        printf("%d %d\n",l,2*l);
    }
}
