#include <bits/stdc++.h>
using namespace std;

template<typename T>
void out(T x) { cout << x << endl; exit(0); }
#define watch(x) cout << (#x) << " is " << (x) << endl





typedef long long ll;
const ll mod = 1e9+7;
const int maxn = 1e6 + 5;


ll solve() {
    ll a,b; cin>>a>>b;
    if (a%b==0) return 0;
    ll nxt = (a/b +1) * b;
    return nxt - a;
}


int main() {
    ios_base::sync_with_stdio(false); cin.tie(0);  cout.tie(0);

    int t; cin>>t;
    while (t--) {
	cout<<solve()<<"\n";
    }

    return 0;
}
