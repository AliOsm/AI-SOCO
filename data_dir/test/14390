#include<bits/stdc++.h>
#define P(x,y) make_pair(x,y)
using namespace std;
int main(){
    int n , r , ans=1 , tt;
    cin>>n>>r;
    tt = n;
    while(1){
        if(n%10 == r || n%10 == 0){
            cout<<ans<<endl;
            return 0;
        }
        ans++;
        n+=tt;
    }
    cout<<ans<<endl;
}
