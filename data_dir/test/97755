#include<bits/stdc++.h>
#define ll long long
#define f first
#define s second
#define pb push_back
using namespace std;

int main(){
    ios::sync_with_stdio(false);
    ll q;
    cin >> q;
    
    while(q--){
        ll n;
        cin >> n;
        if(n == 1){
            cout << "-1\n";
            continue;
        }
        cout<<2;
        for(int i=1; i<n; i++)cout << 3;
        cout << '\n';
    }
    
    
    
    
    return 0;
}
