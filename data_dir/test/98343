typedef long long ll;
typedef unsigned long long ull;
typedef long double ld;

const double PI=3.1415926535897932384626433832795;
const ll INF = 1LL<<62;
const ll MINF = -1LL<<62;

template<typename T> T getint() {
	T val=0;
	char c;
	
	bool neg=false;
	while((c=gc()) && !(c>='0' && c<='9')) {
		neg|=c=='-';
	}

	do {
		val=(val*10)+c-'0';
	} while((c=gc()) && (c>='0' && c<='9'));

	return val*(neg?-1:1);
}

//mt19937 rng(chrono::steady_clock::now().time_since_epoch().count()); uniform_int_distribution<int>(0, n-1)(rng)


int main() {
	IO;
	ll r,c;
	cin>>r>>c;
	if(r+c==2) {
		cout<<"0\n";
		return 0;
	}else {
		if(c==1) {
			for(int i=0;i<r;++i) {
				cout<<i+2<<"\n";
			}
		}else {
			ll curr=1;
			for(int j=0;j<r;++j) {
				for(int i=2;i<=c+1;++i) cout<<i*curr<<" ";
				cout<<"\n";
				if(j==0) curr+=c+1;
				else curr++;
			}
		}
	}
	return 0;
}
