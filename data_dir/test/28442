#include <bits/stdc++.h>
using namespace std;
typedef pair<int, int> pii;
typedef long long ll;
const int oo = 2e9;
const int MOD = 1000000007;
const int MX = 500010;

ll n , m;

int main() {
	cin >> n >> m;
	ll cur = 1;
	for(int i = 0 ;i < n ; ++i){
		cur *= 2;
		if(cur > m)break;
	}	
	cout << m%cur << endl;
	return 0;
}