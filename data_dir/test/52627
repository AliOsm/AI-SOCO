//besme taala
//ya_hossein
#include <bits/stdc++.h>
using namespace std;
int f = 1;
bool io(char x, char y)
{
	return x > y;
}
int main()
{
	string a;
	cin >> a >> a;
	sort(a.begin(), a.end(), io);
	for(int i = 0; a[i]; i++)
	{
		if(a[i] == '1' && f)
		{
			cout << 1;
			f = 0;
		}
		else if(a[i] == '0')
		cout << 0;
	}	
}