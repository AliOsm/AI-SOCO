#include <bits/stdc++.h>

using namespace std;
typedef long long ll;

int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    /*
    freopen("input.txt","r",stdin);
    freopen("output.txt","w",stdout);
    */

    int y,k,n;
    cin >> y >> k >> n;
    bool ans=0;
    for(int i=k;i<=n;i+=k){
        if(i>y){
            ans=1;
            cout << i-y << " ";
        }
    }
    if(!ans)
        cout << -1;
}
