#include <bits/stdc++.h>
using namespace std;
long long  i, j, n, c[123456],a[111234], b[1125678],d[123456], l,tt,ll,r, s, t, k, m,x,z;
string p[534567],q,qq;
set <long long> u,uu;
map <long long,long long> mm;
int main() {
    a[0]=1;
    for (i = 1; i <=40; i ++)
        a[i]=a[i-1]*2+1;
    cin >> tt;
    while (tt --){
        cin >> n;
        for(i = 1; i <40; i ++){
            if(n%a[i]==0)
                break;
        }
        cout <<n/a[i]<<endl;
    }
    return 0;
}