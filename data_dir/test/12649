#include<iostream>
#include <bits/stdc++.h>
#define ll long long
#define IO ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
using namespace std;
int t, n ;
int main()
{
    IO
    cin>>t;
    while(t--)
    {
        cin>>n;
        if(n==1)
            cout<<-1<<"\n";
        else
        {
            for(int i=1;i<n;i++)
                cout<<5;
            cout<<4;
            cout<<"\n";
        }
    }
}
