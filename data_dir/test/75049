//ะก670

#include <iostream>
#include <string>
#include <cmath>
#include <vector>
#include <algorithm>
#include <iterator>
#include <cstdlib>
#include <cstdio>
#include <map>
#include <set>
#include <queue>
#include <deque>
#include <stack>
#include <list>

using namespace std;

#define pii pair <int, int>

int size_n, size_m;
int result = 0;
map <int, int> used;
vector <pair <pii, int>> zn(0);
vector <pii> el(0);

bool cmp(pair <pii, int> a, pair <pii, int> b);

int main()
{
	int ii, jj;
	int tmp;
	map <int, int>::iterator it;

	cin >> size_n;
	
	for (ii = 0; ii < size_n; ii++)
	{
		cin >> tmp;

		used[tmp]++;
	}

	cin >> size_m;

	zn.resize(size_m);
	el.resize(size_m);

	for (ii = 0; ii < size_m; ii++)
	{
		cin >> el[ii].first;

		zn[ii].first.first = used[el[ii].first];
		zn[ii].second = ii;
	}

	for (ii = 0; ii < size_m; ii++)
	{
		cin >> el[ii].second;
		zn[ii].first.second = used[el[ii].second];
	}

	sort(zn.begin(), zn.end(), cmp);

	result = zn[0].second + 1;

	cout << result << endl;
	
	return 0;
}

bool cmp(pair <pii, int> a, pair <pii, int> b)
{
	if (a.first.first > b.first.first)
		return 1;
	else if (a.first.first < b.first.first)
		return 0;
	else if (a.first.second > b.first.second)
		return 1;
	else
		return 0;
}