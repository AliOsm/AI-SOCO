#include<bits/stdc++.h>
using namespace std;

int main()
{
    int i;
    char ch[101], s[101];

    cin >> ch;
    cin >> s;

    for(i = 0; i < strlen(ch); i++){
        if(ch[i] == s[i]){
            cout << "0";
        }
        else{
            cout << "1";
        }
    }

    cout << endl;

    return 0;
}
