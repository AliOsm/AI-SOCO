#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define all(x) x.begin(),x.end()

int const N = 200001;
int t, n, v[N];

int main(){
    #ifndef ONLINE_JUDGE
    freopen("read.txt","r",stdin);
    #endif
    scanf("%d", &t);
    while(t--){
        int h, m;
        scanf("%d%d", &h, &m);
        int h0 = 24 - h;
        int m0 = 60 - m;
        int an = (h0-1) * 60 + m0;
        printf("%d\n",an); 
    }
} 
