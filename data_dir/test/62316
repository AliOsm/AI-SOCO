#include <string>
#include <vector>
#include <iostream>
#include <algorithm>
#include <map>
using namespace std;

#define ll long long
const int INF = 1000000000;

#define fr(i,n) for(int i=0;i<n;i++)
#define fru(i,a,b) for(int i=a;i<=b;i++)

int a[5];
string s;
ll res = 0;

void Run() {
  fru(i,1,4)  cin>>a[i];
  cin>>s;  
  
  fr(i, s.length()) 
      res += a[s[i] - 48];
    
cout<<res;
}

int main() {
ios_base::sync_with_stdio(0);
    Run();
    return 0;
}