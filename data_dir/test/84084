#include <bits/stdc++.h>

using namespace std ;

int main ()
{
   int n , p , k , i ;
    cin >> n >> p >> k ;
    if ( p - k > 1  ) cout <<"<< ";
    for ( i = max (p-k , 1 ) ; i <= min (p+k , n ) ; i++ )
    {
        if ( i == p) cout <<"("<<i<<") ";
        else cout <<i<<" ";
    }
    if ( i <= n ) cout <<">>";
}