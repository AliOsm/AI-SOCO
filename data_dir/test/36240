#include <iostream>
#include <algorithm>
using namespace std;
int n,arr[105];
int main()
{
    cin >> n;
    for(int i=0 ; i<n ; i++)
    {
        cin >> arr[i];
    }
    sort(arr,arr+n);
    for(int i=0 ; i<n ; i++)
    {
        if(arr[i]!=arr[0])
        {
            cout << arr[i] << endl;
            return 0;
        }
    }
    cout << "NO" << endl;
}
