#include <bits/stdc++.h>

using namespace std;

int main() {
#ifndef ONLINE_JUDGE
  freopen("input.in", "r", stdin);
#endif
  int n;
  scanf("%d", &n);
  bool odd = false;
  for (int i = 0; i < n; ++i) {
    int a;
    scanf("%d", &a);
    odd |= a % 2;
  }
  puts(odd ? "First" : "Second");
}
