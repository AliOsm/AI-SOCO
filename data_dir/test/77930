#include <iostream>
#include <cstdio>
#include <cstdlib>
#include <algorithm>
#include <cmath>
#include <cstring>
using namespace std;
int T,n,m;
int main()
{
	scanf("%d",&T);
	while (T--)
	{
		scanf("%d%d",&n,&m);
		for (int i=1;i<=n;i++,printf("\n"))
		 for (int j=1;j<=m;j++) if (i==n && j==m) printf("W");else printf("B");
	}
return 0;
}