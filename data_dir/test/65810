#include <bits/stdc++.h>

using namespace std;
 
int main() {
   
    string s,x;
    cin>>s>>x;
    for(int i=0;i<x.size();i++)
    {
        s[i]=tolower(s[i]);
         x[i]=tolower(x[i]);
        
    }
    if(x==s)
    cout<<0;
    else if(x>s)
    cout<<-1;
    else
    cout<<1;
}