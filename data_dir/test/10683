  if(abs(xx1 - xx2) % x == 0 && abs(yy1 - yy2) % y == 0 && abs(xx1 - xx2) / x % 2 == abs(yy1 - yy2) / y % 2){
    cout << "YES";
  }

  else{
    cout << "NO";
  }
}
