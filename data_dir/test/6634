const int infi = 1e9 + 7;
const ll infl = 1e18;
const long double eps = 1e-7;
const long double eps1 = 1e-10;
const int mod = (1e9 + 7);
const ll mod1 = 1e9 + 7;
const ll mod2 = 1e9 + 7;
const ll p1 = 31;
const ll p2 = 37;
const ll p3 = 29;
const int ver1 = (62 * 62);

int ans[1010];

int main() {
   // freopen("input.txt", "r", stdin); freopen("output.txt", "w", stdout);
    //freopen("vase.in", "r", stdin); freopen("vase.out", "w", stdout);
    int n, a, b;
    cin >> n >> a >> b;
    for (int i = 0; i < a; ++i) {
        int q;
        cin >> q;
        ans[q] = 1;
    }
    for (int i = 0; i < b; ++i) {
        int q;
        cin >> q;
        ans[q] = 2;
    }
    for (int i = 1; i <= n; ++i) {
        cout << ans[i] << ' ';
    }

    return 0;
}