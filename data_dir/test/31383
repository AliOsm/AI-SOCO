#define PI 3.14159265358979
#define MEMS(a,b) memset(a,b,sizeof(a))
#define LL long long
#define U unsigned
int usedr[110];
int usedc[110];
int a[110][110];
int sumr[110];
int sumc[110];
int main()
{
#ifdef Fcdkbear
    freopen("in.txt","r",stdin);
    //freopen("out.txt","w",stdout);
    double beg=clock();

#endif

	LL x1,y1,x2,y2,x3,y3;
	cin>>x1>>y1>>x2>>y2>>x3>>y3;
	LL X1=x3-x1;
	LL X2=x2-x1;
	LL Y1=y3-y1;
	LL Y2=y2-y1;
	LL cp=X1*Y2-X2*Y1;
	if (cp>0)
		printf("RIGHT\n");
	if (cp==0)
		printf("TOWARDS\n");
	if (cp<0)
		printf("LEFT\n");
#ifdef Fcdkbear
    double end=clock();
    fprintf(stderr,"*** Total time = %.3lf ***\n",(end-beg)/CLOCKS_PER_SEC);
#endif
	return 0;
}