#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
   ll a,b,c,d,e,i,j,k,l,n,m,x,y,t,p;
   scanf("%lld",&t);
   for(p=1;p<=t;p++)
   {
       scanf("%lld",&n);
       x=y=0;
       for(i=0;i<n;i++)
       {
           scanf("%lld",&a);
           if(a%2==0)
           {
               x++;
           }
           else
           {
               y++;
           }
       }
       if(x==n || y==n)
       {
           printf("YES\n");
       }
       else
       {
           printf("NO\n");
       }
   }
}
