

#include<bits/stdc++.h>
#define ll          long long
#define pb          push_back
#define	endl		'\n'
#define pii         pair<ll int,ll int>
#define vll         vector<ll int>
#define all(a)      (a).begin(),(a).end()
#define ubnd        upper_bound
#define ios	    ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
using namespace std;
ll i,j,k,sum=0,n,ans=0,x,t,c=0,d,q,y,test,a,b;
set<ll> s;
map<ll,ll> m;
int main()
{	 
	vector<pair<ll,ll> > v;
	cin>>x>>y>>a>>b;
	for(i=a;i<=x;i++){
		for(j=b;j<=y;j++){
			if(i>j){
				v.pb({i,j});
			}
		}
	}
	cout<<v.size()<<endl;
	for(auto i:v){
		cout<<i.first<<" "<<i.second<<endl;
	}
	return 0;
}