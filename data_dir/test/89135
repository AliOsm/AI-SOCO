#include <bits/stdc++.h>

using namespace std;
#define ll long long
#define ull unsigned long long
#define sz(a) ((int)(a).size())
#define all(n) (n).begin(),(n).end()
#define EPS 1e-9
#define OO 1e9
#define MAX 50005

int dx[] = { 2, 2, -2, -2, 1, -1, 1, -1 };
int dy[] = { 1, -1, 1, -1, 2, 2, -2, -2 };

int main() {
	int n, m;
	scanf("%d %d", &n, &m);
	double x = log2(m);
	if (x >= n) {
		cout << m % (int) pow(2, n) << endl;
	} else
		cout << m << endl;
}
