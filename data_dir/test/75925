#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

int a[10005];

int main()
{
    ios_base::sync_with_stdio(0),cin.tie(0),cout.tie(0);
    int t;
    cin >> t;
    while(t--){
        int n,m;
        cin >> n >> m;
        int sum = 0;
        for(int i=0;i<n;i++){
            cin>>a[i];
            sum += a[i];
        }
        cout << min(sum,m)<<"\n";

    }

    return 0;
}
