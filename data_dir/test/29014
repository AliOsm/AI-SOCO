#include <bits/stdc++.h>

using namespace std;

#define ll long long
#define ld long double


int main() {
	map<char, int>q;
	q['T'] = 4;
	q['C'] = 6;
	q['O'] = 8;
	q['D'] = 12;
	q['I'] = 20;

	ll ans = 0;

	int n;
	cin >> n;
	while (n--) {
		string s;
		cin >> s;
		ans += q[s[0]];
	}

	cout << ans;
	return 0;
}