#include<bits/stdc++.h>
using namespace std;
int xx1, xx2, xx3, yy1, yy2, yy3;

int main()
{
    cin >> xx1 >> yy1 >> xx2 >> yy2 >> xx3 >> yy3;
    cout << 3 << endl;

    cout << (xx1 + xx2 - xx3) << ' ' << (yy1 + yy2 - yy3) << endl;
    cout << (xx2 + xx3 - xx1) << ' ' << (yy2 + yy3 - yy1) << endl;
    cout << (xx3 + xx1 - xx2) << ' ' << (yy3 + yy1 - yy2) << endl;
    return 0;
}
