multiset<int> mset;
map<int,int> ct;

void put(int a)
{
	++ct[a];
	if(ct[a]!=1) mset.erase(mset.find(ct[a]-1));
	mset.insert(ct[a]);
}
void erase(int a)
{
	mset.erase(mset.find(ct[a]));
	--ct[a];
	if(ct[a]) mset.insert(ct[a]);
}

int getmax(void)
{
	return (mset.empty() ? 0 : *mset.rbegin());
}

const int maxn=400050;
int a[maxn];
int n,k;

int main(void)
{
#ifndef ONLINE_JUDGE
	cout<<fixed<<setprecision(6);
#endif
	cin>>n>>k;
	cin>>RA(a,n);
	int pa=0, pb=-1;
	int64 ans=0;
	while(pb<n)
	{
		while(pb<n && getmax()<k)
		{
			++pb;
			put(a[pb]);
		}
		while(getmax()>=k && pa<=pb && pb<n)
		{
			ans+=n-pb;
			erase(a[pa]);
			++pa;
		}
	}
	cout<<ans<<endl;
}