#include <bits/stdc++.h>
using namespace std;

 
int main() {
ios::sync_with_stdio(false);cin.tie(NULL); cout.tie(NULL);
	int n;
	cin>>n;
	int a[n];
	bool alleven=true;
	for(int i=0;i<n;i++){
		cin>>a[i];
		if(a[i]&1)
		alleven=false;
	}
 
	if(alleven)
	cout<<"Second\n";
	else
	cout<<"First\n";
}