#include <bits/stdc++.h>

// Shabdan Batyrkulov

using namespace std;

#define MAX 107
#define pb push_back
#define fr first
#define sc second
#define mk make_pair
#define ll long long
#define ld long double

const long long N = (int)1e5 + 7;
const int inf = (int)1e9 + 7;

int main () {
        int n, h; scanf ("%d %d", &n, &h);
        int ans = 0;
        for (int i = 1; i <= n; i++) {
                int a; cin >> a;
                ans++;
                if (a > h) ans++;
        }
        printf ("%d", ans);
}
