#include <iostream>

using namespace std;
int main()
{
    int a;
    cin >> a;
    string s;
    for(int i = 1;i <= 1000;i++)
    {
        string t = to_string(i);
        for(int counter = 0;counter < t.length();counter++)
            s += t[counter];
    }
    cout << s[a-1];
    return 0;
}
