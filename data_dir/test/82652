#include<bits/stdc++.h>
#define MP make_pair
#define PB push_back
#define PP pop_back()
#define ALL(x) x.begin(),x.end()
#define st first
#define nd second
#define sz(x) (int) x.size()
#define pii pair<int,int>
#define ii int,int
#define ll long long
#define lf double
#define llu long long unsigned
#define INPUT freopen("inp.in","r",stdin);
#define OUTPUT freopen("oup.out","w",stdout);
#define sag 2*node+1
#define sol 2*node
#define orta ((bas+son)/2)
#define set(x,y) memset(x,y,sizeof(x))
#define MIN3(x,y,z) min(min(x,y),z)
#define MAX3(x,y,z) max(max(x,y),z)
#define FOR(i,j,k) for(int i=j;i<=k;i++)
#define ROF(i,j,k) for(int i=j;i>=k;i--)
#define dbgs(x) cerr<<(#x)<<" --> "<<(x)<<" "
#define dbg(x) cerr<<(#x)<<" --> "<<(x)<<endl;getchar()
#define MOD 1000000007
#define inf 1500000000
#define N 1000005
using namespace std;
char c;
int n;
int dfs(int x,int y,int ad) {
    if(x<1 || y<1 || x>8 || y>8) return 0;
    if(ad==1) return 1;
    return dfs(x+1,y,ad+1)+dfs(x-1,y,ad+1)+dfs(x,y+1,ad+1)+dfs(x,y-1,ad+1)+dfs(x+1,y-1,ad+1)+dfs(x+1,y+1,ad+1)+dfs(x-1,y+1,ad+1)+dfs(x-1,y-1,ad+1);
}
int main() {
    scanf(" %c%d",&c,&n);
    printf("%d",dfs(c-'a'+1,n,0));
}
