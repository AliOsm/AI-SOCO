#include <bits/stdc++.h>

using namespace std;

typedef unsigned long long ll;

#define all(x) (x).begin(),(x).end()
#define rall(x) (x).rbegin(),(x).rend()
#define fst first
#define snd second
#define pb push_back
#define ins insert
#define boost ios_base::sync_with_stdio(0);cout.tie(0);cin.tie(0);
int main()
{
    boost;
    int q;
    cin >> q;
    for(int i =0;i < q;++i){
        ll d,l,r;
        cin  >> l >> r >> d;
        if(d < l){
            cout << d;
        }else{
            cout << (r/d + 1)*d;
        }
        cout << "\n";
    }

    return 0;
}
