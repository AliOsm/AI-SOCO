using namespace std;

double dis(long long x, long long y, long long a, long long b)
{
    return sqrt((x - a) * (x - a) + (y- b) * (y - b));
}

long long x[100], y[100], c;

int main()
{
    cin >> x[1] >> y[1];
    cin >> x[2] >> y[2];
    cin >> x[3] >> y[3];

    cout << 3 << endl;

    cout << x[3] + (x[2] - x[1]) << " " << y[3] + (y[2] - y[1]) << endl;
    cout << x[2] + (x[1] - x[3]) << " " << y[2] + (y[1] -y[3]) << endl;
    cout << x[1] + (x[3] - x[2]) << " " << y[1] + (y[3] - y[2]) << endl;
}
