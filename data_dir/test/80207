#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define fr first
#define sc second
#define r_0 return 0
#define fin freopen("input.txt", "r", stdin);
#define fout freopen("output.txt", "w", stdout);
using namespace std;
    const ll N = 2e5 + 7 ;
int a[5];

int main()
{

    ios_base::sync_with_stdio(0);

    cin >> a[1] >> a[2] >> a[3] >> a[4];
    sort( a + 1 , a + 5 );
    if ( a[1] + a[3] == a[2] + a[4] || a[2] + a[3] == a[4] + a[1] )
    {
        cout << "YES";
        r_0;
    }
    if ( a[4] == a[2] + a[1] + a[3] || a[1] + a[2] == a[3] + a[4] )
    {
        cout << "YES";
        r_0;
    }
    cout << "NO";


}
