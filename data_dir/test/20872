/* In The Name Of God */
#include <bits/stdc++.h>

# define xx first
# define yy second
# define pb push_back
# define pp pop_back
# define eps 1e-9
# define err(x) cerr << #x << " ::   " << x << endl;

using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
typedef vector<int> vint;

int main(){
	ios_base::sync_with_stdio (0);
	int q;cin>>q;
	while(q--){
		bool mark = false;
		int x;cin>>x;
		for(int i=0 ; i<100 ; i++)if(i*3 <= x)
			if((x - i*3)%7 == 0)
				mark = true;
		if(mark)
			cout<<"YES\n";
		else
			cout<<"NO\n";
	}
	return 0;
}

