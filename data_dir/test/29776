//Believe in yourself.
#include<bits/stdc++.h>
#define ll long long
#define ld long double
#define PB push_back
#define MP make_pair
#define INF 1000000000000000007
#define inf 1000000007
#define y1 dasdsvcvd
using namespace std;
int main()
{
  ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
    string s;
    cin >> s;
    bool f=0;
    if(s[0] == '-')
    {
        if(s[s.length()-1] < s[s.length()-2])
            s[s.length()-2]=s[s.length()-1];
            f=1;
    }
    if(s[1]=='0' && s[0]=='-')
        cout << 0;
    else
    {
        if(f)
        for(int i = 0; i < s.length()-1; i++)
            cout << s[i];
        else
            cout << s;
    }
    cout << '\n';


return 0;
}
