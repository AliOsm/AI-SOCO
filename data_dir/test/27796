#include <bits/stdc++.h>

using namespace std;

signed main()
{
#ifdef files
	ifstream cin("input.txt");
	ofstream cout("output.txt");
#endif
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int n,a,b,c,d,e,f;
    cin >> n >> b >> a >> d >> c >> f >> e;
    if(a+d+f>=n)cout << n-d-f << " " << d << " " << f;
    else if(a+c+f>=n)cout << a << " " << n-a-f << " " << f;
    else cout << a << " " << c << " " << n-a-c;
}
