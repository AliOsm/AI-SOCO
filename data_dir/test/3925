// In the name of Allah the Most Merciful.

#include<bits/stdc++.h>
using namespace std;

typedef long long ll;

int ar[100000];

int main(void)
{
    vector<int>v;
    int n , lim;
    cin >> n;
    lim = n*2;
    for(int i=0;i<lim;i++){

        int in;
        cin >> in;
        v.push_back(in);
    }

    sort(v.begin() , v.end());

    if(v[n-1]==v[n])cout << "NO" << endl;
    else cout << "YES" << endl;

    return 0;
}
