#include <stdio.h>
#include <math.h>
#include <string.h>
#include <iostream>
#include <algorithm>
#include <vector>
#include <map>
#include <utility>
#include <stack>
#include <queue>
#include <set>

using namespace std;

int main()
{
    // ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
	// freopen("input.in", "r", stdin);
	string x, y;
	cin >> x >> y;
	bool bol = 1;
	for(int i = 0; i < x.size(); i++)
	{
		if(x[i] < y[i])
			bol = 0;
	}
	if(!bol)
		cout << "-1\n";
	else
		cout << y << "\n";
}




