using namespace std;
inline int input(){int32_t x ; scanf("%d",&x) ; return x ;}
#define int long long
const int maxn = 200 + 10;
int32_t main()
{
    int n = input() , k = input();
    while(k --)
    {
        if (n % 10 ) n--;
        else n /= 10;
    }
    cout <<  n << endl;
}
