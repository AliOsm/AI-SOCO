#include<bits/stdc++.h>
#define MAX   111
#define FOR(i,a,b) for (int i=(a),_b=(b);i<=_b;i=i+1)
#define FORD(i,b,a) for (int i=(b),_a=(a);i>=_a;i=i-1)
#define REP(i,n) for (int i=0,_n=(n);i<_n;i=i+1)
#define FORE(i,v) for (__typeof((v).begin()) i=(v).begin();i!=(v).end();i++)
#define ALL(v) (v).begin(),(v).end()
#define fi   first
#define se   second
#define MASK(i) (1LL<<(i))
#define BIT(x,i) (((x)>>(i))&1)
#define next   ___next
#define prev   ___prev
#define left   ___left
#define right   ___right
using namespace std;
template<class X,class Y>
    void minimize(X &x,const Y &y) {
        if (x>y) x=y;
    }
template<class X,class Y>
    void maximize(X &x,const Y &y) {
        if (x<y) x=y;
    }
bool ok[MAX];
int n,m;
int main(void) {
    scanf("%d%d",&n,&m);
    REP(love,n) {
        int t;
        scanf("%d",&t);
        REP(love,t) {
            int x;
            scanf("%d",&x);
            ok[x]=true;
        }
    }
    FOR(i,1,m) if (!ok[i]) {
        printf("NO\n");
        return 0;
    }
    printf("YES\n");
    return 0;
}
