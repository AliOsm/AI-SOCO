/************************* In the name of Allah *************************/
#include <bits/stdc++.h>
using namespace std;

typedef long long       ll;
typedef long double     ld;
typedef string          str;
typedef pair<ll,ll>     pll;

#define F               first
#define S               second
#define sep             ' '
#define endl            '\n'
#define fast_io         ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);

const ll MOD = 1e9 + 7,
         INF = 1e18,
         MAXN = 1e3 + 10;

int main(){
    fast_io
    ll n, k[MAXN];
    str s[MAXN];
    map<str, pll> mp;
    cin >> n;
    for (ll i = 0; i < n; i++){
        cin >> s[i] >> k[i];
        mp[s[i]] = {mp[s[i]].F + k[i], -i};
    }
    str ans, si;
    pll mx = {-INF, -INF}, q;
    map<str, pll>::iterator itr;
    for (itr = mp.begin(); itr != mp.end(); itr++){
        si = itr->first; q = itr->second;
        if (q > mx){
            ans = si;
            mx = q;
        }
    }
    ll r = mx.F;
    map<str, pll> mp2;
    for (ll i = 0; i < n; i++){
        mp2[s[i]] = {mp2[s[i]].F + k[i], -i};
        if (mp2[s[i]].F >= r && mp[s[i]].F == r){
            cout << s[i];
            return 0;
        }
    }
    return 0;
}

