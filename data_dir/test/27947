#include <iostream>
#include <vector>
#include <algorithm>
#include <memory>
#include <map>
#include <unordered_map>
#include <set>
#include <queue>
#include <unordered_set>
#include <functional>
#include <climits>
#include <cstring>
#include <cmath>

using namespace std;
using ll = long long;
const int oo = numeric_limits<int>::max() / 2;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie();
	int n, m;
	cin >> n >> m;
	int l = 0;
	int s = 0;
	while(s < n) {
		++l;
		s += 4;
	}
	cout << 5;
	for(int i = 0; i < l; ++i) {
		cout << 4;
	}
	cout << 5;
	cout << endl;
	cout << 4;
	for(int i = 0; i < l + 1; ++i) {
		cout << 5;
	}
	cout << endl;
	return 0;
}
