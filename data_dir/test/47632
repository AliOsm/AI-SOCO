#include<bits/stdc++.h>
using namespace std;

main(){
    int n , m , x , y , sum = 0;
    scanf("%d %d" , &n , &m);

    for(int i = 0 ; i < n ; i ++){
        for(int j = 0 ; j < m ; j ++){
            scanf("%d %d" , &x , &y);
            sum += (x + y > 0);
        }
    }
    printf("%d\n" , sum);
}
