using namespace std;
ll n,m,ans,x,y,q,z,w,a[100100];
vector <ll> v[100100];
int main(){
    cin>>x>>y;
    for(int i=0;i<2*y;i++){
        if(x==0)return cout<<"Yes",0;
        if(a[x]==1)return cout<<"No",0;
        a[x]=1;
        x*=2;x%=y;
    }

    return 0;
}
