#include<iostream>
#include<algorithm>
#include<vector>
#include<sstream>
#include<string>
#include<math.h>
#include<iomanip>
#include<map>
#include<set>
#include<stack>
#include<queue>
using namespace std;

int main() {
	string s,t; cin>>s>>t;
	int sz=s.size();
	string ans;
	for (int i=0; i<sz; i++) {
		if(s[i]!=t[i]) ans+='1';
		else ans+='0';
	}
	cout << ans << endl;
	return 0;
}