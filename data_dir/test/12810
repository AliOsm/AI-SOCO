/* In The Name Of God */
#include <bits/stdc++.h>

# define xx first
# define yy second
# define pb push_back
# define pp pop_back
# define eps 1e-9

using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
typedef vector<int> vint;
int _200,_100;
int main(){
	ios_base::sync_with_stdio (0);
	int n;cin>>n;
	while(n--){
		int tmp;cin>>tmp;
		if(tmp==100)
			_100++;
		else
			_200++;
	}
	if(_200 % 2){
		if(_100%2)
			cout<<"NO\n";
		else if(_100)
			cout<<"YES\n";
		else
			cout<<"NO\n";
	}
	else{
		if(_100%2)
			cout<<"NO\n";
		else
			cout<<"YES\n";
	}
	return 0;
}

