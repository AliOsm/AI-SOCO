#include <cstdio>
#include <iostream>
#include <cstring>
#include <vector>
#include <list>
#include <map>
#include <set>
#include <queue>
#include <deque>
#include <stack>
#include <algorithm>
#include <functional>
#include <cmath>
#include <sstream>
#include <utility>
#include <ctime>
#include <memory.h>
#include <cctype>
#include <cstdlib>
using namespace std;
 
#define eps 1e-9
#define pi acos(-1.0)
#define o 1000000007
#define ull unsigned long long
 
int j, i, h, n, m, d, q, v1[100], v2[100], u1[100], u2[100];
char a[100][100];

bool prov (int x1, int y1, int x2, int y2)
{
	return (a[x1][y2]=='B' || a[x2][y1]=='B');
}

int main()
{
    //freopen ("input.txt","r",stdin); freopen ("output.txt","w",stdout);
    //freopen ("nosimple.in","r",stdin); freopen ("nosimple.out","w",stdout);
    scanf ("%d%d\n", &n, &m);
	for (j=1; j<=n; j++)
	{
		for (i=1; i<=m; i++)
		{
			scanf ("%c", &a[j][i]);
			if (a[j][i]=='B')
			{
				if (u2[i]!=0 & u2[i]<j-1)
				{
					printf ("NO");
					return 0;
				}
				else
				{
					u2[i]=j;
				}
				if (u1[i]==0)
				{
					u1[i]=j;
				}
				if (v2[j]!=0 & v2[j]<i-1)
				{
					printf ("NO");
					return 0;
				}
				else
				{
					v2[j]=i;
				}
				if (v1[j]==0)
				{
					v1[j]=i;
				}
			}
		}
		scanf ("\n");
	}
	for (j=1; j<=n; j++)
	{
		if (v1[j]==0)
		{
			continue;
		}
		for (i=j+1; i<=n; i++)
		{
			if (v1[i]==0)
			{
				continue;
			}
			if (!prov(j, v1[j], i, v1[i]))
			{
				printf ("NO");
				return 0;
			}
			if (!prov(j, v2[j], i, v1[i]))
			{
				printf ("NO");
				return 0;
			}
			if (!prov(j, v1[j], i, v2[i]))
			{
				printf ("NO");
				return 0;
			}
			if (!prov(j, v2[j], i, v2[i]))
			{
				printf ("NO");
				return 0;
			}
		}
	}
	for (j=1; j<=m; j++)
	{
		if (u1[j]==0)
		{
			continue;
		}
		for (i=j+1; i<=m; i++)
		{
			if (u1[i]==0)
			{
				continue;
			}
			if (!prov(u1[j], j, u1[i], i))
			{
				printf ("NO");
				return 0;
			}
			if (!prov(u2[j], j, u1[i], i))
			{
				printf ("NO");
				return 0;
			}
			if (!prov(u1[j], j, u2[i], i))
			{
				printf ("NO");
				return 0;
			}
			if (!prov(u2[j], j, u2[i], i))
			{
				printf ("NO");
				return 0;
			}
		}
	}
	printf ("YES");
    return 0;
}