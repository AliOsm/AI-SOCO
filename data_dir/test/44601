// In The Name Of God

#include<bits/stdc++.h>
#define IOS ios::sync_with_stdio(false),cin.tie(0)
#define rep(i,n) for(int i=0;i<(n);i++)
#define pb push_back
#define ll long long

using namespace std;
const int N = 1e5+22 ;

int n;
string str;
int main()
{IOS;
	int x,y,res=0;
	
	cin>>str;
	rep(i,str.size())
	{
		char c=str[i] ;
		if(c=='a' || c=='i' || c=='o' || c=='u' || c=='e' || c=='1' || c=='3' || c=='5' || c=='7' || c=='9')
			res++;
	}
	cout<<res;
	
	return 0;
}
