#include<bits/stdc++.h>
#define F first
#define S second
#define pb push_back
#define mp make_pair
using namespace std;
int dx[] = {-1,1,0,0};
int dy[] = {0,0,-1,1};
int x,y,z,t,t1,t2,t3;
int main()
{
        cin >> x >> y >> z >> t1 >> t2 >> t3;
        if(abs(x-y) * t1 >= (abs(x-y) + abs(x-z)) * t2 +  3*t3) cout << "YES";
        else cout << "NO";
}