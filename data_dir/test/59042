#include <bits/stdc++.h>
using namespace std;
#define finish(x) return cout << x << endl, 0
#define ll long long

int n;
string s;

int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cin >> n >> s;
    ll ans = 0;
    for(int i = 0 ; i < n ; i++)
        if((s[i] - '0') % 2 == 0) ans += i + 1;
    cout << ans << endl;
}
