#include <bits/stdc++.h>
#define int long long
using namespace std;
int32_t main(){
	int t;cin>>t;
	while(t--){
		string s;cin>>s;
		sort(s.begin(),s.end());
		if(s[0]==s.back()){
			cout<<s<<endl;
		} else {
			for(int i=0;i<s.size();i++){
				cout<<"10";
			}
			cout<<endl;
		}
	}

} 