#include <bits/stdc++.h>
using namespace std;
#define eb emplace_back
#define emp emplace
#define fi first
#define se second
#define INF 0x3f3f3f3f
typedef long long ll;
typedef pair<int,int> ii;

int main(void) {
	ios_base::sync_with_stdio(false);

	ll ans = 1;
	ll n, k;
	cin >> n >> k;
	ll ori = n;

	while (k--) ans *= 10;

	cout << (ori*ans)/(__gcd(ori, ans)) << endl;
    
	return 0;
}

