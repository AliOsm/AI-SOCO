/* --- main part --- */

#define TASK "a"

 
int main()
{
    #ifdef home
        assert(freopen(TASK".in", "r", stdin));
        assert(freopen(TASK".out", "w", stdout));
    #endif
    int x1, y1, x2, y2;
    scanf("%d%d%d%d", &x1, &y1, &x2, &y2);
    if (x2 >= x1 && y2 >= y1) return 0 * printf("Polycarp\n");
    if (x2 >= x1 + y1 && y2 <= y1) return 0 * printf("Polycarp\n");
    if (y2 >= x1 + y1 && x2 <= x1) return 0 * printf("Polycarp\n");
    printf("Vasiliy\n");
    #ifdef home
        eprintf("Time: %d ms\n", (int)(clock() * 1000. / CLOCKS_PER_SEC));
    #endif
    return 0;
}
 