#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define MAX_INT 1000000000
int main()
{
   int n,i,j,k,l;
   cin>>n;
   int a[n+1]={0};
   
   for(i=1;i<=n;i++){
       cin>>a[i];
   }
   for(i=1;i<=n;i++){
       j=a[i];
       k=a[j];
       l=a[k];
       if(l==i){
           cout<<"YES"<<endl;
           return 0;
       }
   }
   cout<<"NO"<<endl;
    return 0;
}