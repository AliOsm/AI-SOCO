#include <bits/stdc++.h>

#define ss second
#define ff first
#define pb push_back
#define faster ios::sync_with_stdio(0), cin.tie(0), cout.tie(0)
#define error(args...) {string _s=#args;replace(_s.begin(),_s.end(),',',' ');stringstream _ss(_s);istream_iterator<string> _it(_ss);err(_it,args);}

using namespace std;

void err(istream_iterator<string> it){}
template <typename S37,typename... Args>
void err(istream_iterator<string> it,S37 a,Args... args){cerr<<*it<<" = "<<a<<endl;err(++it,args...);}

typedef long long ll;
typedef long double ld;

const long long N=300100,mod=1e9+7,mod2=1e9+9,mod3=998244353,sq=450,base=727,lg=25,inf=1e18+10;

ll n,m,x,y,w,z,t,k,ans,a[N];

string s,f[N];

int main(){
    faster;
    cin>>x>>y>>z;
    x-=y;
    if(x>0 && x-z>0)return cout<<"+",0;
    if(x==0 && x-z==0)return cout<<"0",0;
    if(x<0 && x+z<0)return cout<<"-",0;
    cout<<"?";
    return 0;
}
