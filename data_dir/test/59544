#include <bits/stdc++.h>
using namespace std;

char s_1[50], s_2[50];
char d[50];
int l;

void read() {
	char s[50];
	scanf("%s", &s);
	int i;
	for(i = 0; s[i] != '|'; ++i) {
		s_1[i] = s[i];
	}
	for(int j = i + 1; s[j]; ++j) {
		s_2[j - i - 1] = s[j];
	}
	scanf("%s", &d);
	
}

void fill() {
	int k = 0;
	for(int i = strlen(s_1); i < l; ++i) {
		s_1[i] = d[k];
		k++;
	}
	for(int i = strlen(s_2); i < l; ++i) {
		s_2[i] = d[k];
		k++;
	}
}

int main (int argc, char *argv[])
{
	read();
	l = strlen(s_1) + strlen(s_2) + strlen(d);
	if(l%2) {
		printf("Impossible");
		return 0;
	}
	l /= 2;
	if(strlen(s_1) > l || strlen(s_2) > l) {
		printf("Impossible");
		return 0;
	}
	fill();
	printf("%s|%s", s_1, s_2);
	return 0;
}

