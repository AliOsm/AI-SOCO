#define endl '\n'
#define fastScan ios::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);

using namespace std;

int main() 
{
   fastScan;
   int n; cin >> n;
   n -= 10;
   if(n <= 0) cout << 0 << endl;
   else if(n <= 9) cout << 4 << endl;
   else if(n == 10) cout << 15 << endl;
   else if(n == 11) cout << 4 << endl;
  	else cout << 0 << endl;
   return 0;
}
