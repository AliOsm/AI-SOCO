#define L long long int
#define LD long double
#define pi 3.141592653589793238462643383
#define M 1000000007

using namespace std;

int main()
{
    int n,i;
    string s;
    while(cin>>s)
    {
              n=s.size();
              map<char,int> m;
              for(i=0;i<n;i++)
              m[s[i]]=1;
              
              if(m.size()%2==1)
              cout<<"IGNORE HIM!";
              else
              cout<<"CHAT WITH HER!";
              
              
              
    }
    return 0;
}
