#include <bits/stdc++.h>
using namespace std;

int main() {
	int n;
	cin >> n;

	vector<int> f(100001);

	for (int i = 0; i < n; i++) {
		int a;
		scanf("%d", &a);
		f[a]++;
	}

	int ans = 1;
	for (int i = 2; i <= 100000; i++) {
		int cnt = 0;
		for (int j = i; j <= 100000; j += i) {
			cnt += f[j];
		}
		ans = max(ans, cnt);
	}
	cout << ans << endl;
}