#include <functional>
#include <algorithm>
using namespace std;
typedef long long ll;
int a[200];
int main()
{
int n;
cin>>n;
for(int i=0;i<n;i++)
{int x;
cin>>x;
a[x]++;
}
int m=0;
for(int i=0;i<=100;i++)
	m=max(m,a[i]);
cout<<m;
}