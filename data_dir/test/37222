#include<bits/stdc++.h>
using namespace std;
int a[1010];

int main(){

    int t;
    cin>>t;

    int n,m;


    while(t--){
        cin>>n>>m;

        int sumx=0;

        for(int i=0;i<n;i++){
            cin>>a[i];
            sumx+=a[i];
        }

        cout<<min(m,sumx)<<endl;


    }
   
}