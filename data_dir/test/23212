#include <cstdio>
#include <cstring>
char s[110];

char R(char c){
	if(c=='w'){
		return('q');
	}
	if(c=='e'){
		return('w');
	}
	if(c=='r'){
		return('e');
	}
	if(c=='t'){
		return('r');
	}
	if(c=='y'){
		return('t');
	}
	if(c=='u'){
		return('y');
	}
	if(c=='i'){
		return('u');
	}
	if(c=='o'){
		return('i');
	}
	if(c=='p'){
		return('o');
	}
	if(c=='s'){
		return('a');
	}
	if(c=='d'){
		return('s');
	}
	if(c=='f'){
		return('d');
	}
	if(c=='g'){
		return('f');
	}
	if(c=='h'){
		return('g');
	}
	if(c=='j'){
		return('h');
	}
	if(c=='k'){
		return('j');
	}
	if(c=='l'){
		return('k');
	}
	if(c==';'){
		return('l');
	}
	if(c=='x'){
		return('z');
	}
	if(c=='c'){
		return('x');
	}
	if(c=='v'){
		return('c');
	}
	if(c=='b'){
		return('v');
	}
	if(c=='n'){
		return('b');
	}
	if(c=='m'){
		return('n');
	}
	if(c==','){
		return('m');
	}
	if(c=='.'){
		return(',');
	}
	if(c=='/'){
		return('.');
	}
	return('1');
}
char L(char c){
	int a;
	for(a=0;a<200;a++){
		if(R(a+'!')==c){
			return(a+'!');
		}
	}
}
int main(){
	int a,b;
	char k;
	scanf("%c",&k);
		scanf("%s",s);
		a=strlen(s);
		if(k=='R'){
			for(b=0;b<a;b++){
				printf("%c",R(s[b]));
			}
		}
		else{
			for(b=0;b<a;b++){
				printf("%c",L(s[b]));
			}
		}
		printf("\n");
	}