main()
{
//    freopen("maximum.dat","r",stdin);freopen("maximum.sol","w",stdout);
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    qq=1;
//    cin>>qq;
    for (int _=0;_<qq;_++)
    {
        cin>>n;
        x=0;
        if (n&1) x+=16;
        if (n&2) x+=2;
        if (n&4) x+=8;
        if (n&8) x+=4;
        if (n&16) x+=1;
        if (n&32) x+=32;
        cout<<x;
    }
}
