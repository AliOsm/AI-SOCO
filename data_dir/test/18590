#include <bits/stdc++.h>
using namespace std;
#define pb push_back
#define ll long long
#define F first
#define S second
#define pi pair<int,int>
#define go ios::sync_with_stdio(0); cin.tie(0); cout.tie(0)
ll ans,n;
string s;
int main()
{
    go;
    cin>>n>>s;
    for(ll i=0;i<n;i++)
    {
        if((s[i]-'0')%2==0)
        {
            ans+=i+1;
        }
    }
    cout<<ans;
    return 0;
}
