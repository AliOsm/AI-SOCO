#define pdd pair<double, double>
#define vi vector<int>
#define vvi vector<vi>
#define mp make_pair
#define pb push_back
#define pii pair<int, int>
#define ull unsigned long long
#define ll  long long
#define loop(xxx,n) for(int xxx = 0; xxx < n; xxx++)
#define loop1(xxx,n) for(int xxx = 1; xxx <= n; xxx++)
#define MLN 1000000
#define HTS 100000
#define DEBUG

#ifdef DEBUGNO
#define printf(fmt, ...) (0)
#endif
using namespace std;

void print(vi x) {
	for (int e : x)
		printf("%d ", e);
	printf("\n");
}


int main() {
	string s;
	//scanf("%s", &s);
	cin >> s;
	//printf("input\n");
	int l = s.length();
	loop(i, l) {
		if (s[i] == 'r')
			printf("%d\n", i + 1);
	}
	for (int i = l; i >= 0; i--)
		if (s[i] == 'l')
			printf("%d\n", i + 1);
	return 0;

}
