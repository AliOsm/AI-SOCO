#include <iostream>
#include <algorithm>
#include <vector>
#include <set>
#include <stack>
#include <map>
#include <iomanip>
#include <cmath>
using namespace std;
int main() {

  ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

  int t = 0;

  cin >> t;

  while(t--) {
    long long y = 0, x = 0;
    cin >> y >> x;
    cout << ((y - 1 == x) ? "NO\n" : "YES\n");
  }

  return 0;
}
