#include <iostream>
#include <stdio.h>
#include <string>
#include <cstring>
#include <map>
#include <set>
#include <vector>
#include <queue>
#include <stack>
#include <algorithm>
#include <cmath>
#include <deque>
using namespace std;

int main()
{
    int n;
    cin>>n;
    if (n>0) cout<<n;
    else {
        cout<<max(n/10,(n/100)*10+n%10);
    }
}
