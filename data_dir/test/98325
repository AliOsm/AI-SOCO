#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;
#define pb push_back
#define make make_pair
#define ff first
#define ss second
#define hell 1000000007
#define test(x) cout<<"x is "<<x<<endl
ll gcd(ll a,ll b)
{
    if(a==0)
        return b;
    else
        return gcd(b%a,a);
}
int main()
{	
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
        ll t;cin>>t;
	while(t--)
	{
		long double n;cin>>n;
		cout<<(ll)(ceil(n/7))<<endl;
	}
}