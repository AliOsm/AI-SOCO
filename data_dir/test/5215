#include <bits/stdc++.h>

using namespace std;

#define pb push_back

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int n;
    cin >> n;

    int sch = 0;
    for(int i = 0; i < n; ++i){
        char a; cin >> a;
        if(a == 'A') ++sch;
    }

    if(sch > n - sch)
        cout << "Anton";
    if(sch < n - sch)
        cout << "Danik";
    if(sch == n - sch)
        cout << "Friendship";
}
