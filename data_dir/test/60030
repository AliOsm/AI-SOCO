#include<bits/stdc++.h>
using namespace std;
#define MOD 1000000000 + 7
#define hell 100000000000000000
#define PI 3.1415926535897932384626
#define int long long int
#define fi first
#define se second
#define pq priority_queue
#define pb push_back
#define IOS ios_base::sync_with_stdio(0);cin.tie(NULL);cout.tie(NULL);
#define endl '\n'
#define fill(x,y) memset(x,y,sizeof(x))
#define what_is(x)  cerr << #x << " is " << x << endl;
#define rep(i, begin, end) for (__typeof(end) i = (begin) - ((begin) > (end)); i != (end) - ((begin) > (end)); i += 1 - 2 * ((begin) > (end)))
template<typename T> T pow(T a,T b, int m){T ans=1; while(b>0){ if(b%2==1) ans=(ans*a)%m; b/=2; a=(a*a)%m; } return ans%m; }
template<typename T> void swap(T *a,T *b){T temp = *a;*a = *b;*b = temp;return ;}
/*******************************Template ends**************************************/
int n,k;
bool di(int x){
	set<int>s;
	while(x){
		int temp = x%10;
		if(temp <= k)
			s.insert(temp);
		x/= 10;
	}
	return ((int)s.size() == k+1);
}
void solve()
{
	cin >> n >> k;
	int count = 0;
	rep(i,0,n)
	{
		int a;
		cin >> a;
		if(di(a))
			count++;
	}
	cout << count << endl;
}
signed main()
{
    IOS;
    solve();
    return 0;
}
