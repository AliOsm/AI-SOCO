#include <bits/stdc++.h>

using namespace std;

int32_t main()
{
    ios_base::sync_with_stdio(false);
    int t;
    cin >> t;
    while(t--){
        int n, k1, k2;
        cin >> n >> k1 >> k2;
        bool ok = false;
        for(int i = 0; i < k1; i++){
            int x;
            cin >> x;
            if(x == n) ok = true;
        }
        for(int i = 0; i < k2; i++){
            int x;
            cin >> x;
        }
        if(ok) cout << "YES\n";
        else cout << "NO\n";
    }
    return 0;
}
