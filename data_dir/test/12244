#include<bits/stdc++.h>
#define len length
#define INF 1000000000
#define fir first
#define sec second
#define pb push_back
#define ll long long
using namespace std;
typedef pair<int,int> ii;
typedef vector<int> vi;
typedef vector<ii> vii;
int main()
{
	int n, k, res = 0;
	cin >> n >> k;
	for(int i = 0; i < n; i++)
	{
		int x; cin>>x;
		if(x > k) res+=2;
		else res++;
	}
	cout << res << "\n";
	return 0;
	
}