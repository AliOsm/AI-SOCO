#include <bits/stdc++.h>
#define ll long long int
#define pb push_back
#define mp make_pair
#define mod 1000000007
#define pi 3.141592653589793
const int N = 200001;
using namespace std;


void f() {
    string s;
    cin >> s;
    int cnt = count(s.begin(), s.end(), '0');
    if (cnt == s.size() || cnt == 0) {
        cout << s;
    }
    else {
        for (int i = 0; i < s.size(); i++) {
            cout << "10";
        }
    }
}


int main() {
//    cout << fixed << setprecision(10);
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);



    int t;
    cin >> t;
    while (t--) {
        f();
        cout << '\n';
    }


}