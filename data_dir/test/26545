#include <bits/stdc++.h>
#define LL long long
#define pb push_back
#define mp make_pair
#define fi first
#define sc second
using namespace std;

int main() {
	int n;
	string s;
	int x = 1;
	int cnt=0;
	cin>>n;
	for(int i=0;i<n;i++){
		if(x) s += "a";
		else s += "b";
		cnt++;
		if(cnt==2) x = !x, cnt=0;
	}
	cout<<s<<endl;
	return 0;
}