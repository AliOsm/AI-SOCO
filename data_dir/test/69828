#include <bits/stdc++.h>
using namespace std;

int main () {
    int n;
    cin >> n;
    cout << "YES" << endl;
    for (int i = 1; i <= n; i++) {
        int x, y;
        cin >> x >> y;
        cin >> x >> y;
        cout << 2 * (((x % 2) + 2) % 2) + (((y % 2) + 2) % 2) + 1 << endl;
    }
   }