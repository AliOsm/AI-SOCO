#include <bits/stdc++.h>

using namespace std;
int const N = 5e5;

int n,m;

int main() {
    cin>>n;
    int mx1 = 0;
    for(int i=0;i<n;i++){
        int a;
        cin>>a;
        mx1=max(mx1,a);
    }
    cin>>m;
    int mx2 = 0;
    for(int i=0;i<m;i++){
        int a;
        cin>>a;
        mx2=max(mx2,a);
    }
    cout<<mx1<<' '<<mx2<<endl;
    return 0;
}