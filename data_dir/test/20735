#include <bits/stdc++.h>
using namespace std;
long long  i, j, n, c[123456],a[1111234], b[1125678],d[123456], l,ll, s, t, k, m, xh, x, x1, y,z;
string p,q;
int main() {
    cin >> n;
    for (i = 0; i < n; i ++){
        cin >> a[i];
    }
    sort (a,a+n);
    for (i = 0; i <n/2; i ++){
        s += (a[i] + a[n-1-i])*(a[i] + a[n-1-i]);
    }
    cout << s;
    return 0;
}