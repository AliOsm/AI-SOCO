#include<bits/stdc++.h>
 
using namespace std;

#define lli long long int

int main(){
	
	ios_base :: sync_with_stdio(false); cin.tie(nullptr); cout.tie(nullptr);
	
	int n,x,y;
	
	cin>>n>>x>>y;
	
	int need = n*y;
	
	need--;
	
	need /= 100;
	
	need++;
	
	if(x >= need){
		
		cout<<"0";
	}
	
	else{
		
		cout<<need-x;
	}
}
