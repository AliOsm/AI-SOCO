#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define pb push_back
#define mk make_pair
#define lw lower_bound
ll b,c,d,e,i,j,n,l,r;
vector<ll> v,v1;
map<ll,ll> m;
int main(){
	ios_base::sync_with_stdio(NULL);
	cin.tie(NULL);
	cout.tie(NULL);
	cin>>b>>c;
	if(b==9&&c==1){
		cout<<"9 10";exit(0);
	}
	if(b+1==c){
		cout<<b<<"9 "<<c<<'0';exit(0);
	}
	if(b==c){
		cout<<b<<"1 "<<c<<2;exit(0);
	}
	cout<<-1;
}
//(n-i+1)*(n-i+1)*(n-i)!*i!
