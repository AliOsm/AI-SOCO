#include <iostream>
#include <fstream>
#include <cstdio>
#include <climits>
#include <vector>
#include <map>
#include <list>
#include <deque>
#include <stack>
#include <set>
#include <string>
#include <cstring>
#include <algorithm>
#include <bitset>
#include <cmath>

using namespace std;

int main()
{
	string a, b;
	cin >> a >> b;
	for(int i = 0; i < a.length(); i++)
		if (tolower(a[i]) > tolower(b[i]))
		{
			cout << 1 << endl;
			return 0;
		}
		else if (tolower(a[i]) < tolower(b[i]))
		{
			cout << -1 << endl;
			return 0;
		}
	cout << 0 << endl;
	return 0;
}