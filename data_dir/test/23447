        int t;
        cin>>t;
        while(t--){
            cin>>n;
            forr(i,1,n)cin>>a[i];
            sort(a+1,a+1+n);
            cout<<a[(n+1)/2]<<" ";
            forr(i,1,(n-1)/2)
            {
                cout<<a[(n+1)/2+i]<<" "<<a[(n+1)/2-i]<<" ";
            }
            if(n%2==0)cout<<a[n];
            cout<<endl;
        }



