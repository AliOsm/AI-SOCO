#include <iomanip>
#include <functional>
#include <stack>
using namespace std;

typedef long long ll;

const long double pi = 3.14159265358979;



int main()
{
    //freopen("bfs.in","r",stdin);
    //freopen("bfs.out","w",stdout);
    //freopen("input.txt","r",stdin);
    //freopen("output.txt","w",stdout);
    int n,k,i;
    scanf("%d%d",&n,&k);
    if(k==0){
    
        for(i=1;i<=n;++i)
            printf("%d ",i);
    }else{
        for(i=1;i<=n-k-1;++i){
            printf("%d ",i);
        } 
        for(int j=n;j>=i;--j){
            printf("%d ",j);
        } 
    }

    return 0;
}