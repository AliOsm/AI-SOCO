#include<bits/stdc++.h>
using namespace std;
const int MAXN = 1e5+5;
const int MOD = 1e9+7;
const int INF = 2e9;

int main(){
    double D = 0, H = 0, V = 0, E = 0;
    scanf("%lf%lf%lf%lf",&D,&H,&V,&E);

    double S = 3.141592653589793238462643383 * D * D * 0.25;
    double Speed = V / S - E;
    double RES = H / Speed;

    if(RES < 0) printf("NO");
    else printf("YES\n%.16f",RES);
    return 0;
}