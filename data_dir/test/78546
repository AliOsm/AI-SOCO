#include<bits/stdc++.h>
#define ll long long
using namespace std;

int main()
{
    int t;

    cin >> t;

    while(t--){
        ll n, d;

        cin >> n >> d;

        if(n >= d){
            cout << "YES" << endl;
            continue;
        }

        ll x = (n * n) + (2 * n) + 1 - (4 * d);

        if(x < 0) cout << "NO" << endl;
        else cout << "YES" << endl;
    }

    return 0;
}
