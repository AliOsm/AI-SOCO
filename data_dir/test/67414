using namespace std;
ll x,y,w,z,n,m,ans,q,xx,yy;
int main(){
   // ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    cin>>x>>y>>xx>>yy;
    if((x+y<=max(yy,xx)) || (x<=xx && y<=yy))return cout<<"Polycarp",0;
    else cout<<"Vasiliy";
    return 0;
}