#include <stdio.h>
#include <string.h>
#include <algorithm>
#include <iostream>

using namespace std;

int a1,a2,b1,b2,c1,c2;

int main()
{
	cin>>a1>>b1>>c1;
	cin>>a2>>b2>>c2;
	if(a1==0 && a2==0 && b1==0 && b2==0)
	{
		cout<<(0)<<endl;
		return 0;
	}
	if(a1*b2-a2*b1==0)
	{
		if(b1*c2-b2*c1==0 && a1*c2-a2*c1==0) cout<<-1<<endl;
		else cout<<0<<endl;
		return 0;
	}
	cout<<1<<endl;
	return 0;
}
