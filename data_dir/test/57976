//https://ravesli.com/urok-61-struktury/#toc-0

#include <bits/stdc++.h>
using namespace std;
struct ib{
    int a ;
    int b ;
};
void print ( ib x , ib y ){
    while ( y.b -- ){
        if ( x.a % 10 == 0 )
            x.a /= 10;
        else
            x.a -- ;
    }
    cout << x.a ;
}
int main(){
    ib n , m ;
    cin >> n.a >> m.b;
    print( n , m );
}
