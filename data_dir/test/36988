#include<bits/stdc++.h>
#include <iostream>
#include<string>
#include<cmath>
#include<algorithm>
#include<vector>
#include<stdio.h>
using namespace std;
#define ll long long
#define rep(i,a,b) for(i=a;i<b;i+=1)
#define rrep(i,a,b) for(i=a;i>b;i-=1)
#define vi vector<int>
#define vii vector<vector<int> >
#define all(c) (c).begin(),(c).end()
#define pb push_back
char arr1[13] = {'*'};
char arr2[13] = {'*'};
int i,j,k;
void solve()
{
    int n;
    cin>>n;
    int arr[2*n];
    rep(i,0,2*n)
        cin>>arr[i];
    sort(arr,arr+2*n);
    if(arr[n-1]<arr[n])
        cout<<"YES";
    else
        cout<<"NO";
}
int main()
{
    ios_base::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
    int t=1;
    //cin>>t;
    while(t--)
        solve();
    return 0;
}