#include<bits/stdc++.h>
using namespace std;
#define forr(i,a,n) for(int i=a; i<n; i++)
#define loop(i,a,n)	for(int i=a; i>=n; i--)
#define conti continue
#define brk break
#define pb push_back
#define SPEED ios_base :: sync_with_stdio(false), cin.tie(NULL), cout.tie(0)
#define in cin>>
#define out cout<<
#define endl <<"\n"
#define ll long long 
#define remove(a,b) a.erase(std::remove(a.begin(),a.end(),b),a.end())
#define runn(it,a) for(auto it = a.begin(); it!= a.end(); it++) 
#define fi first
#define se second

int main() 
{	
	int m; in m; while(m--){
	ll int a=0,b=0,c=0,d=0; 
	ll int e,f; in e;
	forr(i,0,e)	{in f; if(f%2==0)	a++; else b++;}
	ll int g,h; in g; 
	forr(i,0,g)	{in h; if(h%2==0)	c++; else d++;}
	out (a*c)+(b*d) endl;
	}
	return 0;	
}