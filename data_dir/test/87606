template <typename T>inline vector< T > Unique(vector< T > v) {
    sort(v.begin(), v.end());
    v.erase(unique(v.begin(), v.end()),v.end());
    return v;
}
#define rep(i, begin, end) for (__typeof(end) i = (begin) - ((begin) > (end)); i != (end) - ((begin) > (end)); i += 1 - 2 * ((begin) > (end)))
///error///
#define error(args...) {vector<string>_v=split(#args,',');err(_v.begin(),args);cout<<endl;}
vector<string> split(const string &s,char c) {
    vector<string>v;
    stringstream ss(s);
    string x;
    while(getline(ss,x,c))
        v.emplace_back(x);
    return move(v);
}

void err(vector<string>::iterator it) {}
template<typename T,typename... Args>
void err(vector<string>::iterator it,T a,Args...args) {
    cout<<it->substr((*it)[0]==' ',it->length())<< " = " << a << " ";
    err(++it,args...);
}
///This DEBUGGER-tool need c++11///
#define     Int         Int()
#define     Long        Long()
#define     pii         pair<int,int>
#define     ff          first
#define     ss          second
#define     pb            push_back
#define     eb            emplace_back
#define     all(x)      x.begin(),x.end()
#define     mem(a)      memset(a,0,sizeof a)
#define     memn(a)     memset(a,-1,sizeof a)

std::vector< ll > a , tree , lazy;

void build(int i , int b , int e){
    if(b == e){
        tree[i] = a[b] ;
        return ;
    }
    int mid = (b + e) >> 1 ;
    build(i << 1 , b , mid) ;
    build(i << 1 | 1 , mid + 1 , e) ;
    tree[i] = tree[i << 1] + tree[i << 1 | 1] ;
}

void propagate(int i , int b , int e){
    if(lazy[i] != -1){
        tree[i] = lazy[i] ;
        if(b != e){
            lazy[i << 1] = lazy[i << 1 | 1] = lazy[i] ;
        }
        lazy[i] = -1 ;
    }
}

void upd(int i , int b , int e , int l , int r , ll m){
    propagate(i , b , e) ;
    if(b > r or e < l)return ;
    if(l <= b and e <= r){
        tree[i] = m ;
        if(b != e){
            lazy[i << 1] = lazy[i << 1 | 1] = m ;
        }
        return ;
    }
    int mid = (b + e) >> 1 ;
    upd(i << 1 , b ,mid , l , r , m) ;
    upd(i << 1 | 1 , mid + 1 ,e , l , r , m) ;
    tree[i] = tree[i << 1] + tree[i << 1 | 1] ;
}

void point(int i , int b , int e , int p , ll c){
    propagate(i , b , e) ;
    if(b > p or p > e)return ;
    if(b == p and e == p){
        tree[i] -= c ;
        return ;
    }
    int mid = (b + e) >> 1 ;
    point(i << 1 , b , mid , p , c) ;
    point(i << 1 | 1 , mid + 1 , e , p , c) ;
    tree[i] = tree[i << 1] + tree[i << 1 | 1] ;
}

ll query(int i , int b , int e , int l,  int r){
    propagate(i , b , e) ;
    if(b > r or e < l)return 0 ;
    if(l <= b and e <= r){
        return tree[i] ;
    }
    int mid = (b + e) >> 1 ;
    return query(i << 1 , b , mid , l , r) + query(i << 1 | 1 , mid + 1 , e , l , r) ;
}

int main() {
    //freopen("input.txt","r",stdin);
    //freopen("output.txt","w",stdout);
    int t = 1 , tc = 0 ;
    while(t--) {
        int n = Int ;
        a.resize(n + 1) ;
        tree.resize(n << 2) ;
        lazy.resize(n << 2 , -1) ;
        for(int i = 1 ; i <= n ; i++){
            a[i] = Long ;
        }
        build(1 , 1 , n) ;
        int q = Int ;
        while(q--){
            int i = Int ;
            if(i == 1){
                int p = Int;
                ll x = Long ;
                int l = p , r = n , id = 0;
                ll sum = 0 ;
                while(l <= r){
                    int mid = (l + r) >> 1 ;
                    ll k = query(1 , 1 , n , p , mid) ;
                    if(k <= x){
                        l = mid + 1 ;
                        id = mid ;
                        sum = k ;
                    }
                    else r = mid - 1 ;
                }
                x -= sum ;
                //error(id , x , sum)
                if(id){
                    upd(1 , 1 , n , p , id , 0) ;
                }
                if(!id)id = p ;
                else id = id + 1 ;
                if(x and id <= n){
                    point(1 , 1 , n , id , x) ;
                }
                //error(tree[1] , id)
            }
            else{
                int p = Int ;
                ll k = query(1 , 1 , n , p , p) ;
                printf("%lld\n",a[p] - k);
            }
        }
    }
    return 0;
}