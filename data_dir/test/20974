#include <bits/stdc++.h>

using namespace std;

int n, m, s1, s2, a, b;
string s;

int main()
{
    cin >> s;
    for (int i=0;i<s.size();i++)
    {
        if (s[i]=='4')
            s1++;
        if (s[i]=='7')
            s2++;
    }
    if (s1==0 && s2==0)
    {
        cout << -1;
    }
    else if (s1>=s2)
    {
        cout << 4;
    }
    else
        cout << 7;
}
