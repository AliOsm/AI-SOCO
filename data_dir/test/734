int main()
{
    //use %I64d
    //read;
    int i, j, k, cnt=0, p[26], brk[110], n;
    char A[1000];

    for (i=0; i<26; i++) p[i]=0;
    for (i=0; i<110; i++) brk[i]=false;
    int len;
    in(n);

    scanf("%s", A);
    len=strlen(A);

    for (i=0; i<len; i++)
    {
        if (p[A[i]-'a']==0)
        {
            cnt++;
            if (cnt>n) break;

            p[A[i]-'a']++;
            if (i) brk[i]=true;
        }
    }

    if (cnt<n) {printf("NO\n"); return 0;}
    printf("YES\n");
    for (i=0; i<len; i++)
    {
        if (brk[i]==true) printf("\n");
        printf("%c", A[i]);
    }
    pn;

//file?
return 0;
}
