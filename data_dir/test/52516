int main(){
    cin>>n>>k;
    ll q=0,w=0;
    rep(i,1,n){
        scanf("%lld",&a);
        if(c[a]>=c[k])c[a]++;
        if(c[k]>c[q]){
            cout<<"-1"<<endl;
            exit(0);
        }
        if(c[a]>c[q])q=a;
    }
    cout<<q;
}