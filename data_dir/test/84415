#include<bits/stdc++.h>
#define F first
#define S second
using namespace std;
typedef long long ll;
const ll mod=1e9+7;
const int MX=3e5+9;
ll a,b,x,y,z,a1,b1;
int main(){
    cin>>a>>b>>x>>y>>z;
    a1+=2*x;
    a1+=y;
    b1+=3*z;
    b1+=y;
    cout<<max(0ll,b1-b)+max(0ll,a1-a)<<endl;
}
