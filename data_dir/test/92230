#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int arr[26];
int main()
{
    int n;
    cin>>n;
    int sum = 0;
    for(int i = 0 ; i<n ; ++i)
    {
        int x;
        scanf("%d" , &x);
        sum = max(x , sum);
    }
    cout<<max(0 , sum-25)<<endl;
    return 0;
}
