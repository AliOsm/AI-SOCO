#include <bits/stdc++.h>
using namespace std;

int n, m;
int ans = 0;
vector<vector<int> > arr;

int main (int argc, char *argv[])
{
	scanf("%d%d", &n, &m);
	arr.resize(m);
	for(int j = 0; j < m; ++j) {
		arr[j].resize(m + 1, 0);
	}
	for(int i = 0; i < n; ++i) {
		char s[5001];
		scanf("%s", &s);
		int cur = 0;
		for(int j = m - 1; j >= 0; --j) {
			if(s[j] == '0') {
				cur = 0;
			} else {
				cur++;
				arr[j][cur]++;
			}
		}
	}
	for(int j = 0; j < m; ++j) {
		int cur = 0;
		for(int i = m; i > 0; --i) {
			cur += arr[j][i];
			arr[j][i] = cur;
		}
	}
	for(int j = 0; j < m; ++j) {
		for(int i = 1; i <= m; ++i) {
			ans = max(ans, arr[j][i] * i);
		}
	}
	printf("%d", ans);
			
	return 0;
}

