#include <bits/stdc++.h>
using namespace std;
long long  i, j, n, c[123456],a[1111234], b[1125678],d[123456], l,ll,r, s, t, k,x, y,y1,z;
string p,q;
map <string,long long> m;
int main() {
    cin >>n>>p;
    for (i = 0; i < n-10; i ++){
        if (p[i] == '8')
            s ++;
        else t ++;
    }
    if (s > t)
    cout << "YES";
    else cout << "NO";
    //cout << s<<' '<<t;
    return 0;
}