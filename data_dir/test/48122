#include <iostream>
#include <stdio.h>
#include <math.h>
using namespace std;
int main()
{
    double n, a; cin >> n >> a;
    double deg = (n-2)*180;
    deg /= n; deg /= (n-2);
  //  cout << deg << "\n";
    double mn = 10000, ans = n, now = deg;
    for(double i = n; i >= 3; i--)
    {
        if(mn > fabs(a-now))
        {
            ans = i;
            mn = fabs(a-now);
        }
        now += deg;
    }
    cout << "1 2 " << ans << "\n";
    return 0;
}