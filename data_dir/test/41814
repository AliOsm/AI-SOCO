using namespace std;

#define MOD 1000000007LL
#define MAXN 100005

int n, t;
set<int> s;

int main(){
	ios_base::sync_with_stdio(0); cin.tie(0);

	cin >> n;
	for (int i = 0; i < n; i++) cin >> t, s.insert(t);
	
	if (s.size() < 2) cout << "NO\n";
	else {
		s.erase(s.begin());
		cout << *s.begin();
	}

}




