#define _CRT_SECURE_NO_WARNINGS

#include <iostream>
#include <cstdlib>
#include <ctime>
#include <cmath>
#include <cstring>
#include <string>
#include <vector>
#include <stdio.h>
#include <climits>
#include <Windows.h>
#include <iterator>
#include <algorithm>
#define all(a) a.begin(),a.end()

using namespace std;

typedef unsigned long long ull;
typedef long long ll;

void solve()
{
    int n, x, y;
    cin >> n;

    if (!(n % 2))
    {
        x = 4;
        y = n - 4;
    }
    else
    {
        x = 9;
        y = n - 9;
    }
    cout << x << ' ' << y;


}

int main()
{
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);

    solve();

    //cout << "\n\n"; system("pause");
    return 0;
}