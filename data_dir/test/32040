#include <bits/stdc++.h>

using namespace std;

void solve() {
  int a[3];
  cin >> a[0] >> a[1] >> a[2];
  sort(a, a + 3);
  if (a[2] > a[0] + a[1] + 1) {
    cout << "No";
  } else {
    cout << "Yes";
  }
}

int main() {
  ios::sync_with_stdio(0);
  cin.tie(0), cout.tie(0);

  int t;
  cin >> t;
  while (t--) {
    solve();
    cout << '\n';
  }

  return 0;
}