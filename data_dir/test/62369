#include <bits/stdc++.h>
using namespace std;
 
#define IOS ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define endl "\n"
#define int long long  

int32_t main()
{
	IOS;
	int t;
	cin>>t;
	while(t--)
	{
		int n;
		cin>>n;	
		cout<<1;
		int j=1;
		while(n)
		{
			if((j*(j-1))/2>n)
			{
				j--;
				break;
			}
			j++;
		}
		int left=n-j*(j-1)/2;
		cout<<"33";
		for(int i=0;i<left;i++)
			cout<<7;
		j-=2;
		while(j--)
			cout<<3;
		cout<<7;
		cout<<endl;
	}
}