#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define sort(a) sort(a.begin(),a.end())
#define pii pair<int,int>


int main(){
   
  ll n,k;
  cin>>n>>k;
  
  if(n==k){
   cout<<-1;
   return 0; }

   cout<<n-k<<" ";
   for(int i=2;i<=n-k;i++)
   cout<<i-1<<" ";
   
   for(int i=n-k+1;i<=n;i++)
   cout<<i<<" ";
}