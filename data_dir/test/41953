//ll combb[1001][1001];
int main()
{   
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int tests=1;
      //freopen("input.txt", "r", stdin);
    // cin>>tests;
     // freopen("output.txt", "w", stdout);
   while(tests--)
    {
        int n,m;
        cin>>n>>m;
        string s[n];
        for(int i = 0;i<n;i++)
            cin>>s[i];
        if(n==1){
            cout<<0;
            exit(0);
        }
        else
        {
            int count = 0;
          for(int i = 0;i<s[0].size();i++)
          {
            for(int j = 0;j<n-1;j++)
            {
                if(s[j][i] <= s[j+1][i])
                    continue;
                else if(s[j] > s[j+1])
                {
                    for(int k = 0;k<n;k++){
                        s[k].erase(s[k].begin()+i);
                    }
                    count++;
                    i--;
                    break;
                }
            }
          }
          cout<<count;
        }
    }

}


       




    


