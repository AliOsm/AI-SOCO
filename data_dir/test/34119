using namespace std;

#define IOS ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0)
#define ll long long
#define PI 3.1415926535
#define pb push_back
#define ins insert
#define mp make_pair
#define ff first
#define Ln length
#define ss second
#define endl "\n"
#define MOD 1000000007;
const ll N = 1e5 + 7;

// endl can result to TLE sometimes;

int main() 
{
    IOS;
    int n,x;
    cin >> n;
    set <int> s;
    for(int i=0; i<n; ++i)
    {
    	cin >> x;
    	s.ins(x);
    }
    if(s.size()<=1)
    {
    	cout << "NO\n";
    	return 0;
    }
    set <int> :: iterator it;
    it = s.begin();
    it++;
    cout << *it << endl;
 	return 0;   
}