#include <bits/stdc++.h>

typedef long long ll;
using namespace std;

int main(){
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    int k,r;
    cin >> k >> r;
    for(int i=1;;i++){
        if(i*k%10==0 || i*k%10==r){
            cout << i << endl;
            return 0;
        }
    }

    return 0;
}
