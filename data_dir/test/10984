#include<bits/stdc++.h>
using namespace std;




int main(){
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    string t, s;
    cin >> t;
    for(int i = 1; i <= 5; i++){
        cin >> s;
        if(s[0] == t[0] || s[1] == t[1]){
            return cout << "YES", 0;
        }
    }
    cout << "NO";
}
