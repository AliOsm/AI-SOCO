#include<bits/stdc++.h>
using namespace std;
#define ll  long long
#define endl '\n'
const ll mod = 1000000007;
const double PI = 3.141592653589793238463;
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
    int x;
    cin>>x;
    int c=1;
    while (x>3){
        for (int i=1;i<=(x+1)/2;i++)
            cout<<c<<" ";
        x/=2;
        c*=2;
    }
    if (x==3)
        cout<<c<<" "<<c<<" "<<3*c<<endl;
    else if (x==2)
        cout<<c<<" "<<2*c<<endl;
    else
    cout<<c<<endl;
    return 0;

}
