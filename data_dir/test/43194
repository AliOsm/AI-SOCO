#include<bits/stdc++.h>
using namespace std;
long long a,b,c,d,i,e,f,g,n,m,k,l;
int main()
{
           cin>>a>>b>>c;
           k+=a;
           k-=b;
           if(k+c<0) cout<<'-';
           else if(k-c>0) cout<<'+';
           else if(k==0 && c==0) cout<<0;
           else cout<<'?';
}