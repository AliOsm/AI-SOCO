#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
#define se second
#define fi first
#define pb push_back

int main(){

//	freopen("in.txt","r",stdin);

	int n,x;
	cin >> n >> x;
	int ans=0,c=0;
	for(int i=0,a;i < n;i++){
		scanf("%d",&a);
		if(a < x) c++;
		else if(a == x) ans++;
	}
	ans+=x - c;
	cout << ans << endl;

	return 0;
}
