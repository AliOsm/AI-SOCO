#include <bits/stdc++.h>
using namespace std;
int main() {
    int test;
    cin >> test;
    while(test--) {
        int n, i;
        string str;
        cin >> n >> str;
        for(i = 0; i < n && !(str[i] == '8'); i++);
        if(n - i >= 11) cout << "YES\n";
        else cout << "NO\n";
    }
    return 0;
}