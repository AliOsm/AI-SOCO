#include <bits/stdc++.h>
#define pb push_back
#define mp make_pair
using namespace std ;

int main () {
    string s ;
    cin >> s ;
    int a = 0 , b = 0 ;
    for (int i = 0 ; i < s.size() ; i++) {
        if (s[i] == 'o') a++ ;
        else b++ ;
    }
    if (a == 0 or b%a == 0) {
        cout << "YES" ;
    }
    else cout << "NO" ;
}


