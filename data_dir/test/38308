#include <bits/stdc++.h>
#define ll long long
#define ld long double
#define pb push_back
#define f first
#define s second
#define y1 bbp
#define sz size()
using namespace std;
ll const N=1e6+7,inf=1e9+7;
ll n,m,a[N],ans,x,b[N],suf[N],y,k,qq,q,kk,r,x1,y1;
bool us[N];
char ch;
string s,t;
main()
{
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    cin>>qq;
    for (int ii=1;ii<=qq;ii++)
    {
        cin>>s;
        if (s[s.sz-1]=='o') cout<<"FILIPINO";
        else if (s[s.sz-1]=='a') cout<<"KOREAN"; else cout<<"JAPANESE";
        cout<<endl;
    }
}
