#include <iostream>
#include <algorithm>
#include <set>
#include <vector>
#include <map>
#include <queue>
#include <string>
#include <cstring>
#include <cstdio>
// Kobe is the GOAT
// DON'T DIVIDE
// Greedy, Prefix, Sets/Bsearch, Dfs/bfs, DP
// Stack, Bitmask, Dsu 
using namespace std;

int n, h;

int main()
{
	ios::sync_with_stdio(0);
	cin >> n >> h;

	int ans = 0;
	for(int i = 0; i < n; i++)
	{
		int a;
		cin >> a;
		if(a > h)
			ans += 2;
		else
			ans++;
	}

	cout << ans << "\n";
	return 0;
}
