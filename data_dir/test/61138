#include <bits/stdc++.h>

using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    int test;
    cin >> test;
    while (test-- > 0) {
        int x;
        cin >> x;
        cout << 1 << " " << (x - 1) << "\n";
    }

    return 0;
}