#include <bits/stdc++.h>
#define int long long
using namespace std;

int32_t main() {
        int n, m; cin >> n >> m;
        cout << max(0LL, n - 2 * m) << " " ;
        for(int i = 0; i <= n; i++) {
                if(i * (i - 1) / 2 >= m) {
                        cout << n - i << endl;
                        return 0;
                }
        }
        cout << 0 << endl;
}


