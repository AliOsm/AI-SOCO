#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef long double ld;
#define pb push_back
#define mp make_pair


int n;
int main(){
    ios::sync_with_stdio(0);
    cin.tie(0);
    
    int t; cin >> t;
    while(t--){
    	cin >> n;
    	int sec = 1;
    	for(int i = 0; i < n; i++){
    		int l, r; cin >> l >> r;
    		if(sec <= r){
    			if(sec < l) sec = l;
    			cout << sec++ << " ";
    		}
    		else{
    			cout << 0 << " ";
    		}
    	}
    	cout << endl;
    }

    return 0;
}













