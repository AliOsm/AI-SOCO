#include<bits/stdc++.h>
using namespace std;
#define ll           unsigned long long

int main()
{
    ll x,y;
    cin >> x >> y;
    //cout << pow(x,y*1.0) << " " << pow(y,x*1.0) << endl;
    if(x == y)
    {
        cout << "=" << endl;
        return 0;
    }
    else if(x == 2)
    {
        if(y == 3)
            cout << "<" << endl;
        else if(y == 4)
            cout << "=" << endl;
        else
            cout << ">" << endl;
    }
    else if(y == 2)
    {
        if(x == 3)
            cout << ">" << endl;
        else if(x == 4)
            cout << "=" << endl;
        else
            cout << "<" << endl;
    }
    else if(x == 1)
        cout << "<" << endl;
    else if(y == 1)
        cout << ">" << endl;
    else if(x>y)
        cout << "<" << endl;
    else
        cout << ">" << endl;
}
