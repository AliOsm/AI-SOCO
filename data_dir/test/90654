#include <bits/stdc++.h>
using namespace std;

int b, k;

int main(){

    scanf("%d %d", &b, &k);

    int sum = 0, act = 0;
    for(int i = 0; i< k; i++){
        scanf("%d",&act);
        sum += act;
    }

    if(b % 2 == 0){
        printf("%s\n", act % 2 == 0 ? "even" : "odd");
    }
    else{
        printf("%s\n", sum % 2 == 0 ? "even" : "odd");
    }

    return 0;
}
