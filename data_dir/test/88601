#include <bits/stdc++.h>
#define int long long
using namespace std;
    long long powmod(long long a,long long b) {
        if (b==0) return 1; if (b==1) return a; 
       
        if (b%2==0) { long long k=powmod(a,b/2); return (k*k); }
        else {long long k=powmod(a,b/2); return  (k*k) *a; }
    }
    long long gcd(long long a, long long b) {
        if (a==0) return b;
        if (b==0) return a;
        if (a>b) return gcd(a%b,b); else return gcd(b%a,a);
    }
    int prime(int p) { // 1 - простое
        for (int i=2;i*i<=p;i++) {
            if (p%i==0 && i<p) return i;  
        }
        return 1;
    }
   
 
    void solve (int ppppppppp=1) {
       
       
         /* --------- */
     
int a,b,c;
cin>>a>>b>>c;
int ans[a];
for (int i=0;i<a;i++) ans[i]=2;
for (int i=0;i<b;i++) {
    int x; cin>>x; x--; ans[x]=1; 
}
for (int i=0;i<a;i++) cout<<ans[i]<<" "; 
        /* --------- */  
          return;
    }
     
     
     
    signed main() {
       
         ios_base::sync_with_stdio(0);
        cin.tie(0);
        cout.tie(0);
       
       
          int tututu;
          tututu=1;
       // cin>>tututu; // если нет запросов, то закоментить
