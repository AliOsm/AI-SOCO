#include<bits/stdc++.h>
#define ll long long
#define ld long double
#define fi first
#define se second
#define mp make_pair
#define pb push_back
#define pp pop_back
#define pii pair<int,int>
#define pll pair<ll,ll>
#define y1 gfgfg

using namespace std;
int k,n,i;
int main()
{
    cin >>k>>n;
    for (i=0;i<=9;++i)
    {
        if (((i+1)*k)%10==n||((i+1)*k)%10==0)return
         cout <<i+1,0;
    }
    cout <<10;
}


