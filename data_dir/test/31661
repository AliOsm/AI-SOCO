int main(){
	string s,s1;
	cin>>s>>s1;
	i=0;
	while(s[i]==s1[i])i++;
	j=s1.size()-1;
	while(s[j+1]==s1[j])j--;
	//cout<<i<<" "<<j;
	if(i>j)cout<<i-j<<'\n';
	else cout<<0<<'\n';
	for(int k=j+1;k<=i;k++) cout<<k+1<<" ";
}