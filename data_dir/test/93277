#include <iostream>
#include <cmath>
#include <algorithm>
#include <iomanip>
#include <cstdio>
#include <map>
#include <string>
#include <set>
#include <vector>
#include <stack>
#include <deque>
#include <queue>
#include <ctime>
#include <list>
#include <iterator>
#define ll int
#define ss second
#define ff first
#define pb push_back
using namespace std;
ll n,m,x,y,k,a[200],b[200],t;
vector <ll> v;
void bt(ll xx,ll kk){
    if(kk==k){
        v.pb(xx);return;
    }
    ll yy=xx;
    for(int i=2;i<=xx;i++){
        if(xx%i==0){
            yy=i;break;
        }
    }
    v.pb(yy);
    bt(xx/yy,kk+1);
}
int main(){
   //ios::sync_with_stdio(false) , cin.tie(0) , cout.tie(0);
	cin>>n>>k;
	bt(n,1);
	if(v[k-1]==1)return cout<<-1,0;
	for(int i=0;i<v.size();i++)cout<<v[i]<<' ';
	return 0;
}