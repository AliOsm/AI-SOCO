/// NOTHING CAN STOP ME I'M ALL THE WAY UP
#include <bits/stdc++.h>

#define ss second
#define ff first
#define pb push_back
#define faster ios::sync_with_stdio(0), cin.tie(0), cout.tie(0)

using namespace std;

typedef long long ll;
typedef long double ld;

const long long N=1100,mod=1e9+7,mod2=1e9+9,mod3=998244353,sq=450,base=727,lg=25,inf=1e18+10;

ll n,m,x,y,w,z,t,k,d,q,ans,a[N][N],dp1[N][N],dp2[N][N],dp3[N][N],dp4[N][N];
///            12
///            43
int main(){
    faster;
    cin>>n>>m;
    for(int i=1;i<=n;i++)for(int j=1;j<=m;j++)cin>>a[i][j];
    for(int i=1;i<=n;i++)for(int j=1;j<=m;j++)dp1[i][j]=max(dp1[i][j-1],dp1[i-1][j])+a[i][j];
    for(int i=1;i<=n;i++)for(int j=m;j>=1;j--)dp2[i][j]=max(dp2[i][j+1],dp2[i-1][j])+a[i][j];
    for(int i=n;i>=1;i--)for(int j=1;j<=m;j++)dp4[i][j]=max(dp4[i][j-1],dp4[i+1][j])+a[i][j];
    for(int i=n;i>=1;i--)for(int j=m;j>=1;j--)dp3[i][j]=max(dp3[i][j+1],dp3[i+1][j])+a[i][j];
    for(int i=2;i<=n-1;i++)for(int j=2;j<=m-1;j++)
    ans=max(max(
        dp1[i-1][j]+dp3[i+1][j]+dp2[i][j+1]+dp4[i][j-1],
        dp1[i][j-1]+dp3[i][j+1]+dp2[i-1][j]+dp4[i+1][j]),ans);
    cout<<ans;
    return 0;
}
