#include <iostream>
using namespace std;

int main()
{
#ifndef ONLINE_JUDGE
  freopen("input.txt", "rt", stdin);
  freopen("output.txt", "wt", stdout);
#endif

  int n;
  cin >> n;

  int k = 1;
  while(k)
  {
    if(n == k)
    {
      cout << "YES";
      return 0;
    }
    k <<= 1;
  }

  cout << "NO";

  return 0;
}