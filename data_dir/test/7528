#include<cstdio>
#include<cstdlib>
#include<cstring>
#include<cmath>
#include<iostream>
#include<algorithm>
#include<vector>
#include<queue>
#include<stack>
#include<list>
#include<set>
#include<map>
#include<string>
#include<utility>
using namespace std;
#define REP(i,FN) for(int i=0;i<FN;i++)
#define FOR(i,ST,FN) for(int i=ST;i<=FN;i++)
#define FORD(i,FN,ST) for(int i=FN;i>=ST;i--)
#define FORX(i,c) for(typeof(c.begin())i=c.begin();i!=c.end();i++)
#define pause system("pause")
#define S scanf
#define P printf
#define X first
#define Y second
#define pb push_back
#define PII pair<int,int>
#define mp make_pair
#define sz size()
#define eps 1e-8

const int MOD(1000000007);
const int INF((1<<30)-1);
const int MAXN();

vector<PII>a;

int main(){

    int n,k;
    S("%d%d",&n,&k);
    REP(i,n)
    {
        int x;
        S("%d",&x);
        a.pb(PII(x,i+1));
    }

    sort(a.begin(),a.end());

    vector<int>ans;

    REP(i,a.sz)
    {
        if(a[i].X > k) break;
        k -= a[i].X;
        ans.pb(a[i].Y);
    }

    P("%d\n",ans.sz);
    REP(i,ans.sz)
        P("%d ",ans[i]);
}
