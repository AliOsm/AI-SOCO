#include<bits/stdc++.h>
using namespace std;
#define forr(i,a,n) for(int i=a; i<n; i++)
#define loop(i,a,n)	for(int i=a; i>=n; i--)
#define conti continue
#define brk break
#define pb push_back
#define SPEED ios_base :: sync_with_stdio(0), cin.tie(0), cout.tie(0)
#define in cin>>
#define out cout<<
#define endl <<"\n"
#define ll long long 

int main() 
{
	SPEED;
	
	int a[4],b=0,c=0,d=0;
	forr(i,0,4)	in a[i];
	sort(a,a+4);
	b=a[3]-a[0];
	c=a[3]-a[1];
	d=a[3]-a[2];
	out b<<" "<<c<<" "<<d endl;
	
	return 0;
}