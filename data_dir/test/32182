#include <bits/stdc++.h>

using namespace std;

const int N = 3e5 + 5;

int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);

  int t;
  cin>>t;
  while(t--)
  {
    long long x,y;
    cin>>x>>y;
    x-=y;
    if(x == 1) printf("NO\n");
    else printf("YES\n");
  }

  return 0;
}