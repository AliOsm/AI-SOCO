#include <bits/stdc++.h>

#define ll long long
#define fr first
#define sc second
#define mp make_pair
#define N (int) (5e3 + 10)

using namespace std;
int l[3];
int main(){
    int n;
    cin >> n;
    for(int i=1;i<=n;i++){
        int a;
        cin >> a;
        l[a]=i;
    }
    cout << min(l[0],l[1]);
}

