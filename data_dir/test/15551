#include <algorithm>
#include <cmath>
#include <cstdio>
#include <cstdlib>
#include <cstring>
#include <iostream>
using namespace std;

int main(void)
{
    int T, S, q;

    scanf("%d %d %d", &T, &S, &q);

    printf("%.0lf\n", ceil(log10((double)T / S) / log10(q)));

    return 0;
}