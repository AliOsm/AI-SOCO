#include <bits/stdc++.h>
typedef long long ll;
using namespace std;
void solve(){
    ll n,cnt=0;cin>>n;
    ll a[n];for(ll i=0;i<n;i++)cin>>a[i],cnt+=(a[i]&1);
    cout<< (cnt==0 || cnt==n ? "YES\n" : "NO\n");
}
int main(){
    cin.tie(NULL); cout.tie(NULL);
    ios_base:: sync_with_stdio(false);
    ll t;cin>>t;
    while(t--)solve();
}


