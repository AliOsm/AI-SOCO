#include "bits/stdc++.h"
using namespace std;
const int N = 2e5 + 5, M = 1e7 + 5;
int n, k;
int a[N];
int ans[N];
set<int> in;
int go[] = {1,0,0,1,2,0,2};
int main(){
    int x1,x2,x3,x4;
    cin >> x1 >> x2 >> x3 >> x4;
    int o[] = {x1,x2,x3,x4};
    sort(o,o+4);
    tie(x1,x2,x3,x4) = tie(o[0],o[1],o[2], o[3]);
    int a = x4 - x3;
    int b = x4 - x2;
    int c = x4 - x1;
    cout << a << ' ' << b << ' ' << c << "\n";

  return 0;
}
