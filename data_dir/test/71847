#include <bits/stdc++.h>
using namespace std;

#define int long long
#define f(i,n) for(int i=0;i<n;i++)
#define rep(i,a,b) for(int i=a;i<=b;i++)

class CInnaAndHugeCandyMatrix {
public:
    int n, m, x, y, z, p;
	vector<pair<int, int>> arr;

	void C(int t){
		while(t--){
			for(auto &w: arr){
				int ni = w.second;
				int nj = n - w.first + 1;
				w.first = ni;
				w.second = nj;
			}
			swap(n, m);
		}
	}
	void H(){
		for(auto &w: arr) w.second = m - w.second + 1;
	}
	void solve(std::istream& in, std::ostream& out) {
		in >> n >> m >> x >> y >> z >> p;
		f(i, p){
			int a, b; in >> a >> b;
			arr.emplace_back(a, b);
		}
		x = x%4;
		y = y&1;
		z = (4-z%4)%4;
		C(x);
		if(y) H();
		C(z);
		for(auto w: arr) out << w.first << ' ' << w.second << '\n';
	}
};


int32_t main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
	CInnaAndHugeCandyMatrix solver;
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
	return 0;
}
