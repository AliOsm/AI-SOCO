#include<bits/stdc++.h>
#define GO ios::sync_with_stdio(0), cin.tie(0), cout.tie(0)
using namespace std;
typedef long long ll;
typedef vector<int> vec;
const int N = 3e5 + 5;
const int M = 7;
const int Mod = 1e9 + 7;

int main()
{
	GO;
	int t;
	cin >> t;
	while (t--)
	{
		int n;
		cin >> n;
		vec a(n);
		for (auto& i : a)
			cin >> i;
		sort(a.begin(), a.end());
		reverse(a.begin(), a.end());
		for (auto i : a)
			cout << i << ' ';
		cout << '\n';
	}
	return 0;
}