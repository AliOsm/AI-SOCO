#include <bits/stdc++.h>

#define ll long long
#define fr first
#define se second

using namespace std;

const long long N = 1e5 + 7;

ll res, n, m, cnt, q = 1 , ans;
string s, str;
ll used[N];
vector< string > v;
int main()
{
    ll T;
    cin >> T;
    while ( T -- ){
        cin >> n;
        cnt = 3 , ans = 4;
        while( true ){
            if( n % cnt == 0 ){
                cout << n / cnt << endl;
                break;
            }
            cnt+=ans;
            ans*=2;
        }
    }

}
