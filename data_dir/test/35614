#include<bits/stdc++.h>â€¬
using namespace std;
int n,m,k,c,d,sum,ans;
int main()
{
    cin>>c>>d>>n>>m>>k;
    ans=((n*m-k)/n)*c;
    if((n*m-k)%n)ans+=c;
    //cout<<ans<<"\n";
    for(int i=0;i*n<n*m-k;i++)
    {
        ans=min(ans,i*c+(n*m-k-i*n)*d);
    }
    if(ans<0)ans=0;
    cout<<ans<<endl;
}


