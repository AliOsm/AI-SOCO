#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    ll n,i,j;
    cin>>n;
    set<ll> s;
    for(i=0;i<n;i++)
    {
        ll a;
        cin>>a;
        if(a!=0)
            s.insert(a);
    }
    cout<<s.size()<<endl;
}
