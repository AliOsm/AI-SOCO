//Bismillah
#include <bits/stdc++.h>
using namespace std; 
#define FOR(i,begin,end) for (int i=begin;i<=end;i++)
#define rep(i,t) for (int i=0;i<t;i++)
#define pb push_back
#define mp make_pair
#define endl '\n'
typedef long long ll;

int main()
{
    ios_base::sync_with_stdio(false);
    string s;
    cin>>s;
    vector <int> v1,v2;
    rep(i,s.size())
        if (s[i]=='l')
            v1.pb(i+1);
        else
            v2.pb(i+1);
    rep(i,v2.size())
        cout<<v2[i]<<" ";
    reverse(v1.begin(),v1.end());
    rep(i,v1.size())
        cout<<v1[i]<<" ";
    cout<<endl;

    return 0;
}