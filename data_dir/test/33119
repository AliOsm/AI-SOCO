/* In The Name Of God */
#include <bits/stdc++.h>

# define xx first
# define yy second
# define pb push_back
# define pp pop_back
# define eps 1e-9

using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
typedef vector<int> vint;
int suma,sumb;
int main(){
	ios_base::sync_with_stdio (0);cin.tie(0);
	int n;cin>>n;
	for(int i=1 ; i<=n ; i++){
		int a,b;
		cin>>a>>b;
		if(abs(suma+a-sumb) <= 500){
			cout<<'A';
			suma += a;
		}
		else{
			cout<<'G';
			sumb += b;
		}
	}
	cout<<endl;
	return 0;
}
