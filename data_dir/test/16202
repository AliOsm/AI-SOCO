#include <bits/stdc++.h>

using namespace std;
typedef long long intt;

int main()
{
    intt re=0,gr=0,bl=0;
    intt n,k;
    cin>>n>>k;
    re=n*2;
    gr=n*5;
    bl=n*8;
    intt res=re/k+(re%k!=0);
    res+=(gr/k)+(gr%k!=0);
    res+=(bl/k)+(bl%k!=0);
    cout<<res;
    return 0;
}
