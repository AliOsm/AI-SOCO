#include<iostream>
#include<math.h>
#include<algorithm>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.precision(20);
    int n,m,k,x,y,z;
    cin>>n>>m>>k;
    x=ceil((1.0*k)/(2*m));
    y=(x-1)*(2*m)+1;
    z=(k-y)/2+1;
    
    if(k%2==1)
     cout<<x<<" "<<z<<" "<<'L';
    else
    cout<<x<<" "<<z<<" "<<'R';}
    