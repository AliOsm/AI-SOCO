#include<bits/stdc++.h>
using namespace std;

#define si(n) scanf("%d",&n)
#define MAX 200005
#define ll long long

int main()
{
    //freopen("input.txt","r",stdin);
    int i,j,n,m;
    cin>>n>>m;
    string a="9",b="";
    bool flag=true;
    for(i=2;i<=2230;i++){
        if(flag)a+="1";
        else a+="8";
        flag=!flag;
    }
    flag=true;
    for(i=1;i<=2230;i++){
        if(flag)b+="1";
        else b+="8";
        flag=!flag;
    }
    reverse(a.begin(),a.end());
    reverse(b.begin(),b.end());
    cout<<a<<endl<<b<<endl;


    return 0;
}
