//besmellah
#include <bits/stdc++.h>
using namespace std;

int main(){
    unsigned short n, tmp;
    cin >> n;
    for (unsigned short i = 0; i < n; i++){
        cin >> tmp;
        if (tmp == 1){
            cout << "HARD";
            return 0;
        }
    }
    cout << "EASY";
}