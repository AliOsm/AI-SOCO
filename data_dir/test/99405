#include<bits/stdc++.h>
#define ll long long
using namespace std;
map<ll,ll> m;
int32_t main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
    ll n,c;cin>>n;ll a[n];
    for(ll i=0;i<n;i++)
        cin>>a[i],m[a[i]]++;
    for(auto it=m.begin();it!=m.end();it++)
        if(it->second%2)
    {
         cout<<"Conan";return 0;
    }
    cout<<"Agasa";
}
