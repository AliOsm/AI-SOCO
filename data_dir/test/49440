#include <cassert>
#include <iomanip>
#include <random>
  
using namespace std;



#define ll long long
#define ld long double
#define null NULL
#define prev prev228
#define index index228
#define left left228
#define right right228
#define hash hash228



int sumd(int n) {
    int res = 0;
    while (n) {
        res += n % 10;
        n /= 10;
    }
    return res;
}


signed main() {
    int k;
    cin >> k;
    int res = 1;
    for (int i = 1; i <= 1100 * 1000 * 10 + 228; ++i) {
        if (sumd(i) == 10) {
            if (res == k) {
                cout << i << endl;
                return 0;
            }
            ++res;
        }
    }
    cout << res << endl;
    return 0;
}








