#include <iostream>
#include <algorithm>
#include <set>
#include <vector>
#include <string>
#include <cstring>
// DON'T DIVIDE
// Try different approaches
using namespace std;

int n;
const int MAXN = 1000005;
int a[MAXN];

int main()
{
	ios::sync_with_stdio(0);
	cin >> n;

	for(int i = 0; i < n; i++)
		cin >> a[i];
	sort(a, a + n);
	cout << a[(n - 1) / 2] << "\n";

	return 0;
}