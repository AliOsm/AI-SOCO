/*
0611A. New Year and Days
*/

#include<bits/stdc++.h>
using namespace std;

#define ll long long

int main(){
  int x;
  string s, t;
  cin >> x >> s >> t; 
  if(t[0] == 'm'){
    if(x < 30){cout << 12;}
    else if(x < 31){cout << 11;}
    else if(x == 31){cout << 7;}
  }
  else{
    if((x == 5) || (x == 6)){cout << 53;}
    else{cout << 52;}
  }
}