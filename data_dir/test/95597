#include <bits/stdc++.h>
#define F first
#define S second
using namespace std;
typedef long long ll;
typedef pair<ll, ll>pll;
int main()
{
    ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    ll tc;
    cin >> tc;
    while(tc--)
    {
        ll x;
        cin >> x;
        cout << 1 << ' ' << x - 1 << '\n';
    }
    return 0;
}
