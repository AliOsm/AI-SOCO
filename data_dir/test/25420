/*jai mata di
 * let's rock*/
#include<bits/stdc++.h>
using namespace std;
const int N=100004;
char s[N];
int main()
{
	int n;
	scanf("%d",&n);
	scanf("%s",s+1);
	int cl=0,cr=0;
	int i;
	for(i=1;i<=n;i++)
	{
		if(s[i]=='L') cl++;
		else cr++;
	}
	printf("%d\n",cr+cl+1);
	return 0;
}
