 #include <bits/stdc++.h>
 #define f first
 #define s second
 #define endl '\n'
 #define LL long long
 #define pb push_back
 #define sz(a) (int)a.size()
 #define all(a) a.begin(),a.end()
 #define rall(a) a.rbegin(),a.rend()
 #define debug(x) cerr << #x << " is " << x << endl;
 using namespace std;
 int const MAXN = 2e6 + 9;
 LL get (LL B){
    int ret = 0;
    LL x = 9;
    while (x <= B){
        ret++;
        x = x * 10 + 9;
    }
    return ret;
 }
 int main(){
    ios_base::sync_with_stdio (0),cin.tie(0);
    int T;
    cin >> T;
    while (T--){
        LL A, B;
        cin >> A >> B;
        cout << A * get(B) << endl;
    }
 }
