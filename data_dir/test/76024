#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
typedef long double ld;

void solve() {
    int n;
    cin >> n;
    set<set<char> > a;
    for (int i = 0; i < n; ++i) {
        string s;
        cin >> s;
        set<char> cur;
        for (char c : s) cur.insert(c);
        a.insert(cur);
    }
    cout << a.size() << endl;
}

int main() {
    //    freopen("input.txt", "r", stdin);
    ios_base::sync_with_stdio(false);
    int test;
    //    cin >> test;
    test = 1;
    for (int tst = 1; tst <= test; ++tst) {
        solve();
    }
    return 0;
}