#include<bits/stdc++.h>
using namespace std;
#define fast ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);


int main(){
	fast;
	bool k=true;
	string s;
	cin>>s;
	for(int i=0;i<s.length();i++){
		if(k&&i==s.length()-1){
			return 0;
		}
		if(s[i]=='0'&&k){
			k=false;
		}
		else{
			cout<<s[i];
		}

	}
	return 0;
}