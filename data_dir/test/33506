#include <bits/stdc++.h>

#define pb push_back
#define pob pop_back
#define ll long long
#define pll pair < ll, ll >
#define fi first
#define se second
#define sefi second.first
#define sese second.second

using namespace std;

ll n, a[10101010], b[101000];
int main(){
    cin >> n;
    for(int i = 1; i <= n; i++){
        cin >> a[i];
    }
    for(int i = 1; i <= n; i++){
        b[i] = a[i] + a[i + 1];
    }
    for(int i = 1; i <= n; i++){
        cout << b[i] << " " ;
    }
    return 0;
}
