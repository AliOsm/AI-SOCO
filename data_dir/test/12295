#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n, t;
    cin >> n >> t;
    long long sum = 0;
    for(int i = 1;i <= n;i++){
        int a;
        cin >> a;
        sum += 86400 - a;
        if(sum >= t){
            cout << i;
            return 0;
        }
    }
}
