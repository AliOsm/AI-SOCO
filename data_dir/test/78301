#include <iostream>
#include <vector>
#include <set>
#include <algorithm>
#include <tuple>
#include <iomanip>
#include <cmath>
using namespace std;
typedef long long int ll;
typedef pair<int, int> pii;
typedef vector<int> vint;
typedef long long int ll;



int main() {
    ll n, m;
    cin >> n >> m;
    ll val = 1;
    for (int i = 0; i < n; i++) {
        val *= 2;
        if (val > m)
            break;
    }
    cout << m % val << endl;
}
