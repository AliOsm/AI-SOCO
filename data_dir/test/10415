#include <bits/stdc++.h>
using namespace std;

using ll = long long;
const int sz = 1e5 + 10;
const int mod = 1000007;
int n, sum, ans;
int ar[105];

int main() {
        #ifdef CLown1331
                freopen("in.txt", "r", stdin);
        #endif /// CLown1331

        while( cin >> n ) {

                sum = 0;

                for( int i=0; i<n; i++ ) cin >> ar[i], sum += abs( ar[i] );

                cout << sum << "\n";

                cerr << "-----\n";
        }

        return 0;
}
