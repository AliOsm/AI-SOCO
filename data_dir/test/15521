#include<bits/stdc++.h>

using namespace std;

#define ll long long int

int main()
{
    ll n,i,j;

    cin>>n;

    ll cur=1,ans=0;

    while(1)
    {
        if(cur>n)
        {
            break;
        }

        cur*=2;

        ans++;


    }

    cout<<ans<<endl;
}
