#include <bits/stdc++.h>
using namespace std;
const int MaxN = 1e5 + 17;
int n, k, b;
string S, s, SS;
int main ()
{
    ios_base :: sync_with_stdio (0);
    #ifndef ONLINE_JUDGE 
        freopen (".in", "r", stdin); 
    #endif 
    cin >> n;
    for (int i = 1; i <= n; ++ i)
        cin >> s, S += "<3" + s;
    S += "<3";
    cin >> s;
    for (int i = 0; i < s.size(); ++ i)
        if (s[i] == S[k])
            k ++;
    if (k != S.size())
        cout << "no";
    else
        cout << "yes";
    return 0;
}