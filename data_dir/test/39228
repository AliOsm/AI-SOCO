#define output freopen (fname".out", "w", stdout)
//#define int ll
#define N 300000

using namespace std;

const int inf = 1e9 + 123;
const ll INF = 1e18 + 123;
const double pi = acos (-1.0);
const ld eps = 1e-3;

int n;

main ()
{
	speed;
