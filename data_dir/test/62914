#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
typedef pair < int, int > ii;

int main() {

	// freopen("in.txt", "r", stdin);
	// freopen("out.txt", "w", stdout);

	int n;

	cin >> n;

	map < string, int > M;

	while(n--) {
		string s;
		cin >> s;
		puts(M.count(s) ? "YES" : "NO");
		M[s] = 1;
	}

    return 0;

}

