#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef long double ld;
#define F first
#define S second
#define ii pair <int,int>

string ans,curr;

int main()
{
    cin>>ans;
    for(int i=0;i<5;i++)
    {
        cin>>curr;
        if(ans[0]==curr[0] || ans[1]==curr[1])
        {
            cout<<"YES";
            return 0;
        }
    }

    cout<<"NO";
}
