/// إِنَّ مَعَ الْعُسْرِ يُسْرًا
/// لَا أَبْرَحُ حَتَّىٰ أَبْلُغَ

#include <bits/stdc++.h>
#define LL long long
#define LD long double
#define S second
#define F first
#define IO ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

using namespace std;

int main()
{
    IO
    LL n,k;
    cin>>n>>k;
    while(k--)
    {
        if(n%10!=0)
            n--;
        else
            n/=10;
    }
    cout<<n;





    return 0;
}

