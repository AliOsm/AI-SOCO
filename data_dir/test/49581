#include <bits/stdc++.h>

using namespace std;

int main()
{
	int a,b,c,d;
	cin >> a >> b >> c >> d;
	if(c)
	{
		if(a==0||d==0)
		{
			cout << 0 << endl; return 0;
		}
		if(a!=d) {cout << 0 << endl; return 0;}
	}
	if(a!=d){cout << 0 << endl; return 0;}
	cout << 1 << endl;
	return 0;
}