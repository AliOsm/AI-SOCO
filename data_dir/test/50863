 #include <iostream>
#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define inf 1000000000000
#define mod 1000000007
#define pb push_back
#define mp make_pair
#define all(v) v.begin(),v.end()
#define S second
#define F first
#define boost1 ios::sync_with_stdio(false);
#define boost2 cin.tie(0);
#define mem(a,val) memset(a,val,sizeof a)
#define endl "\n"
#define maxn 100001

ll a[maxn];

int main()
{
	boost1;boost2;	
	ll i,j,n,x,y,f=0,ans=0,lane,m,k;
	char ch;
	string s;
	cin>>n>>m>>k;
	x=2*m;
	if(k%x==0)
	lane=k/x;
	else
	lane=k/x+1;
	y=k%x;
	if(y==0)
	y=2*m;
	if(y%2==0)
	y=y/2;
	else
	y=y/2+1;
	cout<<lane<<" "<<y<<" ";
	if(k%2)
	cout<<"L";
	else
	cout<<"R";
	return 0;
}
