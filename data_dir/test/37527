int main()
{
	ios_base::sync_with_stdio(false);
	//freopen("input.txt", "r", stdin);
	ll m; cin >> m;
	ll zero = 0;
	vector<ll> ans;
	int cnt = 1; ll tmp;
	while(zero <= m)
	{
		tmp = cnt;
		while(tmp % 5 == 0)
		{
			tmp /= 5;
			zero++;
		}
		if(zero == m)
		{
			ans.pb(cnt);
		}
		cnt++;
	}
	cout << ans.size() << "\n";
	for(int i = 0; i < int(ans.size()); i++) 
	{
		cout << ans[i] << " ";
	}
	return 0;
}


