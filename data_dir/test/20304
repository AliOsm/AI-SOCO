#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;



int main()
{
    int m;
    cin >> m;
    int d;
    cin >> d;
    int a;
    if (m==1){a=31;}
    if (m==2){a=28;}
    if (m==3){a=31;}
    if (m==4){a=30;}
    if (m==5){a=31;}
    if (m==6){a=30;}
    if (m==7){a=31;}
    if (m==8){a=31;}
    if (m==9){a=30;}
    if (m==10){a=31;}
    if (m==11){a=30;}
    if (m==12){a=31;}
    int kol=1;
    a-=(7-d+1);
    kol=kol+(a+6)/7;
    cout << kol;
    return 0;
}
