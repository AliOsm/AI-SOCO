#include "bits/stdc++.h"
using namespace std;

int main(){

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	int n;
	char arr[123];
	int arrC;


	cin >> n >> arrC;


	for(int i=0;i<arrC;i++){

		arr[i] = char(i + 97);
	}

	int counter = 0;
	while(n--){

		cout << arr[counter];
		counter = (counter + 1)%arrC;
	}
}