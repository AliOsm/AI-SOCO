///God help me.

#include<bits/stdc++.h>

using namespace std;

int main()
{
    int n;
    string s;
    cin >> n >> s;
    if(n == 1)cout << s << endl;
    else{
        int one = 0;
        int zero = 0;
        for(int i = 0; i<n; i++){
            if(s[i] == '1')one++;
            else zero++;
        }
        cout << 1 ;
        for(int i = 0; i<zero; i++){
            cout << 0;
        }
    }
    return 0;
}
