#include<iostream>
#include<string>
#include<sstream>
using namespace std;
int main()
{
    int n;
    while (cin >> n)
    {
        if (n % 2 == 1)
            cout << -1 << endl;
        else
            for (int i = 2; i <= n; i += 2)
                cout << i << " " << i - 1 << " ";
        cout << endl;
    }
    return 0;
}