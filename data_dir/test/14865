using namespace std;

int n, m, k;
int t[2000005], x[500005];
int main()
{
  cin >> n;
  for(int i = 0;i < n;i++){
    int a;
    cin >> a;
    x[a]++;
  }
  
  for(int i = 0;i <= 1e5;i++){
    if (x[i] & 1){
      cout << "Conan";
      return 0;
    }
  }
    
  cout << "Agasa";
  
    return 0;
