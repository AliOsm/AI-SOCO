#include<bits/stdc++.h>
using namespace std;
int main()

{
     int a,b;


     cin >>a>>b;

     int ans1=0;
     int ans2=0;
     int ans3=0;
     for(int i=1;i<=6;i++)
     {
         if(abs(i-a) < abs(i-b)) ans1++;
         if(abs(i-a)>abs(i-b))ans2++;
         if(abs(i-a)==abs(i-b))ans3++;
     }
      cout << ans1<<" "<<ans3<<" "<<ans2<<endl;
}
