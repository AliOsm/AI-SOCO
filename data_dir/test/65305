/// I won't go back on my words ... that's my ninja way !!!
#include<bits/stdc++.h>
using namespace std;
const int N = 1e5+5 ;
int t ,n ,x ;
int main(){
    scanf("%d",&t);
    while(t--){
        scanf("%d",&n);
        long long sum = 0 ;
        for(int i=0;i<n;++i){
            scanf("%d",&x);
            sum += x ;
        }
        printf("%lld\n" ,sum/n + (sum%n!=0));
    }
    return 0;
}
