#include <bits/stdc++.h>

using namespace std;

#define DIM 200009
#define INF ((long long)1e18+9ll)
#define pairll pair<long long,long long>
#define fi first
#define se second
#define ld long double
#define amen exit(0)
#define fast ios_base::sync_with_stdio(false),cin.tie(0),cout.tie(0)
#define endl '\n'

long long i,j,k,l,n,m;

long long a[DIM];

int main(){
    
    fast;

    int nt;
    cin>>nt;
    while(nt--){
        cin>>n>>m;
        long long num=sqrt(m)-1;

        if(m<=n || num+(m/(num+1))+((m%(num+1))!=0)<=n)cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
    }

    amen;
}