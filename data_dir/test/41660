#include <iostream>
#include <fstream>
#include <vector>
#include <string>
#include <math.h>
#include <iomanip>
#include <algorithm>
#include <map>
#include <set>
#include <queue>
#include <sstream>

using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);

    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        if (n == 1) {
            cout << -1 << '\n';
        } else {
            if (n == 2) {
                cout << 57 << '\n';
            } else {
                for (int i = 0; i < n - 1; ++i) {
                    cout << 5;
                }
                cout << 8 << '\n';
            }
        }
    }

    return 0;
}