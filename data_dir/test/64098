#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

template<typename T>
void out(T x) { cout << x << endl; exit(0); }
#define watch(x) cout << (#x) << " is " << (x) << endl

const int maxn = 1e6 + 5;
int n, a[maxn], b[maxn];

int main() {
  ios_base::sync_with_stdio(false); cin.tie(0);  cout.tie(0);

  cin>>n;
  set<int> s;
  for (int i=0; i<n; i++) {
    cin>>a[i];
    s.insert(a[i]);
  }
  for (int i=0; i<n; i++) {
    cin>>b[i];
    s.insert(b[i]);
  }


  int res = 0;
  for (int i=0; i<n; i++) {
    for (int j=0; j<n; j++) {
      if (s.count(a[i]^b[j])) res++;
    }
  }

  if (res%2==0) out("Karen");
  out("Koyomi");
  

  return 0;
}
