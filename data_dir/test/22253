#include <bits/stdc++.h>

using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr); cout.tie(nullptr);

    long double ans = 0.0, n;
    cin >> n;

    for(long double i = 1; i <= n; ++i){
        ans += 1.0 / i;
    }

    cout << fixed << setprecision(10) << ans;
}
