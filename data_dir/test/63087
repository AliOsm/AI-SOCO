#include<iostream>
#include<set>
#include<cmath>
using namespace std;
int main()
{
    int n,t=0;
    cin>>n;
    string s[n];
    int a[n],b[n];
    for(int i=0;i<n;i++)
    cin>>s[i]>>a[i]>>b[i];
    for(int i=0;i<n;i++)
    {
    	if(a[i]>=2400 && b[i]>a[i])
    	{
    		cout<<"YES";
    		t=1;
    		break;
    	}
    }
    if(t==0)
    cout<<"NO";
}
