#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i, j, k, p, q, x, y, a, b;
    vector<int>vec;
    for(i = 0; i < 10; i++)
    {
        printf("%d%d%d%d\n", i, i, i, i);
        fflush(stdout);
        scanf("%d %d", &a, &b);
        if(a)
            vec.push_back(i);
    }
    while(1)
    {
        printf("%d%d%d%d\n", vec[0], vec[1], vec[2], vec[3]);
        fflush(stdout);
        scanf("%d %d", &a, &b);
        if(a == 4)
            break;
        p = next_permutation(vec.begin(), vec.end());
    }
    return 0;
}
