#include <bits/stdc++.h>
 
#define f first
#define s second
#define ll long long
#define ull unsigned long long
#define mp make_pair
#define pb push_back
#define vi vector <int>
#define ld long double
#define pii pair<int, int>
#define y1 sda
#define all(x) x.begin(), x.end()
 
using namespace std;    
const int N = int(3e5) + 12, mod = int(1e9)  + 7; 

void solve(){
	string t;
	cin >> t;
	bool eq = 1;
	for(int i = 1; i < t.size(); i++){
		if(t[i] != t[0]) eq = 0;
	}
	if(eq){
		cout << t << endl;
	}
	else{
		for(int i = 0; i < t.size(); i++){
			cout << "01";
		}
		cout << endl;
	}

}

int main () {
	int T = 1;
	scanf("%d", &T);

	while(T--){
		solve();
	}

return 0;
}