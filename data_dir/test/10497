#include <bits/stdc++.h>
using namespace std;
struct point {
    long x , y;
};
point a , b , c;
int main()
{
    cin >> a.x >> a.y;
    cin >> b.x >> b.y;
    cin >> c.x >> c.y;
    cout << 3 << endl;
    cout << b.x + (a.x - c.x) << " " << b.y - (c.y - a.y) << endl;
    cout << b.x - (a.x - c.x) << " " << b.y + (c.y - a.y) << endl;
    cout << a.x - (b.x - c.x) << " " << c.y - (b.y - a.y);
}
