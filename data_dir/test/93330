#include<bits/stdc++.h>
using namespace std;

typedef long long ll;
#define FIN ios::sync_with_stdio(false);cin.tie(0);cout.tie(0)

int main() {FIN;
  int q;cin>>q;
  while(q--) {
    ll x;cin>>x;
    ll mod=x%14ll;
    if(x>=15 && 1<=mod && mod<=6)cout<<"YES\n";
    else cout<<"NO\n";
  }
}