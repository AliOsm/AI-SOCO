#include<bits/stdc++.h>
using namespace std;
int main()
{
	string s;
	cin>>s;
	int t=s.length();
	s=s+"***";
	vector<string> v1;
	string x("");
	for(int i=0;i<s.length();i++)
	{
		
		
		if((s[i]=='W')&&(s[i+1]=='U')&&(s[i+2]=='B'))
				{i=i+2;
					if(x.length()>0)
						{v1.push_back(x);}
					string r("");
					x=r;}
		else
		{
			if(i<t)
			x=x+s[i];
		}	
		

	}
	v1.push_back(x);
	
	for(int i=0;i<v1.size();i++)
	{
		cout<<v1[i]<<" ";
	}
}