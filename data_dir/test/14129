#include<bits/stdc++.h>â€¬
using namespace std;
int n,a=1;
vector<int>v;
int main()
{
    cin>>n;
    if(n==0)
    {
        cout<<0<<"\n";
        return 0;
    }
    while(a<=n)
    {
        v.push_back(a);
        n-=a;
        a++;
    }

    cout<<v.size()<<"\n";
    for(int i=0;i<v.size()-1;i++)
    {
        cout<<v[i]<<" ";
    }
    cout<<v[v.size()-1]+n;
}
