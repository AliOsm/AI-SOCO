#include<bits/stdc++.h>
#define ll          long long
#define pb          push_back
#define	endl		'\n'
#define pii         pair<ll int,ll int>
#define vi          vector<ll int>
#define all(a)      (a).begin(),(a).end()
#define F           first
#define S           second
#define sz(x)       (ll int)x.size()
#define hell        1000000007
#define rep(i,a,b)	for(ll int i=a;i<b;i++)
#define lbnd        lower_bound
#define ubnd        upper_bound
#define bs          binary_search
#define mp          make_pair
using namespace std;

#define N  100005

int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	int TESTS=1;
	cin>>TESTS;
	while(TESTS--)
	{
		ll n;
		cin>>n;
		vi l(n),r(n),ans(n,0);
		rep(i,0,n) cin>>l[i]>>r[i];
		rep(t,1,5005)
		rep(i,0,n)
		if(!ans[i])
			if(l[i]<=t&&r[i]>=t) 
			{
				ans[i]=t;
				break;
			}
		for(auto i:ans)
			cout<<i<<" ";
		cout<<endl;		
	}
	return 0;
}