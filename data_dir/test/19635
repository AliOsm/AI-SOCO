#include <bits/stdc++.h>
#pragma GCC optimize("O3")
 
using namespace std;
 
typedef long long ll;
 
#define f first
#define s second
 
int h, m, t;
 
int main()
{
  ios_base::sync_with_stdio(false);
  cin.tie(0); cout.tie(0);
  cin >> t;
  for (int i = 1; i <= t; i++) {
    cin >> h >> m;
    int aux = m  + h*60;
    cout << 1440 - aux << "\n";
  }
 
  return 0;
}
