#include<bits/stdc++.h>
using namespace std;

int main()
{
    int l1, r1, l2, r2, t, i;

    cin >> t;

    for(i = 0; i < t; i++){

        cin >> l1 >> r1 >> l2 >> r2;

        cout << l1 << " ";

        if(l1 == r2){
            cout << l2 << endl;
        }
        else{
            cout << r2 << endl;
        }
    }

    return 0;
}
