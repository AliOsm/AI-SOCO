#include <bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(NULL);cout.tie(NULL)
#define ll long long int
#define ld long double
using namespace std;
const int N = 1e6 + 5;
const int MOD = 1e9 + 7;


int main(){
	fast;
	string s;
	cin >> s;
	for(int i = 1;i <= 5; i++){
		string temp;
		cin >> temp;
		if(s[0] == temp[0] || s[1] == temp[1])
			return cout << "YES", 0;
	}
	cout << "NO";
	
	return 0;
}
