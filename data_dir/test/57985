#include <bits/stdc++.h>

#define ll long long

#define f first
#define s second

#define pb push_back

#define pii pair <int, int>

using namespace std;

const int N = 1e6 + 10;
const int INF = 1e6 + 10;

int n;

int k;

int main() {

  cin >> n >> k;

  char cur = 'a';

  for (int i = 1; i <= n; i++) {
    cout << cur;
    cur++;
    if (cur == 'a' + k) cur = 'a';
  }

  return 0;
}
