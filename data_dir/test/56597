////////////////////////////
using namespace std;
ll valid(ll x,ll y,ll z)
{
 if(x+y>z&&x+z>y&&y+z>x)return 1;
 else return 0;
}
int main()
{
 ll x,y,p,n,i,j;
 while(cin>>n)
 {
  ll cn=0;
  for(i=1;i<=n;i++)
  {
   for(j=i;j<=n;j++)
   {
    p=i^j;
    if(p>=j&&p<=n)
    {
     cn+=valid(i,j,p);
    }
   }
  }
  cout<<cn<<endl;
 }
 return 0;
}
