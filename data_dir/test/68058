#define sim template < class c
#define ris return * this
#define dor > debug & operator <<
#define eni(x) sim > typename   enable_if<sizeof dud<c>(0) x 1, debug&>::type operator<<(c i) {
sim > struct rge { c b, e; };
sim > rge<c> range(c i, c j) { return rge<c>{i, j}; }
sim > auto dud(c* x) -> decltype(cerr << *x, 0);
sim > char dud(...);
struct debug {
#ifdef LOCAL
~debug() { cerr << endl; }
eni(!=) cerr << boolalpha << i; ris; }
eni(==) ris << range(begin(i), end(i)); }
sim, class b dor(pair < b, c > d) {
  ris << "(" << d.first << ", " << d.second << ")";
}
sim dor(rge<c> d) {
  *this << "[";
  for (auto it = d.b; it != d.e; ++it)
    *this << ", " + 2 * (it == d.b) << *it;
  ris << "]";
}
#else
sim dor(const c&) { ris; }
#endif
};
#define imie(...) " [" << #__VA_ARGS__ ": " << (__VA_ARGS__) << "] "
typedef long double ld;
typedef pair<int,int> pii;
///////////////////////////////////////////////////////////////////////////////////////////////////
///////////////////////////////////////////////////////////////////////////////////////////////////
const int N=1e5+5;
map<int,int> mp1,mp2;
int32_t main(){__
    int n;
    cin>>n;
    vi a(n),b(n);
    multiset<int> s1,s2;
    for(int i=0;i<n;i++){
        cin>>a[i];
        b[i]=a[i];
        mp1[a[i]]++;
    }
    sort(all(a));
    int v1=a[0],v2=a[1],v3=a[2];
    int ans=0;
    a=b;
    for(int i=0;i<n;i++){
        mp1[a[i]]--;
        if(a[i]==v1){
            ans+=mp1[v2]*mp2[v3];
            if(v2!=v3)
            ans+=mp1[v3]*mp2[v2];
        }
        else if(a[i]==v2){
            ans+=mp1[v1]*mp2[v3];
            if(v1!=v3)
            ans+=mp1[v3]*mp2[v1];
        }
        else if(a[i]==v3){
            ans+=mp1[v2]*mp2[v1];
            if(v1!=v2)
            ans+=mp1[v1]*mp2[v2];
        }
        mp2[a[i]]++;
    }
    cout<<ans<<"\n";
return 0;
}