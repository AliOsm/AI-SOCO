#include <algorithm>
#include <iostream>
#include <vector>
using namespace std;

int main() {
    ios_base::sync_with_stdio(false); cin.tie(NULL);
    int t; cin >> t;
    while ( t-- ) {
        vector<vector<char>> board(9, vector<char>(9));
        char x;
        
        for ( int i = 0; i < 9; ++i ) {
            for ( int j = 0; j < 9; ++j ) {
                cin >> x;
                cout << (x == '5' ? '4' : x);
            }
            cout << '\n';
        }
    }
}