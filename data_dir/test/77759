#include <bits/stdc++.h>
using namespace std;

int main()
{
    long long n,m;
    cin >> n >> m;
    if(n == 0 && m == 0){
        cout << 0 << " " << 0;
    }else if(n == 0){
        cout << "Impossible";
    }else if(m == 0){
        cout << n << " " << n;
    }else{
        if(n > m){
            cout << n << " " << n + m - 1;
        }else{
            cout << m << " " << n + m - 1;
        }
    }
}