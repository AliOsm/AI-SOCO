#include <bits/stdc++.h>
using namespace std;
int main (){
 ios_base::sync_with_stdio(0);
 cin.tie (0);
 int a,b,c,d;
  cin>>a>>b>>c>>d;
  int s=max(3*a/10,a-(a/250)*c);
  int s2=max(3*b/10,b-(b/250)*d);
  if (s==s2)
    cout<<"Tie";
  else if (s>s2)
     cout<<"Misha";
  else cout<<"Vasya";
}
