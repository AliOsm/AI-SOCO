#include <bits/stdc++.h>
#define pb push_back
#define mp make_pair
#define inf 0x3f3f3f3f
#define all(v) (v).begin() , (v).end()

using namespace std;

typedef vector<int> vi;
typedef long long ll;

int main(){
    int n ;
    cin>>n;
    string s;
    map<string , bool> info;
    while(n--){
        cin>>s;
        if(info[s])
            puts("YES");
        else puts("NO");
        info[s] = 1;
    }
}
