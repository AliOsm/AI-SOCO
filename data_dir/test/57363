#include <bits/stdc++.h>
#define pb push_back
#define mp make_pair
#define fi first
#define se second

using namespace std;

typedef pair<int, int> ii;
typedef long long ll;

int main()
{
	int n, m, z, ans = 0;
	cin >> n >> m >> z;
	for (int i = 1; i <= z; i++)
	{
		if (i%n == 0 and i%m == 0)
			ans++;
	}
	cout << ans << '\n';
	return 0;
}


