#include <bits/stdc++.h>
using namespace std;


string s,t;

int main(){




    cin >> s >> t;


    if ((int)s.size() != (int)t.size())
        cout << max((int)s.size(),(int)t.size());

    else{

        if (s != t) cout << s.size();
        else cout << -1;

    }




}
