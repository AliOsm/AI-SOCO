#pragma GCC optimize("-O3")
#include <bits/stdc++.h>
#define ll long long
#define f first
#define s second
#define ios ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define y1 ibibibibbibibibiibigbabypanfewewf
using namespace std;
ll const N=3*1e5+2;
ll x1,r,t,x2,c,q,y1,y2,d,ans,a,aa[N],aaa[N],b,y3,k,pref[5005][4];
ll m,n;
string s;
pair <ll,ll> p[N];
set <ll> st;
int  main()
{
    ios
cin>>n;
ll dv=1;
while(5*dv<n)
{  
    n=n-(5*dv),dv*=2;
}
if (n<=dv) cout<<"Sheldon"; else if (n<=dv*2) cout<<"Leonard"; else if (n<=dv*3) cout<<"Penny"; else if (n<=dv*4) cout<<"Rajesh"; else cout<<"Howard";
}