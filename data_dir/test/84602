#include <iostream>
#include <algorithm>
#include <vector>
#include <cstdio>
using namespace std;
int i,j,n,m,k,t,a[111111],b[555],d[333][333];

int main()
{
cin>>n;
a[1]=1;t=n-1;
for (i=2;i<=n;i++)
{ if (i%2) a[i]=a[i-1]-t;else a[i]=a[i-1]+t;
 t--;


}
for (i=1;i<=n;i++) cout<<a[i]<<' ';
}
