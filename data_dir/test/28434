#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i,j,l;
    char ch;
    string k;
    cin>>ch;
    cin>>k;
    l=k.size();
    if(ch=='R')
    {
        for(i=0;i<l;i++)
        {
            if(k[i]=='t')
                k[i]='r';
            else if(k[i]=='r')
                k[i]='e';
            else if(k[i]=='e')
                k[i]='w';
            else if(k[i]=='w')
                k[i]='q';
            else if(k[i]=='y')
                k[i]='t';
            else if(k[i]=='u')
                k[i]='y';
            else if(k[i]=='i')
                k[i]='u';
            else if(k[i]=='o')
                k[i]='i';
            else if(k[i]=='p')
                k[i]='o';
            else if(k[i]=='g')
                k[i]='f';
            else if(k[i]=='f')
                k[i]='d';
            else if(k[i]=='d')
                k[i]='s';
            else if(k[i]=='s')
                k[i]='a';
            else if(k[i]=='h')
                k[i]='g';
            else if(k[i]=='j')
                k[i]='h';
            else if(k[i]=='k')
                k[i]='j';
            else if(k[i]=='l')
                k[i]='k';
            else if (k[i]==';')
                k[i]='l';
            else if(k[i]=='b')
                k[i]='v';
            else if(k[i]=='v')
                k[i]='c';
            else if(k[i]=='c')
                k[i]='x';
            else if(k[i]=='x')
                k[i]='z';
            else if(k[i]=='n')
                k[i]='b';
            else if(k[i]=='m')
                k[i]='n';
            else if(k[i]==',')
                k[i]='m';
            else if(k[i]=='.')
                k[i]=',';
            else if(k[i]=='/')
                k[i]='.';
        }
    }
    else if(ch=='L')
    {
        for(i=0;i<l;i++)
        {
            if(k[i]=='q')
                k[i]='w';
            else if(k[i]=='w')
                k[i]='e';
            else if(k[i]=='e')
                k[i]='r';
            else if(k[i]=='r')
                k[i]='t';
            else if(k[i]=='t')
                k[i]='y';
            else if(k[i]=='y')
                k[i]='u';
            else if(k[i]=='u')
                k[i]='i';
            else if(k[i]=='i')
                k[i]='o';
            else if(k[i]=='o')
                k[i]='p';
            else if(k[i]=='a')
                k[i]='s';
            else if(k[i]=='s')
                k[i]='d';
            else if (k[i]=='d')
                k[i]='f';
            else if(k[i]=='f')
                k[i]='g';
            else if(k[i]=='g')
                k[i]='h';
            else if(k[i]=='h')
                k[i]='j';
            else if(k[i]=='j')
                k[i]='k';
            else if(k[i]=='k')
                k[i]='l';
            else if(k[i]=='l')
                k[i]=';';
            else if(k[i]=='z')
                k[i]='x';
            else if(k[i]=='x')
                k[i]='c';
            else if(k[i]=='c')
                k[i]='v';
            else if(k[i]=='v')
                k[i]='b';
            else if(k[i]=='b')
                k[i]='n';
            else if(k[i]=='n')
                k[i]='m';
            else if(k[i]=='m')
                k[i]=',';
            else if(k[i]==',')
                k[i]='.';
            else if(k[i]=='.')
                k[i]='/';
    }
    }
    cout<<k;
    return 0;
}
