//=======================================CODING=================================================//
const  long long int N=5e5+5,MOD=1e9+7,inf=1e18*1LL;
LL ar[N],A[N];
LL a,b,c,r,rr,n,m,t,ks;
string s[1234],ss;
int main()
{
    //IOS;//READ;


    rep(i,10) cin>>s[i];


    rep(i,10)
    {
        rep(j,10)
        {

            r=0,rr=0;
            for(int k=i;k<i+5;k++)
            {

                if(k>=10)
                {
                    r=200,rr=200; break;
                }
                if(s[k][j]=='.')r++;
               else if(s[k][j]=='O')rr++;
               else
               {
                  // if(s[k][j]!='X')rr++;
               }

            }
           // r=0,rr=0;

            if(r<=1  and  rr==0)
            {
                cout<<"YES"; return 0;
            }

            r=0,rr=0;

             for(int k=j;k<j+5 ;k++)
            {


                 if(k>=10)
                {
                    r=200,rr=200; break;
                }

                if(s[i][k]=='.')r++;
               else if(s[i][k]=='O')rr++;
               else
               {
                   //if(s[i][k]!='X')rr++;
               }

            }


             if(r<=1  and  rr==0)
            {
                cout<<"YES"; return 0;
            }
            r=0,rr=0;


            rep(k,5)
            {

                 if(i+k>=10  or j+k>=10)
                {
                    r=200,rr=200; break;
                }
                if(s[i+k][j+k]=='.')  r++;
                 else if(s[i+k][j+k]=='O')rr++;
               else
               {
                   //if(s[i+k][j+k]!='X')rr++;
               }
            }

             if(r<=1  and  rr==0)
            {
                cout<<"YES"; return 0;
            }


            r=0,rr=0;



            rep(k,5)
            {

                if( (i+k)>=10 or  (j-k)<0)
                {
                    r=200,rr=200; break;
                }
                if(s[i+k][j-k]=='.')  r++;
                 else if(s[i+k][j-k]=='O')rr++;
               else
               {
                  // if(s[i-k][j-k]!='X')rr++;
               }
            }


               if(r<=1  and  rr==0)
            {
                cout<<"YES"; return 0;
            }

            r=0,rr=0;
        }
    }

    cout<<"NO"; return 0;


     return 0;
}
