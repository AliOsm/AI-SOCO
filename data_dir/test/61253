#include <bits/stdc++.h>
using namespace std;

int a, b, c;

int main(){



    cin >> a >> b >> c;
    int v = a, vv = b;
    bool f = false;
    if (v < vv) swap(v, vv), f = true;
    if (v == vv && c == 0) return cout << 0, 0;
    if (vv + c >= v) return cout << '?', 0;
    if (f) cout << '-';
    else cout << '+';


}
