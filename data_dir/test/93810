void Init ()
{
   n = 0, k = 0;
   a = 0, b = 0;
   l = 0;
   d = 0;
}

void Main ()
{
   for ( int l=0; l < m; l++ )
   {
      for ( int j=0, d=12/c [l]; j < c [l]; j++ )
      {
         ok = true;
         for ( int i=j; i < 12; i+=c [l] )
            if ( s [i] != 'X' )
               { ok = false; break; }
         if ( ok ) break;
      }
      if ( ok )
         { xx [k].first = 12/c [l], xx [k].second = c [l]; x [k] = c [l]; y [k] = 12/c [l]; k++; }
   }

# ifndef ONLINE_JUDGE
   PrintData (1);
# endif
}

