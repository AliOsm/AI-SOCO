#include<bits/stdc++.h>
#define X first
#define Y second
#define ll long long
#define nl cout<<endl
#define SZ(x) x.size()
#define pii pair<int,int>
#define all(v) v.begin(),v.end()
#define pb(x) push_back(x)
#define clr(a) memset(a,0,sizeof(a))
#define SET(a) memset(a,-1,sizeof(a))
#define fr(i,a,b) for(i=a;i<=b;i++)
#define rf(i,b,a) for(i=b;i>=a;i--)
using namespace std;

int main()
{
    int n,k,i,j,K;
    cin>>n>>K;
    int l=1,r=n;
    k=(K)/2;
    while(k--)
    {
        cout<<l<< " "<< r<< " ";
        l++;
        r--;
    }
    if(K%2==0)rf(i,r,l)cout<<i<< " ";
    else fr(i,l,r)cout<<i<< " ";
    nl;
}
