#include<bits/stdc++.h>
#define ll long long
using namespace std;
const int N = 3e5 + 5;
ll a[N];
int main()
{
    ios_base ::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
    ll n;
    cin >> n;
    int ans;
    for(int i = 0; i < 63; i++){
        if((1LL << i) & n) ans = i;
    }
    cout << ans + 1;
return 0;
}
