// rc.cpp : Defines the entry point for the console application.
//

#include <math.h>

#include <set>
#include <list>
#include <cmath>
#include <ctime>
#include <map>
#include <deque>
#include <queue>
#include <stack>
#include <string>
#include <bitset>
#include <cstdio>
#include <limits>
#include <vector>
#include <climits>
#include <cstring>
#include <cstdlib>
#include <fstream>
#include <numeric>
#include <sstream>
#include <iostream>
#include <algorithm>
#include <unordered_map>
#include <functional>
#include <iomanip>
#include <iterator>
#include <regex>

using namespace std;

#define PI 3.14159265358979323846
#define EPS 1.E-8

#define FIN(a, n) { for (ul _ = 0; _ < n; ++_) cin >> (a)[_]; }
#define FOUT(a, n) { cout << a[0]; for (ul _ = 1; _ < n; ++_) cout << ' ' << a[_]; cout << '\n'; }

typedef unsigned char uc;
typedef unsigned int ui;
typedef unsigned long long ul;
typedef long long sl;

sl x[100000];

int main(int argc, char* argv[])
{
    ios_base::sync_with_stdio(false);

    sl t;
    cin >> t;

    for (; t; --t)
    {
        sl n, r;
        cin >> n >> r;

        FIN(x, n);
        sort(x, x + n);

        sl z = 0,
           e = n - 1,
           d = 0;
        while (e >= 0 && x[e] - d > 0)
        {
            ++z;

            sl ce = e;
            while (ce >= 0 && x[ce] == x[e])
                --ce;
            e = ce;
            d += r;
        }

        cout << z << '\n';
    }
    return 0;
}
