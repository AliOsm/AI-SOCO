#include <bits/stdc++.h>
using namespace std;

const int N = 1e5 + 10;

int a[N];

int main()
{
  ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
  int n;
  cin >> n;
  for (int i = 1; i <= n; ++i) cin >> a[i];
  for (int i = 1; i <= n; ++i) {
    if (i > 1) cout << ' ';
    cout << a[i] + a[i+1];
  }
  cout << '\n';
}
// g++ -W -Wall -Wextra -pedantic -std=c++0x -o p main.cpp
