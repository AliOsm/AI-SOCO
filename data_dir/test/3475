#include <bits/stdc++.h>
using namespace std;

char s[100];
int main()
{
    scanf("%s", s);
    int n = strlen(s);

    int ans = 0;
    for (int i = 0; i < n; i++) {
        if (s[i] == 'a' || s[i] == 'e' || s[i] == 'i' || s[i] == 'o' || s[i] == 'u')
            ans++;
        else if (s[i] == '1' || s[i] == '3' || s[i] == '5' || s[i] == '7' || s[i] == '9')
            ans++;
    }
    printf("%d", ans);
}
