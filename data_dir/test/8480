#include <bits/stdc++.h>

#define pb push_back
#define pob pop_back
#define ll long long
#define pll pair < ll, ll >
#define fi first
#define se second
#define sefi second.first
#define sese second.second

using namespace std;

ll n, m, s = 0, ans, a[1000001],  r = 0, b[101010], d[1010110];
int main(){
    cin >> n;
    for(int i = 1; i <= n; i++){
        cin >> a[i];
        if(a[i] == 0){
            r++;
        }
        else {
            s++;
        }
    }
    if(r == 0){
        cout << "-1";
        return 0;
    }
    for(int i = 1; i <= (s / 9) * 9; i++){
        cout << 5;
    }
    if(s >= 9){
        for(int i = 1; i <= r; i++){
            cout << 0;
        }
        return 0;
    }
    cout << 0;
    return 0;
}
