typedef pair<int,ii> iii2;
typedef pair<ii,ii> iiii;
typedef vector<int> vi;
long long mod = 1000000007;
long long large = 2000000000000000000LL;




int main(){

	int n;
	cin>>n;
	int c = 1;
	int cnt = 0;
	while(n){
		cnt++;
		n-=min(n,c);
		c*=2;
	}

	cout<<cnt<<endl;





    return 0;
}
