#include<bits/stdc++.h>

using namespace std;
int dp[1010];
int main()
{

   int a,b;
   cin >>a>>b;
  if(a==0   && b==0 )
  {
        cout <<"NO"<<endl;
  }
else    if(abs(a-b)==1   || abs(a-b)==  0 )
   {
         cout<<"YES"<<endl;
   }
   else
   {
          cout <<"NO"<<endl;
   }
}
