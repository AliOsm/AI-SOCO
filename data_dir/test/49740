#include <bits/stdc++.h>
using namespace std;
long long a,b,c,k;
main()
{
	cin>>a>>b>>c>>k;
	
	if(k>2*c)
	{
		cout<<-1<<endl;
		return 0;
	}
	
	if(c>2*k)
	{
		cout<<-1<<endl;
		return 0;
	}
	
	if(k>=b)
	{
		cout<<-1<<endl;
		return 0;
	}
	
	cout<<2*a<<" "<<2*b<<" "<<max(k,c);
}