#define mp make_pair
#define sq(x) ((x)*(x))
#define tmin(x,y,z) (min(min((x),(y)),(z)))
#define getbit(x, mask) (((mask) >> (x)) & 1)
#define onbit(x, mask) ((mask) | ((unsigned int)(1) << (x)))
#define offbit(x, mask) ((mask) & (~((unsigned int)(1) << (x))))
