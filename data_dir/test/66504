#include <bits/stdc++.h>
#include <stdio.h>
#include <string.h>

using namespace std;

int main()
{
	long  a,b,c,d;
	cin>>a>>b>>c>>d;
	if (a!=d) printf("0");
	else
	{
		if (c==0) printf("1");
		else
		if (a==0) printf("0");
		else
		printf("1");	
	}	
	ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
	return 0;
}

