using namespace std;

int zxc;

int s(ull n) {
	int sum = 0;
	while (n > 0) {
		sum += n % 10;
		n /= 10;
	}
	return sum;
}
ull p(ull a, ull e) {
	ull res = 1;
	loop(i, e) {
		res *= a;
	}
	return res;
}
int main() {
	int n, k;
	cin >> n >> k;
	for (int i = k+1; i <= n; i++)cout << i << ' ';
	for (int i = k; i >= 1; i--)	cout << i << ' ';
	cout << endl;
	return 0;
}

