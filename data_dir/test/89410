#include <bits/stdc++.h>
using namespace std;
typedef long long int ll;

class ProblemBTheBestGift {
public:
	void solve(std::istream& in, std::ostream& out) {
		int n, m; in>>n>>m;
		vector<int> arr(m+1, 0);
        for(int i = 0; i<n; ++i){
            int x; in>>x;
            arr[x]++;
        }
        ll res = 0;
        for(int i = 1; i<=m; ++i){
            int remain = n-arr[i];
            res += remain*arr[i];
        }
        out<<(res/2);
	}
};


int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
	ProblemBTheBestGift solver;
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
	return 0;
}
