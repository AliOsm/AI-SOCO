#include <vector>
#include <iostream>
#include <string>

using namespace std;

int a[123456];

int main()
{
    int a, b, c;
	cin >> a >> b >> c;
	if (a >= b)
		swap(a, b);
	if (a >= c)
		swap(a, c);
	if (b >= c)
		swap(b, c);
	if (a == 3 && b == 3 && c == 3)
		return cout << "YES\n", 0;
	if (a == 2 && b == 2)
		return cout << "YES\n", 0;
	if (a == 2 && b == 4 && c == 4)
		return cout << "YES\n", 0;
	if (a == 1)
		return cout << "YES\n", 0;
	cout << "NO\n";
    return 0;
}