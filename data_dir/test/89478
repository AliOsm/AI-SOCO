// KALAM

# include <bits/stdc++.h>

using namespace std;

const int N = 100000 + 77;
int n , m , a[N] , c[N] , A , t = 1;
int main(){
	cin >> n >> m;
	for(int i = 1;i <= n;i++){
		cin >> c[i];
	}
	for(int i = 1;i <= m;i++){
		cin >> a[i];
	}
	for(int i = 1;i <= n;i++){
		if(a[t] < c[i]){continue;}
		A ++;t ++;
	}
	cout << A;
	return 0;
}
