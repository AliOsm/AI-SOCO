#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

int main()
{
    optimiza_io
    int N;
    cin >> N;
    bool ok = false;
    string s;
    cin >> s;
    for( int i = 0; i < s.size() - 1; i ++ )
    {
        if( ! ok && s[i] > s[i + 1] )
            ok = 1;
        else
            cout << s[i];
    }
    if( ok )
        cout << s[s.size() - 1];
    return 0;
}