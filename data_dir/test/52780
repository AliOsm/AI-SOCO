#define ll long long
#define MAXN 1000000


using namespace std;


int main() {
    int n;
    cin>>n;
    
    set<int> X;
    
    for(int i=1;i<=n;i++) {
        int x;
        cin>>x;
        if (x != 0)
            X.insert(x);
    }
    
    cout << X.size() << endl;
    
    
    
}
