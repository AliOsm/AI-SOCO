#include <bits/stdc++.h>

using namespace std;
int arr[20];
int main(){

    for(int i=1; i<=11; i++){
        if(i==10)arr[i]=15;
        else arr[i]=4;
    }
    int n;
    cin>>n;
    int ans=0;
    for(int i=1; i<=11; i++){
        if(10+i==n)ans+=arr[i];

    }
    cout<<ans<<endl;

}
