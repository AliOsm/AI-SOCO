#include <bits/stdc++.h>
using namespace std;
 
#define mp make_pair
#define pb push_back
#define pp pop_back
#define F first
#define S second
#define int long long
 
typedef long long ll;
typedef pair<int, int> pii;
typedef pair<string, string> pss;
typedef vector<pii> vpii;
 
const int mod = 1000000007LL;
const int inf = 1e18;
const int maxx = 1e2 + 19;
 
int n, m;
 
int32_t main() {
    ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
    cin >> n >> m;
    int ans = 0;
    for (int i = 1; i <= n; i++)
        ans += (m + i % 5) / 5;
    cout << ans;
    return 0;
}