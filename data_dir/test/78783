#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    ll n,m;
    cin>>n>>m;
    for(ll i=0;i<m;i++){
        ll l,r;
        cin>>l>>r;
    }
    for(ll i=0;i<n;i++)
    if(i%2)
    cout<<0;
    else
    cout<<1;
} 
