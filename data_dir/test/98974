
#include<iostream>
#include <bits/stdc++.h>
#include <math.h>
#include <string>
#include <sstream>
#define ll long long
#define ld long double
using namespace std;
ll mode=100000000+7;
ll gcd (ll a,ll b)
{
    if(b==0)
        return a;
    else
        return gcd(b,a%b);
}
int main()
{
    ll n;
    cin>>n;
    string ms="abcd";
    string ns;
    for(ll i=0;i<n;i++)
        ns+=ms[i%4];
    cout<<ns;
}












