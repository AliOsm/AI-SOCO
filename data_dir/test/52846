#include <bits/stdc++.h>
#define int long long
#define endl "\n"
using namespace std;
inline int in(){int32_t x ; scanf("%d",&x);return x;}
const int maxn = 1e5 + 10;
const int mod = 1e9 + 7;
const int inf = 1e9 + 10;
int32_t main()
{
    int a = in() , b = in() , c = in(); a %= b;
    for (int i = 0  ; i < 1e6 ; i ++)
    {
        a *= 10;
        if (a / b == c) return cout << i + 1 << endl , 0 ;
        a %= b;
       
    }
    cout << -1 << endl;
}
