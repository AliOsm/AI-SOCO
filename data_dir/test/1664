/*****int exgcd(int A, int B, int *X, int *Y){


    int x2, y2, x1, y1, x, y, r, r1, r2, q;
    x2=1; y2=0;
    x1=0; y1=1;
    for(r2=A, r1=B; r1!=0 ; r2=r1, r1=r, x2=x1, y2=y1, x1=x, y1=y){

        q=r2/r1;
        r=r2%r1;
        x=x2-(x1*q);

        y=y2-(y1*q);
    }
    *X=x2;
    *Y=y2;
    return r2;

}******/




///-----------------------------START FROM HERE--------------------------------------///

string SS;

int main(){

    int n;
    cin>>n>>SS;

    int l=0, r=0;
    for(int i=0; i<SS.size(); i++){
        if(SS[i]=='L')l++;
        else r++;
    }
    int ans=l+r+1;
    cout<<ans;
}
