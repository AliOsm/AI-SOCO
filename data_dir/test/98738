#include<bits/stdc++.h>

#define SZ(x) ((int(x.size())))

typedef long long ll;
typedef long double ld;

using namespace std;

const int maxn = 110;
int t[maxn], n, a, T;

int main()
{
    cin >> n;
    for (int i = 0; i < n; i++)
    {
        cin >> a;
        t[a]++;
    }
    for (int i = 1; i <= 100; i++)
        T += t[i] / 2;
    cout << T / 2;
	return 0;
}
