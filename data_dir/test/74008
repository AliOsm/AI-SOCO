#include<bits/stdc++.h>
#define int long long

using namespace std;

signed main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);

    int n, t;
    cin >> n >> t;

    if (t == 10)
        if (n == 1)
            cout << "-1\n";
        else
        {
            cout << "10", n -= 2;
            while (n--)
                cout << '0';
            cout << '\n';
        }
    else
    {
        cout << t;
        while (--n)
            cout << '0';
        cout << '\n';
    }

    return 0;
}
