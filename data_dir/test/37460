#include <bits/stdc++.h>
using namespace std;
const int N = 1010, MAXN = 100000;
int r[N][N], c[N][N];
int a[MAXN];
int ps[MAXN];
int main() {	
	int x;
	cin >> x;
	string s, t;
	cin >> s >> t;
	if (t[0] == 'm') {
		if (x <= 29) {
			cout << 12;
			return 0;
		}
		if (x == 30) {
			cout << 11;
			return 0;
		}
		if (x == 31) {
			cout << 7;
			return 0;
		}
	}
	else {
		x--;
		if (x == 4 or x == 5) {
			cout << 53;
			return 0;
		}
		else {
			cout << 52;
			return 0;
		}
	}
	return 0;
}
