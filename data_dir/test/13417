#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <iostream>
using namespace std;
int main(){
    long long a, b;
    int n;
    scanf("%d", &n);
    cin >> a >> b;
    for (int i = 0; i < n; ++i){
        long long x;
        cin >> x;
        if (i)printf(" ");
        cout << ((x * a) % b) / a;
    }
    printf("\n");
    return 0;
}