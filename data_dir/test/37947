#include <bits/stdc++.h>

using namespace std;


const int N = 1e6, MOD = 1e9 + 7, MAXX = 508;
const long double EPS = 0.000000001;

#define int long long

int a[N];

signed main(){
	ios::sync_with_stdio(false);
	cin.tie(0);
	int n;
	cin >> n;
	if (n == 13 || n == 24 || n == 1 || n == 10 || n == 11 || n == 16 || n == 20 ||
		n == 28 || n == 37 || n == 41 || n == 49 || n == 75 || n == 99) {
		cout << "NO";
		return 0;
	}
	cout << "YES";
}