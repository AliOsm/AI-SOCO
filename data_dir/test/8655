#include <bits/stdc++.h>

#define F first
#define S second
#define pii pair<int, int>
#define pb push_back

using namespace std;

typedef long long ll;
typedef long double ld;

const int maxN = 1e5 + 10;

int main() {
	ios_base::sync_with_stdio(0); cin.tie(0);
	int t; cin >> t;
	while(t--) {
		int a, b, n; cin >> a >> b >> n;
		n %= 3;
		if(n == 0) cout << a << '\n';
		else if(n == 1) cout << b << '\n';
		else cout << (a ^ b) << '\n';
	}
}
