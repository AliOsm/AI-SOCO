#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using vi = vector<int>;

void solve(){
	ll n;
	cin >> n;
	ll sum = 3;
	ll pot = 4;
	while( true){
		if( n% sum == 0){
			cout << n/sum << "\n";
			return;
		}
		sum += pot;
		pot*=2;
	}

}

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int t;
	cin >> t;
	while( t--){
		solve();
	}
}
