#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define MAX 100007
#define MOD 1000000007


int main()
{
    string s;
    cin >> s;

    int ln = 0; int prl = 0;

    for (int i = 0; i < s.size(); i++) {
        if (s[i]=='o') prl++;
        else ln++;
    }


    if (prl==0 || ln%prl==0) cout << "YES" << endl;
    else cout << "NO" << endl;




    return 0;
}
