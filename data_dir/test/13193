#include<bits/stdc++.h>
#define f first
#define s second
using namespace std;
int n,ans=1;
int main()
{
        cin >> n;
		while(n)
        {
        		if(n == 3) { cout << ans << " " << ans << " " << ans * 3; return 0; }
                for(int j=1; j<=(n+1)/2; j++)
                {
                        cout << ans << " ";
                }
                ans *= 2;
                n /= 2;
        }
}