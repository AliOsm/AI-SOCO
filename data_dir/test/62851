#include <bits/stdc++.h>
using namespace std;
typedef long long int ll;
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll t;
    cin>>t;
    while(t--){
        ll x,y;
        cin>>x>>y;
        ll k=y/x;
        if(k*x<=y){
            cout<<x<<" "<<k*x<<endl;
        }
    }

    return 0;
}