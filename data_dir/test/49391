#include<bits/stdc++.h>
using namespace std;

int main(){
    string a,b;
    cin>>a>>b;
    int zero=abs((int)a.size()-(int)b.size());
    string change(zero,'0');
    if((int)a.size()<(int)b.size())a=change+a;
    else b=change+b;
    cout<<((a>b)?">\n":(a<b)?"<\n":"=\n");
    return 0;
}
