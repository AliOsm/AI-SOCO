#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef pair <int , int> pii;

const int N = 1e5+10 , mod = 1e9+7;

int32_t main(){
	ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
	ll l , r;
	cin >> l >> r;
	cout << "YES" << endl;
	for(ll i = l ; i <= r ; i += 2)
		cout << i << " " << i + 1 << "\n";
	return 0;
}
