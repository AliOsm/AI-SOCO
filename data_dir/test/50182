#include <vector>
#include <map>
#include <set>
#include <bitset>
#include <algorithm>
#include <numeric>
#include <sstream>
#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int i;
    string s1, s2;
    cin >> s1;
    cin >> s2;
    for (i = 0; i < s1.size(); i++)
    {
        s1[i] = tolower(s1[i]);
        s2[i] = tolower(s2[i]);
    }
    if (s1>s2)
        cout << 1 << endl;
    else if (s2 > s1)
        cout << -1 << endl;
    else
        cout << 0 << endl;
    return 0;
}
