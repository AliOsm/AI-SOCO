#include <bits/stdc++.h>
#define pb push_back
#define SALI main
//#define int long long
using namespace std;
typedef long long ll;
typedef pair<string,int> pii;
typedef long double ld;


int SALI()
{
    int q; cin >> q;
    while(q--){
        int n; cin >> n;
        if(n == 1 || n == 2 || n == 3){
            cout << -1 << endl;
            continue;
        }
        if(n % 2 == 0){
            cout << n/4 << endl;
        }
        else{
            if(n < 9 || n == 11){
                cout << -1 << endl;
            }
            else{
                cout << 1 + (n-9)/4 << endl;
            }
            
        }
    }
}
