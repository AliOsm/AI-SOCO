#include <bits/stdc++.h>
using namespace std;
#define pb  push_back
#define ll  long long
#define vi  vector<int>
#define vvi vector<vi >
#define all(x) x.begin(), x.end()

int t, n, m;

int main() {
    ios_base::sync_with_stdio(false); cin.tie(0);
    cin >> t;
    while (t--) {
        cin >> n >> m;
        if (n > m) swap(n, m);
        cout << (n == 1 || m == 2 ? "YES\n" : "NO\n");
    }

    return 0;
}