#include<bits/stdc++.h>
using namespace std;
#define int long long int
#define ld long double
#define pb 	push_back
#define endl '\n'
#define all(x) x.begin(),x.end()
#define mod 1000000007LL
#define F first
#define S second
#define speed ios_base::sync_with_stdio(0); cin.tie(0);cout.tie(0)
 
main()
{
    int n,k;
    cin>>n>>k;
    int z=n/k;
    if(k>n)return cout<<"NO",0;
    if(z&1)cout<<"YES";
    else cout<<"NO";
}