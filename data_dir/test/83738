#include<bits/stdc++.h>
#define r_0 return 0;
#define fr first
#define ll long long
#define se second
#define pb push_back
#define real_madrid ios_base::sync_with_stdio(NULL);
using namespace std;
int n,x,y,cnt;
int a;
int b;
int main()
{
    real_madrid
    cin >> n;
    for ( int i = 1; i <= n; i ++ ){
        cin >> x;
        if ( x == 0 ){
            a ++;
        }
        else{
            b ++;
        }
    }
    if ( a == 0 ){
        cout << -1;
        return 0;
    }
    y = b / 9;
    if ( y ){
        for ( int i = 1; i <= y * 9; i ++ ){
            cout << 5;
        }
        while( a -- ){
            cout << 0;
        }
    }
    else{
        cout << 0;
    }
}
