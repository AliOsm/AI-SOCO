#include <bits/stdc++.h>

using namespace std;

void solve(const int &test_id)
{
int a,b,c;

 cin >> a >> b >> c;
 if (a+c<b) cout << "-" << endl;
 else if (a>b+c) cout << "+" << endl;
 else if (a==b && c==0) cout << "0" << endl;
 else cout << "?" << endl;
}

void solve_cases()
{
int test_cases=1;

 //cin >> test_cases;
 for(int i=1;i<=test_cases;i++) solve(i);
}

void fast_io()
{
 ios::sync_with_stdio(false);
 cin.tie(0);
 cout.tie(0);
 cout << fixed << setprecision(15);
}

int main()
{
 fast_io();
 solve_cases();
 return EXIT_SUCCESS;
}
