/* In the name of Allah */
#include<bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin >> t;
	while (t--) {
		string t;
		cin >> t;
		int cnt[2] = {};
		for (auto c: t)
			cnt[c - '0']++;
		if (!cnt[0] || !cnt[1]) {
			cout << t << endl;
			continue;
		}
		for (int i = 0; i < t.size(); i++)
			cout << "01";
		cout << endl;
	}
}
