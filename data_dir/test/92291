#include <iostream>
#include <fstream>
#include <stdio.h>
#include <algorithm>
#include <vector>
#include <set>
#include <queue>
#include <stack>
#include <math.h>

int main(){
   // freopen("input.txt","r",stdin);
   // freopen("output.txt","w",stdout);
     int n,t=1,size=0;
     int a[10000],p[10000];
     scanf("%d",&n);
     for (int i=0;i<n;i++){
            scanf("%d",&a[i]);
            if (a[i]==t){
                p[size++]=2000+i+1;
                ++t;
            }
        }
        printf("%d\n",size);
        for (int i=0;i<size;i++)printf("%d ",p[i]);
        return 0;
    }
