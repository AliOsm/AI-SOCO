#include <bits/stdc++.h>
using namespace std;
#define int long long
typedef long long ll;

int32_t main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int n;
    cin >> n;
    int sum = 0;
    int half = n/2;
    for(int i = 0; i < n; ++i) {
        for(int j = 0; j < n; ++j) {
            int num;
            cin >> num;
            if(i == half || j == half || i == j || i+j == n-1) {
                sum += num;
            }
        }
    }
    cout << sum << '\n';
    return 0;
}































