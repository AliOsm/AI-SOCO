#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef long double ld;
#define F first
#define S second
#define ii pair <int,int>

int n,m,ans;

int main()
{
    scanf("%d%d",&n,&m);
    for(int i=0;i<n;i++)
        for(int j=0;j<m;j++)
        {
            int x,y;
            scanf("%d%d",&x,&y);
            ans += ( (x==1) || (y==1) );
        }

    printf("%d",ans);
}
