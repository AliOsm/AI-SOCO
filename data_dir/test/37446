//1096A
#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <set>
#include <map>
using namespace std;

#define IOS ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
long long T, l, r;
int main() {
	IOS;
	cin >> T;
	for (int i = 0; i < T; ++i) {
		cin >> l >> r;
		if (2 * l <= r) {
			cout << l << " " << l * 2 << "\n";
		}
	}
	return 0;
}