/// إِنَّ مَعَ الْعُسْرِ يُسْرًا

#include <bits/stdc++.h>
#define LL long long
#define LD long double
#define S second
#define F first
#define IO ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

using namespace std;

int main()
{
    IO
    string s,s1,s2;
    cin>>s1>>s2;
    for(int i=0; i<s1.size() ; i++)
    {
        if(s1[i]==s2[i])
            s+='0';
        else
            s+='1';

    }
    cout<<s;

    return 0;
}
