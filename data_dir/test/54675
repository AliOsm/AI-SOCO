#include<bits/stdc++.h>

using namespace std;

int ara[1005][2],flower[1005];

int main()
{
    int n,m,i,j;

    scanf("%d %d",&n,&m);

    for(i=1; i<=m; i++)
    {
        scanf("%d %d",&ara[i][0],&ara[i][1]);
    }

    for(i=1; i<=n; i++)
    {
        if(i%2)
        {
            printf("1");
        }

        else
        {
            printf("0");
        }
    }

    printf("\n");

    return 0;


}

