#include <bits/stdc++.h>
using namespace std;
#define ll long long

void solve()
{
    ll a, b, c, d;
    cin>>a>>b>>c>>d;

    cout<<b<<" "<<c<<" "<<c<<endl;
}

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    ll n;
    cin>>n;
    for(ll i=0; i<n; i++)
        solve();

    return 0;
}
