#include<iostream>
#include<cstdio>
#include<cstring>
#include<vector>
#include<algorithm>
#define maxn 2001
using namespace std;
int n,k;
int main(){
    while(cin>>n>>k){
          if(k>=n*(n-1)/2){
                           cout<<"no solution"<<endl;
                           continue;
          }
          for(int i = 0;i<n;++i)
                  cout<<0<<" "<<i<<endl;
    }
    return 0;
}
