#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

ll a1, b1, a2, b2;
ll a, b, c = 0, d = 0, e = 0, f = 0;

int main (int argc, char *argv[])
{
	scanf("%lld%lld%lld%lld", &a1, &b1, &a2, &b2);
	a = a1 * b1;
	b = a2 * b2;
	while(a % 2 == 0) {		
		a /= 2;
		++c;
	}
	while(a % 3 == 0) {
		a /= 3;
		++d;
	}
	while(b % 2 == 0) {
		b /= 2;
		++e;
	}
	while(b % 3 == 0) {
		b /= 3;
		++f;
	}
	if(a != b) {
		printf("-1");
	} else {
		ll u = min(d, f);
		d -= u;
		f -= u;
		c += d;
		e += f;
		u = min(c, e);
		c -= u;
		e -= u;
		printf("%lld\n", d + f + c + e);
		while(d--) {
			if(a1 % 3 == 0) {
				a1 /= 3;
				a1 *= 2;
			} else {
				b1 /= 3;
				b1 *= 2;
			}
		}
		while(f--) {
			if(a2 % 3 == 0) {
				a2 /= 3;
				a2 *= 2;
			} else {
				b2 /= 3;
				b2 *= 2;
			}
		}
		while(c--) {
			if(a1 % 2 == 0) {
				a1 /= 2;
			} else {
				b1 /= 2;
			}
		}
		while(e--) {
			if(a2 % 2 == 0) {
				a2 /= 2;
			} else {
				b2 /= 2;
			}	
		}
		printf("%lld %lld\n%lld %lld", a1, b1, a2, b2);
	}

	return 0;
}

