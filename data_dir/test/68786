#include <bits/stdc++.h>
#pragma GCC optimization ("unroll-loops")
#pragma GCC optimization ("O2")
#pragma GCC target("popcnt")
#pragma GCC target ("avx2")
using namespace std;

int main(){
	int n;
	cin >> n;
	int in[n];
	for(int i = 1 ; i < n ; i += 1){
		cin >> in[i];
	}
	int a,b,ans=0;
	cin >> a >> b;
	for(int i = a ; i < b ; i += 1){
		ans += in[i];
	}
	cout << ans << endl;
}