#include <algorithm>
#include <cmath>
#include <cstdio>
#include <cstdlib>
#include <cstring>
#include <iostream>
#include <string>
#include <vector>
#define SZ(x) ((int)(x).size())
#define FOR(it, c) for(__typeof((c).begin()) it = (c).begin(); it != (c).end(); ++it)
using namespace std;
typedef long long LL;

int main(void) {
  int n;
  cin >> n;
  for(int i=30;i>=0;i--) {
    if(n&(1<<i))
      printf("%d ", i+1);
  }
  puts("");
  return 0;
}
