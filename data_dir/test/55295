#include<bits/stdc++.h>
#define fastio() ios_base::sync_with_stdio(false);cin.tie(NULL);
#define ll int
#define pb push_back
#define x first
#define y second
using namespace std;
double pi=3.1415926535897932384;
int main(){
    fastio()
    double d,h,v,e;
    cin>>d>>h>>v>>e;
    d/=2.0;
    double ar=pi*d*d;
    v/=ar;
    if(e>v)cout<<"NO";
    else{
       // cout<<"YES\n";
        v-=e;
        double ans=h/v;
        printf("YES\n%.9f",ans);
    }
       	return 0;
}
