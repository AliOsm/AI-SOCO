#include <bits/stdc++.h>

using namespace std;
typedef long long ll;
int x[3][2];

int main()
{
    ios_base::sync_with_stdio(false);
    int n, a, b, c; cin>>n;
    while(n--)
        cin>>a>>b>>c,
        x[a][0]+=b, x[a][1]+=c;
    if(x[1][0]>=x[1][1])
        cout<<"LIVE"<<endl;
    else cout<<"DEAD"<<endl;
    if(x[2][0]>=x[2][1])
        cout<<"LIVE"<<endl;
    else cout<<"DEAD"<<endl;
    return 0;
}
