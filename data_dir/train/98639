#include<iostream>
using namespace std;
int a,b,c,d,i,e,f,g,n,m,k,l[100005],r[100005];
int main()
{
           cin>>n;
           for(i=1;i<=n;i++)
           {
           cin>>l[i]>>r[i];
           if(l[i]==1) a++;
           else b++;
           if(r[i]==1) c++;
           else d++;
           }
           cout<<min(a,b)+min(c,d);
}