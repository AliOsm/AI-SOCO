#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

int T, N, a[MAX], ans[MAX];

int main()
{
    optimiza_io
    cin >> T;
    while( T -- ) {
        cin >> N;
        for( int i = 1; i <= N; i ++ ) {
            cin >> a[i];
            ans[i] = 0;
        }
        for( int i = 1; i <= N; i ++ ) {
            if( ! ans[i] ) {
                int sz = 1;
                int p = a[i];
                while( p != i ) {
                    sz++;
                    p = a[p];
                }
                ans[i] = sz;
                p = a[i];
                while( p != i ) {
                    ans[p] = sz;
                    p = a[p];
                }
            }
            cout << ans[i] << " ";
        }
        cout << "\n";
        
    }
    return 0;
}