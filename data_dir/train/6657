#include <iostream>

using namespace std;

int main()
{
    long long int a,b,c,d,e,f;
    cin >> a >> b >> c >> d >> e >> f;
    if (!c&&d){cout << "Ron"; return 0;}
    if (!a&&b&&d){cout << "Ron"; return 0;}
    if (!(b*d*f)) {cout << "Hermione"; return 0;}
    if (!(a*c*e)) {cout << "Ron"; return 0;}
    if (a*c*e/a*b/c*d/e*f>a*c*e){cout << "Ron"; return 0;} else  {cout << "Hermione"; return 0;}
    return 0;
}