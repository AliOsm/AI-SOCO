#include <iostream>
#include <algorithm>
#include <cmath>
#include <vector>
#include <cstring>
using namespace std;

// DEFINES //

#define pb              push_back
#define mp              make_pair
#define lli             long long int
#define all(v)          v.begin(),v.end()
#define FP(i,a,b)       for(int i=a;i<=b;i++)
#define FM(ii,a,b)      for(int i=a;i>=b;i--)
#define N 100005

// GLOBALS //

// MAIN PROGRAM //

int main()
{
	int a[4], ans = 0;
	cin >> a[0] >> a[1] >> a[2] >> a[3];
	string str;
	cin >> str;
	FP (i, 0, str.length()-1)
		ans += a[str[i]-49];
	cout << ans << endl;
	return 0;
}
