#include <bits/stdc++.h>
 
using namespace std;
 
int n;
 
int main ()
{
    cin >> n;
 
 
    if ( n >= 11 && n <= 19 )
        cout << 4 << endl;
 
    if ( n == 20 )
        cout << 15 << endl;
 
    if ( n == 21 )
        cout << 4 << endl;
 
    if ( n < 11 || n > 21 )
        cout << 0 << endl;
 
}