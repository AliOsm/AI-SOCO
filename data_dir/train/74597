#include<bits/stdc++.h>
 
using namespace std;
 
#define lli long long int
 
#define ulli unsigned long long int
 
int main(){
   
    ios_base::sync_with_stdio(false); cin.tie(nullptr); cout.tie(nullptr);
    
    int n;
    
    cin>>n;
    
    set <int> a;
    
    for(int i=1; i<=n; i++){
    	
    	a.insert(i);
	}
	
	for(int i=0; i<n; i++){
		
		int x;
		
		cin>>x;
		
		a.erase(x);
	}
	
	cout<<*a.begin();
}
