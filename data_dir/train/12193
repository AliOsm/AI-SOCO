#include<iostream>
#include<fstream>
#include<cstring>

using namespace std;

int main()
{
    int t,i,n;
    ifstream in("input.txt");
    ofstream out("output.txt");
    in>>t;
    int v[t];
    for(i=0;i<t;i++)
    {
                    in>>n;
                    if(n%2==0)
                    v[i]=1;
                    else
                    v[i]=0;
    }
    for(i=0;i<t;i++)
    out<<v[i]<<"\n";
    return 0;
}
                     
