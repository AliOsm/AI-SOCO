typedef long long ll;
typedef long double ld;
typedef vector< int > vi;
typedef vector< char > vc;
typedef vector< vi > vvi;
typedef map< ll, ll > mapT;
typedef pair< int, int > pairT;

int main()
{
	//8 4 2 6
	int n, a[4] = {8, 4, 2, 6};
	scanf("%d", &n);
	if (n == 0)
		printf("1");
	else
		printf("%d", a[(n - 1) % 4]);
    return 0;
}