#include<bits/stdc++.h>
using namespace std;
bool cmp( pair<int,int>a ,pair<int,int> b)
{
   if(a.first==b.first)
         return a.second <b.second;
    else return a.first>b.first;
}
int main()
{

     int n,k;
     cin>>n;

    if(n==2)   cout <<"2"<<endl;
    else   cout <<"1"<<endl;
}
