#include <algorithm>
#include <cstdio>
#include <cstdlib>
#include <cstring>
#include <iostream>
#define N 100020
using namespace std;

char s[N];
int c[1 << 8];

int main(void)
{
    int n;
    int i;

    scanf("%d", &n);
    scanf("%s", s);

    for(i = 0; i < n; i ++)
        ++ c[s[i]];

    for(i = 0; i < c['n']; i ++)
        printf("1 ");
    for(i = 0; i < c['z']; i ++)
        printf("0 ");
    
    return 0;
}