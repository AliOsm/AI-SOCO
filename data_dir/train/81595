#include <bits/stdc++.h>

using namespace std;
const int MAXN = 100100;
int n;
char s[MAXN];

int main()
{
	scanf("%d %s", &n, s + 1);
	int cnt8 = 0;
	for(int i = 1; i <= n - 10; ++i)
		if(s[i] == '8')
			cnt8++;
	if(cnt8 > (n - 11) / 2)
		printf("YES\n");
	else
		printf("NO\n");
}