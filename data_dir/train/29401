#include <bits/stdc++.h>
using namespace std;

int main()
{
    int q; cin>>q;
    while (q--) {
        long long l, r, d; cin>>l>>r>>d;
        if (d<l) {
            cout<<d<<endl;
        }
        else {
            if (r%d==0)
                cout<<r+d<<endl;
            else
                cout<<(r+d-1)/d*d<<endl;
        }
    }
}
