int main(){
	cin>>n;
	rep(i,1,n)cin>>c[i];
	map<ll,ll>m;
	rep(i,1,n){
		cin>>a;
		m[a]=i;
	}
	rep(i,1,n){
		if(m[c[i]] > m[c[i+1]]){
			cout<<n-i;
			end;
		}
	}
}