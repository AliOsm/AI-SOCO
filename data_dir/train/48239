// Sky's the limit :)
#include<bits/stdc++.h>
using namespace std;
#define int long long

signed main() 
{
    //int t; cin >> t; while(t--) 
    {
        int a[11];
        for (int i = 0; i < 11; i++)
            cin >> a[10 - i];
        
        for (int i = 0; i < 11; i++) {
            double x = sqrt(abs(a[i])) + 5 * pow(a[i], 3);
            cout << "f(" << a[i] << ") = ";
            if (x <= 400)
                cout << fixed << setprecision(2) << x << '\n';
            else
                cout << "MAGNA NIMIS!\n";
        }
    }
    
    return 0;
}