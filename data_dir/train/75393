	int turn = 0;
	for (int i = 1;; i++) {
		if (i & 1) {
			a -= i;
			if (a < 0)
				return puts("Vladik");
		} else {
			b -= i;
			if (b < 0)
				return puts("Valera");
		}
	}
}

