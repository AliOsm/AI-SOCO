#include <bits/stdc++.h>
using namespace std;

int n, m;

int main() {
    cin >> n >> m;
    int t(1);
    for (int i = 0; i < n; ++i) {
        t = t * 2;
        if (t > m) {
            cout << m << endl;
            return 0;
        }
    }
    cout << m % t << endl;
    return 0;
}