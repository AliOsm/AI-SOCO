#include <bits/stdc++.h>
using namespace std;

const int MOD = int(1e6) + 3;

int pow(int a) {
	int cnt = 1;
	for(int i = 0; i < a; ++i) {
		cnt *= 2;
		cnt %= MOD;
	}
	return cnt;
}

string str;
char ch[101];
int ans = 0;

void solve() {
	reverse(str.begin(), str.end());
	for(int i = 0; str[i]; ++i) {
		if(str[i] == '1') {
			ans += pow(i);
			ans %= MOD;
		}
	}
	printf("%d", ans);
}


int main (int argc, char *argv[])
{
	scanf("%s", &ch);
	for(int i = 0; ch[i]; ++i) {
		if(ch[i] == '>') {
			str += "1000";
		} else if(ch[i] == '<') {
			str += "1001";
		} else if(ch[i] == '+') {
			str += "1010";
		} else if(ch[i] == '-') {
			str += "1011";
		} else if(ch[i] == '.') {
			str += "1100";
		} else if(ch[i] == ',') {
			str += "1101";
		} else if(ch[i] == '[') {
			str += "1110";
		} else {
			str += "1111";
		}
	}
	solve();
	
	return 0;
}

