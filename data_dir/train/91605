#include <bits/stdc++.h>
using namespace std;
#define pf push_front
#define pb push_back
#define FOR(i,begin,end) for (ulli i=begin;i<=end;i++)
#define rep(i,t) for (LL i=0;i<t;i++)
#define pii pair<LL,LL>
#define pi pair<pii,pii>
#define X first
#define Y second
typedef long long LL;
typedef unsigned long long int ulli;
const LL maxN=1e6+7,MOD=1e9+7,inf=1e9,N=1e4+7;
LL n,m,x,y,z,ans,dp[maxN];
int main()
{
  cin>>n;
  if(n%2)
  {    
    FOR(i,1,n/2)
      if(i%2)
        cout<<"aa";
      else
        cout<<"bb";
    cout<<"c\n";
    FOR(i,1,n/2)
      if(i%2)
          cout<<"bb";
      else
        cout<<"aa";
    cout<<"c\n";
    cout<<"d";
    FOR(i,1,n/2)
      if(i%2)
        cout<<"tt";
      else
        cout<<"oo";
    cout<<"\n";
    cout<<"d";
    FOR(i,1,n/2)
      if(i%2)
        cout<<"ii";
      else
        cout<<"xx";
    return cout<<"\n",0;
  } 
  FOR(i,1,n/2)
    if(i%2)
      cout<<"aa";
    else
      cout<<"bb";
  cout<<"\n";
  FOR(i,1,n/2)
    if(i%2)
        cout<<"bb";
    else
      cout<<"aa";
  cout<<"\n";
  cout<<'c';
  FOR(i,1,(n/2)-1)
    if(i%2)
        cout<<"ee";
    else
        cout<<"ff";
  cout<<"d\n";
  cout<<"c";
  FOR(i,1,(n/2)-1)
    if(i%2)
      cout<<"pp";
    else
      cout<<"tt";
  return cout<<"d\n",0;
}