#include<bits/stdc++.h>
using namespace std;
int N, Z, P, i, j, A[100];
int main () {
	ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
	cin>>N;
	for (i=1;i<=N;i++) {
		cin>>P;
		if (P==Z+1) {
			A[Z]=2000+i;
			Z++;
		}
	}
	cout<<Z<<'\n';
	for (i=0;i<Z;i++) {
		cout<<A[i]<<' ';
	}
	cout<<'\n';
}
