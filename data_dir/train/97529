#include <bits/stdc++.h>

using namespace std;





int main()
{

    ios_base::sync_with_stdio(true);
    cin.tie(NULL);

    long long q; cin >> q;
    while(q--) {
    	long long p; cin >> p;
    	long long o1 = 0, e1 = 0;
    	while(p--) {
    		long long ele; cin >> ele;
    		if(ele % 2) o1++;
    		else e1++;
    	}
    	long long q; cin >> q;
    	long long o2 = 0, e2 = 0;
    	while(q--) {
    		long long ele; cin >> ele;
    		if(ele % 2) o2++;
    		else e2++;
    	}
    	cout << e1 * e2 + o1 * o2 << endl;
    }

    return 0;



}



