#include<bits/stdc++.h>
using namespace std;

int main(){
    int Cnt=0;
    string s;
    cin>>s;

    for(int i=0;i<s.size();i++){
        if(s.substr(i,3)=="Ann") Cnt++;
        if(s.substr(i,4)=="Olya") Cnt++;
        if(s.substr(i,5)=="Danil") Cnt++;
        if(s.substr(i,5)=="Slava") Cnt++;
        if(s.substr(i,6)=="Nikita") Cnt++;
    }
    if(Cnt==1) printf("YES");
    else printf("NO");
}