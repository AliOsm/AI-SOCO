#include <iostream>
#include <algorithm>
#include <vector>
#include <cstdio>

using namespace std;

int main()
{
    int n;
    scanf("%d",&n);
    int res = 0;
    for(int i=0;i<n;i++)
    {
        int a;
        scanf("%d",&a);
        if(a>res) res  = a;
    }
    cout << res <<endl;
    return 0;
}
