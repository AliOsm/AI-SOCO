#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

int main() {
	int tc;
	cin >> tc;
	while(tc--) {
		int ang;
		cin >> ang;
		for (int i = 3; i <= 1e4; i++)
		{
			if( (ang * i) % 180 == 0 && ang * i < 180 * (i - 1)) {
				cout << i << "\n";
				break;
			}
		}
	}
}
