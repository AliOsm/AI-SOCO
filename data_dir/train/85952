#include <bits/stdc++.h>
using namespace std;
int n,m;
int l,r;

int main()
{
    //freopen("in.txt","r",stdin);
    //freopen("out.txt","w",stdout);
    scanf("%d %d",&n,&m);
    while(m--){
        scanf("%d %d",&l,&r);
    }
    int p=1;
    while(n--){
        printf("%d",p);
        p=!p;
    }
    return 0;
}
