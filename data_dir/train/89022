#include <iostream>
#include <cassert>
#include <cstdio>
#include <math.h>
#include <algorithm>
#include <vector>
#include <set>
#include <time.h>
#include <map>
#include <set>
#define ll long long
#define MAXX 111111
using namespace std;

string X[5];

int main() {
    string S;
    cin>>S;
    for(int i=0;i<5;i++)
        cin>>X[i];
    
    for(int i=0;i<5;i++) {
        if (X[i][0]==S[0] || X[i][1]==S[1]) {
            cout << "YES" << endl;
            return 0;
        }
    }
    cout << "NO" << endl;
    return 0;
}
