#include <bits/stdc++.h>

#define ll long long
#define fr first
#define se second
#define pb push_back

using namespace std;

const long long N = 1e3 + 7;

ll res, n, m , n1 , k , k1 , ans , sum , cnt;
string s;

const ll mod = 1e8;

ll d[N][N][2];
int main()
{
    cin >> n;
    while( n ){
            s = to_string(n);
            sort( s.begin() , s.end() );
            cnt = (s[s.size() - 1] - 48);
            n-=cnt;
            ans ++;
    }
    cout << ans;
}
