#include <bits/stdc++.h>
#define ll long long
using namespace std;
string s;
void print(int j)
{
	for (int i = 0; i < s.size(); i++)
		if (i != j)
			cout << s[i];
	exit(0);
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	cin >> s;
	for (int i = 0; i < s.size(); i++)
		if (s[i] == '0')
			print(i);
	for (int i = 1; i < s.size(); i++)
		cout << '1';
	return 0;
}
