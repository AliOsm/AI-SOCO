using namespace std;
typedef long long ll;
#define F first
#define S second
#define ii pair < int , int >

int main()
{
    int n ;

    cin >> n ;
    string s ;
    int x , y ;
    bool sol = 0 ;
    for ( int i = 0 ; i < n ; i++)
    {
        cin >> s >> x >> y ;

        if ( x >= 2400 && y > x )
            sol = 1 ;
    }
   cout <<(sol ? "YES" : "NO");

    return 0 ;
}
