#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

int main()
{
    optimiza_io
    int N;
    cin >> N;
    for( int i = 2; i <= N; i ++ )
        if( N % i == 0 ) {
            cout << i << N / i << "\n";
            exit( 0 );
        }
    return 0;
}

