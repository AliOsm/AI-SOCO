#include <bits/stdc++.h>

using namespace std;

int n, m, s1, s2, a, b;

int main()
{
    cin >> n;
    for (int i=1;i<=n;i++)
    {
        cin >> a >> b;
        if (a>b)
            s1++;
        if (b>a)
            s2++;
    }
    if (s1==s2)
        cout << "Friendship is magic!^^";
    if (s1>s2)
        cout << "Mishka";
    if (s2>s1)
        cout << "Chris";
}
