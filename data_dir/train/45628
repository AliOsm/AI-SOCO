#include <bits/stdc++.h>
using namespace std;
 
typedef pair<int,int > pp;
typedef long long ll;
 
int const N=3e5+2,oo=1e9,M=1e6;
ll const OO=1e18;
double const eps=1e-6,PI=acos(-1);
int mod=oo+7;

int q;
int main(){
    ios::sync_with_stdio(0);
    cin.tie(0);
    cin>>q;
    while(q--){
        ll l,r,d;
        cin>>l>>r>>d;
        if(d<l||d>r){
            cout<<d<<'\n';
            continue;
        }
        cout<<((r/d+1)*d)<<'\n';
    }
    return 0;
}

