#include<iostream>
#include<bits/stdc++.h>
using namespace std;
#define int1  long long int
int main(){
    
   int n;
   cin>>n;
   int i,j,k;
   int a[n];
   for(i=0;i<n;i++)
   cin>>a[i];
   
   sort(a,a+n);
   k=1;
   for(i=0;i<n;i++){
       if(a[i]>=k){
           k++;
       }
   }
    cout<<k-1<<endl;
    return 0;
}