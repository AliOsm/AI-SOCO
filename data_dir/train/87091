#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef long double ld;
#define F first
#define S second
#define ii pair <int,int>

int n;

int main()
{
    scanf("%d",&n);
    if(n<=10)
    {
        printf("0");
        return 0;
    }
    if(n<=19 || n==21)
    {
        printf("4");
        return 0;
    }
    if(n==20)
    {
        printf("15");
        return 0;
    }
    printf("0");
}
