/*
0855A. Tom Riddle's Diary
*/

#include<bits/stdc++.h>
using namespace std;

#define ll long long

int main(){
  int n;
  cin >> n;
  string s;
  map<string,int> oc;
  while(n--){
    cin >> s; oc[s]++;
    if(oc[s] == 1){cout << "NO";}
    else{cout << "YES";}
    cout << endl;
  }
}