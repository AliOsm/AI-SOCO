#include <iostream>

using namespace std;

int main()
{
    int n,m,l,r;
    string s;
    char c1,c2;
    cin>>n>>m;
    cin>>s;
    for (int i=0;i<m;i++)
    {
        cin>>l>>r>>c1>>c2;
        l--; r--;
        for (int i=l;i<=r;i++)
            if (s[i]==c1) s[i]=c2;
    }
    cout<<s;
    return 0;
}