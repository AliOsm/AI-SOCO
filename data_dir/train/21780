#include <bits/stdc++.h>
#define pb push_back
#define mp make_pair
#define pp pair<int, int>
#define ppp pair<int, pp>
#define fi first
#define se second
#define esp 1e-9
#define MAXE 1000000001
#define mod 1000000007
#define N 123
#define M 22345
typedef long long ll;
using namespace std;
int n;
string s, t;

int main() {
    //freopen("in.in", "r", stdin); //freopen("ou.ou", "w", stdout);
    cin>>n>>s>>t;
    if (t == "week") {
        if (n == 5 || n == 6) cout<<53;
        else
        cout<<52;
    }
    else {
        if (n <= 29) cout<<12;
        else
            if (n <= 30) cout<<11;
            else
                cout<<7;
    }
    /**/return 0;
}
