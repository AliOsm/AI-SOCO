#include <bits/stdc++.h>
using namespace std;

int a, b, c, d;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);

	cin >> a >> b >> c >> d;
	if (a == d && c == 0) {
		cout << 1 << '\n';
	} else if (c > 0 && a > 0 && a == d) {
		cout << 1 << '\n';
	} else {
		cout << 0 << '\n';
	}

	return 0;
}