#include <iostream>

using namespace std;

int main()
{
    long long a,b,c,x,y,z,ans=0;
    cin>>a>>b;
    cin>>x>>y>>z;
    if( (2*x + y ) >=a)
    {

        ans+=( (2*x + y) - a);
        
    }
    if( (3*z+y) >=b)
    {
        ans+=( (3*z + y) - b);
    }

    cout<<ans ;
    return 0;
}