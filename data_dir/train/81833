#include <bits/stdc++.h>

using namespace std;

#define MOD 1000000007
#define N 100005
#define M 30
#define ll long long 
#define ld long double
#define pb push_back
#define ff first
#define ss second

int main(){
    ios::sync_with_stdio(false); cin.tie(0); cout.tie(0);
    //freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout);
    int t; cin>>t;
    string x, y;
    int i, j, ans;
    while(t--){
        cin>>x>>y;
        reverse(x.begin(), x.end());
        reverse(y.begin(), y.end());
        i = 0; ans = 0;
        while(y[i] == '0'){
            i++;
        }
        while(x[i] == '0'){
            i++; ans++;
        }
        cout<<ans<<"\n";
    }
    return 0;
}