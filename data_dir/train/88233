#include <bits/stdc++.h>

#define SZ(x) (((int)x.size()))

typedef long long ll;
typedef long double ld;

using namespace std;

int na, nb, k, m, x, a, b;

int main()
{
	cin >> na >> nb >> k >> m;
	for (int i = 0; i < na; i++)
	{
		cin >> x;
		if (i == k - 1)
			a = x;
	}
	for (int i = 0; i < nb; i++)
	{
		cin >> x;
		if (i == nb - m)
			b = x;
	}
	if (a < b)
		cout << "YES";
	else
		cout << "NO";
	return 0;
}
