string a, b ;

void removing( string a, string b )
{
    //cout << a << " " << b << endl ;
    int j = 0 ;
    for( int i = 0 ; i < a.size() ; i ++ )
    {
        if( a[ i ] == b[ j ] )
        {
            j ++ ;
            v1.pb( i ) ;
        }
    }
    if( j == b.size() )
    {
//        for( auto x : v1 )
//        {
//            cout << x << " " ;
//        }
//        cout << endl;
        ll mx = v1[ 0 ] ;
        for( int i = 1 ; i < v1.size() ; i ++ )
        {
            mx = max( mx, ( ll ) v1[ i ] - v1[ i - 1 ] - 1 ) ;
        }
        mx = max( mx, ( ll ) a.size() - v1[ v1.size() - 1 ] - 1 ) ;
        //cout << mx << endl ;
        v.pb( mx ) ;
    }
    v1.clear() ;
}

void check( int x )
{
    for( int i = 0 ; i <= a.size() - x ; i ++ )
    {
        string s = a ;
        for( int j = i ; j < i + x ; j ++ )
        {
            s[ j ] = 'A' ;
        }
        removing( s, b ) ;
    }
}

int main()
{
    CIN ;
    cin >> a >> b ;
    for( int i = 0 ; i < a.size() ; i ++ )
    {
        check( i + 1 ) ;
    }
    v.pb( 0 ) ;
    cout << Max( v ) << endl ;

    return 0 ;
}
