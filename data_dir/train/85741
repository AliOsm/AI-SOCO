# include <stdio.h>
# include <string.h>
# include <math.h>

# include <algorithm>

using namespace std;

const int N = 100;

int n, m, k, l;
int a [N];

void Print (int par=0);

int main ()
{
# ifndef ONLINE_JUDGE
   freopen ("A.txt", "r", stdin);
   freopen ("A.out", "w", stdout);
# endif


# ifndef ONLINE_JUDGE
   int nn;
   scanf ("%d\n", &nn);
   for ( int ii=0; ii < nn; ii++ )
   {
      printf ("\n\nSAMPLE %d\n", ii);
# endif

      scanf ("%d %d\n", &m, &n);
      for ( int i=0; i < n; i++ )
         scanf ("%d", &a [i]);

      sort (a, a+n);

# ifndef ONLINE_JUDGE
   Print ();
# endif

      k = a [m-1] - a [0];
      for ( int i=1; i <= n-m; i++ )
         if ( a [i+m-1] - a [i] < k )
            k = a [i+m-1] - a [i];

      printf ("%d", k);

# ifndef ONLINE_JUDGE
   }
# endif

   return 0;
}

void Print (int par)
{
   if ( par == 0 )
   {
      printf ("\nm=%d n=%d\n", m, n);
      for ( int i=0; i < n; i++ )
         printf ("%d ", a [i]);
   }
   else
   {

   }
   printf ("\n");
}
