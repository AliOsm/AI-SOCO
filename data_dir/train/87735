#include <bits/stdc++.h>
#pragma GCC optimization ("unroll-loops")
#pragma GCC optimization ("O2")
#pragma GCC target("popcnt")
#pragma GCC target ("avx2")
using namespace std;

int main(){
	int n,t;
	cin >> n >> t;
	if(n == 1 && t == 10){
		cout << -1 << endl;
		return 0;
	}
	if(t == 10){
		cout << 1;
		for(int i = 1 ; i < n ; i += 1){
			cout << 0;
		}cout << endl;
	}else{
		for(int i = 0 ; i < n ; i += 1){
			cout << t;
		}cout << endl;
	}
}