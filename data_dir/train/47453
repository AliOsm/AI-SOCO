#include <bits/stdc++.h>
#define N 1000005
#define ll  long long
#define x first
#define y second
#define all(v) v.begin(),v.end()
using namespace std;
int main() {
            ios_base::sync_with_stdio(false);
            cin.tie(NULL );
            cout.tie(NULL );
            int t; cin>>t;
            while(t--){
                int n,m;
                cin>>n>>m;
                if(n%m==0)cout<<"YES\n";
                else cout<<"NO\n";
            }
            return 0;
}


