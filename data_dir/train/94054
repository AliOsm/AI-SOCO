#include<bits/stdc++.h>
using namespace std;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(NULL);

	int T;
	cin >> T;

	while(T--) {
		string s;
		cin >> s;
		int n = s.length();
		if(s[n-1] == 'o') cout << "FILIPINO\n";
		else if(s[n-1] == 'u') cout << "JAPANESE\n";
		else cout << "KOREAN\n";
	}

	return 0;
} 