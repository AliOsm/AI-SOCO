#include<iostream>
int main()
{
    int n;
    std::cin>>n;
    int *arr=new int[n];
    for(int i=0;i<n;i++)
    {
        std::cin>>arr[i];
    }
    for(int i=0;i<n;i++)
    {
        if(arr[i]==1)
        {
            std::cout<<"HARD";
            return 0;
        }
        else
        {
            
        }
    }
    std::cout<<"EASY";
}