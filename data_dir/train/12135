// Author: πα

#include <bits/stdc++.h>
using namespace std;
 
#define IOS ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define endl "\n"
#define long long long

int main()
{
    IOS;
    string s;
    cin >> s;
    int count = 0;
    for(auto c: s)
        if(c == '4' or c == '7')
            count++;
    if(count == 4 or count == 7)
        cout << "YES";
    else
        cout << "NO";
    return 0;
}
