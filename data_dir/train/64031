#include<iostream>
#include<math.h>
#include<algorithm>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.precision(20);
    int a,b,c,n,d=0,x;
    cin>>a>>b>>c>>n;
    for(int i=0;i<n;i++)
    {
        cin>>x;
        if(x>b&&x<c)
         d++;
    }
    cout<<d;}