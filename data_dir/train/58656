    #include <bits/stdc++.h>
    using namespace std;
    long long  i, j, n, c[123456],a[1234], b[125678],d[123456], l,ll, r, s, t, k, m, xh, x, x1, y,z;
    string p;
    vector <string> q[123];
    int main() {
        cin >>n>>m;
        for (i = 0; i < n; i ++){
            cin >>a[i];
            //cout << a[i]<<endl;
        }
        for (i = 0; i < m; i ++){
            cin >>b[i];
            //cout <<b[i]<<endl;
            while (a[l] > b[i] && l<n){
                l ++;
            }
            if (l >= n){
                cout <<i;
                return 0;
            }
            l++;
        }
 
        cout <<m;
        return 0;
    }