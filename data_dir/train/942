#include<bits/stdc++.h>
#define MOD 1000000007
#define ll long long
#define mp make_pair
#define pb push_back 
using namespace std;
int main(){
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	int a, b, c;
	cin >> a >> b >> c;
	int x = a - b;
	if(a == b && c == 0){
		cout << 0 << '\n';
		return 0;
	}
	if((x + c) > 0 && (x - c) > 0)
		cout << '+' << '\n';
	else if((x - c) < 0 && (x + c) < 0)
		cout << '-' << '\n';
	else 
		cout << '?' << '\n';
	

}