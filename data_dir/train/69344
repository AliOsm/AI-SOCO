#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef long double ld;
#define F first
#define S second
#define ii pair <int,int>

int n,h,ans;

int main()
{
    scanf("%d%d",&n,&h);
    while(n--)
    {
        int x;
        scanf("%d",&x);
        if(h>=x)
            ans++;
        else
            ans+=2;
    }

    printf("%d",ans);
}
