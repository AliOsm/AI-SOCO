#include <bits/stdc++.h>

using namespace std;

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0); cout.tie(0);

	int t;
	cin >> t;
	while (t--) {
		int a, b, n;
		cin >> a >> b >> n;
		if (n % 3 == 2) {
			cout << (a ^ b);
		}
		else if (n % 3 == 0) {
			cout << a;
		}
		else {
			cout << b;
		}
		if (t) {
			cout << '\n';
		}
	}

	return 0;
}