#include <bits/stdc++.h>
using namespace std;

const int MAX = 120;
char S[MAX];

int main(){
	int t;
	scanf("%d",&t);
	while( t--){
		int l;
		scanf("%d",&l);
		scanf("%s",S);
		int possible = 0;
		for(int i = 0;i<= l-11;i++){
			if( S[i] == '8' ) {
				possible = 1;
				break;
			}
		}
		if( possible ) printf("YES\n");
		else printf("NO\n");
	}
}
