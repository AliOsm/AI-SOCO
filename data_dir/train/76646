#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

string s;
int uno, dos, cero;

int main()
{
    optimiza_io
    cin >> s;
    // Busca el ultimo dos
    for( int i = 0; i < s.size(); i ++ )
    {
        if( s[i]  == '0' )
        {
            cout << "0";
            s[i] = 'X';
        }
        if( s[i] == '2' )
            break;
    }
    for( int i = 0; i < s.size(); i ++ )
        if( s[i] == '1' )
        {
            cout << "1";
            s[i] = 'X';
        }
    for( int i = 0; i < s.size(); i ++ )
        if( s[i] != 'X' )
            cout << s[i];
    return 0;
}
