//++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
//++--++--++--++--++--++--++--++--++--+--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
const int MAXN = 100001;
const int inf = 1000000001;
const ld INF = 1.0 / 0.0;
const ld EPS = 1e-7;
const ll mod = inf + 6;
const ld PI = acos(-1.0);
ll ans = 0;
signed main()
{
	fastio();
	string s;
	cin >> s;
	vector<int> p(s.size());
	int n = s.size();
	string s1(n, '#'), s2(n, '#'), s3(n, 'a');
	for (int i = 0; i < n; ++i)
	{
		s1[i] = (char)('a' + i % 26);
		s2[i] = (char)('a' + (i / 26) % 26);
		s3[i] = (char)('a' + ((i / 26) / 26) % 26);
	}
	string t1, t2, t3;
	cout << "? " << s1 << endl;
	cin >> t1;
	cout << "? " << s2 << endl;
	cin >> t2;
	cout << "? " << s3 << endl;
	cin >> t3;
	string ans(n, '#');
	for (int i = 0; i < n; ++i)
	{
		p[i] = (t1[i] - 'a') + (t2[i] - 'a') * 26 + (t3[i] - 'a') * 26 * 26;
	}
	for (int i = 0; i < n; ++i)
	{
		ans[p[i]] = s[i];
	}
	cout <<"! " << ans << endl;
//	sp;
}