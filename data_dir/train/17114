#include <bits/stdc++.h>
#define pb push_back
#define f first
#define s second
#define all(v) v.begin(),v.end()
#define rall(v) v.rbegin(),v.rend()
#define SZ(a) (int)a.size()
#define Flush fflush(stdout);
typedef long long ll ;
using namespace std ;
int main()
{
    ios::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
    int t ;
    cin >> t;
    while (t--) {
      int n, a, b, c, d;
      cin >> n >> a >> b >> c >> d ;
      int mn = n * (a-b);
      int mx = n * (a+b);
      if (mn > c + d || mx < c - d)
        cout <<"No\n";
      else
        cout <<"Yes\n";

    }
}
