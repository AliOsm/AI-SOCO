#include<bits/stdc++.h>
#include<cmath>
using namespace std;
#define ll long long
#define F first
#define S second
#define pb push_back
#define mp make_pair
const int MX=1*1e5 + 10;
const double PI = acos(-1.0), EPS = 1e-9;
int N, Q, M, k;
int main()
{
    cin>>N;
    vector<int> ans;
    N-=3;
    int i=1;
    while(N)
    {

        ans.pb(i);
        N--;
        i++;
        if(i > 4) i=1;
    }
    for(int i=5;i<8;i++) ans.pb(i);
    char s[]={'R','O','Y','G','B','I','V'};
    for(int i=0;i<ans.size();i++)
         cout<<s[ans[i]-1];
    return 0;
}
