int a, b;
signed main() {
	ios_base::sync_with_stdio(false); cin.tie(0);
	cin >> a >> b;
	for (int i = 1;; i++) {
		a *= 3; b *= 2;
		if (a > b) {
			cout << i << endl;
			return 0;
		}
	}
}