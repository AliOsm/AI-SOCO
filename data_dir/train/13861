#include<bits/stdc++.h>
using namespace std;
int main()
{
    int r, d, xx, yy, ri, d2, n, x = 0;
    cin>>r>>d>>n;
    while(n--)
    {
        scanf("%d %d %d", &xx, &yy, &ri);
        d2 = xx*xx+yy*yy;
        if(d2 <= (r-ri)*(r-ri) && d2 >= (r-d+ri)*(r-d+ri))
            x++;
    }
    cout<<x<<endl;
    return 0;
}
