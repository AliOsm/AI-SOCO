#define endl "\n"

inline void read(register int *n) {
	register char c; *n = 0;
	do { c = getchar(); } while (c < '0' || c > '9');
	do { *n = c-'0'+*n*10; c = getchar(); } while (c >= '0' && c <= '9');
}

int main() {
	string s;
	cin >> s;
	if (Size(s) == 1) {
		cout << 0 << endl;
		return 0;
	}
	int sum = 0, tot = 0;
	FOR(i, 0, Size(s)) sum += s[i]-'0';
	while (sum > 9) {
		int tmp = 0;
		while (sum) tmp += sum%10, sum /= 10;
		sum = tmp;
		tot++;
	}
	cout << tot+1 << endl;
	return 0;
}

