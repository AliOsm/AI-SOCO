#include<bits/stdc++.h>
#define GO ios::sync_with_stdio(0), cin.tie(0), cout.tie(0)
using namespace std;
typedef long long ll;
typedef vector<int> vec;
const int N = 3e5 + 5;
const int M = 7;
const int Mod = 1e9 + 7;

int main()
{
	GO;
	int t;
	cin >> t;
	while (t--)
	{
		ll n, m;
		cin >> n >> m;
		cout << (n % m == 0 ? "YES" : "NO") << '\n';
	}
	return 0;
}