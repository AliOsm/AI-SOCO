#include<bits/stdc++.h>
using namespace std;
long long a,b,c,d,i,e,f,g,n,m,k,l,A[100005];
int main()
{
           cin>>n;
           for(i=1;i<=n;i++)
           {
                      cin>>A[i];
                      if(A[i]==1) k=1;
           }
           if(k==1) cout<<"HARD";
           else cout<<"EASY";
}
