//In The Name of Allah
#include <bits/stdc++.h>

using namespace std;

typedef pair <int,int> pii;
typedef long long int LL;
typedef long double LD;

#define L first
#define R second

int main() {
	ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
	int tc;
	cin >> tc;
	while (tc--) {
		int a, b, n;
		cin >> a >> b >> n;
		if (n % 3 == 0)
			cout << a << '\n';
		if (n % 3 == 1)
			cout << b << '\n';
		if (n % 3 == 2)
			cout << (a ^ b) << '\n';
	}
	return 0;
}