//I_F_A
#include "bits/stdc++.h"
using namespace std;

int main(){

	long long a,b,c,d;
	cin >> a >> b >> c >> d;

	string s;
	cin >> s;

	long long len = s.length();
	long long ans = 0;

	for(long long i = 0 ; i < len ; i++){
		if(s[i] == '1'){
			ans = ans + a;
		}else if(s[i] == '2'){
			ans = ans + b;
		}else if(s[i] == '3'){
			ans = ans + c;
		}else{
			ans = ans + d;
		}
	}
	cout << ans;
}