#include<bits/stdc++.h>
using namespace std;
main()
{
    string a,b;
    cin>>a>>b;
    int f1=0,f2=0;
    if(a.size()!=b.size())
    {
        cout<<"NO"<<endl;
        return 0;
    }
    for(int i=0;i<a.size();i++)
    {
        if(a[i]=='1')f1=1;
    }
    for(int i=0;i<b.size();i++)
    {
        if(b[i]=='1')f2=1;
    }
    if(f1!=f2)
    {
        cout<<"NO"<<endl;
        return 0;
    }
    cout<<"YES"<<endl;
    return 0;
}
