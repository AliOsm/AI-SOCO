#include <iostream>

using namespace std;

int main () {
	int n;
	scanf ("%d", &n);
	string s;
	int d, a;
	d = a = 0;
	cin >> s;
	for (int i = 0; i < n; i++) {
		if (s[i] == 'A') a++;
		else {
			d++;
		}
	}
	if (a > d) cout << "Anton";
	else if (d > a) cout << "Danik";
	else {
		cout << "Friendship";
	}
}