#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define mp make_pair
using namespace std;
int main()
{
  ios::sync_with_stdio(false);
    ll n,k;
    cin>>n>>k;
    string sol="";
    for(int i=0;i<k;i++)
    {
        sol+=(char)(i+'a');
    }
    while(sol.length()<=n)
    {
        sol+=sol;
    }
    for(int i=0;i<n;i++)
    {
        cout<<sol[i];
    }
}
