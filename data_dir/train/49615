	cin.tie(0); cout.tie(0);
	cout.precision(20); cout.setf(ios::fixed);
	int n;
	cin >> n;

	cout << n + 1;
	return 0;
	string s;
	cin >> s;
	int l = 0, r = 0;
	for (auto& c : s) {
		if (c == 'L') l++;
		else r++;
	}
	return 0;
}