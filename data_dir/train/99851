#include <bits/stdc++.h>
#define pb push_back
#define f first
#define s second
#define all(v) v.begin(),v.end()
#define rall(v) v.rbegin(),v.rend()
#define SZ(a) (int)a.size()
#define Flush fflush(stdout);
typedef long long ll ;
using namespace std ;
int main()
{   
    int t ;
    cin >> t ;
    while (t--)
    {
        int a, b ;
        cin >> a >> b ;
        if (a == b || a % b == 0 )
        {
            cout << 0 <<"\n";
            continue ; 
        }
        if (a > b )
        {
            a %= b ;
            cout << b - a <<"\n"; 
        }else 
        {
            cout << b - a <<"\n";
        }
    }
}
