#include<iostream>
#include<cmath>
#include<vector>
#include<string>
#include<sstream>
#include<fstream>
#include<algorithm>
#include<set>
#include<map>
#include<string.h>
#include<cstdio>
#include<hash_map>
#include<queue>
using namespace std;
const int inf = 1000000001;
const int MOD = 1000000007;
typedef long long Int;
#define FOR(i,a,b) for(int i=(a); i<=(b);++i)
#define mp make_pair
#define pb push_back
#define sz(s) (int)((s).size())

const int N = 200009;

int mult(int a, int b, int c) {
	return min(a, min(b,c));
}

int cmp(int a, int b) {
	return a-b;
}

int a[N+1], b[N+1], c[N+1];

int main() {
	//freopen("input.txt","r",stdin);freopen("output.txt", "w", stdout);
	map<pair<int,int>, vector<pair<int,int> > > mem;
	int n;cin>>n;

	int best=0;
	int id1=0, id2=0;
	
	FOR(i,1,n) {
		int a,b,c;
		cin>>a>>b>>c;
		//scanf("%d %d %d",&a,&b,&c);
		int cur = mult(a, b, c);
		if(cmp(cur, best) > 0) best=cur, id1=i;

		mem[mp(a,b)].pb(mp(c, i));mem[mp(b,a)].pb(mp(c, i));
		mem[mp(a,c)].pb(mp(b, i));mem[mp(c,a)].pb(mp(b, i));
		mem[mp(b,c)].pb(mp(a, i));mem[mp(c,b)].pb(mp(a, i));
	}

	for(map<pair<int,int>, vector<pair<int,int> > >::iterator it=mem.begin(); it!=mem.end(); ++it) {
		vector<pair<int,int> > &cur = it->second;
		sort(cur.begin(), cur.end());
		int i1=0, i2=0;
		int v1=0, v2=0;
		
		for(int i=sz(cur)-1; i>=0; --i) {
			if(i1==0) {
				i1=cur[i].second;
				v1=cur[i].first;
			} else
			if(i2==0 && cur[i].second != i1) {
				i2=cur[i].second;
				v2=cur[i].first;
			}
		}


		if(i1 && i2) {
			int curr=mult(it->first.first,it->first.second,v1+v2);
			if(cmp(curr, best) > 0) best=curr, id1=i1, id2=i2;
		}
	}
	//cerr<<best<<endl;

	int cnt = int(bool(id1))+int(bool(id2));
	cout<<cnt<<endl;
	cout<<id1;
	if(id2) cout<<" "<<id2;
	cout<<endl;
}