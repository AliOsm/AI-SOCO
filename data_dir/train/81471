#include <bits/stdc++.h>
using namespace std;
#define eb emplace_back
#define emp emplace
#define fi first
#define se second
#define INF 0x3f3f3f3f
typedef long long ll;
typedef pair<long long, long long> ii;

int main(void) {
	ios_base::sync_with_stdio(false);

	int n; cin >> n;

	for (int i = 0; i < n; i++) {
		int x; cin >> x;

		bool ok = 0;
		for (int j = 0; j < 100; j++) {
			for (int k = 0; k < 100; k++) {
				if (7*j+3*k == x) ok = 1;
			}

		}

		if (ok) cout << "YES" << endl;
		else cout << "NO" << endl;

	}
    
	return 0;
}

