#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

int main()
{
    ios_base::sync_with_stdio(false);
    int test;
    cin >> test;
    while(test--) {
        int x;
        cin >> x;
        for(int i = 0; i < x; i++) {
            for(int j = 0; j < x; j++) {
                int cur = 3 * i + 7 * j;
                if(cur == x) {
                    cout << "YES" << endl;
                    goto endLoop;
                }
            }
        }
        cout << "NO" << endl;
        endLoop: ;
    }
}

