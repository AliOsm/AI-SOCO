#include<bits/stdc++.h>
using namespace std;
#define ll long long
const int N=3e5+5;
int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);
    int n,m;
    cin>>n>>m;
    if(n<=27){
            ll x=1;
        for(int i=0;i<n;i++){
            x*=2;
        }
    cout<<m%x<<endl;
    }
    else {
        cout<<m<<endl;
    }

	return 0;
}
