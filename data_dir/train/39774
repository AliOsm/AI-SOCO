#include<bits/stdc++.h>
using namespace std;
#define fr first
#define sc second
typedef long long ll;
typedef double ld;
typedef pair<int,int> p2;
typedef pair<ll,p2> p3;
typedef vector<int> vi;
const ll Mod = 7 + 1e9;
const ll INF32 = 5 + 2e9;
const ll INF64 = 1 + 1e3;
const ll MAX = 5+ 1e6;

int N,M,a[MAX];

int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    cin >> N;
    while(N>=10)
    {
        N++;
        while(N%10 == 0)
            N/=10;
        M++;
    }
    cout << M+9 << endl;
    return 0;
}
