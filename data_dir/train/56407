#include <bits/stdc++.h>
using namespace std;
#define forn(i, n) for(int i = 0; i < (n); i++)

int n, m, tot, a, b;

int main(){
    scanf("%d %d", &n, &m);
    forn(i, n){
        forn(j, m){
            scanf("%d %d", &a, &b);
            if(a || b) tot++;
        }
    }
    printf("%d\n", tot);
    return 0;
}
