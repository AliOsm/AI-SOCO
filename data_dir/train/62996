#include <bits/stdc++.h>
using namespace std;
typedef long long intt;

int main()
{
    int n,i;
    cin>>n;
    string s;
    intt sum=0;
    for(i=0;i<n;i++)
    {
        cin>>s;
        if(s[0]=='I')
            sum+=20;
        else if(s[0]=='D')
            sum+=12;
        else if(s[0]=='O')
            sum+=8;
        else if(s[0]=='C')
            sum+=6;
        else
            sum+=4;
    }
    cout<<sum;

    return 0;
}
