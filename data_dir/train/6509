#include<bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    int n;
    string s;
    cin>>n;
    int sum=0;
    for(int i=1; i<=n; i++)
    {
        cin>>s;
        if(s=="Tetrahedron")
        {
            sum+=4;
        }
        else if(s=="Cube")
        {
            sum+=6;
        }
        else if(s=="Octahedron")
        {
            sum+=8;
        }
        else if(s=="Dodecahedron")
        {
            sum+=12;
        }
        else
        {
            sum+=20;
        }
    }
    cout<<sum<<endl;
    return 0;
}
