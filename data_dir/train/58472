#include <bits/stdc++.h>

using namespace std;


int main()
{
    int n,k; cin >> n >> k;
    int ans = 0;
    ans += (n*2)/k;
    if(n*2 % k  != 0) ans++;
    ans += (n*5)/k;
    if(n*5 % k  != 0) ans++;
    ans += (n*8)/k;
    if(n*8 % k  != 0) ans++;
    return cout << ans ,0;
}
