#include<bits/stdc++.h>
#define pb push_back
#define adelkhah main
//#define int long long
using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
typedef long double ld;
int32_t adelkhah()
{
    int n,m;cin >> n >> m;
    int sw = 0;
    char a;
    for(int i = 0; i < n; i++){
        for(int j = 0; j < m; j++){
            cin >> a;
            if(a == 'B' || a == 'W' || a == 'G'){
                continue;
            }
            else{
                sw = 1;
            }
        }
    }
    if(sw == 1){
        cout << "#Color";
        return 0;
    }
    cout << "#Black&White";
    return 0;
}
