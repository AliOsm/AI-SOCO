#include <bits/stdc++.h>
#define fi first
#define se second
#define mp make_pair
#define lo long long 
#define inf 1000000000
#define md 1000000007
#define pb push_back
#define li 100005
using namespace std;
lo int n,t,l,r;
int main(){
	scanf("%lld",&t);
	for(int i=1;i<=t;i++){
		scanf("%lld %lld %lld",&n,&l,&r);
		if((n/l)*r>=n){
			printf("Yes\n");
		}
		else printf("No\n");
	}
	
	return 0;
}

