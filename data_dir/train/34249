#include <bits/stdc++.h>

using namespace std;
 
int main(){
	
	ios_base ::sync_with_stdio(0); cin.tie(0); cout.tie(0);
	
	long long int n,ans=0;
	
	cin>>n;
	
	string N;
	
	cin>>N;
	
	for(int i=0; i<n; i+=1){
		
		if(N[i] == '2' || N[i] == '4' || N[i] == '6' || N[i] == '8'){
			
			ans += i+1;
		}
	}
	
	cout<<ans;
}
