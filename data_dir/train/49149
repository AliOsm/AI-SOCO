#include <bits/stdc++.h>
using namespace std;

int n, a, d;
string str;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(NULL);

    cin >> n >> str;
    for (int i = 0; i < n; i++){
        if (str[i] == 'A'){
            a++;
        } else{
            d++;
        }
    }

    if (a == d){
        cout << "Friendship" << endl;
    } else{
        cout << (a < d ? "Danik" : "Anton") << endl;
    }
    return 0;
}
