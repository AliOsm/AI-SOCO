int nnext[4][2] = { { 0, 1 }, { 1, 0 }, { 0, -1 }, { -1, 0 } };
ll gcd(ll x, ll y){ return !y ? x : gcd(y, x%y); }

int main(){
    //freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout
    double n, ans = 0;
    cin >> n;
    vd array(n);
    rep(i, n) cin >> array[i], ans += array[i] * array[i] * pi;
    sorta(array);
    rep(i, n) (i & 1) ? ans -= (2 * array[i] * array[i] * pi) : 0;
    if (ans < 0) ans *= -1;
    cout << sp(10) << ans << endl;
    return 0;
}