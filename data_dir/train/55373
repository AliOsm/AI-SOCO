/*jai mata di
let's rock*/
#include<bits/stdc++.h>
using namespace std;
int cnt[10];
int main()
{
    int n=4;
    int i;
    for(i=1;i<=n;i++)
       scanf("%d",&cnt[i]);
    if(cnt[3]==0)
    {
        if(cnt[1]==cnt[4])
            printf("1\n");
        else printf("0\n");
    }
    else
    {
        if(cnt[1]==0) printf("0\n");
        else
        {
            if(cnt[1]==cnt[4])
                printf("1\n");
            else printf("0\n");
        }
    }
    return 0;
}
