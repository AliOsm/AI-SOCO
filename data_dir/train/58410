#include<bits/stdc++.h>
using namespace std;

#define fr first
#define sc second

typedef long long ll;
typedef pair<ll,int> pii;

ll const N = 1e6 + 10;
ll const OO = 1e9;
ll const mod = 998244353;

ll L[N],R[N];
int main() {
#ifdef LOCAL
    freopen("in.in", "r", stdin);
#endif
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    ll n,m;
    cin >> n >> m ;
    ll ans =1;
    for(int i=0;i<n+m;i++){
        ans*=2;
        ans%=mod;
    }
    cout << ans ;
}
