#include <bits/stdc++.h>

using namespace std;

string s;

int main() {
  ios_base::sync_with_stdio(0);
  cin.tie(0);
  cout.tie(0);

  cin >> s;
  set<char> st;
  for (auto e : s) {
    st.insert(e);
  }
  cout << ((int) st.size() & 1 ? "IGNORE HIM!" : "CHAT WITH HER!") << '\n';

  return 0;
}
