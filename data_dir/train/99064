void READ (string INPUT = "NO" , string OUTPUT = "NO" )
{
    if (INPUT != "NO")
    freopen(INPUT.c_str() , "r" , stdin );
    if (OUTPUT!="NO")
    freopen ( OUTPUT.c_str() , "w" , stdout);
}

int main(){
    
    int n ; 
    scanf("%d" , &n); 
    printf ("%d\n" , 1+(n==2)) ; 
   return 0 ;
}

