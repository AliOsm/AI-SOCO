#include<bits/stdc++.h>
using namespace std;
#define F first
#define S second
#define ll long long
#define pb push_back
#define pf push_front
#define Mid ((l+r)/2)
#define pi pair<int,int>
#define pii pair<int,pi>
#define go ios::sync_with_stdio(0); cin.tie(0); cout.tie(0)
int n,s;
int main()
{
    go;
    cin>>n>>s;
    if(n+n>s)
        return cout<<"NO",0;
    cout<<"YES\n"<<2+s-n-n<<" ";
    for(int i=1;i<n;i++)
        cout<<2<<" ";
    cout<<endl<<1<<endl;
    return 0;
}
