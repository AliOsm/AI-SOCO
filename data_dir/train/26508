#include<bits/stdc++.h>
using namespace std;

int n, x[4];

int main() {
    ios_base::sync_with_stdio(false);
    cin >> n;
    cout << "YES\n";
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < 4; ++j)
            cin >> x[j];
        cout << 2 * (abs(x[0]) % 2) + abs(x[1]) % 2 + 1 << "\n";
    }

    return 0;
}