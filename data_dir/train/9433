const ll N = 1e6 + 1;

ll n , m , l , k , cnt , ans , x , y ;
ll a [ N ];
set < ll > st;
map < ll , ll > mp;
vector < ll > v , d ;
string s , t;
int main()
{TIME
    cin >> n ;
    for ( ll i = 0 ; i < n ; i ++ )
    {
        cin >> m ;
        x = max ( x , m );
    }
    if ( x - 25 < 0 )
    cout << 0;
    else
    cout << x - 25;

}













