#include<bits/stdc++.h>
using namespace std;

int main(){
    int n,m=0,t=0,A[105],sum=0;
    string a,b="";
    cin >>a >>b;
    for(int i=0;i<a.size();i++){
        if(!(a[i]==b[a.size()-1-i])){
            cout << "NO";
            return 0;
        }
    }
    cout << "YES";
    return 0;
}
