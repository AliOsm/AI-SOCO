#include <bits/stdc++.h>
using namespace std;
#define ll long long
const int N=1e6+5;
const int mod=1e9+7;
int arr[500];
int main()
{
    int k,n;
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        int a;
        scanf("%d",&a);
        if(a%2){
            cout<<"First"<<endl;
            return 0;
        }
    }
    cout<<"Second"<<endl;

}