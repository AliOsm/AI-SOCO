#include<bits/stdc++.h>
using namespace std;

#define si(n) scanf("%d",&n)
#define ll long long
#define f first
#define s second
#define MAX 200005

int a[MAX],n;
int ans=0;

int main()
{
    //freopen("input.txt","r",stdin);
    int i,j;
    si(n);
    for(i=1;i<=n;i++){
        si(j);
        a[j]++;
        ans=max(ans,a[j]);
    }
    cout<<ans<<endl;


    return 0;
}
