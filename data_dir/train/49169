#define ll          long long
#define pb          push_back
#define mp          make_pair
#define pii         pair<int,int>
#define vi          vector<int>
#define all(a)      (a).begin(),(a).end()
#define F           first
#define S           second
#define sz(x)       (int)x.size()
#define hell        1000000007
#define endl        '\n'
#define rep(i,a,b)	for(int i=a;i<b;i++)
using namespace std;

void solve(){
	int n;
	cin>>n;
	string temp;
	vi x(n);
	rep(i,0,n)cin>>x[i];
	getline(cin,temp);
	rep(i,0,n){
		string s;
		getline(cin,s);
		int k=count_if(all(s),[](char ch){return ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u' || ch=='y';});
		if(k!=x[i]){
			cout<<"NO";
			return;
		}
	}
	cout<<"YES";
}

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	int t=1;
//	cin>>t;
	while(t--){
		solve();
	}
	return 0;
}