#include <bits/stdc++.h>
using namespace std;
 
#define mp make_pair
#define pb push_back
#define pp pop_back
#define F first
#define S second
#define int long long
#define debug(x) cerr << #x << " : " << x << endl;
 
typedef long long ll;
typedef pair<int, int> pii;
typedef pair<string, string> pss;
typedef vector<pii> vpii;
 
const int mod = 1000000007LL;
const int inf = 1e18;
const int maxx = 1e2 + 19;
 
void solve() {
    int n;
    int sum = 0;
    cin >> n;
    for (int i = 0; i < n; i++) {
        int tmp;
        cin >> tmp;
        if (tmp <= 2048)
            sum += tmp;
    }
    cout << ((sum >= 2048) ? "YES" : "NO") << endl;
    return;
}
 
int32_t main() {
    ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
    int test;
    cin >> test;
    while (test--)
        solve();
    return 0;
}
