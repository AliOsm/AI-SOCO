#include<bits/stdc++.h>
using namespace std;
int main()
{
   int n,i,k=0,y=2001,ara[101],a;
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>a;
        if(a==(k+1))
        {
            ara[k]=y+i;
            k++;
        }
    }
    cout<<k<<endl;
    for(i=0;i<k;i++)
        cout<<ara[i]<<' ';
}