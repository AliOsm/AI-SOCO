
#include <bits/stdc++.h>
#define mp make_pair
#define pb push_back
using namespace std;
typedef long long int lli;
typedef pair<int,int> pii;

int main()
{
	int n; cin>>n;
	
	int s = 1 , e = n;
	for ( int i=0 ; i<n ; i++ )
	{
		if ( i%2 == 0 ) cout<<s++<<" ";
		else cout<<e--<<" ";
	}
	cout<<"\n";
	
	return 0;
}   