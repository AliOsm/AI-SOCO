#include <bits/stdc++.h>
using namespace std;



int main() {
    int x1, x2, x3, x4, x5;
    scanf("%d %d %d %d %d", &x1, &x2, &x3, &x4, &x5);
    if((x1 + x2 + x3 + x4 + x5) % 5 == 0 && (x1 + x2 + x3 + x4 + x5) > 0) {
        printf("%d", (x1 + x2 + x3 + x4 + x5) / 5);
    }
    else{
        puts("-1");
    }
}
