#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

const ll MOD = ll(1e9) + 7;

int n, k;
vector<vector<ll> > d;
ll ans = 0;

int main (int argc, char *argv[])
{
	scanf("%d%d", &n, &k);
	d.resize(k + 1);
	for(int i = 1; i <= k; ++i) {	
		d[i].resize(n + 1, 0);
	}
	for(int j = 1; j <= n; ++j) {
		d[1][j] = 1;
	}
	for(int i = 1; i < k; ++i) {
		for(int j = 1; j <= n; ++j) {
			for(int u = 1; j * u <= n; ++u) {
				d[i + 1][j * u] += d[i][j];
				d[i + 1][j * u] %= MOD;
			}
		}
	}
	for(int j = 1; j <= n; ++j) {
		ans += d[k][j];
	}
	printf("%lld", ans % MOD);

	return 0;
}

