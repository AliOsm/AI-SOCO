#include<bits/stdc++.h>
using namespace std;

int main(){
    int n,k,i;
    cin>>n>>k;
    if(n==k)cout<<-1<<endl;
    else{
        k=n-k;
        cout<<k<<" ";
        i=2;
        while(i<=k){
            cout<<i-1<<" ";
            i++;
        }
        while(i<=n){
            cout<<i<<" ";
            i++;
        }
    }
    return 0;
}
