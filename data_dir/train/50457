int main() {
#ifndef ONLINE_JUDGE
freopen("input.txt", "r", stdin);
//freopen("output.txt", "w", stdout);
#endif // ONLINE_JUDGE
ios_base::sync_with_stdio(false);

   string s; cin >> s;
   char cur = 'a';

    for(auto x : s) {
        if(x > cur) {
            cout << "NO\n";
            return 0;
        }
        else if(x == cur) 
            cur++;
    }

    cout << "YES\n";

    return 0;
}










