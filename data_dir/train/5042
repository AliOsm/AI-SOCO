#include <bits/stdc++.h>
using namespace std;
main()
{
    long long n,k;
    while(cin>>n>>k)
    {
        if(k>n)
        {
            printf("NO\n");
        }
        else
        {
            long  long k1=n/k;
            if(k1%2==0)
            {
                printf("NO\n");
            }
            else
                printf("YES\n");
        }
    }
}
