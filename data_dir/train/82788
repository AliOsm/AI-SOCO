#include<bits/stdc++.h>
#define sd(x) scanf("%d",&x)
#define sl(x) scanf("%ld",&x)
#define sld(x) scanf("%lld",&x)
#define sf(x) scanf("%f",&x)
#define slf(x) scanf("%lf",&x)
#define sc(x) scanf("%c",&x)
#define ss(x) scanf("%s",&x)
#define pf printf
#define fpf fprintf
#define rn return
#define br break
#define cn continue
#define pb push_back
#define f(i,x,y,z) for(int i=x;i<y;i=i+z)
#define f1(i,x,y,z) for(int i=x;i>y;i=i-z)
#define N 100005
#define M 1000007
#define MAX 9000005
#define pi 3.1415926535
#define mp make_pair
using namespace std;

///vector < int > v;

int main()
{
    int n;
    sd(n);
    pf("%d\n",n+n/2);
    f(i,2,n+1,2) pf("%d ",i);
    f(i,1,n+1,2) pf("%d ",i);
    f(i,2,n+1,2) pf("%d ",i);
    rn 0;
}
