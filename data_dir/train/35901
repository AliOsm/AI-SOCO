#include <iostream>
#include <string>
#include <vector>
#include <set>
#include <map>

using namespace std;

int main()
{
    int n;
    string s;
    cin >> n >> s;
    for (int i = 0; i+1 < s.size(); ++i)
    {
        if (s[i] != s[i+1])
        {
            cout << "YES\n" << s[i] << s[i+1] << endl;
            return 0;
        }
    }
    cout << "NO";
}
