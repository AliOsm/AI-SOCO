#include <iostream>
#include <string>

using namespace std;

int main()
{
	int n;	cin >> n;
	bool flag=0;
	for(int i=0 ; i<n ; i++)
	{
		string handle;
		int b,a;
		cin >> handle >> b >> a;
		if(b>=2400&&a>b)	flag=1;
	}
	if(flag)	cout << "YES" << endl;
	else cout << "NO" << endl;
}