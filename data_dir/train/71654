#include <bits/stdc++.h>
using namespace std;
typedef long long intt;
#define pb push_back
#define sc second
#define fr first
#define mk make_pair

int main()
{
   
   
int n;
cin>>n;
int a[n];
for(int i=0;i<n;i++)
{
    cin>>a[i];
    if(a[i]==1)
    {
        cout<<"HARD";
        return 0;
    }
}

cout<<"EASY";



    return 0;
}
