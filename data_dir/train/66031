#include <bits/stdc++.h>
#define int long long
using namespace std;


int32_t main() {
        int n; cin >> n;
        int cnt = 0;
        int i;
        for(i = 1; i * i < n; i++) {
                if(n % i==0) cnt += 2;
        }
        if(i * i == n) ++cnt;
        cout << cnt << endl;
}
