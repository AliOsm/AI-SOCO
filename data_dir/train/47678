#include <bits/stdc++.h>
#define ll long long int
#define pb push_back
#define mp make_pair
#define mod 1000000007
#define pi 3.141592653589793
#define debug(x) cout << #x << " = " << x << '\n';
using namespace std;



int main() {
    cout << fixed << setprecision(10);
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);


    int t;
    cin >> t;
    while (t--) {
        ll a, b;
        cin >> a >> b;
        int ans = 0;
        ll cur = 9;
        while (cur <= b) {
            ++ans;
            cur = cur * 10 + 9;
        }
        cout << ans * a << '\n';
    }

}