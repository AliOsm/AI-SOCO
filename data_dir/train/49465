#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
long long i,j,k,l,m,n,a[1000011],b[11][11],c,d,e,s,t,f[1111],g[1111];
string p,q,r;

int main() {
    
    cin >>n;
    
    for (i = 0; i < n; i ++){
        cin >> d;
        s = max (s,min(d-1,1000000-d));
    } 
    cout << s;
    return 0;
}