#include <bits/stdc++.h>

//____By:sirjan13____

#define ll long long int
#define mod 1000000007
#define st struct
#define show(a) for(i=0;i<a.size();i++) cout<<a[i]<<" ";
#define p printf
#define vi vector<int>
#define vs vector<string>
#define vll vector<long long int>
#define pb push_back
#define pi pair<int,int>
#define mp make_pair
#define fast_io() cin.sync_with_stdio(false);cout.sync_with_stdio(false);
#define long_zero 0ll
#define long_one 1ll

using namespace std;


int main() {
    fast_io()
    int n;
    ll x,s=0,d=0;
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>x;
        s+=x;
    }
    for(int i=1;i<n;i++){
        cin>>x;
        d+=x;
    }
    cout<<s-d<<endl;
    for(int i=2;i<n;i++){
        cin>>x;
        d-=x;
    }
    cout<<d;
	return 0;
}
