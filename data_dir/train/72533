# include <bits/stdc++.h>
# define ll long long
# define f first
# define s second
using namespace std;
void query(){
	int n, k1, k2, mx1 = 0, mx2 = 0;
	cin >> n >> k1 >> k2;
	for(int i = 1; i <= k1; i ++){
		int x;
		cin >> x;
		mx1 = max(mx1, x);
	}
	for(int i = 1; i <= k2; i ++){
		int x;
		cin >> x;
		mx2 = max(mx2, x);
	}
	if(mx1 > mx2) cout << "Yes\n";
	else cout << "No\n";
}
int main(){
	ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
	int q = 1;
	cin >> q;
	while(q --){
		query();
	}
}