#include <iostream>
#include<string>
using namespace std;
int main()
{
    int n,x; cin>>n>>x;
    if(((n%6==5||n%6==0)&&x==0)||((n%6==4||n%6==1)&&x==1)||((n%6==2||n%6==3)&&x==2))cout<<0;
    else
        if(((n%6==3||n%6==0)&&x==1)||((n%6==1||n%6==2)&&x==0)||((n%6==4||n%6==5)&&x==2))cout<<1;
    else cout<<2;
}
