#include <iostream>
#include <cmath>
#include <algorithm>
#include <iomanip>
#include <cstdio>
#include <map>
#include <string>
#include <set>
#include <vector>
#define ll long long
using namespace std;
ll n,m,ans,a[1010],k,p,d;
long double x,y;
string s;
int main(){
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    sort(a,a+n);
    for(int i=n-1;i>=0;i--){
        x=a[i];
        y=floor(sqrt(x));
        if(y*y!=x)return cout<<a[i],0;
    }
    return 0;
}
