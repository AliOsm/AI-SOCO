
#include<bits/stdc++.h>
using namespace std;
#define int long long
 
signed main()
{
    ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    
    //int t=1 ; cin>>t ; while(t--)
    {
        long double n, ans = 0;
        cin >> n;
 
        for(int i=1;i<=n;i++)
            ans += 1.0/i;
 
        cout << fixed << setprecision(8) << ans << '\n';
 
 
    }
    
    cerr << "Time elapsed : " << 1.0 * clock() / CLOCKS_PER_SEC << " sec \n";
    return 0;
}
 