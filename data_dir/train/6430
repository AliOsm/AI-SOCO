#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <bitset>
#include <string>
#include <vector>
#include <algorithm>
#include <ctime>
#include <queue>
#include <cstring>
#include <set>
#include <map>
#include <sstream>
#include <cmath>
#include <fstream>
#include <list>
using namespace std;
#define rp(i,n) for(int (i)=0;(i)<(n);++(i))
#define pb push_back
#define L(s) (int)s.size()
#define mp make_pair
#define pii pair<int,int>
#define x first 
#define y second
#define inf 1000000000
#define VI vector<int>
#define ll long long
#define all(s) (s).begin(),(s).end()
#define C(u) memset((u),0,sizeof((u)))
#define ull unsigned ll
#define uint unsigned int 
#define pdd pair<double,double>
int n,m;
inline bool pr(int x)
{
	for(int i=2;i*i<=x;++i)
		if (x%i==0)
			return 0;
	return 1;
}
int main()
{
	cin>>n>>m;
	for(int i=n+1;;++i)
		if (pr(i))
		{
			if (i==m)
				cout<<"YES\n";
			else
				cout<<"NO\n";
			return 0;
		}
}