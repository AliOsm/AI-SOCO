#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef pair<int,int> pii;

int main(){
	int x,y,z,t1,t2,t3;
	scanf("%d%d%d%d%d%d",&x,&y,&z,&t1,&t2,&t3);
	int stairs=abs(x-y)*t1;
	int elevator=abs(z-x)*t2+3*t3+abs(x-y)*t2;
	if(stairs<elevator) puts("NO");
	else puts("YES");
	return 0;
}