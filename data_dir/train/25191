#include<bits/stdc++.h>

using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);

    long long cnt = 0, n;
    string s;
    cin >> n >> s;
    for (int i = 0; i < n; i++)
        if (s[i] == '0' || s[i] == '2' || s[i] == '4' || s[i] == '6' || s[i] == '8')
            cnt += i + 1;
    cout << cnt << '\n';

    return 0;
}
