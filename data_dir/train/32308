#include<bits/stdc++.h>
using namespace std;

#define scl(x) scanf("%lld",&x)
#define sc(x)  scanf("%d",&x)
#define ll long long
#define lop(i,n) for(int i=0;i<n;++i)
typedef pair<int,int> ii;
typedef pair<ll,ll> pll;

ll a,b,c;
int main(){
#ifndef ONLINE_JUDGE
	freopen("i.txt","r",stdin);
#endif
	cin>>a>>b>>c;
	for(int i=0;i*a<=c;++i){
		if((c-i*a)%b==0)return puts("Yes");
	}
	puts("No");
}
