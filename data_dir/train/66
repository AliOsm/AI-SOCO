#include<bits/stdc++.h>
using namespace std;


int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int n,m,ans=0;
    cin>>n>>m;

    while(m>n)
    {
        if(m%2!=0)
        {
            m++;
        }
        else
        {
            m=m/2;
        }
        ans++;
    }
    ans=ans+(n-m);
    cout<<ans<<endl;
    return 0;
}
