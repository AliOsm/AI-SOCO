#include <bits/stdc++.h>

using namespace std;

#define MAX 507
#define pb push_back
#define fr first
#define sc second
#define mk make_pair
#define ll long long

const long long N = (int)1e5 * 2 + 7;
const int INF = (int)1e9 + 7;

int main() {
        int n, res; cin >> n;
        int ans = 0;
        for (int i = 1; i <= n; i++) {
                int a; cin >> a;
                ans += a;
        }
        cout << ans / n;
}
