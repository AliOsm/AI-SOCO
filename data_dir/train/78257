const int maxN = 2e3 + 9, lg = 19, MOD = 1e9 + 7, maxV = 1e6 + 9, INF = INT_MAX / 2;

int arr[maxN], n;

int main(){
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    
    int t;
    cin >> t;
    while(t--){
        string st;
        cin >> st;
        sort(st.begin(), st.end());
        if(st[0] == st.back())cout << "-1\n";
        else cout << st << '\n';
    }
    
}

