#include <bits/stdc++.h>
using namespace std;

#define int long long
#define f(i,n) for(int i=0;i<n;i++)
#define rep(i,a,b) for(int i=a;i<=b;i++)

class BDimaAndTextMessages {
public:
	void solve(std::istream& in, std::ostream& out) {
		int n; in >> n;
		string s = "<3";
		f(i, n){
			string x; in >> x;
			s += x;
			s += "<3";
		}
		string text; in >> text;
		int i = 0, j = 0;
		n = s.length();
		int m = text.length();
		while(i<n and j<m){
			if(s[i] == text[j]) ++i, ++j;
			else ++j;
		}
		if(i==n) out << "yes";
		else out << "no";
	}
};

int32_t main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
	BDimaAndTextMessages solver;
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
	return 0;
}
