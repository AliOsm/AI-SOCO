#include <cstdio>
#include <iostream>
#include <string>
#include <map>
#include <cstring>
#include <set>
#include <unordered_set>
#include <algorithm>
#include <cmath>
#include <vector>

using namespace std;

long long n, k, t;

int main() {
	cin >> n >> k >> t;
	if (t <= k) {
		cout << t << endl;
	} else if (t <= n) {
		cout << k << endl;
	} else {
		cout << k - (t - n) << endl;
	}
}
