#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;

    int ax,ay,bx,by,cx,cy;

        cin>>ax>>ay;
        cin>>bx>>by;
        cin>>cx>>cy;

        int oo = min(bx,cx);
        int oy = max(bx,cx);

        int pp = min(by,cy);
        int py = max(by,cy);

        if(ax>oy && (ay<pp || ay>py))
        {
            cout<<"YES";
        }
        else if(ax<oo && (ay<pp || ay>py))
        {
            cout<<"YES";
        }else{
            cout<<"NO";
        }



}
