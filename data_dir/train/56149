#include <stdio.h>
#include <string>
#include <vector>
#include <iostream>
using namespace std;

int main()
{
	int i;
	int n;
	scanf("%d",&n);
	n--;
	for(i=5;n>=i;i*=2)
	{
		n-=i;
	}
	n/=i/5;
	if(n==0)
		printf("Sheldon\n");
	else if(n==1)
		printf("Leonard\n");
	else if(n==2)
		printf("Penny\n");
	else if(n==3)
		printf("Rajesh\n");
	else if(n==4)
		printf("Howard\n");
	return 0;
}
