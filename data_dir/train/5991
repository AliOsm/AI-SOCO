#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
   ll a,b,c,d,e,i,j,k,l,n,m,x,y,t,p;
   string st;
   scanf("%lld",&t);
   for(p=1;p<=t;p++)
   {
       scanf("%lld",&n);
       cin>>st;
       a=0;
       for(i=0;i<n;i++)
       {
           if(st[i]=='8')
           {
               a=n-i;
               break;
           }
       }
       if(a>=11)
       {
           printf("YES\n");
       }
       else
       {
           printf("NO\n");
       }
   }
}
