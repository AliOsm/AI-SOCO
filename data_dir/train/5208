#define _CRT_SECURE_NO_WARNINGS

#include <iostream>
#include <conio.h>
#include <cstdlib>
#include <ctime>
#include <cmath>
#include <cstring>
#include <string>
#include <vector>
#include <stdio.h>
#include <climits>
#include <Windows.h>
#include <set>
#include <iterator>
#include <algorithm>

using namespace std;

typedef unsigned long long ull;
typedef long long ll;



void bla()
{
    vector<int> vec;
    vector<int>::iterator itmax, itmin;

    int temp;

    for (int i(0); i < 5; i++)
    {
        cin >> temp;
        vec.push_back(temp);
    }

    int sum(0);
    for (int i(0); i < 5; i++) sum += vec[i];



    if (sum % 5 || !(sum)) cout << -1;
    else cout << sum / 5;
}


int main()
{
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);


    bla();


    //_getch();
    return 0;
}