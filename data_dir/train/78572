#include<bits/stdc++.h>
using namespace std;
#define rep(i,a,b) for(int i=a;i<b;i++)
#define repr(i,a,b) for(int i=a;i>=b;i--)
#define mod 1000000007
#define pb push_back
#define mp make_pair
#define fs first
#define ss second
typedef long long LL;
typedef long double ld;
int main()
{
    ios_base::sync_with_stdio(0);
    int a,b,c,flag=0;
    cin>>a>>b>>c;
    rep(i,0,10001)
        rep(j,0,10001)
            if(a*i+b*j==c) {flag=1;break;}
    if(flag) cout<<"Yes";
    else cout<<"No";
}