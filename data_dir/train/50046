/// إِنَّ مَعَ الْعُسْرِ يُسْرًا
/// لَا أَبْرَحُ حَتَّىٰ أَبْلُغَ


#include <bits/stdc++.h>
#define LL long long
#define LD long double
#define S second
#define IO ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define F first
#define PI acos(-1)
#define MAX 200005
using namespace std;


int main()
{
    IO
    int n;
    cin>>n;
    LL x;
    while(n--)
    {
        cin>>x;
       
        if(x==1 || x==2 || x==4 || x==5 || x==8 || x==11)
        cout<<"NO"<<endl;
        else
        cout<<"YES"<<endl;

    }


    return 0;
}

