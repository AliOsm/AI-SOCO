#include <bits/stdc++.h>
#define re return
#define ll long long
#define ull unsigned ll
#define ld long double
#define f first
#define s second
#define pi acos(-1)
#define oo (ll)1e9
#define OO 1e18
#define EPS 1e-6
#define FX(n) fixed<<setprecision(n)
#define mm(o,k) memset(o,k,sizeof o)
#define IO ios_base::sync_with_stdio(0),cin.tie(0),cout.tie(0);
using namespace std;
const int maxn=200005;
vector<pair<int,int> >a,b;
int main()
{
    IO;
    int n;
    cin >> n;
    ll X=0,Y=0;
    for(int i=0;i<2*n;++i){
        ll x,y;
        cin >> x >> y;
        X+=x;
        Y+=y;
    }
    cout<<X/n<<" "<<Y/n<<endl;
    re 0;
}