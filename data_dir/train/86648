bool Check( int val , int pos ){
    return bool ( val & ( 1 << pos ) ) ;
}

int Set( int val , int pos ){
    return val | ( 1 << pos ) ;
}
int Reset( int val , int pos ){
    return val & ( ~ ( 1 << pos ) ) ;
}
int Flip( int val , int pos ){
    return val ^ ( 1 << pos ) ;
}

const LL maxn = 1e5 + 5 ;

LL n , m , t , caseno , k ;

struct info{
    LL val , pos ;
    info(){}
    info( LL val , LL pos ): val(val) , pos(pos) {}
    bool operator < ( const info &p ) const{
        if( val == p.val ) return pos < p.pos ;
        return val > p.val ;
    }
} ;

LL ncr[2005][2005] ;

LL NCR( LL n, LL r )
{
    if( n == r || r == 0 )
        return ncr[n][r] = 1 % MOD ;
    if( r == 1 )
        return ncr[n][r] = n % MOD ;
    if( ncr[n][r] != -1 ) return ncr[n][r] ;
    return ncr[n][r] = ( NCR( n - 1, r ) + NCR( n - 1, r - 1 ) ) % MOD ;
}


LL a[100005] ;

char s[100005] ;

vector< LL > V ;
set< LL > res ;
set< LL > ::iterator it ;

LL Fun( LL d , LL pod ){
    return ( pod * ( 2 + ( pod - 1 ) * d ) ) / 2 ;
}

void Pre(){
    LL i , j ;
    LL Sqrt = sqrt( n ) ;
    Rep( i , 1 , Sqrt ){
        if( n % i == 0 ){
            j = n / i ;
            if( j != i ){
                V.PB( j ) ;
            }
            V.PB( i ) ;
        }
    }
}

int main()
{
    LL i , j , y , b , r ;

    SL1( n ) ;

    Pre() ;

    LL d = V.size() ;
    Rep( i , 0 , d - 1 ){
        LL x = n / V[i] ;
        res.insert( Fun( V[i] , x ) ) ;
    }

    for( it = res.begin() ; it != res.end() ; it ++ ){
        printf("%lld ",*it) ;
    }NL ;


    return 0 ;
}