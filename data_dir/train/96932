#include <bits/stdc++.h>

using namespace std;

const int N = 1e6;

#define int long long


 main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n, x, y;
    cin >> n >> x >> y;
    int kek = abs(x - 1), kek1 = abs(y - 1), lol = abs(x - n), lol1 = abs(y - n);
    if (kek + kek1 <= lol + lol1) {
		cout << "White";
    }
    else {
		cout << "Black";
    }

}
