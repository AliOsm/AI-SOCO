#include <bits/stdc++.h>
#define all(a) (a).begin(),(a).end()

using namespace std;
typedef long long ll;
int x[3][2];

int main()
{
    ios_base::sync_with_stdio(false);
    int a, b, c, d; cin>>a>>b>>c>>d;
    string s; cin>>s;
    cout<< a*count(all(s),'1') + b*count(all(s),'2') +
           c*count(all(s),'3') + d*count(all(s),'4') <<endl;
    return 0;
}
