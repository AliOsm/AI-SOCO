#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define f first
#define s second
#define bp push_back
#define mp make_pair
const int N=1e5+10;
const double eps=1e-9;
const int inf=1e9;
int a[N];
long long lcm(long long a, long long b) {
    return a / __gcd(a, b) * b;
}
main(){

    int t;
    scanf("%d",&t);
    while(t--){
        int n;
        scanf("%d",&n);
        set<int>S;
        for(int i=1;i<=n;i++){
            int x;
            scanf("%d",&x);
            S.insert(x);
        }
        printf("%d\n",min(n,(int)S.size()));
    }



}
