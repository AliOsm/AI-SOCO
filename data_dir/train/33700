#include <bits/stdc++.h>
using namespace std;
int main(){
  ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(0);
  int n,a,b,res;
  cin>>n;
  for(int i=0;i<n;i++){
    cin>>a>>b;
    a = abs(a-b);
    res = a / 5;
    a %= 5;
    res += a / 2;
    a %= 2;
    res+=a;
    cout<<res<<"\n";

  }
  
  return 0;
}

