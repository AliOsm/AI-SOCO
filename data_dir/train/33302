#include <bits/stdc++.h>
using namespace std;


const int N = 1e5 + 5;

int n;
bool t;

int main(){




    cin >> n;


    for (int i = 1;i <= n;i++){

        int x;
        cin >> x;
        if (x == 1) t = true;
    }

    if (t) cout << -1;
    else cout << 1;



}
