#include <iostream>
#include <vector>
#include <string>
#include <cmath>

using namespace std;

typedef long long ll;
typedef vector<int> vi;
typedef pair<int, int> ii;

int main() {
    int a, b;
    cin >> a >> b;
    
    ll answer = 1;
    
    for (int i = 1, last = min(a, b); i <= last; ++i) answer *= i;
    
    cout << answer << endl;
    
    return 0;
}