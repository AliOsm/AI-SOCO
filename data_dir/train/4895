#include <bits/stdc++.h>

using namespace std;

#define MOD 1000000007
#define N 100005
#define M 30
#define ll long long 
#define ld long double
#define pb push_back
#define ff first
#define ss second

int main(){
	ios::sync_with_stdio(false); cin.tie(0); cout.tie(0);
	//freopen("input.txt", "r", stdin);
	//freopen("output.txt", "w", stdout);
	int t; cin>>t;
	ll a, b, n, ans;
	while(t--){
		cin>>a>>b>>n;
		if(n % 3 == 2) ans = a^b;
		else if(n % 3 == 1) ans = b;
		else ans = a;
		cout<<ans<<"\n";
	}
	return 0;
}