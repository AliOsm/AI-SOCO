#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
#define int ll
#define endl '\n';
#define IOS ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define floop(i,a,b) for(int i=a;i<=b;++i)
const ll mod=1e9+7;
const int maxn=2e5+5;
int32_t main(){
	IOS;	
	int t;
	cin>>t;
	while(t--){
		int n;
		cin>>n;
		int k=2;
		while(1){
			int curr = (1ll<<k)-1;
			if(n%curr==0){
				cout<<n/curr<<endl;
				break;
			}
			k++;
		}
	}
	return 0;
}