const int dx[] = {-1, 0, 1,  0};
const int dy[] = { 0, 1, 0, -1};
const int dxK[] = {-1, -1, 0, 1, 1,  1,  0, -1};
const int dyK[] = { 0,  1, 1, 1, 0, -1, -1, -1};
const int dxKn[] = {-2, -1, 1, 2,  2,  1, -1, -2};
const int dyKn[] = { 1,  2, 2, 1, -1, -2, -2, -1};

const int N = int(3e2) + 5;
const int M = int(3e2) + 9;
const int LOGN = 21;
const int SQN = 350;
const int MOD = int(1e9) + 7;
const int INF = int(1e9) + 100;
const li INF64 = 2e18;
const ld PI = ld(3.1415926535897932384626433832795);
const ld e = ld(2.7182818284590452353602874713527);
const ld EPS = 1e-9;

int h, q;
map<int, int> a;
map<int, int> suma;

ld rec(int v, int curh, int mx){
	if(curh == h)
		return max(mx, a[v]);
	
	int l = 0, r = 0;
	if(suma.count(v * 2 + 1))
		l = suma[v * 2 + 1];
	if(suma.count(v * 2 + 2))
		r = suma[v * 2 + 2];
	if(a.count(v))
		l += a[v], r += a[v];

	if(l == r)
		return max(mx, l);
	if(l > r)
		return 0.5 * max(l, mx) + 0.5 * rec(v * 2 + 1, curh + 1, max(mx, r));	
	else
		return 0.5 * max(r, mx) + 0.5 * rec(v * 2 + 2, curh + 1, max(mx, l));
	
}

char buf[10];
void solve(){
	cin >> h >> q;
	forn(i, q){
		scanf("%s", buf);
		if(buf[0] == 'a'){
			int v, e;
			scanf("%d %d", &v, &e);
			--v;
			a[v] += e;
			while(v > 0)
				suma[v] += e, v = (v - 1) / 2;
			suma[v] += e;
		}
		else{
			ld res = rec(0, 0, 0);
			cout << res << endl;
		}
	}
}	

int main(){
#ifdef _DEBUG
	freopen("input.txt", "r", stdin);
	freopen("output.txt", "w", stdout);
#endif
	srand(time(NULL));
	cout << setprecision(25) << fixed;
	cerr << setprecision(25) << fixed;   

	solve();

	return 0;

}