#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n;
    long long x;
    cin >> n >> x;
    long long sum = n - 1;
    for(int i = 1;i <= n;i++){
        int a;
        cin >> a;
        sum += a;
    }
    if(sum == x)
        cout << "YES";
    else cout << "NO";
}
