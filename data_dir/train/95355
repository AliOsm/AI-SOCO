#include <bits/stdc++.h>
using namespace std;

int main() {
	long long n;
	cin >> n;

	for (long long i = 0; i <= n; i += 1234567) {
		for (long long j = 0; i + j <= n; j += 123456) {
			long long k = n - i - j;
			if (k % 1234 == 0) {
				cout << "YES" << endl;
				return 0;
			}
		}
	}
	cout << "NO" << endl;
}