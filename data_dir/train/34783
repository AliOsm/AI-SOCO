#include <bits/stdc++.h>

using namespace std;

int main()
{
    ios_base :: sync_with_stdio(false) ;
    cin.tie(nullptr) ;
    int s, v1 ,v2 , t1 , t2 ;
    cin >> s >> v1 >> v2  >> t1 >> t2 ;

    int p1 = 2 * t1 + s * v1 ;
    int p2 = 2 * t2 + s * v2 ;
    if(p1 < p2)
        cout << "First" ;
    else if(p1 > p2)
        cout << "Second" ;
    else
        cout << "Friendship" ;
    return 0 ;
}