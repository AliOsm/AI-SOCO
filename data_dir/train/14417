#include "bits/stdc++.h"

using namespace std;
using LL = long long;

constexpr int N = 1e5 + 5;

int main() {
	ios::sync_with_stdio(false); cin.tie(nullptr);
	
	int a, b;
	cin >> a >> b;

	int n = a + b + 1;
	
	for (int i = n - a; i <= n; i++) {
		cout << i << ' ';
	}

	while (b) {
		cout << b << ' ';
		b--;
	}
}