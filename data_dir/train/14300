#include <iostream>
#include <cstdlib>
#include <ctime>
#define _CRT_SECURE_NO_WARNINGS

#include <cmath>
#include <cstring>
#include <string>
#include <vector>
#include <stdio.h>
#include <climits>
#include <Windows.h>
#include <iterator>
#include <algorithm>

using namespace std;

typedef unsigned long long ull;
typedef long long ll;
#define all(a) (a.begin(),a.end())

void solve()
{
    string s1, s2;
    cin >> s1 >> s2;

    for (int i(0); i < s1.size(); i++)
    {
        if (s1[i] != s2[i]) cout << '1';
        else cout << '0';
    }
}

int main()
{
    //SetConsoleCP(1251);
    //SetConsoleOutputCP(1251);

    solve();

    //cout << "\n\n"; system("pause");
    return 0;
}