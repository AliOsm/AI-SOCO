#include<bits/stdc++.h>
using namespace std;
#define ll long long
ll i,j;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    vector<ll> a(25);
    for(i=1; i<=24; i++)
        cin>>a[i];
    if(a[1]==a[2] && a[2]==a[3] && a[3]==a[4] && a[9]==a[10] && a[10]==a[11] && a[11]==a[12])
    {
        if(a[13]==a[24] && a[14]==a[23] && a[5]==a[15] && a[6]==a[16] && a[17]==a[7] && a[18]==a[8] && a[22]==a[19] && a[21]==a[20])
        {
            cout<<"YES"<<endl;
            return 0;
        }
        else if(a[13]==a[7] && a[14]==a[8] && a[5]==a[19] && a[6]==a[20] && a[17]==a[24] && a[18]==a[23] && a[22]==a[15] && a[21]==a[16])
        {
            cout<<"YES"<<endl;
            return 0;
        }
        else
        {
            cout<<"NO"<<endl;
            return 0;
        }

    }
    else if(a[5]==a[6] && a[6]==a[7] && a[7]==a[8] && a[21]==a[22] && a[22]==a[23] && a[23]==a[23])
    {
        if(a[1]==a[14] && a[2]==a[16] && a[13]==a[9] && a[15]==a[10] && a[17]==a[11] && a[19]==a[12] && a[18]==a[3] && a[20]==a[4])
        {
            cout<<"YES"<<endl;
            return 0;
        }
        else if(a[1]==a[17] && a[2]==a[19] && a[3]==a[13] && a[4]==a[15] && a[9]==a[18] && a[10]==a[20] && a[11]==a[14] && a[12]==a[16])
        {
            cout<<"YES"<<endl;
            return 0;
        }
        else
        {
            cout<<"NO"<<endl;
            return 0;
        }
    }
    else if(a[13]==a[14] && a[14]==a[15] && a[15]==a[16] && a[17]==a[18] && a[18]==a[19] && a[19]==a[20])
    {
        if(a[1]==a[6] && a[3]==a[8] && a[5]==a[10] && a[7]==a[12] && a[9]==a[21] && a[11]==a[23] && a[22]==a[2] && a[24]==a[4])
        {
            cout<<"YES"<<endl;
            return 0;
        }
        else if(a[1]==a[21] && a[3]==a[23] && a[22]==a[10] && a[24]==a[12] && a[9]==a[6] && a[11]==a[8] && a[5]==a[2] && a[7]==a[4])
        {
            cout<<"YES"<<endl;
            return 0;
        }
        else
        {
            cout<<"NO"<<endl;
            return 0;
        }
    }
    else
    {
        cout<<"NO"<<endl;
        return 0;
    }
}

