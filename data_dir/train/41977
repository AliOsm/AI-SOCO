#define sread(x) scanf("%s",x)
#define dread(x) scanf("%lf",&x)
#define lread(x) scanf("%lld",&x)
using namespace std;

typedef long long ll;
const int inf=0x3f3f3f3f;
const ll INF=0x3f3f3f3f3f3f3f3f;
const int N=200+7;
const int M=12;


int main()
{
    int n; cin>>n;
    if(n&1)
        puts("1");
    else
        puts("0");
    return 0;
}
