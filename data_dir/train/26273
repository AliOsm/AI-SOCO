#include <bits/stdc++.h>
using namespace std;

int main(){
	int a, b, c; cin>>a>>b>>c;
	if(a < b and c > 0){
		if((b-a)%c == 0) cout<<"YES\n";
		else cout<<"NO\n";
	}else if(a > b and c < 0){
		if((a-b)%(-c) == 0) cout<<"YES\n";
		else cout<<"NO\n";
	}else if(a==b) cout<<"YES\n";
	else cout<<"NO\n";
	return 0;
}