#include <iostream>
#include <algorithm>
#include <vector>
#include <cstring>
using namespace std;

int main() {
    int n;
    int ans = 0;
    cin >> n;
    for (int i = 0; i< n; i++) {
        int a;
        cin >> a;
        int b;
        cin >> b;
        if (a < b) {
            ans++;
        } else if (a > b) {
            ans--;
        }
    }
    if (ans > 0) {
        cout << "Chris";
    } else if (ans < 0) {
        cout << "Mishka";
    } else {
        cout << "Friendship is magic!^^";
    }
}
