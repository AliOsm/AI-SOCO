#include<iostream>
#include<cstdlib>
#include<vector>
#include<climits>
#include<cctype>
#include<map>
#include<list>
#include<cstdio>
#include<algorithm>
#include<memory.h>
#include<cmath>
#include<queue>
#include<fstream>
#include<sstream>
#include<set>
#include<iomanip>
#define L long long int
#define LD long double
#define pi 3.141592653589793238462643383
#define M 1000000007

using namespace std;

int main()
{
    int t=1,n,m,ans,a,b;
    while(t--)
    {
              cin>>n>>m;
              ans=0;
              for(a=0;a<=n+m;a++)
              {
                               for(b=0;b<=n+m;b++)
                               if(a*a+b==n&&b*b+a==m)
                               ans++;
              }
              cout<<ans<<"\n";
              
              
              
    }
    return 0;
}
