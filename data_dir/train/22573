//#define HMOD ((int64)(1e18) + 3LL)
//#ifdef _MY_DEBUG
//#define HMOD 1000000007
//#endif
#define MAX 2000000000
#define HBASE 127

const int MAXN = 50100;

int n, p;
int en[50];
bool check(string& a, string& b)
{
	string t = a + b;
	string t1 = b + a;
	if (t <= t1)
		return false;
	else
		return true;
}

vector < vector < string > > sp;

void solve()
{
	cin >> n;
	sp.resize(50);
	for(int i = 0; i < n; ++i)
	{
		string a;
		cin >> a;
		sp[a.size() - 1].push_back(a);
	}
	string ans;
	for(int i = 0; i < 50; ++i)
		sort(sp[i].begin(), sp[i].end());
	for(int i = 0; i < n; ++i)
	{
		string t;
		int id = -1;
		for(int j = 0; j < 50; ++j)
		{
			if (en[j] == sp[j].size())
				continue;
			if (id == -1 || check(t, sp[j][en[j]]))  
			{
				id = j;
				t = sp[j][en[j]];
			}
		}
		en[id]++;
		ans += t;
	}

	cout << ans;
}	

int main()
{
	ios_base::sync_with_stdio(false); cin.tie(0);
#ifdef _MY_DEBUG
    freopen("input.txt", "rt", stdin); freopen("output.txt", "wt", stdout);
#else
    //freopen(TASK ".in", "rt", stdin); freopen(TASK ".out", "wt", stdout);
#endif
    //stresstest(); return 0;
    srand(1313);
	solve();
	
    return 0;
}