#include<bits/stdc++.h>
using namespace std;
const int MX=(1<<20);
int n , arr[MX];
string str;
int main(){
    int K;
    cin>>n>>K;
    for(int j = 1 ; j <= n ; j++){
        K += 5 * j;
        if(K > 240){
            cout<<j-1<<endl;
            return 0;
        }
    }
    cout<<n<<endl;
}
