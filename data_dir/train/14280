#include <fstream>


using namespace std;

void solver()
{
	string s;
	bool one = false;
	bool zero = false;
	cin >> s;
	for (int i = 0; i < s.size(); i++)
	{
		if (s[i] == '0')
		{
			zero = true;
		}
		else
		{
			one = true;
		}
	}
	if (one && zero)
	{
		for (int i = 0; i < s.size(); i++)
		{
			cout << "01";
		}
		cout << endl;
	}
	else
	{
		cout << s << endl;
	}
} 
 
int main()
{
	long long t;
	cin >> t;
	for (long long i = 0; i < t; i++)
	{
		solver();
	}
}
 
