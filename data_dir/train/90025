#include <bits/stdc++.h>
using namespace std;
typedef long long int ll;

class ProblemATetris {
public:
	void solve(std::istream& in, std::ostream& out) {
		int n, m; in>>n>>m;
        int freq[n+1];
        memset(freq, 0, sizeof(freq));
        for(int i = 0; i<m; ++i){
            int x; in>>x;
            freq[x]++;
        }
        int res = INT_MAX;
        for(int i = 1; i<=n; ++i) res = min(res, freq[i]);
        out<<res;
	}
};


int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
	ProblemATetris solver;
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
	return 0;
}
