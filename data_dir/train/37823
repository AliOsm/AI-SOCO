#include <iostream>
#include <algorithm>
#include <iomanip>
#include <set>
#include <string>

#define ll long long
#define ld long double

using namespace std;

int main()
{
	ll vs[5]; for(int i = 1; i <= 4; i++) cin >> vs[i];
	ll ret = 0;
	string s; cin >> s;
	for(int i = 0; i < s.size(); i++)
	{
		ret += vs[(int)(s.at(i)-'0')];
	}

	cout << ret;
	return 0;
}
