//----------------- ------   -------
int n,k,ans; string s;
int main()
{
#ifndef ONLINE_JUDGE
    //freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout);
#endif
    sc(n); sc(k);
    while(n--)
    {
        cin>>s;
        for(int i=0;i<=k;++i)
        {
            if(!count(s.begin(),s.end(),'0'+i))
            {
                ans--;
                break;
            }
        }
        ans++;
    }
    cout<<ans;

    return 0;
}
