#include <cstdio>
#include <cstring>
#include <cstdlib>
#include <set>
#include <map>
#include <iterator>
#include <vector>

using namespace std;

int main()
{
	
	int t;
	scanf("%d", &t);
	
	int n;
	int times;
	for (int ii = 0 ; ii < t ; ii++)
	{
		scanf("%d", &n);
		
		times = n / 7;
		if (n % 7 != 0)
			times++;
		
		printf("%d\n", times);
	}

	return 0;
}

