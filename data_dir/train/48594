#include<bits/stdc++.h>
#define fi first
#define se second
#define log2(a) log(n)/log(2)
#define show(a) cout<<a<<endl;
#define show2(a,b) cout<<a<<" "<<b<<endl;
#define show3(a,b,c) cout<<a<<" "<<b<<" "<<c<<endl;
using namespace std;

typedef long long ll;
typedef pair<int, int> P;
typedef pair<P, int> LP;
const ll inf = 1e17 + 10;
const int N = 1e6 + 10;
const ll mod = 1e9+7;
const int base=131;
const double pi=acos(-1);
map<string, int>ml;



map<int,int> vi;




ll b[N], c[N],num[N],a[N] ,n, m,  k,x,y,z,vis[N];
ll ex, ey, cnt, ans, sum, flag ,w;
//vector<int> v[N];
vector<int> v[N];
map<ll,ll> mp;
map<string,int> can;
set<int> st;
ll dp[N][2];
priority_queue<ll,vector<ll>,greater<ll>> q;
string s,t;
int main()
{
	ios::sync_with_stdio(false);
	cin.tie(0);

	cin>>s>>t;
	if(s.size()!=t.size()) cout<<"NO"<<endl;
	else
	{
		for(int i=0;s[i];i++)
		{
			if(s[i]=='0') x++;
			else y++;
			if(t[i]=='0') n++;
			else m++;

		}
		if(x==n) cout<<"YES";
		else if(y&&m) cout<<"YES";
		else cout<<"NO";
	}





}
