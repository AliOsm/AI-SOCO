#define pb push_back
#define int long long
using namespace std;

const int INF = 2e9;
const int MOD = 1e9 + 7;

void solve() {
	int n, m;
	cin >> n >> m;
	n %= m;
	vector<bool> used(m);
	for (int i = 0; i < m; i++) {
		used[n] = true;
		n += (n % m);
		n %= m;
	}
	if (used[0]) {
		cout << "Yes";
	}
	else {
		cout << "No";
	}
}

signed main() {
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr);
	cout.tie(nullptr);
	cout.precision(12);
	/*
	freopen("distance4.in", "r", stdin);
	freopen("distance4.out", "w", stdout);
	*/
	solve();
}