#include <bits/stdc++.h>

using namespace std;

#define ll long long int

int main()
{
    vector < string > vv;

    string s,t;

    int i,j;

    cin>>s>>t;

    string temp="",bal;

    for(i=0; i<s.size(); i++)
    {
        temp+=s[i];

        bal=temp+t[0];

        vv.push_back(bal);

    }

    sort(vv.begin(),vv.end());

    cout<<vv[0]<<endl;
}
