#include <iostream>
#include <cmath>
#include <vector>
#include <algorithm>
#include <cstdio>
#include <set>
#include <cctype>

using namespace std;

string s;
set <char> st;

int main(){         
	getline(cin, s);
	for(int i=0; i<s.size(); i++){
		if(isalpha(s[i])){
			st.insert(s[i]);
		}
	}
	printf("%d", st.size());	
  	return 0;
}



   				