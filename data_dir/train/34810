#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n, m, k;
    cin >> n >> m >> k;
    int K = k;
    k = k / 2 + k % 2;
    --k;
    //swap(n, m);
    int M = (k / m) + 1;
    int N = (k % m + 1);
    cout << M << ' ' << N << ' ';
    if(K % 2 == 0)
        cout << "R\n";
    else
        cout << "L\n";
}
