const ll MOD = 1e9 + 7;
const ll mod = 555557;
const double pi = acos (-1.0);

map <string, string> ma;
string s, t;
int n, m;     

int main ()
{
	speed;
	cin >> n >> m;
	for (int i = 1; i <= n; i ++)
	{
		cin >> s >> t;
		ma[t] = s;
	}
	for (int i = 1; i <= m; i ++)
	{
		string k = "";
		cin >> s >> t;
		t.er (t.sz - 1);
		for (char j : t)
			k += j;
		cout << s << " " << t << "; #" << ma[k] << endl;
	}
}
