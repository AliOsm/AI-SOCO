#include<bits/stdc++.h>
using namespace std;
main()
{
    string s;
    while(cin>>s)
    {
        long i,k=0,k1=0,a,ans=0;
        for(i=0;i<2;i++)
        {
            k=k*10+s[i]-48;
        }
        for(i=3;i<5;i++)
        {
            k1=k1*10+s[i]-48;
        }
        if(k==1)
        {
            if(k1>10)
            {
                ans=(20-k1)+60;
            }
            else
            {
                ans=10-k1;
            }
        }
        if(k==2)
        {
            if(k1>20)
            {
                ans=30-k1+60;
            }
            else
            {
                ans=20-k1;
            }
        }
        if(k==3)
        {
            if(k1>30)
            {
                ans=40-k1+60;
            }
            else
            {
                ans=30-k1;
            }
        }
        if(k==4)
        {
            if(k1>40)
            {
                ans=50-k1+60;
            }
            else
            {
                ans=40-k1;
            }
        }
        if(k==5)
        {
            if(k1>50)
            {
                ans=240+(60-k1)+1;
            }
            else
            {
                ans=50-k1;
            }
        }
        if(k==6)
        {
            ans=(60-k1)+180+1;
        }
        if(k==7)
        {
            ans=120+(60-k1)+1;
        }
        if(k==8)
        {
            ans=60+(60-k1)+1;
        }
        if(k==9)
        {
            ans=(60-k1)+1;
        }
        if(k==10)
        {
            if(k1>1)
            {
                ans=60+(11-k1);
            }
            else
                ans=1-k1;
        }
        if(k==11)
        {
            if(k1<=11)
            {
                ans=11-k1;
            }
            else
                ans=60+21-k1;
        }
        if(k==12)
        {
            if(k1<=21)
            {
                ans=21-k1;
            }
            else
                ans=60-k1+31;
        }
        if(k==13)
        {
            if(k1<=31)
            {
                ans=31-k1;
            }
            else
                ans=60+41-k1;
        }
        if(k==14)
        {
            if(k1<=41)
            {
                ans=41-k1;
            }
            else
                ans=60+51-k1;
        }
        if(k==15)
        {
            if(k1<=51)
            {
                ans=51-k1;
            }
            else
                ans=240+(60-k1)+2;
        }
        if(k==16)
        {
            ans=180+60-k1+2;
        }
        if(k==17)
        {
            ans=120+60-k1+2;
        }
        if(k==18)
        {
            ans=60+60-k1+2;
        }
        if(k==19)
        {
            ans=60-k1+2;
        }
        if(k==20)
        {
            if(k1>2)
            {
                ans=60+(12-k1);
            }
            else
                ans=2-k1;
        }
        if(k==21)
        {
            if(k1<=12)
            {
                ans=12-k1;
            }
            else
                ans=60+22-k1;
        }
        if(k==22)
        {
            if(k1<=22)
            {
                ans=22-k1;
            }
            else
                ans=60+32-k1;
        }
        if(k==23)
        {
            if(k1<=32)
            {
                ans=32-k1;
            }
            else
                ans=60-k1;
        }
        if(k==0)
        {
            if(k1==0)
            {
                ans=0;
            }
            else
            {
                ans=10+(60-k1);
            }
        }
        printf("%ld\n",ans);
    }
}
