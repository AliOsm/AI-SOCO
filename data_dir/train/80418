#include<cstdio>
#include<iostream>
using namespace std;
int a[2];
int main(){
	int n;
	cin>>n;
	string x;
	cin>>x;
	for(int i=0;i<x.size();i++){
		if(x[i]=='A'){
			a[0]++;
		}
		else{
			a[1]++;
		}
	}
	if(a[0]>a[1]){
		cout<<"Anton";
	}
	else if(a[1]>a[0]){
		cout<<"Danik";
	}
	else{
		cout<<"Friendship";
	}
}