#include <bits/stdc++.h>
using namespace std;

const int N = int(1e5);

typedef long long ll;

ll a[N], b[N];
int n;

int main (int argc, char *argv[])
{
	scanf("%d", &n);
	scanf("%lld", &a[0]);
	b[0] = a[0];
	for(int i = 1; i < n; ++i) {
		scanf("%lld", &b[i]);
		a[i] = b[i] + a[i-1];
	}
	sort(b, b + n);
	for(int i = 1; i < n; ++i) {
		b[i] += b[i-1];
	}
	int m;
	scanf("%d", &m);
	for(int i = 0; i < m; ++i) {
		int type, l, r;
		scanf("%d%d%d", &type, &l, &r);
		l--; r--;
		if(type == 1) {
			if(l) {
				printf("%lld\n", a[r] - a[l-1]);
			} else {
				printf("%lld\n", a[r]);
			}
		} else {
			if(l) {
				printf("%lld\n", b[r] - b[l-1]);
			} else {
				printf("%lld\n", b[r]);
			}
		}
	}
	
	return 0;
}

