#include <bits/stdc++.h>
#define int long long
#define inf 1e18
using namespace std;
const int N = 1123456;
int read()
{
    int x;
    scanf("%I64d", &x);
    return x;
}
int getrand()
{
    srand(time(0));
    int x = rand();
    while(x -- > 0)
        rand();
    return rand() % 767;
}
main()
{
    int n, m, i, j;
    cin >> n;
    vector <int> v;
    for(i = 1; i <= n; i ++)
    {
        cin >> m;
        if(m == v.size() + 1)
        {
            v.push_back(2000 + i);
        }
    }
    cout << v.size() << endl;
    for(i = 0; i < v.size(); i ++)
        cout << v[i] << " ";
}


