void brainfuck()
{
    cin>>n;
    for ( i = 1 ; i <= n; i++ )
    {
        cin>>ar [ 0 ] [ i ]>>ar [ 1 ] [ i ];
    }
    cin>>k;
    for ( i = 1 ; i <= n; i++ )
    {
        if ( ar [ 0 ][ i ] < k && ar [ 1 ] [ i ] < k )continue;
        else
        {
            cout<<n-i+1;
            return;
        }
    }
}
