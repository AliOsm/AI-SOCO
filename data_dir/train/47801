    cin>>qq;
    for (int _=0;_<qq;_++)
    {
        cin>>n>>m;
        q=0;
        for (int i=1;i<=n;i++) cin>>x,q+=x,a[i]=x;
//        cout<<q<<endl;
        cout<<min<int>(m,q)<<endl;
    }
}
