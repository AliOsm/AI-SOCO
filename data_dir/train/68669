/************************* In the name of Allah *************************/
#include<bits/stdc++.h>
using namespace std;

const int N = 101;

char a[N];

int main()
{
  int q;
  cin >> q;
  while (q --)
  {
    string s;
    cin >> s;
    for (int i = 0; i < s.size(); i ++)
      a[i] = s[i];
    sort(a, a + s.size());
    bool f = 1;
    for (int i = 1; i < s.size(); i ++)
    {
      if (a[i] - a[i - 1] != 1)
        f = 0;
    }
    cout << (f ? "Yes" : "No") << '\n';
  }
  return 0;
}
