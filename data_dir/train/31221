#include<bits/stdc++.h>
using namespace std;
int main (){
    int n;
    int x=0 , y=0;
    cin >> n;
    for(int i =0 ; i<n ; i++){
        char c;
        cin >> c;
        if (c=='U')y++;
        if (c=='D')y--;
        if (c=='L')x--;
        if (c=='R')x++;
    }
    cout <<n-abs(x)-abs(y);
    return 0;
}
