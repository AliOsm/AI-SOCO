#include <bits/stdc++.h>
#define  FOR(i,x,n) for(int i=x;i<n;i++)
#define rFOR(i,x,n) for(int i=x;i>n;i--)
#define fi first
#define se second
#define SZ(x) (int)x.size()
typedef long long ll;
typedef long double lD;
using namespace std;

int main()
{
  string s; cin>>s;
  FOR(i,0,SZ(s))
  	if((9-(s[i]-'0'))<(s[i]-'0')) 
  	  s[i]=((9-(s[i]-'0'))+'0');
  if(s[0]=='0') s[0]='9';
  cout<<s;
}