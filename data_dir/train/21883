#include <ext/numeric>

using namespace std;
using namespace __gnu_cxx;

typedef long long ll;
typedef unsigned long long ull;

void fastInOut();

int main() {
#ifndef ONLINE_JUDGE
    freopen("input.txt", "rt", stdin);
#endif
    fastInOut();
    string x;
    getline(cin, x);
    for (int i = 0; i < int(x.size()); ++i) {
        if (x[i] == 'H' || x[i] == 'Q' || x[i] == '9') {
            cout << "YES\n";
            return 0;
        }
    }
    cout << "NO\n";
    return 0;
}

