#include <bits/stdc++.h>
#define MAX (ll)(2e5+5)
#define MOD (ll)(998244353)
#define INF (ll)(1e18 + 3)
#define PI (double)(3.14159265)

using namespace std;
using ll = long long;

int main () {
	int n;
	cin >> n;
	while (n--) {
		int x, y;
		cin >> x >> y;
		if (y <= x || x >= 4 || (x == 2 && y == 3)) {
			cout << "YES\n";
		}
		else {
			cout << "NO\n";
		}
	}
}
			
