#include<bits/stdc++.h>
using namespace std;

#define F first
#define S second
#define ii pair < int , int >
#define LS pair < ll , string >
typedef long long ll;
const int inf = 1<<30;
const int N =100100;
int n , a[N] , q ;

int main ( ){

  ll n , k , t ;
  cin >> n >> k >> t ;
  if ( t <= k ){
    cout << t ;
  }else if ( t <= n ){
  cout << k ;
  }else {
   cout << (k - (t-n));
  }
return 0 ;
}
