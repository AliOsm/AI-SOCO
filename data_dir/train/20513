#include <bits/stdc++.h>
#define pb push_back
#define F first
#define S second
#define all(x) x.begin(), x.end()
using namespace std;
typedef long long ll; 
typedef long double ld;
typedef pair<ll, ll> pll;

const ll N = 2e5 + 30, Mod = 1e9 + 7;
const ll SQ = 330 ;
const ll INF = 1e17;

ll a[N];
int main () {
    ios::sync_with_stdio(0), cin.tie(0);
    ll q;
    cin >> q;
    while (q --) {
    	ll n, m;
    	cin >> n >> m;
    	ll s = 0;
    	for (int i = 0; i < n; i ++) {
    		ll x;
    		cin >> x;
    		s += x;
		}
		cout << min(s, m) << '\n'; 
		
	}
	    
    return (0);
}
