//#pragma GCC optimize("-O3")
#include <bits/stdc++.h>
#define ll long long
#define dd double
#define f first
#define s second
#define pb push_back
#define ios ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define y1 ibibibibbibibibiibigbabypanfewewf
using namespace std;
ll const N=5*1e5+5,inf=1e9+7;
ll m,n,l,k,r,a[N];
pair <ll,ll> p[N];
int  main()
{
    ios
    cin>>n;
    for (int i=1;i<=n;i++)
    {
        for (int j=1;j<=n;j++)
        {
            cin>>m;
            if (m==1) a[i]++;
            if (m==2) a[j]++;
            if (m==3) a[i]++,a[j]++;
        }
    }
    for (int i=1;i<=n;i++)
    {
        if (a[i]==0) k++;
    }
    cout<<k<<endl;
    for (int i=1;i<=n;i++)
    {
        if (a[i]==0) cout<<i<<" ";
    }
    return 0;

}
