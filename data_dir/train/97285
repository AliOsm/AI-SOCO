#include <iostream>
#include <fstream>
#include <cstdio>
#include <climits>
#include <vector>
#include <map>
#include <list>
#include <queue>
#include <stack>
#include <set>
#include <string>
#include <cstring>
#include <algorithm>
#include <bitset>
#include <cmath>

using namespace std;

#define ll long long
#define vt vector

int cnt[10];

int main()
{
	cnt[0] = 2;
	cnt[1] = 7;
	cnt[2] = 2;
	cnt[3] = 3;
	cnt[4] = 3;
	cnt[5] = 4;
	cnt[6] = 2;
	cnt[7] = 5;
	cnt[8] = 1;
	cnt[9] = 2;
	string n;
	int ans = 1;
	cin >> n;
	ans = cnt[(n[0]-'0')];
	ans *= cnt[(n[1]-'0')];
	cout << ans << '\n';
	return 0;
}