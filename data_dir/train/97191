#include <bits/stdc++.h>

using namespace std;

#define MOD 1000000007
#define N 100005
#define ll long long 
#define ld long double
#define pb push_back
#define ff first
#define ss second

int main(){
	ios::sync_with_stdio(false); //cin.tie(0); cout.tie(0);
	//freopen("input.txt", "r", stdin);
	//freopen("output.txt", "w", stdout);
	string s, t;
	cin>>s;
	while(cin>>t){
		if(t[0] == s[0] || t[1] == s[1]){
			cout<<"YES";
			return 0;
		}
	}
	cout<<"NO";
	return 0;
}