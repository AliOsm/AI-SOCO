#include <bits/stdc++.h>
using namespace std;

const int N = 1e6 + 100;

int main() {
    // freopen("in.txt", "r", stdin);
    ios::sync_with_stdio(0);
    cin.tie(0);

    int tc;
    cin >> tc;
    while(tc--) {
        int n;
        cin >> n;
        if(n == 1) cout << -1 << endl;
        else {
            cout << string(n-1, '5') + "4" << endl;
        }
    }

    return 0;
}

