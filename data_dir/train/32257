#include<bits/stdc++.h>

using namespace std;

int main(){
	int t;
	cin >> t;
	
	while(t --){
		int n,m;
		cin >> n >> m;
		
		cout << "W";
		for(int i = 2;i <= m;i++) cout << "B";
		cout << endl;
		
		for(int i = 2;i <= n;i++){
			for(int j = 1;j <= m;j++){
				cout << "B";
			}
			cout << endl;
		}
	}
	
}