const ll nu = 100100;

int main() {
    std::ios_base::sync_with_stdio(false);
    std::cin.tie(NULL);
    std::cout.tie(NULL);
    //start here
    set<int> st;
    int n;
    cin >> n;
    rep (i, n) {
        int x;
        cin >> x;
        st.insert(x);
    }
    int my = 1, ot = 1000000, time = 0;
    while (my <= ot && st.size()) {
        my++;
        ot--;
        time++;
        if (st.count(my)) {
            st.erase(my);
        }
        if (st.count(ot)) {
            st.erase(ot);
        }
    }
    cout << time << endl;
}

