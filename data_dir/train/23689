#include <bits/stdc++.h>
#include <string>
#include <vector>
#include <iostream>
#include <algorithm>
#include <map>
using namespace std;

#define ll long long
const int INF = 1000000000;

#define fr(i,n) for(int i=0;i<n;i++)
#define fru(i,a,b) for(int i=a;i<=b;i++)

#define N 100010
int a[N];
int n, k, m; 

void Run() {
  cin >> n;
  bool f = false;
  int cost, val;

  fru(i, 1, n) {
      cin >> cost >> val;
      if(cost != val)
          f = true;  
  }
     
cout << (f ? "Happy Alex" : "Poor Alex");

}

int main() {
ios_base::sync_with_stdio(0);
    Run();
    return 0;
}