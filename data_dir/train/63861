#include <bits/stdc++.h>
#define ll long long
#define pb push_back
#define f first
#define s second
using namespace std;
int const N=2e5+6,inf=1e9+7,mod=998244353;
ll a[N],b[N],n,k,m,l,x,x1,x2,y,r,ans,c,kk;
long long q;
char ch;
vector <ll> v;
bool us[N];
string s,t;
main()
{
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    cin>>n;
    for (int i=1;i<=n;i++)
    {
        cin>>a[i];
    }
    sort(a+1,a+n+1);
    for (int i=1;i<=n;i++)
    {
        if (a[i]%a[1]) return cout<<-1,0;
    }
    cout<<a[1];
}