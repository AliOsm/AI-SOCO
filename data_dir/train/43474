#include <bits/stdc++.h>
#define MAX (ll)(2e5 + 7)
#define INF (ll)(1e17 + 7)
#define MOD (ll)(1e9 + 7)
#define PI (double)(3.14159265)
#define IO ios_base :: sync_with_stdio (false);
#define LOG 22

using namespace std;
using ll = long long;

int main () {
	int n;
	cin >> n;
	string s;
	cin >> s;
	if (s.size () == 1) {
		cout << s << "\n";
		return 0;
	}	
	int c = count (s.begin (), s.end (), '0');
	cout << "1";
	for (int i = 0; i < c; i++) {
		cout << "0";
	}
	cout << "\n";
}
