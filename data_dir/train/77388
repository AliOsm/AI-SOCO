#include<iostream>
#include<algorithm>
#include<cmath>
using namespace std;
 int n,m,i,j,x,y,z;

int main()
{  
   cin>>n>>m;
   int64_t t=0;
   for (i=2;i<=n;i+=2)
    for (j=2;j<=m;j+=2) {
      t+=(n-i+1)*(m-j+1);
 //  cout<<t<<' ';
}      
   cout<<t;  
 
 
   
// system("pause");
}
