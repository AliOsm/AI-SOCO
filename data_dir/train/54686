#include <bits/stdc++.h>
using namespace std;

int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0); cout.tie(0);

    int A;  cin >> A;
    int B;  cin >> B;
    int C;  cin >> C;
    int N;  cin >> N;

    A -= C;
    B -= C;

    if (A + B + C >= N || A < 0 || B < 0)
        cout << -1 << '\n';
    else cout << N - A - B - C << '\n'; 

    return 0;
}
