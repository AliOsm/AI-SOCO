/*jai mata di
 let's rock*/
#include<bits/stdc++.h>
using namespace std;
const int N=104;
int a[N];
int main()
{
    int n;
    scanf("%d",&n);
    int i;
    for(i=1;i<=n;i++)
        scanf("%d",&a[i]);
    int sum=0;
    for(i=1;i<=n;i++)
    {
        sum=sum+abs(a[i]);
    }
    printf("%d\n",sum);
    return 0;
}
