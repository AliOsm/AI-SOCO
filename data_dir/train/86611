#include<bits/stdc++.h>
using namespace std;


int main() {
  long long a, b, c, x;
  while(cin >> a >> b >> c) {
    if (c == 0) {
        cout << (a == b ? "YES" : "NO") << endl;
        continue;
    }
    x = b - a;
    
    cout << ((x % c == 0 && x / c >= 0) ? "YES" : "NO") << endl;
  }
}