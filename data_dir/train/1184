using namespace std;
#define ll long long
#define mp make_pair
//#define ld long double
const int mod = (1e9) + 7;
const int siz = 1e5 + 5;
int arr[siz], n;
int main()
{
	scanf("%d", &n);
	while (n--) {
		int v;
		scanf("%d", &v);
		arr[v]++;
	}
	for (int i = 1; i <= 1e5; i++) {
		if (arr[i] % 2 == 1) {
			printf("Conan\n");
			return 0;
		}
	}
	printf("Agasa\n");
	return 0;
}