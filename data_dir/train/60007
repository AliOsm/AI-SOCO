#include<bits/stdc++.h>
#define ll long long
using namespace std;

int main()
{
    int n, m;

    cin >> n >> m;

    int sum = 0;

    for(int i = 0; i < n; i++){
        int x;

        cin >> x;

        sum += x;

        cout << sum / m << ' ';

        sum %= m;
    }

    cout << endl;

    return 0;
}

