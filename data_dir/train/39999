inline IN& operator>>(IN& t, std::string& s) {
    char buf;
    s.clear();
    while (delim(buf = getchar()));
    do {
        s += buf;
    } while (!delim(buf = getchar()));
    return t;
}

inline IN& operator>>(IN& t, char& c) {
    while (delim(c = getchar()));
    return t;
}

