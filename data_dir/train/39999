            h.S=c[l[y.F.S]];
            st.insert(h);
            sz[l[y.F.S]]=sz[r[y.F.S]]+sz[l[y.F.S]];
            r[l[y.F.S]]=r[r[y.F.S]];
            l[r[r[y.F.S]]]=l[y.F.S];
        }
    }
    cout << min(ans,n) << endl;
    return 0;
}