//in the name of god
#include<bits/stdc++.h>
#define int long long
using namespace std;

const int MAXN = 1001 * 1001  , MOD = 1e9 + 7 ;

int a [ 300 ] [ 300 ] = {} ;

 main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    a['A']['A'] = 1 ;
    a['b']['d'] = 1 ;
    a['d']['b'] = 1 ;
    a['H']['H'] = 1 ;
    a['I']['I'] = 1 ;
    a['M']['M'] = 1 ;
    a['O']['O'] = 1 ;
    a['o']['o'] = 1 ;
    a['p']['q'] = 1 ;
    a['q']['p'] = 1 ;
    a['T']['T'] = 1 ;
    a['U']['U'] = 1 ;
    a['V']['V'] = 1 ;
    a['v']['v'] = 1 ;
    a['W']['W'] = 1 ;
    a['w']['w'] = 1 ;
    a['x']['x'] = 1;
    a['X']['X'] = 1 ;
    a['Y']['Y'] = 1 ;
    string s ;
    cin >> s ;
    int n = s.size();
    for(int i = 0 ; i < n ; i ++ )
        if(!a[s[i]][s[n-i-1]])
         return cout<<"NIE",0;
    cout<<"TAK";



}
