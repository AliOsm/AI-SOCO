# include <stdio.h>
# include <math.h>
# include <string.h>

const int N = 100010;

int n, m, k;
int a [N];
int kol [10], b [10][N];

void Print ();
bool Check ();
void SortDig ();
void SplitMerge (int p);

int main ()
{
# ifndef ONLINE_JUDGE
   freopen ("B.txt", "rt", stdin);
   freopen ("B.out", "wt", stdout);
# endif

   int i, j, l=0;

   scanf ("%d", &n);
   for ( i=0; i < n; i++ )
      scanf ("%d", &a [i]);

   if ( Check () )
      printf ("YES");
   else
      printf ("NO");

   return 0;
}

bool Check ()
{
   SortDig ();
/*
   for ( int j=n-2; j > 0; j-- )
      if ( a [j]*2 > a [n-1] )
         return true;
*/
/*
   for ( int j=0; j < n-1; j++ )
      if ( a [j]*2 > a [j+1] )
         return true;
*/
/*
   for ( int j=n-2; j > 0; j-- )
      if ( a [j]*2 > a [n-1] && a [j] < a [n-1] )
         return true;
*/
   for ( int j=0; j < n-1; j++ )
   {
      for ( ; j < n-1 && a [j] == a [j+1]; j++ )
         ;
      if ( j < n-1 && a [j]*2 > a [j+1] )
         return true;
   }
   return false;
}

void Print ()
{
   for ( int i=0; i < n; i++ )
      printf (" %d", a [i]);
   printf ("\n");
}

void SortDig ()
{
   for ( int i=0, p=1; i < 9; i++, p*=10 )
      SplitMerge (p);
# ifndef ONLINE_JUDGE
 Print ();
# endif
}

void SplitMerge (int p)
{
   int i, c, j, l;

   for ( i=0; i < 10; i++ )
      kol [i] = 0;
   for ( i=0; i < n; i++ )
      c = (a [i]/p)%10,
      b [c][kol [c]++] = a [i];

   for ( i=0, l=0; i < 10; i++ )
      for ( j=0; j < kol [i]; j++, l++ )
         a [l] = b [i][j];
}
