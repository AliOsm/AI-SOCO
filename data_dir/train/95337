const double eps = 1e-10;
const int siz = 1e5 + 5, siz2 = 18, siz3 = 5032108;
int n;
char arr[105];
int main()
{
	scanf("%d%s", &n, arr);
	int e = 0;
	for (int i = 0; i < n; i++) {
		e += (arr[i] == '8');
	}
	printf("%d\n", min(e, n/11));
	return 0;
}