#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
#define MOD 1000000007

int main()
{
    int n, a;
    cin >> n;
    set<int> vals;
    for(int i = 0; i < n; i++) {
        cin >> a;
        if (a != 0)
            vals.insert(a);
    }
    cout << vals.size() << endl;
    return 0;
}
