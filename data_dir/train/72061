#include <bits/stdc++.h>

using namespace std;
int x;
string s,t;
int main()
{
    cin>>x>>s>>t;
    if(t[0]=='m'&&x<=29)
        cout<<12;
    else if(t[0]=='m'&&x<31)
        cout<<11;
    else if(t[0]=='m')
        cout<<7;
    else if(t[0]=='w'&&(x==5||x==6))
        cout<<53;
    else
        cout<<52;
    return 0;
}
