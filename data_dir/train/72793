#include <bits/stdc++.h>
using namespace std;

#define ll long long
#define fr(i,n) for(int i=0;i<n;i++)
#define fru(i,a,b) for(int i=a;i<=b;i++)
#define frd(i,a,b) for(int i=a;i>=b;i--)

const int INF = 1e9;
const int N = 1000;

int n, k, m; 
int a[N];

int main() {
   ios_base::sync_with_stdio(0);
   
   fru(i, 1, 5) fru(j, 1, 5) 
   {
       cin >> n;
       
       if(n == 1)
       {
           cout << abs(i - 3) + abs(j - 3);
           return 0;
       }
   }
   
   return 0;
}