#include<bits/stdc++.h>
using namespace std;
#define forr(i,a,n)	for(int i=a; i<n; i++)
#define loop(i,a,n)	for(int i=a; i>=n; i--)

int main() 
{		
	ios_base :: sync_with_stdio(false),cin.tie(NULL),cout.tie(0);
	
	int a;
	cin>>a;
	int b[16]={1,0,0,1,0,1,0,1,1,1,0,0,1,0,1,0};
	cout<<b[a-1];
	
	return 0;	
}