#include <cstdio>
#include <iostream>
#include <vector>
#include <cstring>
#include <algorithm>


using namespace std;



int N,M;
int min_num;


int main()
{
    scanf("%d%d",&N,&M);


    min_num=min(N,M);

    if(min_num%2==0)
        printf("Malvika");

    else
        printf("Akshat");


    return 0;
}
