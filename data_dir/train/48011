#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <iomanip>

using namespace std;

int main()
{
    long long a, b;
    long long x, y, z;
    cin >> a >> b;
    cin >> x >> y >> z;
    long long a1 = x * 2 + y;
    long long b1 = y + z * 3;
    long long ans = 0;
    ans += (a1 > a ? a1 - a : 0);
    ans += (b1 > b ? b1 - b : 0);
    cout << ans << '\n';
    return 0;
}