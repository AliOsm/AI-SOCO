#include <iostream>
#include <fstream>
#include <algorithm>
#include <string>
#include <cstdio>
#include <cmath>
#include <vector>
#include <queue>
#include <deque>
#include <stack>
#include <set>
#include <map>

using namespace std;

const int N=550010;
int n,m,k;
int a[N],p[N],b[N],nom[N];

int main()
{
int n,a,b,c,d;
cin>>n>>a>>b>>c>>d;
int tmin=min(min(a+b,a+c),min(d+c,d+b));
int tmax=max(max(a+b,a+c),max(d+c,d+b));

cout<<1LL*n*max(0,(n-(tmax-tmin)));

}


