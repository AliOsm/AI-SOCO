#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false),cin.tie(0),cout.tie(0);
    int n,k,i;
    cin>>n>>k;
    k=240-k;
    for(i=1;i<=n;i++)
    {
        if(5*i<=k)
            k=k-5*i;
        else
            break;
    }
    cout<<i-1;
}