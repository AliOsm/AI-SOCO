#include<bits/stdc++.h>
#define ll long long
#define ld long double
#define PB push_back
#define MP make_pair
#define INF 1e18
#define inf 1e9
#define y1 dasdsvcvd
using namespace std;

int main()
{
ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
int n;
cin >> n;
int sf = 0, sea=0;
string s;
cin >> s;
for(int i = 1; i < s.size(); i++)
{
   if(s[i-1] == 'F' && s[i] == 'S')
    sf++;
   if(s[i-1] == 'S' && s[i] == 'F')
    sea++;
}
if(sf < sea)
{
    cout <<"YES\n";
}
else
{
    cout <<"NO\n";
}
return 0;
}
