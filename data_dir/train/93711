#include <iostream>
#include <string>
using namespace std;

int main()
{
    string x;
    int y=1;
    cin>>x;
    for(int z=0;z<x.length();z++){
        if(x[z]==x[z+1]) y++;
        else y=1;
        if(y==7){
            cout<<"YES";
            return 0;
        }
    }
  cout<<"NO";
}
