#include<bits/stdc++.h>
#define ll long long
#define f first
#define s second
#define mp make_pair
#define bp push_back
const int mx=1e5+10;
using namespace std;
main(){
ios::sync_with_stdio(0);
cin.tie(0);
cout.tie(0);
int a,b,c,n,ans=0;
cin>>a>>b>>c>>n;
ans=n-a-b+c;
if(ans<=0||a<c||b<c){
cout<<-1<<endl;
}
else cout<<ans<<endl;
}
