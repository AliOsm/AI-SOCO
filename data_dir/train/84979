#include<bits/stdc++.h>
using namespace std;
 
typedef long long ll;
#define Mehrkish ios::sync_with_stdio(false),	cin.tie(0),		cout.tie(0)
 
string s1, s2;
ll ans;
bool flag = false;
 
void Read_Input(){
		cin >> s1 >> s2;
}
 
void Solve(){
		if ( s1 == s2 )
				cout << -1 << endl;
		else
				cout << max( s1.size(), s2.size() ) << endl;
}
 
int main(){
		Mehrkish;
		Read_Input(),		Solve();
		return 0;
}