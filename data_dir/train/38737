#include <algorithm>
#include <iostream>
#include <cstring>
#include <cstdlib>
#include <cassert>
#include <cstdio>
#include <vector>
#include <cmath>
#include <ctime>
#include <set>
#include <map>


typedef long long ll;

using namespace std;
int n,a,x,y;
int main()
{

	cin>>n;
	for(int i=0; i<n; i++){
		cin>>a;
		if(a==100)
			x++;
		else
			y++;
	}
	if(x&1||n==1||(!x&&y&1))
		cout<<"NO";
	else
		cout<<"YES";
					
	return 0;
}
