#include <bits/stdc++.h>
using namespace std;
int n, s = 0;

int init();
int solve();

int main()
{
    init();
    solve();
}

int init()
{
    int i, x;

    scanf("%d", &n);
    for (i = 1; i <= n; i++)
    {
        scanf("%d", &x);
        s += x;
    }
}

int solve()
{
    if (s % n == 0) printf("%d", n);
    else printf("%d", n - 1);
}































