#include <bits/stdc++.h>

using namespace std;

int x, l[10], a, c, x1, aa, bb, n, cc;

int main()
{
    cin >> n;
    for (int i=1;i<=n;i++)
        cin >> a,aa+=a;
    for (int i=1;i<n;i++)
        cin >> a, bb+=a;
    cout << aa-bb << endl;
    for (int i=1;i<n-1;i++)
        cin >> a, cc+=a;
    cout << bb-cc;
    return 0;
}
