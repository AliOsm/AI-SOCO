	ios::sync_with_stdio(false);
	cin.tie(0); cout.tie(0);
	int n;
	cin >> n;

	while (n--) {
		int l;
		cin >> l;
		string s;
		cin >> s;
		int f = 0;

		for (int i = 0; i < (int)s.size(); i++) {
			if (s[i] == '8' && i + 10 < l) f = 1;
		}
		if (f) cout << "YES\n";
		else cout << "NO\n";
	}
	return 0;
}