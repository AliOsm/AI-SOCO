#include <bits/stdc++.h>

using namespace std;

#define pb push_back
#define mp make_pair
#define ll long long

int main()
{
    int n;
    cin>>n;
    int ans=0,i=1,ans1=0;
    while (ans+i<=n)
    {
        ans+=i;
        i*=2;
        ans1++;
    }
    if (ans-n<0) ans1++;
    cout<<ans1;
    return 0;
}