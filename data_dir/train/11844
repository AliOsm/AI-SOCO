#include <bits/stdc++.h>

using namespace std;
using namespace __gnu_cxx;


#define f first
#define s second
#define sz size()
#define pb push_back
#define mem(a,b,s) for(int i=0 ; i<=s ; ++i) a[i]=b;
#define mem2(a,r,c,b) for(int i=0 ; i<=r ; ++i)    for(int j=0 ; j<=c ; ++j)    a[i][j]=b;
#define all(a) a.begin(),a.end()
#define mp make_pair
#define in insert
#define sc(a) scanf("%d",&a)
#define sc2(a,b)	scanf("%d%d",&a,&b)
#define rep(x,a,b)	for(int x=a ; x<=b ; ++x)
#define irep(x,a,b)	for(int x=a ; x>=b ; --x)

typedef unsigned long long ull;
typedef long long ll;
typedef pair<int,int> ii;
typedef vector<int> vi;
typedef vector<pair<int,int> > vii;
typedef set<int> si;
typedef set<pair<int,int> > sii;
typedef map<int,int>	mii;


const double pi=3.14159265358979323846264338327950288419716939937510582097494459;
const double EPS = 1e-9;
const int MAXN=2e6+10;
const ll MOD=1e9+7;
const int oo=2e9;
const ll OO=1e17;

int dx[]={0,1,0,-1,-1,-1,1,1};
int dy[]={1,0,-1,0,-1,1,-1,1};

int n,x,l[1010];
bool f;
vi ver[1010];

int solve(int node = 1)
{
	if(!ver[node].sz)	return 1;

	for(int i=0 ; i<ver[node].sz ; ++i)
		l[node]+=solve(ver[node][i]);
	if(l[node]<3)	f=1;
	return 0;
}

int main()
{
 	ios_base::sync_with_stdio(false);
 	cin.tie(NULL);
 	cout.tie(NULL);
//	freopen("In.txt","r",stdin);
//	freopen("Out.txt","w",stdout);

 	cin >> n;
 	rep(i,2,n)	cin >> x,ver[x].pb(i);
 	solve();
 	if(f)	return puts("No");
 	return puts("Yes");
}
