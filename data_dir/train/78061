#include <bits/stdc++.h>
using namespace std;

int n, a, b, x, y;

int main() {
    cin >> n;
    while (n--) {
        cin >> x >> y;
        if (x > y)
            a++;
        else if (x < y)
            b++;
    }
    
    if (a > b)
        cout << "Mishka";
    else if (a < b)
        cout << "Chris";
    else
        cout << "Friendship is magic!^^";
}