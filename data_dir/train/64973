#include <bits/stdc++.h>
using namespace std;
int main(){
	int n, m;
	long long sum = 1;
	cin >> n >> m;
    if(n == 1 && m > 9 || n == 1 && m == 10){
    	cout << -1;
    	return 0;
	}
	if(n == 1 && m <= 9){
		cout << m;
		return 0;
	}
	if(m == 2){
		for(int i = 0; i < n; i ++){
			if(i == 0){
				cout << 1;
			}
			else{
				cout << 0;
			}
		}
	}
	if(m == 3){
      		for(int i = 0; i < n; i ++){
			if(i == 0){
				cout << 3;
			}
			else{
				cout << 0;
			}
		}		
	}
	if(m == 4){
				for(int i = 0; i < n; i ++){
			if(i == 0){
				cout << 4;
			}
			else{
				cout << 0;
			}
		}
	}
	if(m == 5){
				for(int i = 0; i < n; i ++){
			if(i == 0){
				cout << 5;
			}
			else{
				cout << 0;
			}
		}
	}
	if(m == 6){
				for(int i = 0; i < n; i ++){
			if(i == 0){
				cout << 6;
			}
			else{
				cout << 0;
			}
		}
	}
	if(m == 7){
				for(int i = 0; i < n; i ++){
			if(i == 0){
				cout << 7;
			}
			else{
				cout << 0;
			}
		}
	}
	if(m == 8){
				for(int i = 0; i < n; i ++){
			if(i == 0){
				cout << 8;
			}
			else{
				cout << 0;
			}
		}
	}
	if(m == 9){
				for(int i = 0; i < n; i ++){
			if(i == 0){
				cout << 9;
			}
			else{
				cout << 0;
			}
		}
	}
	if(m == 10){
				for(int i = 0; i < n; i ++){
			if(i == 0){
				cout << 1;
			}
			else{
				cout << 0;
			}
		}
	}
}
