#include <iostream>
#include <string>
#include <algorithm>
#include <vector>
#include <set>
#include <deque>

using namespace std;
#include <math.h>
double H, L;

int main() {
	cin >> H >> L;
	double X = (H * H + L * L) / (2 * H);
	double A = sqrt(X * X - L * L);
	cout << fixed << A;
}