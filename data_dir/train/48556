//IOI 2021
#include <bits/stdc++.h>
using namespace std;

#define pb push_back
#define ff first
#define ss second

typedef long long ll;
typedef pair<int, int> pii;

int n, l, r;
string s;

int main() {
	ios_base::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	cin >> n >> s;
	for (int i = 0; i < n; i++)
		if (s[i] == 'L')
			l++;
		else
			r++;
	cout << l + r + 1 << endl;
	return 0;
}
