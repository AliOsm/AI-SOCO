#include<bits/stdc++.h>
#define MEM(dp,i) memset(dp,i,sizeof(dp))
#define R return
#define F first
#define S second
#define C continue
#define pb push_back
using namespace std;
typedef long long ll;
typedef pair<ll,ll> pll;
typedef pair<int,int> pii;
const int MX=2e5+9;
const ll mod=1e9+7;
const ll inf=(1ll<<60);
ll x,y,z,x1,y1,z1,ans,a1,a2,a3,a4,a5,a6;
int main(){
    cin>>x>>y>>z>>x1>>y1>>z1>>a1>>a2>>a3>>a4>>a5>>a6;
    if(x<0)ans+=a5;
    if(y<0)ans+=a1;
    if(z<0)ans+=a3;
    if(x>x1)ans+=a6;
    if(y>y1)ans+=a2;
    if(z>z1)ans+=a4;
    cout<<ans<<endl;
}
