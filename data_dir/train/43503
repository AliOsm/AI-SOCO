LL gcd(LL a, LL b) {
    return b == 0 ? a : gcd(b, a % b);
}
int int_gcd(int a, int b){
    return b == 0 ? a : int_gcd(b, a % b);
}
LL lcm(LL a , LL b ){
    return a / gcd(a , b) * b;
}
LL poww(LL base, LL exp , LL mod){
  base %= mod;
  LL result = 1;
  while (exp > 0) {
    if (exp & 1) result = (result * base) % mod;
    base = (base * base) % mod;
    exp >>= 1;
  }
  return result;
}
int main()
{
    int n ; cin >> n ;

    while(n--){
        string a ; cin >> a;
        sort(all(a));
        bool ans = 1 ;
        for(int i = 0 ; i < a.size() ; i++){
            if((a[0] + i) != a[i]){
                ans = 0 ;
            }
        }

        cout << (ans ? "Yes" : "No") << endl ;
    }
}
