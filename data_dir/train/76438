#include <bits/stdc++.h>
#define pb push_back
#define mp make_pair
#define fi first
#define se second

using namespace std;

typedef pair<int, int> ii;
typedef long long ll;

int main()
{
	int k, r;
	cin >> k >> r;
	for (int i = 1; i <= 10; i++)
	{
		if ((i*k)%10 == 0 or (i * k)%10 == r)
		{
			printf("%d\n", i);
			return 0;
		}
	}
	return 0;
}

