#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n,m;
    cin>>n>>m;
    int temp1,temp2;
    while(m--)
    {
        cin>>temp1>>temp2;
    }
    for(int i=0;i<n;i++)
    {
        if(i%2==0)
        {
            cout<<"0";
        }
        else
        {
            cout<<"1";
        }
    }
    return 0;
}