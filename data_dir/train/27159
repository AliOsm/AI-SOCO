#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define all(sdk) sdk.begin(),sdk.end()
#define mll map<ll,ll>
#define pb push_back
#define rep(i,a,b) for(ll i=a;i<b;++i)
#define repd(i,a,b) for(ll i=a;i>=b;--i)
#define mp make_pair
#define hell 1000000007
#define endl '\n'
#define vvll vector<vector<ll> >
#define vll vector<ll>
#define mint map<int,int> 
#define int long long
#define sz(x) (int)x.size()
#define sll set<int>
#define pll pair<int,int>

int a[1005];
void solve(){  
  string s;
  cin>>s;
  int n=sz(s);
  s+='0';
  
  int ind=-1;
  rep(i,0,n)
  if(s[i]=='a')ind=i;
  if(ind==-1){
  	rep(i,0,n)
  	cout<<a[i]<<' ';
  	cout<<endl;
  	return;
  }
  rep(i,0,ind+1){
  	if(s[i]!=s[i+1])
  		a[i]=1;
  }
  rep(i,0,n)
  cout<<a[i]<<' ';
  cout<<endl;
}

signed main(){
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int t=1;
    // cin>>t;
    while(t--){
        solve();
    }
    return 0;
}