#include<bits/stdc++.h>
using namespace std;
#define forr(i,a,n) for(int i=a; i<n; i++)
#define loop(i,a,n)	for(int i=a; i>=n; i--)
#define conti continue
#define brk break
#define pb push_back
#define SPEED ios_base :: sync_with_stdio(false), cin.tie(NULL), cout.tie(0)
#define in cin>>
#define out cout<<
#define endl <<"\n"
#define ll long long 
#define remove(a,b) a.erase(std::remove(a.begin(),a.end(),b),a.end())
#define runn(it,a) for(auto it = a.begin(); it!= a.end(); it++) 
#define fi first
#define se second

int main() 
{
	SPEED;	
	string s;
	vector<pair<char,char>> v;
	forr(i,0,3)	{in s; v.pb(make_pair(s[0],s[1]));}
	sort(v.begin(),v.end());
	if(int(v[0].fi)+1==int(v[1].fi) && int(v[1].fi)+1==int(v[2].fi)&&v[0].se==v[1].se&&v[1].se==v[2].se) out 0;
	else if(int(v[0].fi)+1==int(v[1].fi)&&v[0].se==v[1].se) out 1;
	else if(int(v[1].fi)+1==int(v[2].fi)&&v[1].se==v[2].se) out 1;
	else if(int(v[0].fi)+1==int(v[2].fi)&&v[0].se==v[2].se) out 1;
	else if(int(v[0].fi)+2==int(v[1].fi)&&v[0].se==v[1].se) out 1;
	else if(int(v[1].fi)+2==int(v[2].fi)&&v[1].se==v[2].se) out 1;
	else if(int(v[0].fi)+2==int(v[2].fi)&&v[0].se==v[2].se) out 1;
	else if(int(v[0].fi)==int(v[1].fi)&&int(v[1].fi)==int(v[2].fi)&& v[0].se==v[1].se&&v[1].se==v[2].se)	out 0;
	else if(int(v[0].fi)==int(v[1].fi)&& v[0].se==v[1].se)	out 1;
	else if(int(v[1].fi)==int(v[2].fi)&&v[1].se==v[2].se)	out 1;
	else if(int(v[0].fi)==int(v[2].fi)&& v[0].se==v[2].se)	out 1;
	else out 2;
	
	return 0;
}