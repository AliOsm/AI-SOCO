#include <bits/stdc++.h>
using namespace std;
int32_t main() {
    ios_base :: sync_with_stdio(false);
    long long n;
    cin >> n;
    for (int i = 0; i < n; ++i) {
        long long a, b, c;
        cin >> a >> b >> c;
        c %= 3;
        if (!c) {
            cout << a;
        } else if (c == 1) {
            cout << b;
        } else {
            cout << (a ^ b);
        }
        cout << endl;
    }
}