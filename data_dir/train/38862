#include <bits/stdc++.h>
#define REP(i, n) for (long long i = 0; i < n; i++)
using namespace std;
typedef long long Long;

//const Long N=;

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.precision(10);
	cout << fixed;
    
    vector<Long> v;
    Long x;
    for(Long i = 0; i < 4; i++){
    	cin >> x;
    	v.push_back(x);
	}
	sort(v.begin(),v.end());
	cout << v[3] -v[0] << " " << v[3] -v[1]  << " " << v[3] -v[2] << endl;
	return 0;
}
