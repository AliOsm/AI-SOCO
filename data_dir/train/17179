#define range 100010

l a[range];
char s[range];


int main()
{
    l i,j,k,r;

    //fil;
    //freopen("out.txt","w",stdout);
    l n=lin();
    l m=lin();
    l d=n*m;
    //sf("%s",s);
    l c=0;
    while(1)
    {
        if(d<=0)break;
        l x=n+m-1;
        d-=x;
        c++;
        m--,n--;
    }
    if(c%2==1)
    {
        puts("Akshat");
    }
    else
        puts("Malvika");
    //sort(a+1,a+1+n);
    return 0;
}



