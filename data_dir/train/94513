//In the name of God
#include<bits/stdc++.h>
#define pb push_back
#define F first
#define S second
#define mod (ll)1000000007
#define ll long long int
#define acm ios_base::sync_with_stdio(0),ios::sync_with_stdio(0),cin.tie(0),cout.tie(0);
using namespace std;
int main()
{
    acm;
    ll n,m;
    cin>>n>>m;
    for (ll i=2;i<=m;++i)
        if (n%i!=i-1) return cout<<"NO\n",0;
    cout<<"YES\n";
    return 0;
}