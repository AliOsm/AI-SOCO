#include <bits/stdc++.h>
using namespace std;

long long n, k, resp;

int main(){

    scanf("%lld %lld",&n,&k);

    resp += (2*n) / k + ((2*n)%k==0 ? 0 : 1);
    resp += (5*n) / k + ((5*n)%k==0 ? 0 : 1);
    resp += (8*n) / k + ((8*n)%k==0 ? 0 : 1);

    printf("%lld",resp);

    return 0;
}
