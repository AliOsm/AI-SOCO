#include<bits/stdc++.h>
#define fr first
#define se second
#define ll long long
#define ld long double
#define pb push_back
using namespace std;

const long long N = 2e6;
ll n;
ll cnt;
ll ans=9;
ll a;
int main(){
    ios_base::sync_with_stdio(NULL);
cin >> n;
while(n/10!=0){
    ans++;
    n++;
    while(n%10==0)
        n/=10;
}
cout<<ans;
}
