#include <bits/stdc++.h>

#include <bits/stdc++.h>
#define int long long

using namespace std;

class DGoodTriple {
public:
    string s;
    int n;
    bool check(int l, int r) {
        for(; l < r; ++l) {
            for(int jump = 1; l + 2*jump <= r; ++jump) {
                if(s[l] == s[l+jump] and s[l] == s[l+2*jump]) return true;
            }
        }
        return false;
    }
	void solve(std::istream& in, std::ostream& out) {
        in >> s;
        n = s.length();
        int res = 0;
        for(int l = 0; l < n; ++l) {
            for(int r = l + 2; r < n; ++r) {
                if(check(l, r)){
                    res += (n - r);
                    break;
                }
            }
        }
        out << res;
	}
};


int32_t main() {
	DGoodTriple solver;
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
	return 0;
}
