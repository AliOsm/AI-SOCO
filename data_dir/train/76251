//++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
//++--++--++--++--++--++--++--++--++--+--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
const int MAXN = 200001;
const int inf = 1000000001;
const ld INF = 1.0 / 0.0;
const ld EPS = 1e-7;
const int mod = inf + 7;
const ld PI = acos(-1.0);
int ans = 0;
signed main()
{
	fastio();
	int n;
	cin >> n;
	vector<int> a(n);
	vin(a);
	bool f = 0; for (int i : a) if (i % 2) f = 1;
	if (!f)
	{
		cout << 0;
		return 0;
	}
	int sod = 0;
	for (int i : a) if (!(i % 2))sod += i;
	int cnt = 0;
	for (int i : a) cnt += i % 2;
	if (cnt % 2)
	{
		int sum = 0;
		for (int i : a)
			sum += i;
		cout << sum;
		return 0;
	}
	int mnod = inf;
	for (int i : a) if (i % 2) mnod = min(mnod, i);
	int sum = 0;
	for (int i : a) sum += i;
	cout << sum - mnod;
}