#include<bits/stdc++.h>
using namespace std;
int i, j, k, l, x, y, z, m, n, ans, p, q;
int ara[20000];
int main()
{
    cin >> n;

    j = 0;

    i = 1;

    while(n){
        if(n % 2 == 1){
            ara[j++] = i;
        }
        i++;
        n = n / 2;
    }


    for(k = j - 1; k >= 0; k--){
        printf("%d ", ara[k]);
    }


    return 0;
}
