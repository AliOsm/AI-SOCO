#include<bits/stdc++.h>
using namespace std;
int main(){
    ios_base::sync_with_stdio(0);cin.tie(0);
	int n,i,j,k,z;
	cin>>n>>k;
	for(j=0;j<k;j++){
		for(i=0;i<n-1;i++){
			if((n-i-2)&(1<<j))
                z=n-(1<<j);
            else
                z=n;
			cout<<z<<" ";
		}
		cout<<n<<"\n";
	}
}
