#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n;
    scanf("%d", &n);
    int l[n], r[n];
    for(int i=0; i<n; i++) scanf("%d %d", &l[i], &r[i]);
    int x;
    scanf("%d", &x);
    int ans = 0;
    for(int i=0; i<n; i++){
        if(x > r[i]){

        }
        else {
            ans++;
        }
    }
    cout << ans;
}

