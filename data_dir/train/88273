int main()
{
	string s;
	cin >> s;
	int _1, _0;
	bool ans = 0;
	for (int i = 0; i < s.size(); i++){
		if (s[i] == 'H' || s[i] == 'Q' || s[i] == '9'){
			ans = 1;
			break;
		}
	}
	if (ans)cout << "YES";
	else cout << "NO";
	//system ("pause");
