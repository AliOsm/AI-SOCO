//------------------------------------------------------------ 
const int N = 200000;
int mas[26][26];
int64 a, b, ans;
int main()
{
#ifdef WIN32
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
#endif
    cin >> a >> b;
    while(b)
    {
        ans += a / b;
        a %= b;
        swap(a, b);
    }
    cout << ans;
} 