/* In the name of Allah */
#include<bits/stdc++.h>
using namespace std;

int main() {
	ios:: sync_with_stdio(0), cin.tie(0), cout.tie(0);

	int n, k;
	cin >> n >> k;
	if (k < 2 * n) {
		cout << "NO";
		return 0;
	}
	cout << "YES\n" << k - 2 * n + 2;
	for (int i = 1; i < n; i++)
		cout << " 2";
	cout << "\n1";

	return 0;
}
