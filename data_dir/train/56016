#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

int a,b;

int main()
{
    ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
    string s;
    cin >> s;
    for(int i=0;i<s.size();i++){
        if(s[i]=='-')a++;
        else b++;
    }
    if (b==0)cout<<"YES";
    else if(a%b==0)cout<<"YES";
    else cout<<"NO";

}
