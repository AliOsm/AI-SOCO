public:
    void solve(istream& in, ostream& out) {
        vector<string> friends = {"Danil", "Olya", "Slava", "Ann", "Nikita"};
        string s;
        in >> s;
        int cnt = 0;
        for (int i = 0; i < 5; ++i) {
            for (int j = 0; j + friends[i].size() <= (int)s.size(); ++j) {
                cnt += friends[i] == s.substr(j, friends[i].size());
                
            }
        }
        out << (cnt == 1 ? "YES" : "NO") << "\n";
    }
};


