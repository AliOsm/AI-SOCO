#include<bits/stdc++.h>
using namespace std;
#define forr(i,a,n) for(int i=a; i<n; i++)
#define loop(i,a,n)	for(int i=a; i>=n; i--)
#define conti continue
#define brk break
#define pb push_back
#define SPEED ios_base :: sync_with_stdio(false), cin.tie(NULL), cout.tie(0)
#define in cin>>
#define out cout<<
#define endl <<"\n"
#define ll long long 
#define remove(a,b) a.erase(std::remove(a.begin(),a.end(),b),a.end())
#define runn(it,a) for(auto it = a.begin(); it!= a.end(); it++) 

int main() 
{
	SPEED;	
	ll int m;
	in m;
	while(m--){
	ll int a,b=0;
	in a;
	ll int c=a, flag=0,flag1=0,flag2=0,e=0,x=0;
	while(c!=1)
	{
		if((c%2)==0)	{c=c/2; e++; conti;}
		else if((c%3)==0)	{c=(2*c)/3;e++; conti;}
		else if((c%5)==0){c=(c*4)/5; e++; conti;}
		else {out -1 endl;x=1; brk;}
		x++;
	}
	if(x==0)out e endl;}
	return 0;
}