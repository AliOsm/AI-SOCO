#include <bits/stdc++.h>
using namespace std;
int n,c=1;
vector<int> ans;
int main(){
    ios_base::sync_with_stdio(false);
    cin>>n;
    for(int i=1,d;i<=n;++i){
        cin>>d;
        if(d==c){
            ++c;
            ans.push_back(2000+i);
        }
    }
    cout<<ans.size()<<endl;
    for(auto i : ans){
        cout<<i<<" ";
    }
}