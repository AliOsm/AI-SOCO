#include<bits/stdc++.h>

using namespace std;
typedef long long ll;

int main()
{
    std::cin.tie(0);
    std::ios::sync_with_stdio(false);
    ll x,y,z,a,b,c,s=0,n=0;
    cin>>a>>b>>c>>x>>y>>z;
    if(a>x)
    s+=(a-x)/2;
    else
        n+=x-a;
    if(b>y)
    s+=(b-y)/2;
    else
        n+=y-b;
    if(c>z)
    s+=(c-z)/2;
    else
        n+=z-c;
    if(n<=s)
        cout<<"YES"<<endl;
    else
        cout<<"NO"<<endl;
    return 0;
}
