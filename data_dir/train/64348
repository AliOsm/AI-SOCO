#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

int N;
int a[MAX];

int main()
{
    optimiza_io
    cin >> N;
    for( int i = 0; i < N; i ++ )
        cin >> a[i];
    
    for( int i = 0; i < ( 1 << N ); i ++ )
    {
        int tmp = 0;
        for( int j = 0; j < N; j ++ )
        {
            if( ( i & ( 1 << j ) ) )
                tmp += a[j];
            else
                tmp -= a[j];
        }
        tmp %= 360;
        if( ! tmp )
        {
            cout << "YES\n";
            return 0;
        }
    }
    cout << "No\n";
    
    return 0;
}


