int n,a,b,c,d,e,f,g,h,i;
int main()
{
    speed;
    cin>>n>>a>>b>>c>>d>>e>>f;
    g=min(n-(c+e),b);
    h=min(n-(g+e),d);
    i=n-(g+h);
    cout<<g<<" "<<h<<" "<<i;
    return 0;
}
