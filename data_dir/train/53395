int main(){
	//read;
	string s;
	int c[3]={0};
	rep(i,3){
		getline(cin ,s);
		rep(j,s.size())if( s[j]=='a' || s[j]=='u'||s[j] =='i' || s[j]=='e' || s[j]=='o' ) c[i]++;
	}
	if( c[0]==5 && c[1]== 7 && c[2]==5 )cout<<"YES";
	else cout<<"NO";
	return 0;
}