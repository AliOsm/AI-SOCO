#include <bits/stdc++.h>

#define ll long long
#define sz size()
#define all(xx) xx.begin(),xx.end()
#define pb push_back
#define in insert
#define er erase
#define x Clarion
#define y Harbour
#define S second
#define F first
#define mp make_pair
#define stop exit (0)
#define Yes cout<<"Yes"
#define No cout<<"No"
#define YES cout<<"YES"
#define NO cout<<"NO"
#define speed ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0) 
#define input freopen (".in", "r", stdin)
#define output freopen (".out", "w", stdout)

using namespace std;

const int SS = 5e2 + 123;
const int N = 1e5 + 123;
const int NN = 1e6 + 123;
const int M = 1e7 + 123;
const int MM = 1e8 + 123;
const int inf = 1e9 + 123;
const ll INF = 1e18 + 123;
const ll MAXN = 1e10 + 123;

bool u[N];     
int n, m;

int main ()
{
	speed;
	cin >> n >> m;      
	for (int i = 1; i <= m; i ++)
	{
		int x, y;
		cin >> x >> y;
		u[x] = u[y] = 1;
	}
	for (int i = 1; i <= n; i ++)
	{
		if (!u[i])
		{
			cout << n - 1 << endl;
			for (int j = 1; j <= n; j ++)
			{                
				if (i != j)
					cout << i << " " << j << endl; 
			}
			break;
		}
	}
}
