#include<bits/stdc++.h>

#define ll long long
#define dl double

using namespace std;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);

    ll T;
    cin >> T;

    while(T--){
        ll a;
        cin >> a;

        dl n = 360.0 / (180.0 - a);
        ll N = n;

        if((dl)N == n) cout << "YES\n";
        else cout << "NO\n";
    }
    return 0;
}
