#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define fr first
#define sc second
#define r_0 return 0
#define fin freopen( "input.txt", "r" , stdin );
#define fout freopen( "output.txt", "w" , stdout );
using namespace std;

const ll N = 2e5 + 1 ;

ll n , m , k , l , x , y , z , cnt , ans , sum , mx = -1e18 , mn = 1e18 ;

ll a[N] ;
map < ll , int > mp{ {0 , 1} };
string s , t ;

int main()
{
    ios_base::sync_with_stdio(0);

    cin >> n >> m ;
    for ( int i = 1; i <= n ;i ++ )
    {
        cin >> x >> y ;
        for ( int j = x ; j <= y ; j ++ )
            mp [ j ] = mp [ x ] ;
    }
    if ( mp [ m ] )
        cout << "YES";
    else
        cout << "NO";
}















