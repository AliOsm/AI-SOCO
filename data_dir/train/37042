using namespace std;

//constexpr long long int MOD = 1000000007;
//constexpr int MOD = 1000000007;
constexpr int MOD = 998244353;
//constexpr long long int MOD = 998244353;
constexpr double EPS = 1e-8;

//int N, M, K, H, W, L, R;
long long int N, M, K, H, W, L, R;


int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);

	cin >> K;
	while (K--) {
		int a, b, c, d, e;
		cin >> a >> b >> c >> d >> e;
		a /= 2;
		int ans = 0;
		if (d > e) {
			int num = min(a, b);
			a -= num;
			b -= num;
			ans += d * num;
			num = min(a, c);
			ans += e * num;
			cout << ans << endl;
		}
		else {
			swap(b, c);
			swap(d, e);
			int num = min(a, b);
			a -= num;
			b -= num;
			ans += d * num;
			num = min(a, c);
			ans += e * num;
			cout << ans << endl;
		}
	}
}
