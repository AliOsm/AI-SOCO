# include <iostream>

using namespace std;

const long long N = 1e5 + 100;

string s;
int n;

int main()
{
	cin >> s;
	for (int i = 1; i <= 5; i++){
		string s1;
		cin >> s1;
		if(s1[0] == s[0]){
			cout << "YES";
			return 0;
		}
		if(s1[1] == s[1]){
			cout << "YES";
			return 0;
		}
	}
	cout << "NO";
	
	
	
}
