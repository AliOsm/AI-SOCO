#include "cstdio"
using namespace std;

int main()
{
    int t, x, y;
    scanf("%d", &t);
    while (t--) {
        scanf("%d%d", &x, &y);
        if (x >= y) {
            printf("YES\n");
        } else if ((x == 1) || ((x <= 3) && (y > 3))) {
            printf("NO\n");
        } else {
            printf("YES\n");
        }
    }
    return 0;
}
