#include <bits/stdc++.h>
using namespace std;
 
#define ll long long int
#define ld long double
#define ff first
#define ss second
#define pb push_back

#define mp make_pair
#define bp __builtin_popcountll
#define MOD  1000000007
const int N = 105;
#define FAST ios_base::sync_with_stdio(false), cin.tie(0), cout.tie(0);srand(time(NULL));

int main()
{
    ll t;
    cin >> t;
    while(t--)
    {
        ll a,b;
        cin >> a >> b;
        if(a==b)  
        {
            cout<<0<<"\n";
        }
        else if(a<b)
        {
            if(a%2!=b%2)
            {
                cout<<1<<"\n";
            }
            else
            {
                cout<<2<<"\n";
            }
        }
        else
        {
            if(a%2==b%2)
            {
                cout<<1<<"\n";
            }
            else
            {
                cout<<2<<"\n";
            }
        }
    }
}