#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;

    cin >>n;
    vector<int >v;
    map<int,int>mp;

    for(int i=0; i<n; i++)
    {
        int x;
        cin >>x;

         mp[x]++;
    }

   map<int ,int > ::iterator  it;
   for(it=mp.begin();it!=mp.end();it++)
   {
       int x=   it->second;
       if(x%2)
       {
           cout <<"Conan"<<endl,exit(0);
       }
   }
      cout <<"Agasa"<<endl;
}


