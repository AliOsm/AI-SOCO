#define ll long long
#define F first
#define S second
#define pb push_back
#define mp make_pair
const double PI = acos(-1.0), EPS = 1e-9;
const int MX=3*1e5+10;
int N, M, k;
int arr[MX];
int main()
{
    string s1, s2;
    cin>>s1>>s2;
    cout<<s1[0];
    for(int i=1; i<s1.size(); i++)

    {
        if(s1[i] < s2[0] )cout<<s1[i];
        else break;
    }
    cout<<s2[0];
    return 0;
}
