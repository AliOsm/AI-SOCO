#include <bits/stdc++.h>
using namespace std;
#define ll long long

void solve()
{
    string s;
    cin>>s;
    ll zero = 0, one = 0;
    for(ll i=0; i<s.size(); i++)
    {
        if (s[i]=='0') zero++;
        else
            one++;
    }
    if ( (zero==0) || (one ==0) )
    {
        cout<<s<<endl;
        return;
    }
    for(ll i=0; i<s.size(); i++)
        cout<<10;
    cout<<endl;
}

int main()
{
    #ifndef ONLINE_JUDGE
    freopen("input.txt","r",stdin);
    #endif // ONLINE_JUDGE

    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    ll n;
    cin>>n;
    for(ll i=0; i<n; i++)
        solve();

    return 0;
}
