#include <bits/stdc++.h>
using namespace std;

vector<int> X;

int main() {

    ios::sync_with_stdio(0);
    cin.tie(NULL), cout.tie(NULL);

    int N, M;
    cin >> N >> M;
    
    int sum = 0;
    
    while(sum < N) {
        sum += 4;
        X.push_back(4);
    }
    
    X.push_back(5);
    
    for(int i=0; i<X.size(); i++)
        cout << 5;
    cout << "\n";
    
    for(int i=0; i<X.size(); i++)
        cout << X[i];
    cout << "\n";
    
    return 0;
}
