#include <bits/stdc++.h>
using namespace std;
#define pb  push_back
#define ll  long long
#define vi  vector<ll >
#define vvi vector<vi >
#define all(x) x.begin(), x.end()

ll n, s;

int main() {
    ios_base::sync_with_stdio(false); cin.tie(0);
    cin >> n >> s;
    if (s < 2 * n) {
        cout << "NO\n";
        return 0;
    }
    cout << "YES\n";
    for (int i = 0; i < n - 1; ++i) {
        cout << 2 << " "; s -= 2;
    }
    cout << s << "\n";
    cout << 1 << "\n";

    return 0;
}