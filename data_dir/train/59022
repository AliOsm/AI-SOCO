#include <cstdio>
#include <cstring>
#include <assert.h>
#include <cmath>
#include <cstdlib>
#include <queue>
#include <map>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
typedef long long ll;
const ll maxn = 1000050;
const ll INF = (1LL << 62) - 1;
const ll mod = 1000000007;

ll gcd(ll a, ll b) {
	return b?gcd(b,a%b):a;
}

int main()
{
	ll l,r;
	cin>>l>>r;
	cout<<"YES"<<endl;
	for(ll i = l; i <=r; i+=2) {
		cout<<i<<" "<<i+1<<endl;
	}
}