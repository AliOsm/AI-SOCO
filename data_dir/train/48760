#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

int X, Y, a[3], ans;

int main()
{
    optimiza_io
    cin >> X >> Y;
    a[0] = a[1] = a[2] = Y;
    while( a[0] < X ) {
        a[0] = a[2] + a[1] - 1;
        sort( a , a + 3 );
        ans++;
    }
    cout << ans;
    return 0;
}