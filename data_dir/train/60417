#include<bits/stdc++.h>
using namespace std;

#define x first
#define y second
#define pb push_back
#define mp make_pair

#define all(x) (x).begin(), (x).end()
#define sz(x) (int)(x).size()

#define ll long long
#define int long long

signed main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    int n, m;
    cin >> n >> m;
    cout << max(0LL, n - m * 2) << " ";
    int k = 0;
    while (k * (k - 1) / 2 < m) k++;
    cout << n - k;
    return 0;
}
