//++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
//++--++--++--++--++--++--++--++--++--+--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
const int MAXN = 300008;
const ll inf = 1000000001;
const ll INF = 2e18;
const ld EPS = 1e-7;
const ll mod = inf + 6;
const int moff = 998244353;

const int mod13 = 1000003;
const ld PI = acos(-1.0);

signed main()
{
	fastio();
	vector<int> kek(228228 + 228228 + 228228 + 228228);
	kek[0] = 1;
	int n, x;
	cin >> n >> x;
	int lol = 0;vector<int>ans;
	for (int i = 19; i >= 0; --i) 
	{
		if (x & (1 << i))
		{
			lol = i;
			break;
		}
	}

	int kekos = 0;
	
	while (true)
	{
		bool f = 0;
		for (int i = 0; i < n; i++) 
		{
			if (i!= lol && kek[kekos ^ (1 << i)] == 0) 
			{
				kekos ^= (1 << i);
				kek[kekos ^ (1 << i)] = 1;
				ans.pbc(1 << i);
				f = 1;
				break;
			}
		}
		if (!f) break;
	}
	cout << ans.size() << endl;
	vout(ans, ' ');
//	sp;
}
