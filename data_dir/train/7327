#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define po pop_back
#define pb push_back
#define mk make_pair
#define lw lower_bound
#define up upper_bound
#define ff first
#define ss second
#define boost ios_base::sync_with_stdio(); cin.tie(0); cout.tie(0);
#define MOD 1000000007
#define MAX INT_MAX 
#define MIN INT_MIN
#define rep(i,a,b) for(ll i=a;i<=b;i++)
#define per(i,a,b) for(ll i=a;i>=b;i--)
#define con continue
ll a,b,c[1234567],d[1234567],c1[1234][1234],e[1234567],n,j,i,l,r,x,y,k,ans;
bool used[1234567];
ll T;
char ch[1234][1234];
ll vis[1234][1234];
vector<ll> v[1234567],v1;
string s1[1234567];

int main(){
    cin>>T;
    while(T--){
        scanf("%lld%lld",&x,&y);
        n = x * y;
        a=pow(n,1.0/3)+0.5;
        //cout<<a<<'\n';
        if(a*a*a!=n || x % a >0 || y % a >0){
            puts("NO");
        }    
        else puts("YES");
    }
}