#include<bits/stdc++.h>

#define ll long long
#define dl double

using namespace std;

int main()
{
    string s[110], t[110];
    int n;
    cin >> n;

    for(int i = 0; i < n; i++){
        cin >> s[i] >> t[i];
        if(t[i] == "rat") cout << s[i] << endl;
    }

    for(int i = 0; i < n; i++){
        if(t[i] == "woman" || t[i] == "child") cout << s[i] << endl;
    }

    for(int i = 0; i < n; i++){
        if(t[i] == "man") cout << s[i] << endl;
    }

    for(int i = 0; i < n; i++){
        if(t[i] == "captain") cout << s[i] << endl;
    }

    return 0;
}
