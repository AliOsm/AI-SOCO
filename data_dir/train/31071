#include <bits/stdc++.h>

using namespace std;

int main()
{
    ios_base :: sync_with_stdio(false) ;
    cin.tie(nullptr) ;

    int TC ;
    cin >> TC ;

    while(TC --)
    {
        long long a , b , n ;
        cin >> a >> b >> n ;
        long long  arr[3] {a , b , a ^ b};
        cout << arr[n % 3] << '\n' ;
    }

    return 0 ;
}