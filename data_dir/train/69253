#include <bits/stdc++.h>
using namespace std;
#define pf pop_front
#define pb push_back
#define ulli unsigned long long int
#define LL int
#define FOR(i,begin,end) for (LL i=begin;i<=end;i++)
#define rep(i,t) for (LL i=0;i<t;i++)
#define pii pair<int,int>
#define pi pair<pii,pii>
#define X first
#define Y second
#define double long double 
const LL MAX=2007,inf=1e9+1,N=2000;
ulli a,n,help,ans;
int main()
{
	cin>>n;
	ans=n;
	a+=n/2;
	a+=n/3;
	a+=n/5;
	a+=n/7;
	ans+=n/6;
	ans+=n/10;
	ans+=n/14;
	ans+=n/15;
	ans+=n/21;
	ans+=n/35;
	a+=n/105;
	a+=n/70;
	a+=n/30;
	a+=n/42;
	ans+=n/210;
	return cout<<ans-a<<"\n",0;
}
