#include <bits/stdc++.h>

using namespace std;

void solve() {
  int x, y;
  cin >> x >> y;
  if (x == 1) {
    cout << (y == 1 ? "YES" : "NO");
    return;
  }
  if (x == 2 or x == 3) {
    cout << (y <= 3 ? "YES" : "NO");
    return;
  }
  cout << "YES";
}

int main() {
  ios::sync_with_stdio(0);
  cin.tie(0), cout.tie(0);

  int t;
  cin >> t;
  while (t--) {
    solve();
    cout << '\n';
  }

  return 0;
}