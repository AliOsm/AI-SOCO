#include <bits/stdc++.h>

#define F first
#define S second
#define pb push_back
#define ll long long
using namespace std;

const ll inf=1e18;
const int mod=1e9+7;

int main(){
  ios_base::sync_with_stdio(0);
  ll n,m,s;
  cin>>n>>m>>s;
  cout<<((n + s - 1) / s*((n - 1) % s + 1))* ((m + s - 1) / s * ((m - 1) % s + 1))<<endl;
}