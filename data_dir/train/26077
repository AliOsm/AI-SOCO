const ll M   = 1e7+9 ;
const int si = 1e5+9 ;
int n,m;
char a[101][101];
int main()
{
    cin>>n>>m;
    lop(i,n)
    lop(j,m)
    {
        cin>>a[i][j];
        if (a[i][j]=='C'){
            cout<<"#Color\n";r0;
        }
        if (a[i][j]=='M'){
            cout<<"#Color\n";r0;
            
        }
        if (a[i][j]=='Y'){
            cout<<"#Color\n";r0;
            
        }
    }
    cout<<"#Black&White\n";

    r0;
}
