#define prarr(a,s,e) for(int i=s;i<=int(e);i++) printf("%d ",a[i]);
#define rng(i,a,b) for(int i=a;i>=int(b);i--)
#define rep(i,a,b) for(int i=a;i<=int(b);++i)
#define lop(i,n)   for(int i=0;i<int(n);i++)
