#include<bits/stdc++.h>
#define ll long long
#define ld long double
#define pb push_back
#define mp make_pair
#define fi first
#define se second
using namespace std;
ll a,b,c,ans,e,i,o,u;
string s;
int main()
{
    ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
    cin >>s;
    o=0;
    u=0;
    while (s.size()>0)
    {
        if (s.size()>=3){if (s[0]=='W'&&s[1]=='U'&&s[2]=='B'){s.erase(0,3);u=1;continue;}}
        if (u==1&&o==1){cout <<" ";}
        u=0;
        o=1;
        cout <<s[0];
        s.erase(0,1);
    }
}
