#include <bits/stdc++.h>
using namespace std;

int n, m;
char ic[502][502];
int r_1, c_1, r_2, c_2;
bool yes = false;

void solve(int i, int j) {
	ic[i][j] = 'X';
	if(i == r_2 && j == c_2) {
		if(ic[i - 1][j] == '.' || ic[i + 1][j] == '.' || ic[i][j - 1] == '.' || ic[i][j + 1] == '.') {
			yes = true;
		}
	} else {
		if(ic[i - 1][j] == '.') {
			solve(i - 1, j);
		} else if(i - 1 == r_2 && j == c_2) {
			yes = true;
		}
		if(ic[i + 1][j] == '.') {
			solve(i + 1, j);
		} else if(i + 1 == r_2 && j == c_2) {
			yes = true;
		}
		if(ic[i][j - 1] == '.') {
			solve(i, j - 1);
		} else if(i == r_2 && j - 1 == c_2) {
			yes = true;
		}
		if(ic[i][j + 1] == '.') {
			solve(i, j + 1);
		} else if(i == r_2 && j + 1 == c_2) {
			yes = true;
		}
	}
}

int main (int argc, char *argv[])
{
	memset(ic, 'X', sizeof(ic));
	scanf("%d%d", &n, &m);
	for(int i = 1; i <= n; ++i) {
		for(int j = 1; j <= m; ++j) {
			scanf(" %c", &ic[i][j]);
		}
	}
	scanf("%d%d%d%d", &r_1, &c_1, &r_2, &c_2);
	solve(r_1, c_1);
	(yes) ? printf("YES") : printf("NO");

	return 0;
}

