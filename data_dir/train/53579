#include "bits/stdc++.h"

using namespace std;
using LL = long long;

constexpr int N = 1e5 + 5;

int main() {
    ios::sync_with_stdio(false); cin.tie(nullptr);
    
    int n, k, p, s;
    cin >> k >> n >> s >> p;

    int papers = (n + s - 1) / s * k;
    cout << (papers + p - 1) / p << endl;
}