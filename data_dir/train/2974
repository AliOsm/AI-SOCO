# include <bits/stdc++.h>

using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--) {
        int A, B;
        cin >> A >> B;
        long long ans = 0;
        int x = 9;
        while (x <= B) {
            ans += A;
            x *= 10;
            x += 9;
        }
        cout << ans << endl;
    }
}
