#include<bits/stdc++.h>
#define ll long long
#define T ios_base::sync_with_stdio(0) ;
#define sz size()
#define r return
#define pb push_back
#define fr first
#define sc second
using namespace std;

const ll N = 1e6 + 1;

ll n , m , l , cnt , ans , k , x , y , mn  , mx ;
string s , t = "abcdefghijklmnopqrstuvwxyz";
vector < ll > v , d;
map < ll , ll > mp;
pair < ll , ll > p [ N ];
set < ll > st;
int main()
{
T
    cin >> s ;
    for ( ll i = 0 ; i < s.sz; i ++ ){
        if ( s [ i ] == s[ i + 1 ] ){
            for ( ll j = 0 ; j < t.sz; j ++ )
            {
                if ( t [ j ] != s [ i + 1 ] && s[ i + 2 ] != t [ j ] ){
                    s[ i + 1 ] = t[ j ];
                    break;
                }
            }
        }
    }
    cout << s ;



}











