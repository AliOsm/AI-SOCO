#include<bits/stdc++.h>
#define ff first
#define ss second
#define mp make_pair
#define pb push_back
#define ll long long
#define bs ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
#define sqr(a) ((a)*(a))
#define all(a) a.begin(),a.end()
#define endl '\n'
#define y1 y12312123
#define int ll

using namespace std;

const int N=2e5+3;
const int inf=1e9+3;
const int md=1e9+3;
const ll linf=1e18+3;
const double eps=0.000000000000001;

int n,m;
map<string,bool> used;
string s;

main()
{
    bs
    cin>>n;
    for(int i=1;i<=n;++i)
    {
        cin>>s;
        if(!used[s])
        {
            cout<<"NO";
            used[s]=true;
        }else cout<<"YES";
        cout<<endl;
    }
}

