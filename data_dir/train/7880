# include <bits/stdc++.h>
# define FILE
using namespace std;

int main(){

    # ifdef FILEs
        freopen( "input.txt", "r", stdin );
        freopen( "output.txt", "w", stdout );
    # endif
    ios_base::sync_with_stdio(false);

    int t;
    cin >> t;
    while( t -- ){
        int n;
        cin >> n;
        set < int > st;
        for( int i = 0; i < n; i ++ ){
            int a; cin >> a;
            st.insert( a );
        }
        cout << st.size() << endl;
    }
}