#include <bits/stdc++.h>
#define ll long long

using namespace std;

int main ()
{
    ios_base::sync_with_stdio(false);
    set<int> x;
    for(int i=1;i<=3001;i++) x.emplace(i);
    int n, a; cin >> n;
    for(int i=0;i<n;i++) cin >> a, x.erase(a);
    cout << *x.begin() << endl;
    return 0;
}
