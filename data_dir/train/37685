#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

int T, N, a[MAX];

int main()
{
    optimiza_io
    cin >> T;
    while( T -- ) {
        cin >> N;
        for( int i = 0; i < N; i ++ )
            cin >> a[i];
        sort( a , a + N );
        for( int i = N-1; i >= 0; i -- )
            cout << a[i] << " \n"[i == 0];
    }
    return 0;
}

// CHECAR LIMITES 