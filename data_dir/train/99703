#include <bits/stdc++.h>

using namespace std;

int main() {
    //freopen("battery.in", "r", stdin);
    //freopen("output.txt", "w", stdout);
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int t;
    cin>>t;
    while(t--){
        int n,k;
        cin>>n>>k;
        string alp="abcdefghijklmnopqrstuvwxyz";
        for(int i=0;i<n;i++){
            cout<<alp[i%k];
        }
        cout<<endl;
    }
    return 0;
}