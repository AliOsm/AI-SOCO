#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
#define what_is(x) cerr << #x << " is " << x << endl;
#define pii pair < int , int >
#define fi first
#define se second
using namespace std;
typedef long long ll;

int main()
{
    optimiza_io
    int T, a, b, ans;
    cin >> T;
    while(T --){
        cin >> a >> b;
        int ans = 1;
        while(1){
            if(2 * a <= ans && b <= ans)
                break;
            if(2 * b <= ans && a <= ans)
                break;
            ans++;
        }
        cout << ans * ans << "\n";
    }
    return 0;
}

