int main() {
	string s[4];
	for(int i =0; i < 4; i++) cin >> s[i];
	for(int i =0; i < 3; i++) for(int j =0; j < 3; j++) {
		int pb =0, pw =0;
		for(int k =0; k < 2; k++) for(int l =0; l < 2; l++)
			if(s[i+k][j+l] == '#') pb++;
			else pw++;
		if(pb <= 1 || pw <= 1) {cout << "YES\n"; return 0;}
		}
	cout << "NO\n";
	return 0;}

