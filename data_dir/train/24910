#include <iostream>
#include <iomanip>
#include <stdio.h>
#include <vector>
#include <algorithm>
#include <string>
#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdlib.h>
#include <string.h>
#include <time.h>
#include <bitset>
#include <deque>
#include <map>
#include <queue>
#include <set>
#include <sstream>
#include <numeric>
using namespace std;

typedef long long ll;

int main(int argc, char **argv) {
//	freopen("in.txt", "r", stdin);
	int n;
	scanf("%d", &n);
	string sol = "";
	for (int i = 0; i < n; ++i) {
		if (!sol.empty())
			sol += " ";
		if (i & 1) {
			if (i + 1 == n)
				sol += "I love it";
			else
				sol += "I love that";
		} else {
			if (i + 1 == n)
				sol += "I hate it";
			else
				sol += "I hate that";
		}
	}
	puts(sol.c_str());
	return 0;
}