//I_F_A
//ROYGBIV
#include "bits/stdc++.h"
using namespace std;

int main(){

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	long long n;
	cin >> n;

	n = n - 7;
	cout << "ROYGBIV";
	long long counter = 0;
	while(n--){


		if(counter == 0)
			cout << "G";
		else if(counter == 1)
			cout << "B";
		else if(counter == 2)
			cout << "I";
		else
			cout << "V";
		counter = (counter + 1)%4;
	}
}