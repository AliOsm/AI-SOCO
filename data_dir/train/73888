#include <bits/stdc++.h>
#define LL long long
#define VI vector<int>
#define pb push_back
#define mp make_pair
#define pii pair<int,int>
#define INF 0x3f3f3f3f
using namespace std;
char A[1010];
int n;
int main(){
	scanf("%d %s", &n, A);
	for(int i = 0; i < n - 1; i++){
		if(A[i] != A[i+1]){
			printf("YES\n%c%c\n", A[i], A[i+1]);
			return 0;
		}
	}
	printf("NO\n");
	return 0;
}