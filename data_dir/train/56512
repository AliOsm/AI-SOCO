#include<bits/stdc++.h>
#define f(p,t) max(p*3/10,p-p/250*t)
using namespace std;
main() {

    int a, b, c, d;
    scanf("%d%d%d%d", &a, &b, &c, &d);

    int x = f(a, c), y = f(b, d);

    if (x == y) {
        printf("Tie");
    } else {
        printf(x > y ? "Misha" : "Vasya");
    }

    return 0;
}
