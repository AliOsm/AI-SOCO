#include <bits/stdc++.h>

using namespace std;

char h[] = "hello";
int main(){
    string s;
    cin >> s;
    int r = 0;
    for(int i = 0; i < s.size(); i++){
        if(s[i] == h[r]){
            r++;
        }
    }
    if(r == 5){
        cout << "YES";
        return 0;
    }
    else {
        cout << "NO";
    }
}
