// KALAM
# include <bits/stdc++.h>

using namespace std;

const int N = 100000 + 77;
char Ca , Cb;
int main(){
   ios::sync_with_stdio(0);cin.tie(0);
   cin >> Ca >> Cb;
   for(int i = 1;i <= 5;++ i){
      char Ta , Tb;
      cin >> Ta >> Tb;
      if(Ca == Ta || Tb == Cb)
         return cout << "YES\n" , 0;
   }
   cout << "NO\n";
   return 0;
}
