#include<bits/stdc++.h>
using namespace std;

#define si(n) scanf("%d",&n)
#define ll long long
#define MAX 200005

int main()
{
    //freopen("input.txt","r",stdin);
    int i,j;
    ll n,s;
    cin>>n>>s;
    ll ans=(s/n);
    if(s%n)ans++;
    cout<<ans<<endl;

    return 0;
}
