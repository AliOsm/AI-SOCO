#pragma GCC optimize("Ofast")
#pragma GCC optimize("unroll-loops")
#pragma GCC target("sse,sse2,sse3,ssse3,sse4,popcnt,abm,mmx,avx,tune=native")
#include <bits/stdc++.h>
using namespace std;

int main()
{
	ios::sync_with_stdio(false);
	int t; cin >> t;
	for (int tt = 0; tt < t; tt++)
	{
		int x; cin >> x;
		int res = x / 3;
		if (x % 3 != 0)
			res++;
		cout << res << endl;
	}
}