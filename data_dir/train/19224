#include<bits/stdc++.h>
using namespace std;
int main()
{
	int x1,y1,x2,y2,x3,y3;
	cin>>x1>>y1>>x2>>y2>>x3>>y3;
	if (x1>=(x2+x3) && y1>=max(y2,y3)) cout<<"YES"; else
	if (x1>=(x2+y3) && y1>=max(y2,x3)) cout<<"YES"; else
	if (x1>=(y2+x3) && y1>=max(x2,y3)) cout<<"YES"; else
	if (x1>=(y2+y3) && y1>=max(x2,x3)) cout<<"YES"; else
	if (y1>=(y2+y3) && x1>=max(x2,x3)) cout<<"YES"; else
	if (y1>=(y2+x3) && x1>=max(x2,y3)) cout<<"YES"; else
	if (y1>=(x2+x3) && x1>=max(y2,y3)) cout<<"YES"; else
	if (y1>=(x2+y3) && x1>=max(y2,x3)) cout<<"YES"; else
	cout<<"NO";
 	return 0;
}