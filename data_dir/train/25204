#include <iostream>
#include <set>
using namespace std;

int main(void)
{
    int n;
    set<long double> v;
    cin >> n;
    for (int i = 0; i != n; ++i)
    {
        long double k, b;
        cin >> k >> b;
        if (k)
            v.insert((-1*b)/k);
    }
    cout << v.size();
    return 0;
}