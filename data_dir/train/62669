// In The Name Of God

#include<bits/stdc++.h>
#define IOS ios::sync_with_stdio(false),cin.tie(0)
#define rep(i,n) for(int i=0;i<(n);i++)
#define pb push_back
#define ll long long

using namespace std;
const int N = 1e5+22 ;

int n;

int main()
{IOS;
	int x,y,k,m,res=1000000;
	
	cin>>n>>m>>k , m-- ;
	rep(i,n)
	{
		cin>>x;
		if(!x || x>k) continue;
		res=min(res,10*(abs(m-i))) ;
	}
	cout<<res;

	
	return 0;
}
