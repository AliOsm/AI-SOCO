#define N 300009

ll gcd(ll a,ll b)
{
    if(a==0)
        return b;
    else
        return gcd(b%a,a);
}
ll power(ll a,ll b,ll m)
{
	ll ans=1;
	while(b)
	{
		if(b&1)
			ans=(ans*a)%m;
		b/=2;
		a=(a*a)%m;
	}
	return ans;
}
int main()
{	
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
        ll n,m;cin>>n>>m;
	/*if(n==m)
	cout<<0<<endl;
	else*/
	{
		if(m%n !=0)
		{
			cout<<-1<<endl;
		}
		else
		{
			ll x=m/n;
			ll ans=0;
			while(x%2==0)
			{
				ans++;
				x/=2;
			}
			while(x%3==0)
			{
				ans++;
				x/=3;
			}
			if(x==1)
			cout<<ans<<endl;
			else
			cout<<-1<<endl;
		}
	}
}
				