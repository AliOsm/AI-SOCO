#include <iostream>
#include <algorithm>
#include <vector>
#include <string.h>
#include <cmath>
#include <ctime>
#include <cstring>
#include <sstream>
#include <string.h>
#include <array>
#include <tuple>
#include <set>
#include <iterator>
#include <cstdio>
#include <string>
#include <ctime>
#include <valarray>
#include <utility>
#include <iomanip>
#include <list>
#include <regex>
#include <map>
using namespace std;

#define ll long long
#define PI 3.1415926535
#define pb push_back
#define ins insert
#define mp make_pair
#define ff first
#define ss second
#define MOD 1000000007

int main() 
{
	ios_base::sync_with_stdio(false);
    cin.tie(NULL),cout.tie(NULL);
    int n,m,maxx,mi;
    cin >> n >> m;
    if(n==0 && m>0)
    {
    	cout << "Impossible" << endl;
    	return 0;
    }
    maxx = n + max(0,m-1);
    mi = n + m - min(n,m);
    cout << mi << " " << maxx << endl; 
    return 0;
}