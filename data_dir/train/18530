#include <bits/stdc++.h>
#define ll long long
#define mp make_pair
#define PI 3.14159265

/*
*
* Zaiiiiiin
*
*/

using namespace std;

typedef pair<int,int> ii;

int a, b, x = 0, y = 0, z = 0;

int main(){
  scanf("%d%d", &a, &b);
  for(int i=1; i<=6; i++){
    if(abs(a - i) < abs(b - i)) x++;
    else if(abs(a - i) > abs(b - i)) z++;
    else y++;
  }
  printf("%d %d %d\n", x, y, z);
  return 0;
}
