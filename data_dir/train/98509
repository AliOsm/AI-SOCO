#pragma comment(linker, "/STACK:33554432")
#define _CRT_SECURE_NO_WARNINGS

#include <cstdio>
#include <iostream>
#include <vector>
#include <queue>
#include <map>
#include <string>
#include <complex>
#include <functional>
#include <cmath>

using namespace std;

typedef long long LL;


int main()
{
	int n;
	scanf("%d", &n);
	int a = 0, b = 0;
	for (int i = 0; i < n; ++i)
	{
		int x;
		scanf("%d", &x);
		a |= x;
	}
	for (int i = 0; i < n; ++i)
	{
		int x;
		scanf("%d", &x);
		b |= x;
	}
	printf("%d\n", a + b);

	return 0;
}