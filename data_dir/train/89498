#include<bits/stdc++.h>

using namespace std;

typedef long long ll;

const int N=1e3+5;

int main(){
    cin.tie(0);
    ios_base::sync_with_stdio(0);
    map<char,int> mp;
    int n;
    cin>>n;
    string s;
    cin>>s;
    for(auto i:s)
        mp[i]++;
    while(mp['n']--)
        cout<<1<<" ";
    while(mp['z']--)
        cout<<0<<" ";
    return 0;
}
