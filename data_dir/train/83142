#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
const int N = 2e5+50;
const int M = 1e6;
const ll oo = 1e18;
const int mod = 1e9+7;

int main(){
	#ifndef ONLINE_JUDGE
		freopen("input.txt","r",stdin);
	#endif	
	string s;
	cin>>s;
	for(int i=0;i<5;i++){
		string a;
		cin>>a;
		if(a[0] == s[0] || a[1] == s[1]){
			puts("YES");
			return 0;
		}
	}
	puts("NO");
}
