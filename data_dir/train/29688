ll dx[] = { 0, 0, 1, -1 };
ll dy[] = { 1, -1, 0, 0 };
int arr[3010];
map<int, int>mp;
int main()
{
	int i,n;
	cin >> n;
	for (i = 0; i < n; i++) { cin >> arr[i]; mp[arr[i]] = 1; }
	for (i = 1; i < 3010; i++) { if (mp[i] == 0) { cout << i << endl; return 0; } }
	return 0;
}

