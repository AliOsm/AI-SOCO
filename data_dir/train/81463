const int N = 3e5 + 5;

template<class T>struct ST{
    vector<vector<T>>mx, mn;

    template<class A>void modify(A& a, int l, int r){
        mx.resize(r+1);
        mn.resize(r+1);
        int len(log2(r-l+1)+2);
        for(int i=l;i<=r;i++)
            mx[i].resize(len), mn[i].resize(len),
            mx[i][0] = mn[i][0] = a[i];
        for(int i=1;l+(1<<i)-1<=r;i++)
            for(int j=l;j+(1<<i)-1<=r;j++)
                mx[j][i] = max(mx[j][i-1], mx[j+(1<<(i-1))][i-1]),
                mn[j][i] = min(mn[j][i-1], mn[j+(1<<(i-1))][i-1]);
    }
    T queryMax(int l, int r){
        int k(log2(r-l+1));
        return max(mx[l][k], mx[r-(1<<k)+1][k]);
    }
    T queryMin(int l, int r){
        int k(log2(r-l+1));
        return min(mn[l][k], mn[r-(1<<k)+1][k]);
    }
};

str s;
int r[N];
ST<int> table;

XOR{
    rd(s);
    int n = sz(s);
    clr(r,INF);
    for(int j=0;j<n;j++){
        for(int k=1;j+2*k<n;k++){
            if(s[j]==s[j+k]&&s[j]==s[j+k*2]){
                r[j] = j + k * 2;
                break;
            }
        }
    }
    for(int i=n-2;i>=0;i--) r[i] = min(r[i], r[i+1]);
    // table.modify(r,0,n-1);
    ll ans = 0;
    rep(i,0,n-1){
        // int mn = table.queryMin(i,n-1);
        int mn = r[i];
        if(mn!=INF) ans += n - mn;
    }
    wt(ans);
}