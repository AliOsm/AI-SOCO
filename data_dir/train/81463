            cout << fixed << setprecision(rnd) << x << endl;}
        template<class T>void wt(const vector<T>& v){for(T x:v) wt(x,' ');wt("");}
        template<class A,class B>void wt(const pair<A,B>& pr){
            cout << pr.fi << ' ' << pr.se << endl;}
    }using namespace _io;
