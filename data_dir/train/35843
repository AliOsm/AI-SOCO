#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    if (n<20)
    {
        if (n==0)
        {
            cout<<"zero";
            return 0;
        }
        if (n==1)
        {
            cout<<"one";
            return 0;
        }
            if (n==2)
        {
            cout<<"two";
            return 0;
        }
            if (n==3)
        {
            cout<<"three";
            return 0;
        }
            if (n==4)
        {
            cout<<"four";
            return 0;
        }
            if (n==5)
        {
            cout<<"five";
            return 0;
        }
            if (n==6)
        {
            cout<<"six";
            return 0;
        }
            if (n==7)
        {
            cout<<"seven";
            return 0;
        }
            if (n==8)
        {
            cout<<"eight";
            return 0;
        }
            if (n==9)
        {
            cout<<"nine";
            return 0;
        }
            if (n==10)
        {
            cout<<"ten";
            return 0;
        }
            if (n==11)
        {
            cout<<"eleven";
            return 0;
        }   if (n==12)
        {
            cout<<"twelve";
            return 0;
        }
            if (n==13)
        {
            cout<<"thirteen";
            return 0;
        }
            if (n==14)
        {
            cout<<"fourteen";
            return 0;
        }
                if (n==15)
        {
            cout<<"fifteen";
            return 0;
        }
                if (n==16)
        {
            cout<<"sixteen";
            return 0;
        }
                if (n==17)
        {
            cout<<"seventeen";
            return 0;
        }
                if (n==18)
        {
            cout<<"eighteen";
            return 0;
        }
                if (n==19)
        {
            cout<<"nineteen";
            return 0;
        }
    }
    int a, b;
    a=n/10; b=n%10;
    if (a==2)
    {
        cout<<"twenty";
    }
        if (a==3)
    {
        cout<<"thirty";
    }
        if (a==4)
    {
        cout<<"forty";
    }
        if (a==5)
    {
        cout<<"fifty";
    }
        if (a==6)
    {
        cout<<"sixty";
    }
        if (a==7)
    {
        cout<<"seventy";
    }
        if (a==8)
    {
        cout<<"eighty";
    }
        if (a==9)
    {
        cout<<"ninety";
    }
    if (b==1)
    {
        cout<<"-"<<"one";
    }
        if (b==2)
    {
        cout<<"-"<<"two";
    }
        if (b==3)
    {
        cout<<"-"<<"three";
    }
        if (b==4)
    {
        cout<<"-"<<"four";
    }
        if (b==5)
    {
        cout<<"-"<<"five";
    }
        if (b==6)
    {
        cout<<"-"<<"six";
    }
        if (b==7)
    {
        cout<<"-"<<"seven";
    }
        if (b==8)
    {
        cout<<"-"<<"eight";
    }
        if (b==9)
    {
        cout<<"-"<<"nine";
    }
}