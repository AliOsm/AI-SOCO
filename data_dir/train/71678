#include <bits/stdc++.h>

using namespace std;

int main()
{
    ios_base::sync_with_stdio();
    cin.tie(0) ;
    int n ;
    int x , y ;
    cin >> n ;
    int a = 0 , b= 0 ;
    while ( n-- )
    {
        cin >> x >>y ;
        if ( x > y ) a++ ;
        else if ( x < y ) b++ ;
    }
    if ( a > b )
        cout <<"Mishka";
    else
        if ( a < b )
            cout << "Chris";
    else
        cout << "Friendship is magic!^^";
    return 0;
}
