#include <bits/stdc++.h>

using namespace std;

#define int long long
#define double long double

main()
{
    ios_base::sync_with_stdio(0);
    int n, m;
    cin >> m >> n;
    if(m == 0 && n != 0) cout << "Impossible" << endl;
    else{
        cout << m + n - min(n, m) << ' ' << m + n - (n != 0) << endl;
    }
}
