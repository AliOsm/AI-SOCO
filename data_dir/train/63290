bool as(P a,P b)
{
    return a.x<b.x;
}


int main()
{
    scanf("%d %d %d %d %d %d",&a,&b,&c,&d,&n,&m);
    int x=0,y=0;
    x=max(a-b,b-a)*d;
    y=max(a-c,c-a)*n+m*3+max(b-a,a-b)*n;
    //printf("%d %d",x,y);
    if(x<y) puts("NO");
    else puts("YES");
}
