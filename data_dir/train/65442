#include "bits/stdc++.h"
using namespace std;
#ifdef ONLINE_JUDGE
#define trace(...)
#endif

using LL = long long;

int main() {
	ios_base::sync_with_stdio(false); cin.tie(NULL);
	string a; cin >> a;
	bool yes = false;
	for (int i = 0; i < 5; i++) {
		string b; cin >> b;
		if (b[0] == a[0] || b[1] == a[1]) yes = true;
	}
	cout << (yes ? "YES" : "NO") << endl;
	return 0;
}
