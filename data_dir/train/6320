int main()
{
	//freopen("Ganiyev.txt", "r", stdin");
	//freopen("Asadullo.txt", "w", stdout);
	//ios::sync_with_stdio(0);
	//cin.tie(0); cout.tie(0);
	int n, a, b, x = 0, y = 0;
	scanf("%d%d%d", &n, &a, &b);
	int t  = 0;
	if(n % a == 0)
	{
		x = n/a;
		t = 1;
	}
	if(!t && (n % b == 0))
	{
		t = 1;
		y = n/b;
	}
	if(!t)
	f0r(i, 0, n)
	{
		if(a * i > n)
			break;
		if((n - a * i) % b == 0)
		{
			x =i;
			y = (n - a * i) / b;
			t = 1;
			break;
		}
	}
	if(!t)
	{
		puts("-1");
		return 0;
	}
	t = 1;
	while(x --)
	{
		int k = a-1, q = t;
		while(k --)
		{
			t ++;
			printf("%d ", t);
		}
		printf("%d ", q);
		t ++;
	}
	while(y --)
	{
		int k = b-1, q = t;
		while(k --)
		{
			t ++;
			printf("%d ", t);
		}
		printf("%d ", q);
		t ++;
	}
	getchar();
	getchar();
	return 0;
}