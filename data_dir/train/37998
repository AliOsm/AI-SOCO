#include <bits/stdc++.h>

using namespace std;

int main() {
  ios::sync_with_stdio(0);
  cin.tie(0), cout.tie(0);

  int da, db;
  cin >> da >> db;
  if (da == 9 and db == 1) {
    cout << "9 10";
  } else if (da > db or da + 1 < db) {
    cout << -1;
  } else if (da + 1 == db) {
    cout << da << ' ' << db;
  } else {
    cout << da << 228 << ' ' << db << 229;
  }

  return 0;
}