#include <bits/stdc++.h>
using namespace std;

int main() {
  int n, k;
  cin >> n >> k;
  if (k != 1) { cout << "Yes" << '\n'; return 0; }
  vector<int> a(n);
  for (int i = 0; i < n; ++i) cin >> a[i];
  cin >> (*find(a.begin(), a.end(), 0));
  cout << (is_sorted(a.begin(), a.end()) ? "No" : "Yes") << '\n';
}