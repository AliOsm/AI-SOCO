 #include <bits/stdc++.h>

#define dbg(x) cerr<<#x": "<<x<<"\n"
#define dbg_p(x) cerr<<#x": "<<x.first<<","<<x.second<<"\n"
#define dbg_v(x, n) do{cerr<<#x"[]: ";for(int _=0;_<n;++_)cerr<<x[_]<<" ";cerr<<'\n';}while(0)
#define dbg_ok cerr<<"OK!\n"

#define st first
#define nd second

using namespace std;

int n, k, a, x;
map<int, int> m;

template<class T>
ostream& operator<<(ostream& out, vector<T> v) {
	out << v.size() << '\n';
	for(auto e : v) out << e << ' ';
	return out;
}

int main() {
	ios_base::sync_with_stdio(false);
	cin >> n;
	for(int i = 1; i <= n; i++) {
		cin >> a >> x;
		m[a] = max(m[a], x);
	}
	cin >> n;
	for(int i = 1; i <= n; i++) {
		cin >> a >> x;
		m[a] = max(m[a], x);
	}

	long long ans = 0;
	for(auto i : m) {
		ans += 1LL * i.second;
	}
	cout << ans << '\n';

}	