#include<bits/stdc++.h>
#define ll long long
#define pii pair < int, int >
#define pll pair < ll, ll >
#define pci pair < char, int >
#define pb push_back
#define maxn 200005
#define inf 1e9
#define pi acos(-1)
#define fast ios_base::sync_with_stdio(false)
#define fffc cin.tie(NULL)
using namespace std;

int main()
{
    fast;
    fffc;

    int n;

    cin >> n;

    int a[5];

    for(int i = 0; i < n; i++) cin >> a[i];

    sort(a, a + n);

    cout << a[n / 2] << endl;

    return 0;
}

