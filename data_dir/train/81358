#include <iostream>
#include <cmath>
#include <map>
#include <cstdio>
#include <string>
#include <cstring>
#include <algorithm>
#include <vector>
using namespace std;
long long i,k,j,l,s,t,a[223556],b[223456],c[212345],tt,n,m,d,e,f,x[12345],y[12345];
string p,r,q;

int main(){
    cin >>n>>p>>q;
    for (i = 0; i < n; i ++){
        t = abs(p[i] - q[i]);
        t = min (t,10-t);
        s += t;
    }
    cout << s;
}