#include<bits/stdc++.h>
using namespace std ;
#define ll long long int 
int main()
{
  long double A,B,C,D,p,q;
   cin>>A>>B>>C;
   D=B*B-4*A*C;
   p=(-B-sqrt(D))/(2*A);
   q=(-B+sqrt(D))/(2*A);

   cout<<fixed<<setprecision(15)<<max(p,q)<<endl;
   cout<<fixed<<setprecision(15)<<min(p,q)<<endl;
    return 0;
}