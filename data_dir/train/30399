#include<bits/stdc++.h>
#define all(s) s.begin(), s.end()
#define pb push_back
#define pf push_front
#define mp make_pair
#define ins insert
#define ll long long
#define ull unsigned long long
#define IOS ios_base::sync_with_stdio(false);cin.tie(NULL);
#define F first
#define S second 
#define sz size()
#define file(s) freopen(s".in","r",stdin); freopen(s".out","w",stdout);
using namespace std;
int main () {
	string s, s1, s2;
	int c = 0, d = 0, r = 0;
	getline(cin, s);
	getline(cin, s1);
	getline(cin, s2);
	for(int i = 0; i < s.sz; i++){
		if(s[i] == 'a' || s[i] == 'e' || s[i] == 'i' || s[i] == 'u' || s[i] == 'o')
		c++;
	}
	for(int i = 0; i < s1.sz; i++){
		if(s1[i] == 'a' || s1[i] == 'e' || s1[i] == 'i' || s1[i] == 'u' || s1[i] == 'o')
		d++;
	}
	for(int i = 0; i < s2.sz; i++){
		if(s2[i] == 'a' || s2[i] == 'e' || s2[i] == 'i' || s2[i] == 'u' || s2[i] == 'o')
		r++;
	}
	if(c == 5 && d == 7 && r == 5)
	cout << "YES";
	else
	cout << "NO";
    return 0;
}