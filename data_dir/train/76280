#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

#define pii pair<int,int>
#define F first
#define S second
#define mp make_pair

int main()
{
    int i , j , m1 , m2 , ans , n;
    scanf("%d",&n);

    m1 = m2 = 0;
    int k;

    for(i = 1; i <= n ; i++)
    {
        scanf("%d",&k);
        if(k == 0)m1 = i;
        else m2 = i;
    }
    ans = min(m1,m2);

    printf("%d\n",ans);


    return 0;
}

