#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int a,b,c,d,e,i,j,k,l,n,m,x,y,t,p;
    scanf("%lld",&n);
    x=0;
    for(i=0;i<n;i++)
    {
        cin>>a;
        if(a==1)
        {
            x++;
        }
    }
    if(x>0)
    {
        printf("HARD\n");
    }
    else
    {
        printf("EASY\n");
    }

}
