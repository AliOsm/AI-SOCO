#include<bits/stdc++.h>
using namespace std;


int main() {
    ios::sync_with_stdio(0);
    cin.tie(0); cout.tie(0);
    int a[] = {1, 6,28, 120,496, 2016, 8128, 32640,130816};
    int n;
    cin >> n;
    for(int i = 7; i >=0; i--) {
        if(n%a[i] == 0) {
            cout << a[i];
            break;
        }
    }
    return 0;
}
