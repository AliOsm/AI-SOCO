#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

int main() {
    ios_base::sync_with_stdio(false);

    int test;
    cin >> test;

    while(test--) {
        ll a, b, n;
        cin >> a >> b >> n;

        if(n == 0) cout << a << endl;
        else if(n == 1) cout << b << endl;
        else {
            if(n % 3 == 0) cout << a << endl;
            else if(n % 3 == 1) cout << b << endl;
            else cout << (a ^ b) << endl;
        }
    }

}

