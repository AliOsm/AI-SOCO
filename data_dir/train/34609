#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

int main()
{
	int n, q;
	string s;
	cin >> n >> q;
	cin >> s;
	for(int i = 0; i < q; i++) {
		int l, r, c1, c2;
		scanf("%d %d %c %c", &l, &r, &c1, &c2);
		for(int j = l-1; j <= r-1; j++)
			if (s[j] == c1)
				s[j] = c2;
	}
	cout << s << endl;
	return 0;
}
