#include <bits/stdc++.h>
using namespace std;

int main(){

//	freopen("readin.txt","r",stdin);

	char c;
	int d;
	cin >> c >> d;
	if((d == 8 || d == 1) && (c == 'a' || c == 'h')){
		puts("3");
	}
	else if(d == 8 || d == 1){
		puts("5");
	}
	else if(c == 'a' || c == 'h'){
		puts("5");
	}
	else
		puts("8");
	return 0;
}
