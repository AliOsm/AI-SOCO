#include <bits/stdc++.h>
using namespace std;

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    string str;
    cin>>str;

    bool flg=false;
    string a[5];
    for(int i=0;i<5;i++){
        cin>>a[i];
        if(a[i][0]==str[0] || a[i][1]==str[1])
            flg=true;
    }

    if(flg)
        cout<<"YES";
    else
        cout<<"NO";
}
