#include <bits/stdc++.h>
#define INT long long
#define oo 6e18
using namespace std;
INT n;
bool s;

int main()
{
    ios::sync_with_stdio(false);
    cin>>n;
    for(int i = 0, t ; i < n ; i++ )
    {
        cin>>t;
        s |= t;
    }
    if( s ) cout<<"HARD";
    else cout<<"EASY";
    return 0;
}
