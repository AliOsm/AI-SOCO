//I_F_A
#include "bits/stdc++.h"
using namespace std;

long long func(long long a,long long b){

	long long n = min(a,b);
	long long ans = 1;
	for(long long i=1;i<=n;i++){

		ans = ans * i;
	}
	return ans;
}

int main(){

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	long long a,b;
	cin >> a >> b;

	cout << func(a,b) << endl;
}
