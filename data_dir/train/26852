#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
    /*ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);*/
    ll a,b,c,d,e,i,j,k,l,n,m,x,y,t,p;
    cin>>t;
    for(p=1;p<=t;p++)
    {
        cin>>n;
        c=0;
        for(i=1;i<=6;i++)
        {
            if((n-i)>=14 && (n-i)%14==0)
            {
                c=1;
                break;
            }
        }
        if(c==1)
        {
            printf("YES\n");
        }
        else
        {
            printf("NO\n");
        }
    }


}
