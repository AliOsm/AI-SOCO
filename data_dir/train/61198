//.cpp
#include <bits/stdc++.h>

using namespace std;

const double eps = 1e-7;
const int N = 2e5+9;

int main()
{
    int n, k;
    cin>>n>>k;
    map<char, int> mp;
    char c;
    while(n--) {
        cin>>c;
        mp[c]++;
        if(mp[c]>k) {
            cout<<"No\n";
            return 0;
        }
    }
    cout<<"Yes\n";
    return 0;
}
