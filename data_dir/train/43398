#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define fr first
#define sc second
#define r_0 return 0
using namespace std;

const ll N = 1e5 + 1 ;

ll n , m , k , l , x , y  , z , cnt , ans , sum , mn = 1e18 , mx = -1e18 ;
ll a[N] ;
string s , t ;
vector < int > v ;
int main(){
    //789A
    ios_base::sync_with_stdio(0);

    cin >> n ;
    map < int , int > mp , mp1 ;
    for ( int i = 1; i <= n ; i++ )
        cin >> a[i] , mp [ a[i] ] ++ ;
    for ( int i = 1; i <= n ;i ++ ){
        mp1[ a[i] ] ++ ;
        if ( mp1[ a[i] ] == mp [ a[i] ] )
            v.pb ( a[i] );
    }
    cout << v.size()<< endl;
    for ( int i = 0 ; i < v.size(); i ++ ) {
        cout << v[i] << " ";
    }

















}












