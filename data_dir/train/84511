#include <bits/stdc++.h>
using namespace std;

bool check_rect(vector <int> &x, vector <int> &y) {
	for (int i = 0; i < 4; ++i) {
		if ((x[(i + 1) % 4] - x[i]) * (x[(i + 3) % 4] - x[i]) + (y[(i + 1) % 4] - y[i]) * (y[(i + 3) % 4] - y[i])) {
			return 0;
		}
	}
	return 1;
}

bool check_square(vector <int> &x, vector <int> &y) {
	int len = (x[1] - x[0]) * (x[1] - x[0]) + (y[1] - y[0]) * (y[1] - y[0]);
	for (int i = 0; i < 4; ++i) {
		if ((x[(i + 1) % 4] - x[i]) * (x[(i + 1) % 4] - x[i]) + (y[(i + 1) % 4] - y[i]) * (y[(i + 1) % 4] - y[i]) != len) {
			return 0;
		}
	}
	return check_rect(x, y);
}

int main() {
	vector <int> x(8), y(8), a(8);
	for (int i = 0; i < 8; ++i) {
		cin >> x[i] >> y[i];
		a[i] = i;
	}
	do {
		vector <int> x1, y1, x2, y2;
		for (int i = 0; i < 8; ++i) {
			if (i < 4) {
				x1.push_back(x[a[i]]);
				y1.push_back(y[a[i]]);
			} else {
				x2.push_back(x[a[i]]);
				y2.push_back(y[a[i]]);
			}
		}
		if (check_square(x1, y1) && check_rect(x2, y2)) {
			cout << "YES" << endl;
			for (int i = 0; i < 4; ++i) {
				cout << a[i] + 1 << ' ';
			}
			cout << endl;
			for (int i = 0; i < 4; ++i) {
				cout << a[i + 4] + 1 << ' ';
			}
			return 0;
		}
	} while (next_permutation(a.begin(), a.end()));
	cout << "NO";
	return 0;
}