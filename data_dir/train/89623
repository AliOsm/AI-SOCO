#include <iostream>

using namespace std;

int main()
{
    int n ;
    cin>>n ;
    string s ;
    cin>>s ;
    long long ans =0ll ;
    for(int i = 0 ; i < n ; ++i)
    {
        int x = s[i] - '0' ;
        if(x % 2 == 0)
            ans += (i+1ll * 1ll) * 1ll ;
    }
    return cout<<ans<<"\n" , 0 ;
}
