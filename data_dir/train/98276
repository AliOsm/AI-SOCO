#include<bits/stdc++.h>
using namespace std;
#define ll long long 
#define MOD 998244353 




int main()
{
    int n;
    cin>>n;
    string s;
    int b,a; int check=0;
    for(int i=0;i<n;i++)
    {
        cin>>s>>b>>a; if(b>=2400 && a-b>0) check=1;
    }
    
    if(check)
    cout<<"YES\n";
    else
    cout<<"NO\n";
    
    
    
}