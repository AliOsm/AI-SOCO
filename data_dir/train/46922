#include<bits/stdc++.h>

#define ll long long
#define dl double

using namespace std;

ll a[10];

int main()
{
    cin >> a[1] >> a[2] >> a[3] >> a[4] >> a[5] >> a[6];

    ll A = a[1] + a[2] + a[3];

    cout << A * A - a[1] * a[1] - a[3] * a[3] - a[5] * a[5] << '\n';
    return 0;
}
