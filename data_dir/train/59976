#include<iostream>
#include<math.h>
#include<algorithm>
#include<string>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.precision(20);
    long long int x=1,w,h;
    cin>>w>>h;
    for(int i=1;i<=w+h;i++)
    {
        x=(x*2)%998244353;
        
    }
    cout<<x;}