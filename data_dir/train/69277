// rc.cpp : Defines the entry point for the console application.
//

#include <math.h>

#include <set>
#include <list>
#include <cmath>
#include <ctime>
#include <map>
#include <deque>
#include <queue>
#include <stack>
#include <string>
#include <bitset>
#include <cstdio>
#include <limits>
#include <vector>
#include <climits>
#include <cstring>
#include <cstdlib>
#include <fstream>
#include <numeric>
#include <sstream>
#include <iostream>
#include <algorithm>
#include <unordered_map>
#include <functional>
#include <iomanip>
#include <iterator>
#include <regex>

using namespace std;

#define PI 3.14159265358979323846
#define EPS 1.E-8

#define FIN(a, n) { for (ul _ = 0; _ < n; ++_) cin >> (a)[_]; }
#define FOUT(a, n) { cout << a[0]; for (ul _ = 1; _ < n; ++_) cout << ' ' << a[_]; cout << '\n'; }

typedef unsigned char uc;
typedef unsigned int ui;
typedef unsigned long long ul;
typedef long long sl;

int a[50][50];
int n, m;

bool u[50][50];

bool check(int i, int j, int& x, int& y)
{
    int sr[] = {-1, -1, 0, 0},
        sc[] = {-1, 0, -1, 0},
        mx = 0;

    for (int k = 0; k < 4; ++k)
    {
        int str = i + sr[k],
            stc = j + sc[k];

        if (str >= 0 && stc >= 0 && str + 1 < n && stc + 1 < m)
        {
            bool ok = true;
            int c = 0;
            for (int ii = str; ok && ii < str + 2; ++ii)
            {
                for (int jj = stc; jj < stc + 2; ++jj)
                {
                    if (a[ii][jj] != 1)
                    {
                        ok = false;
                        break;
                    }

                    if (!u[ii][jj])
                        ++c;
                }
            }

            if (ok && c > mx)
            {
                mx = c;
                x = str;
                y = stc;
            }
        }
    }

    return mx > 0;
}

int main(int argc, char* argv[])
{
    ios_base::sync_with_stdio(false);

    cin >> n >> m;
    for (int i = 0; i < n; ++i)
        FIN(a[i], m);

    vector<int> f;
    bool ok = true;
    for (int i = 0; ok && i < n; ++i)
    {
        for (int j = 0; j < m; ++j)
        {
            if (a[i][j] == 1 && !u[i][j])
            {
                int str = 0, stc = 0;
                if (!check(i, j, str, stc))
                {
                    ok = false;
                    break;
                }

                for (int ii = str; ii < str + 2; ++ii)
                    for (int jj = stc; jj < stc + 2; ++jj)
                        u[ii][jj] = true;
                f.push_back(str + 1);
                f.push_back(stc + 1);
            }
        }
    }

    if (ok)
    {
        cout << f.size() / 2 << '\n';
        for (int i = 0; i < f.size(); i += 2)
            cout << f[i] << ' ' << f[i + 1] << '\n';
    }
    else
        cout << "-1\n";
    return 0;
}
