#include <bits/stdc++.h>

using namespace std;

int main()
{
    int T;  cin >> T;

    while(T--)
    {
        int n;  scanf("%d", &n);

        set <int> st;

        while(n--)
        {
            int x;
            scanf("%d", &x);
            st.insert(x);
        }

        printf("%d\n", st.size());
    }
}
