#include<bits/stdc++.h>
using namespace std;
#define Jewel ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
int n, i, h, pay;
int main()
{
    Jewel;
    cin>>n;
    for(i=0; i<n; i++)
    {
        cin>>h;
        pay = max(pay, h);
    }
    cout<<pay<<endl;
    return 0;
}
