#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef long double ld;
#define F first
#define S second
#define ii pair <int,int>
#define ever (;;)

ll a,b,ans;

int main()
{
    scanf("%lld%lld",&a,&b);

    for ever
    {
        if( !a || !b )
            break;

        if( a < b )
            swap(a,b);

        ans += a/b;

        a = a%b;
    }

    printf("%lld",ans);
}
