#include<bits/stdc++.h>
#define PB push_back
#define MP make_pair
typedef long long ll;
using namespace std;
int main()
{
	int t;
	scanf("%d",&t);
	while(t--)
	{
		double d;
		cin >> d;
		if(d*d-4*d < 0)
			puts("N");
		else
		{
			double a,b;
			a = (sqrt(d*d-4*d)+d)/2;
			b = (sqrt(d*d-4*d)+d)/2 - sqrt(d*d-4*d);
			printf("Y %.12f %.12f\n",a,b);
		}
	}
}
