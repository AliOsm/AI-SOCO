#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
typedef unsigned long long ull;
const int N = 2e5+50;
const int oo = 1e9;
const int mod = 1e9+7;

int main(){
	int n;
	cin>>n;
	for(int i=31;i>=0;i--){
		if((n>>i)&1){
			cout << i+1 << endl;
			return 0;
		}
	}
}