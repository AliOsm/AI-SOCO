#include <deque>
#include <cassert>
#include <iomanip>
#include <random>

using namespace std;



#define ll long long
#define ld long double
#define null NULL
#define prev prev228
#define index index228
#define left left228
#define right right228



int f[10000];


signed main() {
	int n;
	scanf("%d", &n);
	for (int i = 1; i <= n; ++i) {
		scanf("%d", &f[i]);
	}
	for (int i = 1; i <= n; ++i) {
		int fi = f[i];
		int fifi = f[fi];
		if (f[fifi] == i) {
			puts("YES");
			return 0;
		}
	}
	puts("NO");
	return 0;
}










