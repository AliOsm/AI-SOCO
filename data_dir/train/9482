#include<bits/stdc++.h>
#define F first
#define S second
using namespace std;
typedef double D;
typedef long long ll;
typedef pair<ll,ll> pll;
const ll inf=(1ll<<62);
const ll mod=1e9+7;
const int MX=3e5;
int n,x,a[MX];
ll sum;
int main(){
    cin>>n>>x;
    for(int i=0;i<n;i++){
        cin>>a[i];
        sum+=a[i];
    }
    sum+=n-1;
    if(sum==x){
        puts("YES");
    }
    else puts("NO");
}
