# include <stdio.h>
# include <math.h>
# include <string.h>

# define isDigit(x) ((x) >= 'A' && (x) <= 'Z')
# define isdigit(x) ((x) >= 'a' && (x) <= 'z')
# define convert(x) ( isdigit (x) ? (x)-'a'+'A' : (x)-'A'+'a' )

const int N = 1000;

int n, m, k, f, d;
int a [N], b, x, y;
double sum;
char s [N];

void Print ();

int main ()
{
# ifndef ONLINE_JUDGE
   freopen ("A.txt", "rt", stdin);
   freopen ("A.out", "wt", stdout);
# endif

   scanf ("%s", s);

   bool nado=isdigit (s [0]);

   k = 1;
   m = ( ! nado ? 1 : 0 );
   for ( int i=1; s [i]; i++, k++ )
      if ( isDigit (s [i]) ) m++;

   if ( m == k || m == k-1 && nado )
      for ( int i=0; s [i]; i++ )
         printf ("%c", convert (s [i]));
   else
      printf ("%s", s);

//printf ("\n%d %d %d", nado, m, k);

   return 0;
}

void Print ()
{
}
