#include <bits/stdc++.h>
#define fast ios::sync_with_stdio(0); cin.tie(0); cout.tie(0)
#define pb push_back
#define endl '\n'
#define MOD 1000000007

using namespace std;
typedef long long int ll;

int main(void)
{
	#ifndef ONLINE_JUDGE
	freopen("in.txt", "r", stdin);
	#endif
	fast;
	int x, y, z, t1, t2, t3; cin >> x >> y >> z >> t1 >> t2 >> t3;
	int a = abs(x-y)*t1;
	int b = abs(x-z)*t2 + t3 + t3 + abs(x-y)*t2 + t3;
	if (a < b) cout << "NO\n";
	else cout << "YES\n";
}