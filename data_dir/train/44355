#include <bits/stdc++.h>
using namespace std;
#define eb emplace_back
#define emp emplace
#define fi first
#define se second
#define INF 0x3f3f3f3f
typedef long long ll;
typedef pair<int, int> ii;

int n;

int main(void) {
	ios_base::sync_with_stdio(false);

	cin >> n;

	int a = 0, b = 0;
	for (int i = 0; i < n; i++) {
		int x; cin >> x;
		if (x < 0) b += -x;
		else a+= x;
	}

	cout << a+b << endl;

	return 0;
}

