#include<bits/stdc++.h>
 
using namespace std;
 
#define DIM 200009
#define INF 1e18+9ll
#define pairll pair<long long,long long>
#define fi first
#define se second
#define ld long double
#define fast ios_base::sync_with_stdio(false),cin.tie(0),cout.tie(0)
#define amen exit(0)
#define endl '\n'
 
long long i,j,k,l,n,m;

long long a[DIM];

int main(){

	fast;

	int nt;
	cin>>nt;

	while(nt--){
		l=0;
		cin>>n;

		for(i=1;i<=n;i++){
			cin>>k;
			l+=k;
		}

		cout<<(l+n-1)/n<<endl;
	}

	amen;
} 