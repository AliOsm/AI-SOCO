#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;
int main(){
    ll n;
    cin>>n;
     map<ll,ll>M;
    for(int i=0;i<n;i++){
        ll x;
        cin>>x;
        M[x]++;
    }
    for(int i=1;i<=n;i++){
        if(M[i]==0){
            cout<<i;
            return 0;
        }
    }
}