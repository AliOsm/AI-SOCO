#include <bits/stdc++.h>
typedef long long ll ;
using namespace std;

int main()
{
   int n ;
   scanf("%d",&n);

    int even = 0 , odd = 0 ;
    ll sum  = 0 ;
    for(int i = 1 ; i <= n ; i++){
         int x ;
         scanf("%d",&x);
         sum += x ;
         if(sum & 1) odd ++ ;
         else even ++ ;
    }
    if(!odd && even) puts( "Second" ) ;

    else puts ("First" ) ;





    return 0;
}
