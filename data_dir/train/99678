#include<bits/stdc++.h>
#define ll long long 
#define hell 1000000007
#define F first
#define re 15000000
#define S second
#define pb push_back
#define all(a)  (a).begin(),(a).end()
#define rep(i,a,b) for(ll int i = a;i<b;i++)
char a[2000][2000];
using namespace std;
ll gcd(ll a,ll b)
{
    if (a%b==0)
        return b;
    else
        return gcd(b,a%b);
}
void palindrome(string s)
{
    int i =0;
    int j =s.size()-1;
    while(i<j)
    {
        if(s[i]!=s[j])
            return ;
        i++;
        j--;
    }
    cout<<s;
    exit(0);
}
int sum (ll a)
{
    int sum =0;
    while(a>0)
    {
        sum = sum + (a%10);
        a=a/10;
    }
    return sum;
}
int count_digit(ll n)
{
   int  count =0;
    while(n>0)
    {
        n = n/10;
        count++;
    }
    return count;
}
int binarySearch(int x,int y ,int z ,vector<pair<int,int>> &v)
{
    int low = x;
    int high = y;
    int mid = (x+y)/2;
    while(low<=high)
    {
        if(v[mid].F==z)
            return mid;
        if(v[mid].F<z)
            return binarySearch(mid+1,high,z,v);
        if(v[mid].F>z)
            return binarySearch(low,mid-1,z,v);
    }
    return -1;
}
int check(vector<ll> &v , ll day,int m)
{
   ll sum = 0;
   ll d = 1;
   for(int i =0;i<v.size();i++)
   {
    if(sum+v[i] > day)
    {
        d++;
        sum = v[i];
    }
    else
        sum = sum + v[i];
   }
   if(d<=m)
    return 1;
else
    return 0;
}
int fac(int n)
{
    if(n==0)
        return 1;
    if(n==1)
        return 1;
    else

        return n*fac(n-1);
}
int main()
{   
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int n,m,x,k;
    cin>>n>>m>>k;
    int total = n+m+k;
    if(total&1)
        cout<<"Impossible";
    else
    {
        total = total /2;
        int ab = total-k;
        int bc = total-n;
        int ca = total-m;
        if(ab>=0 &&bc>=0&&ca>=0)
            cout<<ab<<" "<<bc<<" "<<ca;
        if(ab <0 || bc<0 || ca<0)
            cout<<"Impossible";
    }

    
}


