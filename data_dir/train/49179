#include<bits/stdc++.h>

using namespace std;

#define lli long long int

int main(){
	
	ios :: sync_with_stdio(false); cin.tie(nullptr); cout.tie(nullptr);
	
	int q;
	
	cin>>q;
	
	while(q--){
		
		int a,b,n;
		
		cin>>a>>b>>n;
		
		n %= 3;
		
		if(n == 0){
			
			cout<<a;
		}
		
		else if(n == 1){
			
			cout<<b;
		}
		
		else{
			
			cout<<(a^b);
		}
		
		cout<<endl;
	}
}
