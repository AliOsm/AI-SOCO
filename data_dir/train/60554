#include <bits/stdc++.h>
using namespace std;

/*

    3 2 1
    -2 0 2
    
    6   5  4 3 2 1 1
    -5 -3 -1 1 3 5 6
    
    1 2 3 4
    5 3 1 1
    
    -4 -1 2 3
    
*/

int a[105];

int main(){
    int t; cin >> t;
    while(t--){
        int n; cin >> n;
        for(int i = 0; i < n; i++)
            cin >> a[i];
        sort(a, a+n); reverse(a, a+n);
        for(int i = 0; i < n; i++)
            cout << a[i] << " ";
        cout << endl;
    }
    
	return 0;
}