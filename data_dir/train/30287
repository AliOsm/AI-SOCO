#include <bits/stdc++.h>

using namespace std ;
typedef unsigned long long ll ;


int main ()
{
    int n  , cnt = 1 ;

    cin>>n;

    int x ,y , lasty = -1 ;

    while ( n--)
    {



        cin>>x;

        y = x % 10 ;

        if (y == 0 ) x = 1 ;
        else x = 0 ;

        //cout<<x<<" "<<lasty<<endl;
        if (lasty == x ) cnt++;

        lasty = y ;


    }

    cout<<cnt<<endl;

    return 0 ;
}



