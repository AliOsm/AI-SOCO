#include <algorithm>
#include <iostream>
#include <cstring>
#include <cstdlib>
#include <cassert>
#include <cstdio>
#include <vector>
#include <cmath>
#include <ctime>
#include <set>
#include <map>


typedef long long ll;

using namespace std;
int a,b,c,d,x,y;
int main()
{

	cin>>a>>b>>c>>d;

	x=max(3*a/10,a-a/250*c);
	y=max(3*b/10,b-b/250*d);
	if(x==y)	
		cout<<"Tie";
	else
	if(x>y)
		cout<<"Misha";
	else
		cout<<"Vasya";
	
	
	return 0;
}
