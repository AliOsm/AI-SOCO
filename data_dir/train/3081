#pragma GCC optimize ("O3")
#include <bits/stdc++.h>
#define ll long long
#define ld long double
using namespace std;
int t, n, x;
set<int>s;
int main()
{
    ios::sync_with_stdio(0), cin.tie(0), cout.tie(0);
    cin >> t;
    while(t--){
        cin >> n;
        while(n--)
            cin >> x, s.insert(x);
        cout << s.size() << '\n';
        s.clear();
    }
    return 0;
}
