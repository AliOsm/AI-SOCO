#include <bits/stdc++.h>
using namespace std;

int main() {
    int le=0, ro=0;
    int n ;
    int a, b;

    cin >> n ;
    for(int i=0; i<n; i++) {
        cin >> a >> b ;
        if(a<0)
            le++;
        else
            ro++;
    }

    cout << (le<=1 || ro<=1? "Yes" : "No") << endl;
}
