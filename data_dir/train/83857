#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef long double ld;
typedef pair<int, int> pii;

#define F first
#define S second;
#define SZ(v) ((int)(v.size()))

const int MOD = 1e9 + 7;
const int N = 1e5 + 7;

int main()
{
	ios::sync_with_stdio(false);

	cout << "YES\n";
	int n;
	cin >> n;
	for (int i=0; i<n; i++)
	{
		int x, y, X, Y;
		cin >> x >> y >> X >> Y;
		cout << (x & 1) * 2 + (y & 1) + 1 << "\n";
	}
	
	return 0;
}
