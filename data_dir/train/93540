#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

string s;
char a, b, c;
int N;
int in[MAX][3];
int out[MAX][3];
char ans[MAX];
map < string , int > mp;

int main()
{
    optimiza_io
    cin >> s;
    N = s.size(); 
    a = b = c = 'a';
    for( int i = 0; i < N; i ++ )
    {
        in[i][0] = a;
        in[i][1] = b;
        in[i][2] = c;
        string s = "";
        s += a;
        s += b;
        s += c;
        mp[s] = i;
        
        c++;
        if( c > 'z' )
            c = 'a', b++;
        if( b > 'z' )
            b = 'a', a++;
    }
    
    for( int r = 0; r < 3; r ++ )
    {
        string q = "";
        for( int i = 0; i < N; i ++ )
            q += in[i][r];
        cout << "? " << q << "\n";
        cout.flush();
        string x;
        cin >> x;
        for( int i = 0; i < N; i ++ )
            out[i][r] = x[i];
    }
    cout << "! ";
    for( int i = 0; i < N; i ++ )
    {
        string tmp = "";
        tmp += out[i][0];
        tmp += out[i][1];
        tmp += out[i][2];
        ans[mp[tmp]] = s[i];
    }
    for( int i = 0; i < N; i ++ )
        cout << ans[i];
    
    cout << "\n";
    cout.flush();
    
    return 0;
}

