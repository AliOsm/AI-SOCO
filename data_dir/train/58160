#include<bits/stdc++.h>

using namespace std;

#define pb push_back
const int maxi = 1e6+10;
string s;
int n;
int a[maxi];
vector<int> v[maxi];
int cnt[maxi];
int main()
{
    cin>>n;

    for (int i=1;i<n;i++)
    {
        int x,y;
        scanf("%d%d",&x,&y);
        cnt[x]++;
        cnt[y]++;

    }

    for (int i=1;i<=n;i++)
        if (cnt[i]==2) return !printf("NO\n");
    printf("YES\n");
    return 0;
}
