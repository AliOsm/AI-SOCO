#include <bits/stdc++.h>

using namespace std;

#define MOD 1000000007
#define N 100005
#define ll long long 
#define ld long double
#define pb push_back
#define ff first
#define ss second

int main(){
	ios::sync_with_stdio(false); cin.tie(0);
	//freopen("input.txt", "r", stdin);
	int x, y, z, t1, t2, t3;
	cin>>x>>y>>z>>t1>>t2>>t3;
	int s = abs(x - y)*t1;
	int e = abs(z - x)*t2 + 3*t3 + abs(x - y)*t2;
	if(e <= s){
		cout<<"YES\n";
	}
	else{
		cout<<"NO\n";
	}
	return 0;
}