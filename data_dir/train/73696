#include <bits/stdc++.h>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */
int a[100000 + 2];
int main(int argc, char** argv) {
	int n, x;
	cin >> n;
	for(int i = 0; i < n; i++)
	{
		cin >> x;
		a[x]++;
	}
	for(int i = 100000 + 1; i >= 1; i--)
		if(a[i] % 2)
		{
			cout << "Conan";
			return 0;
		}
	
	cout << "Agasa";
	return 0;
}