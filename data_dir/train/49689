#include <bits/stdc++.h>
using namespace std;
typedef long long int ll;
typedef pair<int, int> pii;

int main() {
    int a, b, c, d;
    cin >> a >> b >> c >> d;
    ll ans = 0;
    while (a > 0 && c > 0 && d > 0) {
        a--;
        c--;
        d--;
        ans += 256;
    }
    while (a > 0 && b > 0) {
        a--;
        b--;
        ans += 32;
    }
    cout << ans;
}
