#include <bits/stdc++.h>
#define FOR(x,y) for (int x = 0 ; x < y ; ++ x)
#define FOR1(x,y,z) for (int x = y ; x < z ; ++ x)
#define FOR2(x,y,z) for (int x = y ; x >= z ; -- x)
#define pb(x) push_back(x)
#define clr(x) memset (x , 0 , sizeof(x))
using namespace std;

typedef long long ll;

const int MAX = 3 * 1e6;
int arr[MAX];
int n ;
int main (){

	cin >> n ;
	FOR(i , n) cin >> arr[i];
	sort (arr , arr + n);
	cout << arr[(n - 1) >> 1] << endl;

	return 0;
}
