#include<bits/stdc++.h>
#define ll long long
#define f first
#define s second
#define pb push_back
using namespace std;
ll n,m;
string s[200004],t[200005];
int main(){
    ios::sync_with_stdio(false);
    cin >> n >> m;
    for(int i=0; i<n; i++){
        cin >> s[i];
    }
    
    for(int i=0; i<m; i++){
        cin >> t[i];
    }
    int q;
    cin >> q;
    while(q--){
        ll x;
        cin >> x;
        cout << s[(x - 1) % n] << t[(x - 1) % m] << '\n';
    }
    
    
    
    return 0;
}
