#include<bits/stdc++.h>

using namespace std;

int main()
{
    string s;
    cin>>s;
    int counter=1;
    for(int i=0;i<s.size()-1;i++)
    {
        if(s[i]!=s[i+1])
            counter=0;
        counter++;
        if(counter==7)
        {
            cout<<"YES";
            return 0;
        }
    }
    cout<<"NO";

    return 0;
}
