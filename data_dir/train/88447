#include <iostream>
using namespace std;

int fact(int n)
{
    int result = 1;
    for(int i = 2; i <= n; i++)
        result *= i;
    return result;
}

int main()
{
    int a, b;
    cin >> a >> b;
    if (a > b)
        swap(a, b);
    cout << fact(a) << endl;
    return 0;
}
