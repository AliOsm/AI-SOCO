#include <bits/stdc++.h>
using namespace std;
#define pb  push_back
#define ll  long long
#define vi  vector<ll >
#define vvi vector<vi >
#define all(x) x.begin(), x.end()

int t, n;

int main() {
    ios_base::sync_with_stdio(false);
    cin >> t;
    while (t--) {
        cin >> n;
        if (n == 1)
            cout << "-1\n";
        else
            cout << "2" + string(n - 1, '3') + "\n";
    }

    return 0;
}