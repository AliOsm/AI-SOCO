//KingArthur problems are easy
#include<bits/stdc++.h>
using namespace std;
typedef long long int uli;
int main(){
   int n,v,o=0;
   scanf("%d",&n);
   for(int i=0;i<n;i++){
      scanf("%d",&v);
      if(v==1)o++;
   }
   if(n==1){
      if(o==1)puts("YES");
      else puts("NO");
   }
   else{
      if(o==n-1)puts("YES");
      else puts("NO");
   }
   return 0;
}
