#define ll             long long
#define vi             vector<int>
#define pb             push_back
#define mp             make_pair
#define pii            pair<int,int>
#define vit            vector<int> :: iterator
#define vrit           vector<int> :: reverse iterator

const int N = (int) 2e5 + 5;
const int INF = (int) 1e9 + 7;

int dx[8]= {1,0,-1,0,-1,-1,1,1};
int dy[8]= {0,1,0,-1,-1,1,-1,1};
int ini(){
    int x;
    scanf("%d",&x);
    return x;
}
long long inl(){
    long long x;
    scanf("%lld",&x);
    return x;
}

int a[100009];
int main()
{
    int n,i,j;
    n=ini();
    for(i=1;i<=n;i++){
       a[i]=ini();
    }
    sort(a+1,a+n+1);

    for(int i=2;i<n;i++){
        if(a[i+1]<a[i]+a[i-1]){
            cout<<"YES"<<endl;
            return 0;
        }
    }
    cout<<"NO"<<endl;
    return 0;
}

