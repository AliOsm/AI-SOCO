#include<bits/stdc++.h>
#include<vector>
using namespace std;

int main()
{
    long long int t, a, b, n, r;

    cin >> t;

    while(t--){
        cin >> a >> b >> n;

        if(n % 3 == 0){
            cout << a << endl;
        }
        else if(n % 3 == 1){
            cout << b << endl;
        }
        else{
            cout << (a ^ b) << endl;
        }
    }

    return 0;
}
