///--------------------**********----------------------------------

vector < ll > v, v1, v2, v3, v4 ;
vector < pair < ll, ll > > vec, vec1 ;
vector < int > adj[ 5 ] ;
map < ll, ll > Mp, Mp1, Mp2 ;
set < ll > st, st1,st2 ;
queue < ll > Q ;
stack < ll > Stk ;
multiset < ll > S ;

///---------------------**********--------------------------------

int n, q, x ;

void pre() {}
void Reset() {}

int Tree[ 1000005 ] ;

void update( int idx , int x )
{
    while( idx <= n )
    {
        Tree[ idx ] += x ;
        idx += ( idx & ( -idx ) ) ;
    }
}

int query( int idx )
{
    int sum = 0 ;
    while( idx > 0 )
    {
        sum += Tree[ idx ] ;
        idx -= ( idx & ( -idx ) ) ;
    }
    return sum ;
}

int Find( int x )
{
    int left = 1, right = n ;
    while( left < right )
    {
        int mid = ( left + right ) >> 1 ;
        if( query( mid ) < x ) left = mid + 1 ;
        else right = mid ;
    }
    return left ;
}

void Input()
{
    cin >> n >> q ;
    for( int i = 0 ; i < n ; i ++ )
    {
        cin >> x ;
        update( x, 1 ) ;
    }
}

void Calculation()
{
    while( q-- )
    {
        cin >> x ;
        if( x > 0 ) update( x, 1 ) ;
        else update( Find( -x ), -1 ) ;
    }

    for( int i = 1 ; i <= n ; i ++ )
    {
        if( Tree[ i ] > 0 )
        {
            cout << i << endl ;
            return ;
        }
    }

    cout << 0 << endl ;
}

void solve()
{
    Reset() ;
    Input() ;
    Calculation() ;
}

int main()
{
    FaRaBi ;
    int t ;
    //pre() ;
    //cin >> t ;
    t = 1 ;
    while( t-- )
    {
        solve() ;
    }

    return 0 ;
}
