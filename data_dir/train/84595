#include<bits/stdc++.h>
#define ll long long
#define f first
#define s second
#define mp make_pair
#define bp push_back
const int mx=1e5+10;
using namespace std;
main(){
ios::sync_with_stdio(0);
cin.tie(0);
cout.tie(0);
string a;
int n,q,l,r;
char x,y;
cin>>n>>q>>a;
int len=a.length();
for(int i=0;i<q;i++){
	cin>>l>>r>>x>>y;
	for(int i=l-1;i<r;i++){
		if(a[i]==x){
			a[i]=y;
		}
	}
}
cout<<a<<endl;
}
