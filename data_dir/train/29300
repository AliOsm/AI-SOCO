#include <bits/stdc++.h>
#define MAX (ll)(2e5 + 7)
#define INF (ll)(1e18)
#define MOD (ll)(998244353)

using namespace std;
using ll = long long;

int main () {
	int t;
	cin >> t;
	while (t--) {
		int n, a, b, c, d;
		cin >> n >> a >> b >> c >> d;
		int l = (a - b) * n, r = (a + b) * n, st = c - d, en = c + d;
		if (l > en || st > r) {
			cout << "No\n";
		}
		else {
			cout << "Yes\n";
		}
	}
}
