#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define pb push_back
#define mp make_pair
#define F first
#define S second
void go() {
	ll foo, n, m, i, x, mx1, mx2;
	cin >> foo >> n >> m;
	mx1 = mx2 = 0;
	for(i = 0; i < n; i++) {
		cin >> x;
		mx1 = max(mx1, x);
	}
	for(i = 0; i < m; i++) {
		cin >> x;
		mx2 = max(mx2, x);
	}
	if(mx1 > mx2) cout << "YES\n";
	else cout << "NO\n";
	return;
}
int main() {
	ll t;
	cin >> t;
	while(t--)
		go();
	return 0;
}
