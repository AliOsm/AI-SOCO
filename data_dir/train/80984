#include <bits/stdc++.h>
using namespace std;
long long  i, j, n, c[123456],a[1111234], b[1125678],d[123456], l,ll,r, s, t, k, m, xh, x, x1, y,z;
string p[34567],q;
map<long long,long long> mm;
int main() {
    cin >> n;
    while (n > 9){
        s += n%10;
        if (n%10 < 9)
            x = 1;
        if (x > 0)
            t ++;
        n/=10;
    }
    cout << s + t*9+n;
 
 
 
    return 0;
}