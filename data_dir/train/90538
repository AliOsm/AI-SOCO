#include<iostream>
#include<set>
#include<cmath>
using namespace std;
int main()
{
	string s;
	getline(cin,s);
	int c;
	for(int i=s.length()-1;i>=0;i--)
	{
		if((s[i]>='a' && s[i]<='z')||(s[i]>='A' && s[i]<='Z'))
		{
			if(s[i]=='a' || s[i]=='A' || s[i]=='e' || s[i]=='E' || s[i]=='i' || s[i]=='I'|| s[i]=='O'||s[i]=='o'||s[i]=='u'||s[i]=='U'||s[i]=='y'||s[i]=='Y')
		    c=1;
		    else
		    c=0;
		    break;
		}
		
	}
	if(c==1)
	cout<<"YES";
	else
	cout<<"NO";
	
}
