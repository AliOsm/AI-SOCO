#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
const ll prime = 1e9+7;

int main(){
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  
  int t, a, b;
  cin >> t;
  while(t--){
    cin >> a >> b;
    if(__gcd(a,b) == 1){cout << "Finite\n";}
    else{cout << "Infinite\n";}
  }
}