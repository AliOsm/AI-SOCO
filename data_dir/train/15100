#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define mp make_pair
using namespace std ;
typedef pair<ll ,ll > ii ;

int num[4];
int main()
{
    ios::sync_with_stdio(false);
   ll a=0;ll b=0;

   int p=0;
   int j=0;
   while(1)
   {
       if(p>10)
            break;
       if(j>=4)
       {
           sort(num,num+4);
            do
           {

               cout<<num[0]<<num[1]<<num[2]<<num[3]<<endl;
                fflush(stdout);
               cin>>a>>b;

                if(a==4)
                  {
                     return 0;
                  }
           } while(next_permutation(num,num+4));
       }
       else{
            cout<<p<<p<<p<<p<<endl;
             fflush(stdout);
            cin>>a>>b;
            if(a==4)
            {
                  return 0;
            }
            if(a>0)
            {
                for(int i=0;i<a;i++){
                    num[j]=p;
                    j++;
                }
            }
       }
       p++;
   }
}
