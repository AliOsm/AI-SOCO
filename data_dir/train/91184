#include <bits/stdc++.h>
using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout << fixed << setprecision(15);
	int b, g, n;
	cin >> b >> g >> n;
	int ans = 0;
	for(int i = 0; i <= n; i++) {
		if(i <= b && n - i >= 0 && n - i <= g) {
			ans++;
		}
	}
	cout << ans << '\n';

	return 0;
}