/*...Part - 05...*/

#define     debug(x)    cerr << #x << " = " << x << '\n' ;
#define     rep(i,b,e)  for(__typeof(e) i = (b) ; i != (e + 1) - 2 * ((b) > (e))  ; i += 1 - 2 * ((b) > (e)))
#define     Int         Int()
#define     Long        Long()
#define     all(x)      x.begin() , x.end()
#define     rall(x)     x.rbegin() , x.rend()
#define     sz(x)       (int)x.size()
#define     ff          first
#define     ss          second
#define     pb          push_back
#define     eb          emplace_back
#define     mem(a)      memset(a , 0 ,sizeof a)
#define     memn(a)     memset(a , -1 ,sizeof a)
#define     filein      freopen("input.txt","r",stdin)
#define     fileout     freopen("output.txt","w",stdout)

inline int add(int a, int b, int mod){a += b ;return a >= mod ? a - mod : a ;}
inline int sub(int a, int b, int mod){a -= b ;return a < 0 ? a + mod : a ;}
inline int mul(int a, int b, int mod){return (ll)a * b % mod ;}

/*...Part - 06...*/
/*...... ! Code start from here ! ......*/

int boy[2][N][N], girl[2][N][N], cost[N][N] ;

int main(){
    int test = 1 , tc = 0 ;
    while(test--){
        int n = Int, m = Int ;
        for(int i = 1 ; i <= n ; ++i){
            for(int j = 1 ; j <= m ; ++j)
                cost[i][j] = Int ;
        }
        for(int i = 1 ; i <= n ; ++i){
            for(int j = 1 ; j <= m ; ++j){
                boy[0][i][j] = max(boy[0][i - 1][j], boy[0][i][j - 1]) + cost[i][j] ;
            }
        }
        for(int i = n ; i >= 1 ; --i){
            for(int j = m ; j >= 1 ; --j){
               boy[1][i][j] = max(boy[1][i + 1][j], boy[1][i][j + 1]) + cost[i][j] ;
            }
        }
        for(int i = n ; i >= 1 ; --i){
            for(int j = 1 ; j <= m ; ++j){
                girl[0][i][j] = max(girl[0][i + 1][j], girl[0][i][j - 1]) + cost[i][j] ;
            }
        }
        for(int i = 1 ; i <= n ; ++i){
            for(int j = m ; j >= 1 ; --j){
                girl[1][i][j] = max(girl[1][i - 1][j], girl[1][i][j + 1]) + cost[i][j] ;
            }
        }
        int res = 0 ;
        for(int i = 2 ; i < n ; ++i){
            for(int j = 2 ; j < m ; ++j){
                int case1 = boy[0][i][j - 1] + boy[1][i][j + 1] + girl[0][i + 1][j] + girl[1][i - 1][j] ;
                int case2 = boy[0][i - 1][j] + boy[1][i + 1][j] + girl[0][i][j - 1] + girl[1][i][j + 1] ;
                res = max(res, max(case1, case2)) ;
            }
        }
        printf("%d\n",res);
    }
    return 0 ;
}

