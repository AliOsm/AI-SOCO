#define ll long long
#define ld long double
#define ss second
#define ff first
#define pb push_back
using namespace std;
ll n,m,x,y,z,w,t,r,d,a[300100],b[300100];
string s,f[300100];
int main(){
    //ios::sync_with_stdio(0), cin.tie(0), cout.tie(0);
    cin>>n>>x>>y;
    w=(abs(x-y)+min(x,y)-1);
    z=(abs(-x+y)+min(n-x,n-y));
    if(w<=z)return cout<<"White",0;
    else cout<<"Black";
	return 0;
}
