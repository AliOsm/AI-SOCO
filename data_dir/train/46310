#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define rep(i,a,b) for(ll i=a;i<b;i+=1)
#define all(c) (c).begin(),(c).end()
#define sz( s ) int( s.size() )
#define pb emplace_back
#define mp make_pair
#define hell 1000000007
#define F first
#define S second
ll t = 1 ;
void solve()
{
    ll x , y , z , t1 , t2 , t3 ;
    cin >> x >> y >> z >> t1 >> t2 >> t3 ;
    ll xx = abs( x - y ) * t1 ;
    ll yy = abs( z - x ) * t2 + t3 + t3 + t3 + abs( x - y ) * t2 ;
    if( xx < yy )
        cout << "NO" ;
    else
        cout << "YES" ;
}
signed main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0) ;
    cout.tie(0) ;
    //cin>>t;
    while(t--)
        solve() ;
    return 0 ;
}
