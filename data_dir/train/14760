#include <bits/stdc++.h>
#define F first
#define S second
using namespace std;
typedef long long ll;
ll n, x;
int main()
{
    ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    cin >> n;
    for(ll i = 1;i * i <= n;++i)
        if(n % i == 0)
            x = i;
    cout << x << ' ' << n / x;
    return 0;
}
