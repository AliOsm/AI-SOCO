#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define ld long double
#define ull unsigned long long
#define F first
#define S second
#define pb push_back
#define pi 3.14159265
#define endl '\n'

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    #ifdef LOCAL
    freopen("input.txt" , "r" , stdin);
    freopen("output.txt" , "w" , stdout);
    #else

    #endif
    double r , x , y , x1 , y1;
    cin>>r>>x>>y>>x1>>y1;
    double rast = (x1 - x) * (x1 - x) + (y1 - y) * (y1 - y);
    rast = sqrt(rast);
    cout<<ceil(rast / (2 * r));
    return 0;
}