using namespace std;



#define ll long long
#define ld long double
#define null NULL
#define prev prev228
#define index index228
#define left left228
#define right right228




inline bool check(int a, int b, int c) {
	return a + b > c && a + c > b && b + c > a;
}



signed main() {
	int n;
	cin >> n;
	int res = 0;
	for (int a = 1; a <= n; ++a) {
		for (int b = a; b <= n; ++b) {
			int c = a ^ b;
			if (check(a, b, c)) {
				if (c >= b&&c<=n) {
					++res;

				}
			}
		}
	}
	cout << res << "\n";
	return 0;
}






