#ifdef LOCAL_EXEC
#include "include.h"
#else
#include <bits/stdc++.h>
using namespace std;
#define trace(...)
#endif

using LL = long long;

int main() {
	ios_base::sync_with_stdio(false); cin.tie(NULL);

	int t; cin >> t;
	while (t--) {
		int a, b; cin >> a >> b;
		cout << (__gcd(a, b) == 1 ? "finite" : "infinite") << endl;
	}

	return 0;
}
