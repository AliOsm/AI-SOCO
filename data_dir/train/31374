#include <bits/stdc++.h>
#define fore(x,a,b) for(int x=a, qwerty=b; x<qwerty; x++)
#define ALL(a) (a).begin(),(a).end()
#define SZ(a) ((int)(a).size())
#define pb push_back
#define FIN ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0)
#define fst first
#define snd second
using namespace std;
typedef long long ll;

int a,b,c,n;

int main(){FIN;
    cin >> a >> b >> c >> n;
    int aub = a + b - c;
    if(n-aub>=1 && n-aub<=n && aub<=n && min(a,b)>=c)
        cout << n - aub << "\n";
    else cout << "-1\n"; 
}