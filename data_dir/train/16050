#include <iostream>
#include <string>
#include <cstdio>
using namespace std;
int type;
string dir;
int main(){
  freopen("input.txt","r",stdin);
  freopen("output.txt","w",stdout);
  cin>>dir>>type;
  if(dir=="back")type=3-type;
  cout<<((type==1)?"L\n":"R\n");
  //system("pause");
  return 0;
}
