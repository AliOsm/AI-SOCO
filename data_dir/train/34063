#include <bits/stdc++.h>
#define fi first
#define se second
#define mp make_pair
#define inf 1000000000
#define md 1000000007
#define pb push_back
#define li 100005
#define lo long long
using namespace std;
int x,y,z,a,b,c,A[8],cev;
int main(){
	scanf("%d %d %d %d %d %d",&x,&y,&z,&a,&b,&c);
	for(int i=1;i<=6;i++) scanf("%d",&A[i]);
	if(x<0) cev+=A[5];
	if(y<0) cev+=A[1];
	if(z<0) cev+=A[3];
	if(x>a) cev+=A[6];
	if(y>b) cev+=A[2];
	if(z>c) cev+=A[4];
	printf("%d\n",cev);
	return 0;
}




