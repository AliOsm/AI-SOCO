#include <stdio.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>
#include <string.h>
#include <iostream>
#include <algorithm>
#include <vector>
#include <deque>
#include <set>
#include <map>
#include <string>
using namespace std;

void solve() {
    long long l, r, d;
    cin >> l >> r >> d;
    
    if (d < l) {
        cout << d << "\n";
    } else {
        long long k = (r + d) / d;
        cout << k * d << "\n";
    }
}

int main(int argc, char * argv[]) {
    //freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout);

    long long n;
    cin >> n;
 
    for (int i = 0; i < n; i++)
        solve();
}