#include<bits/stdc++.h>
#define ff first
#define ss second
#define pb push_back
#define mp make_pair
#define ll long long
#define y1 asdf
#define all(a) a.begin(),a.end()
using namespace std;

const int N=1e5+1;

main ()
{
    int n;
    cin>>n;
    int k=0;
    for (int i=1;i<=n;++i){
        if ((n-i)%i==0)++k;
    }
    --k;
    cout<<k<<endl;
}
