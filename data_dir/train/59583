#include <bits/stdc++.h>

using namespace std;

#define MOD 1000000007
#define N 100005
#define ll long long 
#define ld long double
#define pb push_back
#define ff first
#define ss second

int main(){
	ios::sync_with_stdio(false); cin.tie(0); cout.tie(0);
	//freopen("input.txt", "r", stdin);
	//freopen("output.txt", "w", stdout);
	int t; cin>>t;
	ll l, r;
	while(t--){
		cin>>l>>r;
		cout<<l<<" "<<(2*l)<<"\n";
	}
	return 0;
}