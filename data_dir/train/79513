#include <bits/stdc++.h>

using namespace std;

int main()
{
    string s;
    cin >> s;
    int h = (s[0]-'0')*10 + s[1]-'0', m = (s[3]-'0')*10 +s[4]-'0', mh , mm;
   // cout << h << endl << m <<endl;
    mm = m*6;
    if(h>12)
        h-=12;
    double x = 30*double(h) + double(m)*0.5;
    while(x>=360)
        x-=360;
    cout <<  x<< " " << mm;


    return 0;
}
