#include <bits/stdc++.h>

using namespace std;
typedef long long ll;

int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    int n;
    cin >> n;
    int c[1001]={0};
    for(int i=0;i<n;i++){
        int x;
        cin >> x;
        c[x]++;
        if(c[x]>n/2+n%2){
            cout << "NO";
            return 0;
        }
    }
    cout << "YES";

}
