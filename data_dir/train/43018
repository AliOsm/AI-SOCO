#include<bits/stdc++.h>
using namespace std;

int main(){
    char c;
    int n,i,a=0,d=0;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        cin>>c;
        if(c=='D')d++;
        else a++;
    }
    if(a>d)printf("Anton\n");
    else if(a<d)printf("Danik\n");
    else printf("Friendship\n");
    return 0;
}
