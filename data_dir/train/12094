#include <bits/stdc++.h>
using namespace std;
#define ll long long int
#define ld long double
#define pb push_back
#define mp make_pair
#define ff first
#define ss second
int main()
{
	int n,i;
	cin>>n;
	string s;
	cin>>s;
	for(i=1;i<=n;i++)
	{
		if(n%i==0)
			reverse(s.begin(),s.begin()+i);
	}
	cout<<s<<endl;
	return 0;
}