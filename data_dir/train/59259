#include <bits/stdc++.h>
#define MOD 1000000007
#define Nmax 1000005
#define INF 2000000000

using namespace std;

int a[Nmax],n;

int main()
{
    int a,t;
    #ifndef ONLINE_JUDGE
        freopen ("date.in","r",stdin);
        freopen ("date.out","w",stdout);
    #endif
    cin.sync_with_stdio(0);
    cin>>t;
    while(t--)
    {
        cin>>a;
        if(360%(180-a)==0) cout<<"YES\n";
        else cout<<"NO\n";
    }
    return 0;
}
