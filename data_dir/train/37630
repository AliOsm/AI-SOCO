#include <bits/stdc++.h>

using namespace std;

void run_case() {
    int n , m;
    cin >> n >> m;

    int sum = 0;
    for (int i = 0 ;i < n ;i++) {
        int x;
        cin >> x;
        sum += x;
    }

    cout << min(sum , m) << '\n';
}
int main() {
    ios::sync_with_stdio(0), cin.tie(0), cout.tie(0);
#ifndef ONLINE_JUDGE
    freopen("input.in" , "r" , stdin);
#endif
    int t;
    cin >> t;
    while (t--)
        run_case();
}