#include <iomanip>

#define pb push_back
#define mp make_pair

using namespace std;

typedef long long ll;
typedef pair<int, int> pi;
typedef pair<long long, long long> pll;

const int MOD = 1e9 + 7;

map<int, int> cnt;

int main(){

	ios::sync_with_stdio(0);
	
	int n, k; cin >> n >> k;
	int ans = 0;
	
	for(int i = 0; i < n; i++){
		int h; cin >> h;
		
		if( h > k) ans += 2;
		else ans += 1;
	}
	cout << ans << endl;
	return 0;
}