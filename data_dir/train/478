#include <iostream>
#include <string>
#include <vector>
#include <cstdint>
#include <cmath>
#include <cstdlib>
using namespace std;

int main(void)
{
	int n;
	cin >> n;
	string str;
	cin >> str;
	int a = 0, b = 0;
	for (int i = 0; i < n; ++i)
	{
		if (str[i] == '0')
			++a;
		else
			++b;
	}
	int ans = abs(a - b);
	cout << ans << endl;
	return 0;
}