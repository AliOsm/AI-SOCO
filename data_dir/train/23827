#include <bits/stdc++.h>
#define fast ios::sync_with_stdio(0); cin.tie(0); cout.tie(0)
#define pb push_back
#define endl '\n'
#define MOD 1000000007

using namespace std;
typedef long long int ll;

int main(void)
{
	#ifndef ONLINE_JUDGE
	freopen("in.txt", "r", stdin);
	#endif
	fast;
	int n; cin >> n;
	set<string> s;
	for (int i = 0; i < n; ++i)
	{
		string t; cin >> t;
		if (s.find(t) == s.end()) s.insert(t), cout << "NO\n";
		else cout << "YES\n";
	}
}