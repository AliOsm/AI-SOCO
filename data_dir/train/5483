#include <bits/stdc++.h>
#define pb push_back
#define F first
#define S second
using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
typedef pair<ll,ll> pll;
const int MX=1e6;
const ll inf=(1ll<<60);
ll n,k;
int main(){
    cin>>n>>k;
    ll x=(n*(n-1))/2;
    if(k>=x){
        puts("no solution");
        return 0;
    }
    ll l=1e4;
    ll f=-1e9;
    for(int i=0;i<n;i++){
        cout<<i<<" "<<f<<endl;
        f+=l;
    }
}
