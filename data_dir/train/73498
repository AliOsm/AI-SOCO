#include <bits/stdc++.h>
#define INT long long int
#define MOD 998244353
#define oo 6*(1e18)
using namespace std;
INT n;
string s;
int main()
{
    ios::sync_with_stdio(false);
    cin>>n>>s;
    for(int i = 0 ; i < n-1 ; i++ )
        if( s[i] != s[i+1])
        {
            cout<<"YES\n"<<s[i]<<s[i+1];
            return 0;
        }
    cout<<"NO\n";
    return 0;
}
