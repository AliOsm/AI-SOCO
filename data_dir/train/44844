
#include <bits/stdc++.h>
using namespace std;
#define pb push_back
#define Size(x) ((int)(x).size())
typedef long long ll;
typedef long double ld;
typedef pair<int, int> pii;
const int INF = 1e9 + 10;
const int MOD = 1e9 + 7;

int main() {
	ios_base :: sync_with_stdio(false), cin.tie(0), cout.tie(0);
	int t;
	cin >> t;
	while (t--) {
		int n;
		cin >> n;
		if (n == 1) {
			cout << -1 << endl;
			continue;
		} else {
			for (int i = 0; i < n-1; i++) {
				if (i < 2)
					cout << 2;
				else{
					if (i%2)
						cout << 7;
					else
						cout << 2;
				}
			}
			cout << 7 << endl;
		}
	}
	return 0;
}
