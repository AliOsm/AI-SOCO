#include<bits/stdc++.h>
#define int long long
using namespace std;
main()
{
   int n;
   cin>>n;
   for(int i=1;i<=n;i++)
   {
       int x;
       cin>>x;
       int p=x/14;
       int tmp=x-(p*14);
       //cout<<tmp<<endl;
       if(tmp>=1 && tmp<=6 && x>=15)
       {
           cout<<"YES"<<endl;
       }
       else
       {
           cout<<"NO"<<endl;
       }
   }











    return 0;
}
