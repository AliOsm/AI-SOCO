using namespace std;

#define REP(i,n) for((i)=0;(i)<(int)(n);(i)++)
#define foreach(c,itr) for(__typeof((c).begin()) itr=(c).begin();itr!=(c).end();itr++)

int main(void){
    int N,i;
    
    cin >> N;
    
    if(N == 2){
        cout << -1 << endl;
        return 0;
    }
    
    cout << 6 << endl;
    cout << 10 << endl;
    cout << 15 << endl;
    
    for(i=1;i<=N-3;i++) cout << 30 * i << endl;
    
    return 0;
}
