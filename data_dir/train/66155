#include <bits/stdc++.h>

// Shabdan Batyrkulov

using namespace std;

#define MAX 1007
#define pb push_back
#define fr first
#define sc second
#define mk make_pair
#define ll long long
#define ld long double

const long long N = (int)1e5 + 7;
const int INF = (int)1e9 + 7;
const int mod = (int)1e9 + 7;

void solve () {
        ll a, b;
        cin >> a >> b;
        int ans = 0;
        while (a <= b) {
                a *= 3;
                b *= 2; ans++;
        }
        cout << ans;
}

int main() {
        int test = 1; // scanf ("%d", &test);
        while (test--) {
                solve();
        }
}
