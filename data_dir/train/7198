#include <bits/stdc++.h>

using namespace std;

int main ()
{
    ios_base::sync_with_stdio(false);
    int n, p=1; cin >> n;
    vector<int> a(n);
    for(int i=0;i<n;i++) cin >> a[i];
    sort(a.begin(),a.end());
    for(int i=0;i<n;i++) {
        if(a[i]>=p) p++;
    }
    cout << p-1 << endl;
    return 0;
}
