#include <bits/stdc++.h>

using namespace std;

#define DIM 200009
#define INF ((long long)1e10+9ll)
#define amen exit(0)
#define fast ios_base::sync_with_stdio(false),cin.tie(0),cout.tie(0)
#define pairll pair<long long,long long>
#define fi first
#define se second
#define ld long double

long long i,j,k,l,n,m,l1,l2,r1,r2;

long long a[DIM];

string s;

char t;

int main()
{
    fast;

    cin>>n;

    for(i=1;i<=n;i++){
        cin>>l1>>r1>>l2>>r2;
        if(l1==l2 && r1==r2)cout<<l1<<' '<<r1<<endl;
        else{
            if(l1!=l2)cout<<l1<<' '<<l2<<endl;
            else{
                if(r1!=r2)cout<<r1<<' '<<r2<<endl;
                else{
                    if(l1!=r2)cout<<l1<<' '<<r2<<endl;
                    else cout<<r1<<' '<<l2<<endl;
                }
            }
        }
    }

    amen;
}
