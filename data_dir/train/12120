#include<bits/stdc++.h>
using namespace std;
#define ma 100001
#define sz size()
#define pb push_back
#define ma 100001
#define ll long long int
vector<ll>v[ma];
ll mark1[ma],c[ma];
#define ma 1000000
int main()
{
 ll n,i,s,s1,max1,p;
 while(cin>>n)
 {
  ll a[n+5];
  s=0;
  s1=0;
  for(i=0;i<n;i++)
  {
   cin>>a[i];
   if(a[i]>0)s+=a[i];
   else s1+=a[i];
  }
  cout<<s-s1<<endl;
 }
 return 0;
}
