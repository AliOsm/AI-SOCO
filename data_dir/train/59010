#include <bits/stdc++.h>
using namespace std;
#define LL long long int
#define N 200005

int main() {
  ios::sync_with_stdio(false);
  cin.tie(NULL);cout.tie(NULL);
  string s, t[5];
  cin >> s;
  cin >> t[0] >> t[1] >> t[2] >> t[3] >> t[4];
  if (t[0][0] == s[0] || t[0][1] == s[1]) cout << "YES\n";
  else if (t[1][0] == s[0] || t[1][1] == s[1]) cout << "YES\n";
  else if (t[2][0] == s[0] || t[2][1] == s[1]) cout << "YES\n";
  else if (t[3][0] == s[0] || t[3][1] == s[1]) cout << "YES\n";
  else if (t[4][0] == s[0] || t[4][1] == s[1]) cout << "YES\n";
  else cout << "NO\n";
  return 0;
}
