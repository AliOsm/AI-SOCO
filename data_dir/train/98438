#include <string>
#include <vector>
#include <algorithm>
#include <iostream>
#include <string>
#include <vector>
#include <array>
#include <ctime>
#include <cmath>
#include <cassert>
using namespace std;

typedef long long ll;

int main()
{
	int n,m;
	scanf("%d%d",&n,&m);
	ll sum=0;
	for(int i=0;i<n;i++)
	{
		ll prv=sum;
		int a;
		scanf("%d",&a);
		sum+=a;
		printf("%lld ", sum/m-prv/m);
	}
	puts("");
	return 0;
}
