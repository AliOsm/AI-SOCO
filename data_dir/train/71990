#include <stdio.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>
#include <string.h>
#include <iostream>
#include <algorithm>
#include <vector>
#include <queue>
#include <set>
#include <map>
#include <string>
using namespace std;
 
 
 
int main() {
	int n;
	scanf("%d", &n);
 
	int a[1010];
	for (int i = 0; i < n; i++)
		scanf("%d", &a[i]);
 
	sort(a, a + n);
	printf("%d", a[(n - 1) / 2]);	
}