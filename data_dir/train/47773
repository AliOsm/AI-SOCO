#include <bits/stdc++.h>
#define ll long long int
#define pb push_back
#define mp make_pair
#define mod 1000000007
#define pi 3.141592653589793
using namespace std;



int main() {
//    cout << setprecision(30);
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);


    int a, b, c, d;
    cin >> a >> b >> c >> d;

    if (a != d) {
        cout << 0;
        return 0;
    }


    if (c) {
        cout << (a && d ? 1 : 0);
    }

    else {
        cout << 1;
    }

}
