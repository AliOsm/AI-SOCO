#include<bits/stdc++.h>
using namespace std;
#define fi first
#define se second
#define LL long long 
#define pb push_back
#define ls rt<<1, l, m
#define rs rt<<1|1, m+1, r
#define mem(a, b) memset(a, b, sizeof(a))

const int N = 555;
char s[N];
int main() {
	int n;
	scanf("%d", &n);
	scanf("%s", s);
	if(n==1&&s[0]=='0')puts("0");
	else {
		int cnt = 0;
		for(int i = 0; i < n; i++) if(s[i] == '0') cnt++;
		putchar('1');
		for(int i = 0; i < cnt ; i++) putchar('0');
		puts(""); 
	}
	return 0;
}