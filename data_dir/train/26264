#define pb push_back
#define eb emplace_back
#define X first
#define Y second

// main: main.cpp
//     g++ -Wall -Wshadow -pthread -Wno-sign-compare -Wno-unused-result -DEgQrauETGh -O3 -std=c++14 -o main main.cpp
// ulimit -s unlimited
//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// clang-format on

CI N = 1 << 19, K = 30, T = N * K;
VINT bitr(int a) {
  VINT s(K, 0);
  REP(i, K) s[i] = bool(a & (1 << i));
  reverse(ALL(s));
  return s;
}
int n, nx[T][2], c[T], sz;
int A[N], P[N];
int go(int v, const VINT &t, int rs, int p) {
  if (v == -1 || c[v] == 0) return -1;
  --c[v];
  if (p == K) return rs;
  REP(i, 2) {
    int e = t[p] ^ i;
    int z = go(nx[v][e], t, rs + e * (1 << (K - p - 1)), p + 1);
    if (z!=-1) return z;
  }
  throw;
}
int fnd(int a) {
  VINT t = bitr(a);
  return go(0, t, 0, 0);
}
void run() {
  SVAL(nx, -1);
  sz = 1;
  scanf("%d", &n);
  REP(i, n) scanf("%d", &A[i]);
  REP(i, n) scanf("%d", &P[i]);
  REP(i, n) {
    VINT t = bitr(P[i]);
    int v = 0;
    ++c[v];
    REP(j, K) {
      int e = t[j];
      if (nx[v][e] == -1) {
        nx[v][e] = sz;
        ++sz;
      }
      v = nx[v][e];
      ++c[v];
    }
  }
  REP(i, n) printf("%d%c", A[i] ^ fnd(A[i]), "\n "[i + 1 < n]);
}
