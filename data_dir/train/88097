#define pb push_back
#define se second
#define fr first
using namespace std;
const ll N=1e4+7;
ll n,m,cnt,ans,a,x;
string s;
vector<ll>arr;
pair<ll,ll>p[N];
int main() {
    ll arr[4];
for(  int i=0;  i< 4;i++){
    cin >> arr[i];
}
sort(arr,arr+4);
              if( arr[0]+arr[3]==arr[2]+arr[1] ){
        cout<<"YES";
        return 0;
       }
       if( arr[0]+arr[1]+arr[2]==arr[3] ){
        cout<<"YES";
       return 0;
       }
cout<<"NO";
}
 