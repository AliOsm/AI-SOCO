#define re return(0);
#define ce continue;
typedef long long ll;
typedef long double ld;
#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
#include <string>
#include <cstring>
#include <set>
#include <iomanip>
#include <fstream>
using namespace std;
double const pi=3.1415926536;
int main()
{
   int q;
   cin>>q;
   for(int k=0;k<q;k++)
   {
   	ll n,m;
   	cin>>n>>m;
   	n/=m;
   	int d[10];
   	for(int i=0;i<10;i++)
   	d[i]=((i+1)*m)%10;
   	ll sum=0;
   	for(int i=0;i<n%10;i++)
   	sum+=d[i];
   	ll s=0;
   	for(int i=0;i<10;i++)
   	s+=d[i];
   	cout<<sum+n/10*s<<endl;
   }
   
}


