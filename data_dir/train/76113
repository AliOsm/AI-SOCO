#include<bits/stdc++.h>
#include<unordered_map>
#define pb push_back
#define F first
#define S second
#define MEM(dp,i) memset(dp,i,sizeof(dp))
#define R return
using namespace std;
typedef long long ll;
typedef double D;
typedef pair<int,int> pii;
typedef pair<ll,ll> pll;
const int MX=100;
const ll INF=(1ll<<60);
const int inf=(1<<30);
const ll mod=1e9+7;
int n,k,a[MX];
int main(){
    scanf("%d%d",&n,&k);
    for(int i=0;i<n;i++)scanf("%d",&a[i]);
    sort(a,a+n);
    if(k>n){
        puts("-1");
        return 0;
    }
    cout<<a[n-k]<<" "<<a[n-k]<<endl;
}
