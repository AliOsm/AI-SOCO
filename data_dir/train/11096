#include <iostream>
#include <vector>
#include <string>
#include <algorithm>

using namespace std;

int n, k;

int main() {
    cin >> n >> k;

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (i != j)
                cout << 0 << " ";
            else
                cout << k << " ";
        }
        cout << endl;
    }
}
