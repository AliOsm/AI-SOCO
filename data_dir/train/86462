#include<bits/stdc++.h>
using namespace std;

#define si(n) scanf("%d",&n)
#define ll long long
#define MAX 200005

int main()
{
    //freopen("input.txt","r",stdin);
    int i,j;
    ll x,y,z,t1,t2,t3;
    cin>>x>>y>>z>>t1>>t2>>t3;

    ll a1=abs(x-y)*t1;
    ll a2=abs(x-z)*t2 + 3*t3 + abs(x-y)*t2;
   // cout<<a1<<' '<<a2<<endl;
    if(a2<=a1)cout<<"YES"<<endl;
    else cout<<"NO"<<endl;

    return 0;
}
