#define re return(0);
typedef long long ll;
typedef long double ld;
#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
#include <string>
#include <cstring>
#include <set>
#include <iomanip>
#include <fstream>
using namespace std;
double const pi=3.1415926536;
int main()
{
	int n;
	cin>>n;
	int a[n],b[n];
	for(int i=0;i<n;i++)
	cin>>a[i]>>b[i];
	int s=0;
	for(int i=0;i<n;i++)
	{
		if(a[i]==b[i])
		
		{
		s++;
		}
	}
	if(s==n)
	cout<<"Poor Alex";
	else
	cout<<"Happy Alex";
	
	
	
}

