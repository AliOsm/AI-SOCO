#include <bits/stdc++.h>

using namespace std;

inline void prep()
{
    cin.tie(0);
    cin.sync_with_stdio(0);
};

#define X first
#define Y second
#define ll long long
#define inf (0x7fffffffffffffff)
#define all(x) x.begin(), x.end()
#define debug(x) cerr << #x << " = " << (x) << " ";
#define ZXC cerr << clock() * 1.0 / CLOCKS_PER_SEC << endl;

const int mod = (int)1e9 + 7;
const int MX = (int)1e4 + 20;

int main()
{
    prep();

    int n;
    cin >> n;

    string s;
    cin >> s;

    for(int i = 1; i <= n; ++i){
        if(n % i) continue;
        
        string x = s.substr(0, i);
        reverse(all(x));
        s = x + s.substr(i);
    }

    cout << s << endl;

    // ZXC
    return 0;
}