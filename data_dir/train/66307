#include <iostream>
#include <cstdio>
#include <algorithm>
#include <vector>
#include <list>
#include <map>
#include <set>
#include <queue>
#include <cmath>
#define rank krevedka
#define left trulala
#define rigth trulalo
#define ran rank
using namespace std;

int main()
{
    int n;
    cin >>n;
    int res = 0;
    for(int i=0;i<n;i++)
    {
        int x,y;
        cin >>x>>y;
        if(x>y) res++;
        if(x<y) res--;
    }
    if(res>0)cout << "Mishka";
    if(res<0)cout << "Chris";
    if(res==0) cout <<"Friendship is magic!^^";
    return 0;
}

