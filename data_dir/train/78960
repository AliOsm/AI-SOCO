#include <bits/stdc++.h>
using namespace std;
#define F first
#define S second
#define pb push_back
#define ll long long
#define pi pair<int,int>
#define all(X) X.begin(),X.end()
#define go ios::sync_with_stdio(0); cin.tie(0); cout.tie(0)
int n;
vector <int> ans;
int main()
{
    go;
    cin>>n;
    for(int i=2;i<=n;i+=2)
        ans.pb(i);
    for(int i=1;i<=n;i+=2)
        ans.pb(i);
    for(int i=2;i<=n;i+=2)
        ans.pb(i);
    cout<<ans.size()<<endl;
    for(int x : ans)
        cout<<x<<" ";
    return 0;
}
