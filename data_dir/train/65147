#include<bits/stdc++.h>
#define ll long long
using namespace std;
int main()
{
    ll a, b, c, d;
    cin >> a >> b >> c >> d;
    ll x = max(3*a/10, a- (a/250 * c));
    ll y = max(3*b/10, b- (b/250 * d));
    if (x == y)
        cout << "Tie";
    if (x > y)
        cout << "Misha";
    if (y > x)
        cout << "Vasya";

}
