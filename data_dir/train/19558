#include <ctime>
#include <fstream>
#include <cstdio>
typedef long long ll;
//typedef double d;
typedef unsigned long long ull;
using namespace std;
int main()
{
    std::cin.tie(0);
    std::ios::sync_with_stdio(false);
    //freopen("read.txt", "r", stdin);
    //freopen("out.txt", "w", stdout);
    ll a,b,c;
    cin >> a>>b>>c;
    for(int i =0;i<=10000;i++)
    {
        for(int h=0;h<=10000;h++)
        if(a*i+b*h==c)
        {
            cout << "Yes" << endl;
            return 0;
        }
    }
    cout << "No"<< endl;
    return 0;
}
