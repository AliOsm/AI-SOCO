#include<bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);std::cin.tie(0);std::cout.tie(0);
#define int long long int 
using namespace std;
int32_t main()
{
    string s;
    cin>>s;
    int flag=0;
    for(int i=0;i<s.size();i++)
    {
        if((s[i]=='H')||(s[i]=='Q')||(s[i]=='9'))
        {
            flag=1;
        }
        
    }
    if(flag==1)
    {
        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"NO"<<endl;
    }
    
    return 0;
}