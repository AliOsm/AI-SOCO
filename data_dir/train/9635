#include <bits/stdc++.h>

using namespace std;

int32_t main()
{
    ios_base::sync_with_stdio(false);
    int n;
    string s;
    cin >> n >> s;
    for(int i = 0; i < n - 1; i++){
        if(s[i] != s[i+1]){
            cout << "YES\n";
            cout << s[i] << s[i+1];
            return 0;
        }
    }
    cout << "NO";
    return 0;
}
