#include <bits/stdc++.h>
using namespace std ;

int n , x, y , sx , sy ;
int main()
{
    cin >> n ;

    while(n--)
    {
        cin >> x >> y ;

        if( x > y ) sx++ ;
        else if( x < y ) sy++ ;
    }

    if(sx > sy ) cout << "Mishka" ;
        else if( sx < sy ) cout << "Chris" ;
        else cout << "Friendship is magic!^^" ;









return 0 ;
}
