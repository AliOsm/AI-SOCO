#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(nullptr);
	cout.tie(nullptr);
  int n;
  cin >> n;
  string s;
  cin >> s;
  ll ret = 0;
  for(int i = 0; i < n; ++i) {
    int j = s[i] - '0';
    if (j % 2 == 0) {
      ret += i + 1;
    }
  }
  cout << ret << endl;
	return 0;
}
