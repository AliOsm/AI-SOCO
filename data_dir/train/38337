using namespace std;

typedef long long Int;
#define FOR(i,a,b) for(int i=(a); i<=(b);++i)
#define mp make_pair
#define pb push_back
#define sz(s) (int)((s).size())
const int inf = 1000000000;
const int MOD = 1000000007;


int main() {
	//freopen("input.txt","r",stdin);freopen("output.txt","w",stdout);
	int x1,x2,y1,y2;
	cin>>x1>>y1>>x2>>y2;
	string one="Polycarp";
	string two="Vasiliy";
	
	if(x2>=x1 && y2>=y1) {
		cout<<one<<endl;
		return 0;
	}

	if(x1+y1 > max(x2,y2) ) {
		cout<<two<<endl;
		return 0;
	}

	cout<<one<<endl;

}