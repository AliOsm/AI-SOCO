#include<bits/stdc++.h>
#define ll long long
#define fi first
#define se second
#define mp make_pair
#define pb push_back
#define pi pair<int,int>
#define pl pair<ll,ll>
using namespace std;
ll x,y,z;
int main()
{
    cin >>x>>y>>z;
    cout <<min(abs(y-x)+abs(x-z),min(abs(x-y)+abs(z-y),abs(z-x)+abs(z-y)));
}
