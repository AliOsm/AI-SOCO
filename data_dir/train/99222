#include <iostream>
#include <string>
#include <cmath>
#include <algorithm>
#include <vector>
#include <set>
#include <map>
#include <sstream>
#include <cstdio>
#include <cstdlib>
#include <cstring>
#include <iomanip>
using namespace std;
int arr[101];
int main()
{
    int n, x, y, xx, yy;
    cin >> n >> xx;
    for (int i = 0; i < xx; i++)
        cin >> x,arr[x]++;
    cin >> yy;
    for (int i = 0; i < yy; i++)
        cin >> y, arr[y]++;
    for (int i = 1; i <= n; i++)
        if (arr[i] == 0)
            return cout << "Oh, my keyboard!", 0;
    cout << "I become the guy.";
}