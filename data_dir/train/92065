#include<bits/stdc++.h>
using namespace std;




int main(){
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    int n, k;
    cin >> n >> k;
    if(n == k){
        return cout << "-1", 0;
    }
    cout << n - k << " ";
    for(int i = 2; i <= n - k; i++){
        cout << i - 1 << " ";
    }
    for(int i = n - k + 1; i <= n; i++){
        cout << i << " ";
    }
}
