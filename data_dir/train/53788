#include<bits/stdc++.h>
int ara[500];
using namespace std;
int main()
{

    string s;
    getline(cin,  s);
 //   cout   <<s<<endl;

    for(int i=0; i<s.size(); i++)
    {
        ara[s[i]]++;

    }

    int ans= 0 ;
    for(int i=97; i<= 122; i++)
    {
        if(ara[i])
        {
          // .// cout <<i <<endl;
             ans++;
        }

    }
    cout <<ans <<endl;

}
