#include <cstdio>
int main() {
    int n, t, d;
	scanf("%d", &n);
    if(n == 0) {
    	printf("zero\n"); return 0;
	}
    if(n == 10) {
		printf("ten\n"); return 0;
	}
    if(n == 11) {
		printf("eleven\n"); return 0;
	}
    if(n == 12) {
		printf("twelve\n"); return 0;
	}
    if(n == 13) {
    	printf("thirteen\n"); return 0;
	}
    if(n == 14) {
		printf("fourteen\n"); return 0;
	}
    if(n == 15) {
		printf("fifteen\n"); return 0;
	}
    if(n == 16) {
		printf("sixteen\n"); return 0;
	}
    if(n == 17) {
		printf("seventeen\n"); return 0;
	}
    if(n == 18) {
		printf("eighteen\n"); return 0;
	}
    if(n == 19) {
		printf("nineteen\n"); return 0;
	}
    t = n / 10;
	d = n % 10;
    if(t > 1) {
		if(t == 2) printf("twenty");
        if(t == 3) printf("thirty");
        if(t == 4) printf("forty");
        if(t == 5) printf("fifty");
        if(t == 6) printf("sixty");
        if(t == 7) printf("seventy");
        if(t == 8) printf("eighty");
        if(t == 9) printf("ninety");
    }
    if(t > 1 && d > 0) printf("-");
    if(d > 0) {
		if(d == 1) printf("one");
        if(d == 2) printf("two");
        if(d == 3) printf("three");
        if(d == 4) printf("four");
        if(d == 5) printf("five");
        if(d == 6) printf("six");
        if(d == 7) printf("seven");
        if(d == 8) printf("eight");
        if(d == 9) printf("nine");
    }
    return 0;
}