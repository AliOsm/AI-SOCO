#include <bits/stdc++.h>
using namespace std;

int n, m;
vector<int> v;
int d[1001][1001];

void solve() {
	for(int i = 1; i <= n; ++i) {
		for(int j = 0; j < m; ++j) {
			d[i][j] = max(d[i][j], d[i - 1][j]);
			if(d[i - 1][j]) {
				d[i][(j + v[i]) % m] = 1;
			}
		}
		d[i][(v[i] % m)] = 1;
	}
	(d[n][0]) ? printf("YES") : printf("NO");
}

int main (int argc, char *argv[])
{
	scanf("%d%d", &n, &m);
	if(n > m) {
		printf("YES");
		return 0;
	}
	v.resize(n + 1);
	for(int i = 1; i <= n; ++i) {
		scanf("%d", &v[i]);
		v[i] %= m;
	}
	memset(d, 0, sizeof(d));
	solve();

	return 0;
}

