#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n,r;
    cin>>n>>r;
    for(int i = 1 ; i<=10 ; i++)
    {
       int x = n*i;
       if(x%10==0 || x%10==r)
       {
           printf("%d\n" , i);
           return 0;
       }
    }
    return 0;
}
