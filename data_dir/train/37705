#include <bits/stdc++.h>

using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
    string str1, str2;
    cin >> str1 >> str2;
    string ans = "";
    for(int i = 0;i < str1.length();i++){
        if(str1[i] < str2[i]){
            cout << "-1";
            return 0;
        }
        else ans += str2[i];
    }
    cout << ans;
}
