#include <bits/stdc++.h>
using namespace std;
#define eb emplace_back
#define emp emplace
#define fi first
#define se second
#define INF 0x3f3f3f3f
typedef long long ll;
typedef pair<int, int> ii;

int main(void) {
	ios_base::sync_with_stdio(false);

	int n; cin >> n;

	int ans = 0;
	for (int i = 1; i < n; i++) {
		if ((n-i)%i == 0) {
			ans++;
		}
	}

	cout << ans << endl;

	return 0;
}

