#include<bits/stdc++.h>
using namespace std;
#define forr(i,a,n) for(int i=a; i<n; i++)
#define loop(i,a,n)  for(int i=a; i>=n; i--)
#define conti continue
#define brk break
#define pb push_back
#define SPEED ios_base :: sync_with_stdio(false), cin.tie(NULL), cout.tie(0)
#define in cin>>
#define out cout<<
#define endl <<"\n"
#define ll long long 
#define remove(a,b) a.erase(std::remove(a.begin(),a.end(),b),a.end())
#define runn(it,a) for(auto it = a.begin(); it!= a.end(); it++) 
#define fi first
#define se second

int main() 
{
  SPEED;
  
  int a,b,c,flag=0; in a;
  forr(i,0,a){
    in b>>c;
    if(b!=c&&flag==0) {
      out "Happy Alex"; flag=1;
    }
  }
  if(flag!=1) out "Poor Alex";
  
  return 0;
}