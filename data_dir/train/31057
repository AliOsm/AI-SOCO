#include <bits/stdc++.h>
using namespace std;
typedef long long int ll;

class ProblemBHammingDistanceSum {
public:
	void solve(std::istream& in, std::ostream& out) {
		string s1, s2; in>>s1>>s2;
		ll n = s2.length();
		ll suffix[n+1];
		suffix[n] = 0;
		for(int i = n-1; i>=0; --i){
			suffix[i] = suffix[i+1];
			if(s2[i]=='1') ++suffix[i];
		}
		ll res = 0, m = s1.length(), sub = 0;
		for(int i = m-1; i>=0; --i){
			ll ones = suffix[i]-suffix[n-sub];
			ll zeros = (n-sub-i-ones);
			if(s1[i]=='0') res += ones;
			else res += zeros;
			++sub;
		}
		out<<res;
	}
};


int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
	ProblemBHammingDistanceSum solver;
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
	return 0;
}
