#include <bits/stdc++.h>
#define ff first
#define ss second
using namespace std;
#define MP make_pair
#define PB push_back
#define ll long long
#define int long long
#define f(i,x,n) for(int i=x;i<n;i++)
#define mod 1000000007
int32_t main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int n;
    cin>>n;
    n--;
    while(n/5>0)
    {
        n=(n-5)/2;
    }
    if(n==0)
    cout<<"Sheldon"<<endl;
    else if(n==1)
    cout<<"Leonard"<<endl;
    else if(n==2)
    cout<<"Penny"<<endl;
    else if(n==3)
    cout<<"Rajesh"<<endl;
    else
    cout<<"Howard"<<endl;
    return 0;
}
