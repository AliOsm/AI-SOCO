#include <algorithm>
#include <iostream>
#include <cstring>
#include <cstdlib>
#include <cassert>
#include <cstdio>
#include <vector>
#include <cmath>
#include <ctime>
#include <set>
#include <map>


typedef long long ll;

using namespace std;
int n,a;
bool ok[3333];
int main()
{
	cin>>n;
	for(int i=0; i<n; i++){
		cin>>a;
		ok[a]=1;
	}
	for(int i=1; i<=3001; i++)
		if(!ok[i]){
			cout<<i;
			return 0;
		}
		

	
	return 0;
}
