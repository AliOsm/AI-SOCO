const ll mod19 = 1000000009;
const ll mod17 = 1000000007;




int main(){
    
    int n,k;
    cin >> n >> k;
    if(n*(n-1)/2>k){
        for(int i=0;i<n;++i){
            cout << 0 << ' ' << i << endl;
        }
    }else
        cout << "no solution";




