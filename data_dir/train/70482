#include <iostream>

using namespace std;

int main() {
    int N, M;
    cin >> N >> M;
    int x, y;
    int ans = 0;
    for (int i = 0; i < N; ++i) {
        for (int j = 0; j < M; ++j) {
            cin >> x >> y;
            ans += (x || y);
        }
    }

    cout << ans << '\n';
    return 0;
}
