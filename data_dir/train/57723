#include <bits/stdc++.h>
typedef long long ll;
const int MOD = 998244353;
using namespace std;
int main() {
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    //freopen("input.txt" , "r" , stdin);freopen("output.txt" , "w" , stdout);
    int n;
    cin >> n;
    for (int i = 0; i < n; i++) {
        int x;
        cin >> x;
        if (x == 1) {
            cout << "HARD";
            return 0;
        }
    }
    cout << "EASY";
}
