#include<bits/stdc++.h>
using namespace std;

int main()
{
    string n;
    cin >> n;

    int c=0;

    int x = n.size();

    for (int i=0; i<x; i++){
        if (n[i]=='4' || n[i]=='7') c++;
    }

    if (c==4 || c==7) cout << "YES" << endl;
    else cout << "NO" << endl;
    return 0;
}
