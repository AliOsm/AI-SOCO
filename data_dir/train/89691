// In the name of Allah the Most Merciful.

#include<bits/stdc++.h>
using namespace std;

typedef long long ll;

int main(void)
{
    cin.tie(0);

    ll a , b , counter = 1;;
    cin >> a >> b;

    while(true){

        a-=counter;
        if(a<0){cout << "Vladik" << endl;break;}
        counter++;
        b-=counter;
        if(b<0){cout << "Valera" << endl;break;}
        counter++;
    }

    return 0;
}
