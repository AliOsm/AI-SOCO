#pragma GCC optimize ("O3")
#include <bits/stdc++.h>
#define abdelrahman010 ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
typedef long long ll;
using namespace std;
int main()
{
	abdelrahman010
	ll n,m;
	cin >> n >> m;
	string s[n+5],t[m+5];
	for(int i = 0;i<n;i++)
		cin >> s[i];
	for(int i = 0;i<m;i++)
		cin >> t[i];
	ll q;
	cin >> q;
	while(q--)
	{
		ll x;
		cin >> x;
		cout << s[(x - 1)%n] << t[(x - 1)%m] << '\n';
	}
	return 0;
}
