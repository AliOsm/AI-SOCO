#include <bits/stdc++.h>

using namespace std;
long long a,s,n,m,k,i,j,d,sum,mini;
char t;
int main()
{
cin>>s;
for(i=0;i<s;i++){
    cin>>t;
    if(t=='D'){
        d++;
    }
    else{
        a++;
    }
}
if(a>d){
    cout<<"Anton"<<endl;
}
if(a<d){
    cout<<"Danik"<<endl;
}
if(a==d){
    cout<<"Friendship"<<endl;
}
    return 0;
}
