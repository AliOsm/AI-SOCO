    freopen("input.txt", "r", stdin);
    clock_t start = clock();
#endif
    cout << setprecision(12) << fixed;
    smain();
#ifdef ART_HOME
    cout << "\n\n\n\nTOTAL EXECUTION TIME: " << float( clock () - start ) /  CLOCKS_PER_SEC << endl;
#endif
}


const int M1 = 1000000093, p1 = 27162;
const int M2 = 1000000087, p2 = 241;

const ld pi = atan2(1, 0) * 2, EPS = 1e-9;
const int pw = 10, N = 1e5 + 10, oo = 1e9 + 100, B = 316, M = 1e9 + 7;


int stupid(vi pos, vvi kek, int n, int m)
{
    int ans = 0;

    for (int _1 = 0; _1 < n; ++_1)
        for (int _2 = 0; _2 < n; ++_2)
            for (int _3 = 0; _3 < n; ++_3)
                for (int _4 = 0; _4 < n; ++_4) {
                    vi cnt(n);

                    for (int i = 0; i < n; ++i) {

                        inmax(cnt[i], kek[(_1 + i) % n][pos[0]]);

                        if (SZ(pos) > 1)
                            inmax(cnt[i], kek[(_2 + i) % n][pos[1]]);

                        if (SZ(pos) > 2)
                            inmax(cnt[i], kek[(_3 + i) % n][pos[2]]);

                        if (SZ(pos) > 3)
                            inmax(cnt[i], kek[(_4 + i) % n][pos[3]]);

                    }

                    int cur = 0;
                    for (int v : cnt)
                        cur += v;

                    ans = max(ans, cur);
                }


    return ans;
}


int solution(vvi kek, int n, int m)
{

    set<pii> lel;
    for (int i = 0; i < m; ++i)
    {
        int cur = 0;
        for (int j = 0; j < n; ++j)
            cur = max(cur, kek[j][i]);

        lel.insert(mp(cur, i));
    }

    while (SZ(lel) > n)
        lel.erase(lel.begin());

    vi pos;
    for (auto p : lel)
        pos.push_back(p.second);

    return stupid(pos, kek, n, m);
}


void smain()
{
    int t;
    cin >> t;

    while (t--)
    {
        int n, m;
        cin >> n >> m;
        vvi kek(n);

        for (int i = 0; i < n; ++i)
        {
            kek[i].resize(m);
            for (int j = 0; j < m; ++j)
                cin >> kek[i][j];
        }

        cout << solution(kek, n, m) << '\n';
    }
}