#include <iostream>
#include <algorithm>
#include <cmath>
using namespace std;

int main() {
   int n,m,i,j,t=0,x,y,k;
   
   cin>>n>>k>>t;   
  
  x=0;
   x=n*k*t/100;  
  for (i=1;i<=x/k;i++) cout<<k<<' ';
  y=x-(x/k)*k;;
  
 if (t<100)    cout<<y;
  for (i=x/k+2;i<=n;i++) cout<<" 0";
    
 //system("pause");
}
