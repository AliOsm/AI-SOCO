#include<bits/stdc++.h>
using namespace std;

#define F first
#define S second
#define PB push_back
#define all(x) x.begin(), x.end()

typedef long long ll; 

const int N = 1000 * 100 + 10, mod = 1000 * 1000 * 1000 + 7; 

int a[N];

int32_t main()
{
	ios::sync_with_stdio(0);
	cin.tie(0);
	int n, k;
	cin>> n >> k;
	int a[3] = {2,5,8};
	int ans = 0;
	for(int i = 0; i < 3; i++) a[i] *= n, ans += (a[i]+k-1)/k;
	cout<< ans << endl;
}
