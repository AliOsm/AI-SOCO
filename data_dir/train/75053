#define In_the_name_of_allah_the_merciful ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
#define Alhamdulillah return 0 ;

int main()
{
    In_the_name_of_allah_the_merciful
    int n,prev = 1 , next ;
    cin>>n;
    next = n;
    while(next-prev >= 1){
        cout << prev << " " << next << " ";
        next--;
        prev++;
    }
    if (n % 2 != 0)
        cout << n/2+1 << endl;

    Alhamdulillah
}
