#include<bits/stdc++.h>
 using namespace std;
 int main()
 {
    /*freopen("input.txt","r",stdin);
    freopen("output.txt","w",stdout);*/
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int n;
    cin >> n;
    string a[n];
    int b[n];
    int c[n];
    for(int i=0;i<n;i++)
    {
       cin >> a[i] >> b[i] >> c[i];
    }
    for(int i=0;i<n;i++)
    {
        if(b[i]>=2400 && b[i]<c[i])
        {
           cout << "YES";
           return 0;
        }
    }
    cout << "NO";
    return 0;
 }
