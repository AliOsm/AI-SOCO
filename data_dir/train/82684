#define lw lower_bound
#define up upper_bound
#define ff first
#define ss second
#define boost ios_base::sync_with_stdio(); cin.tie(0); cout.tie(0);
#define MOD 1000000007
#define MAX INT_MAX 
#define MIN INT_MIN
#define rep(i,a,b) for(ll i=a;i<=b;i++)
#define per(i,a,b) for(ll i=a;i>=b;i--)
#define con continue
vector<ll> v[1234567],vv,v1;
ll a,b,c[1234567],c1[1234][1234],e,d[1234567],i,j,n,x,y,l,r,k;
string s,s1;
ll used[1234567];
ll ans,sum;
bool ok[123];
set<ll> se;
ll T;
int main(){
    cin>>T;
    while(T--){
        cin>>n>>s>>s1;
        string ss,s2;
        ans=0;
        for(int i=0;i<n;i++)
        if(s[i]!=s1[i]){    
            ans++;
            ss+=s[i]; s2+=s1[i];
        }
        if(ans==2 && ss[0]==ss[1]&&s2[0]==s2[1]) cout<<"YES";    
        else cout<<"NO";
        puts("");
    }
}