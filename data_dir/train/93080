#include<bits/stdc++.h>
using namespace std;
 
typedef long double ld;
typedef long long ll;

typedef vector<ll> vec; 
#define INF LLONG_MAX
 
#define PP pair<int,int>
#define pb push_back
 
#define ff first
#define ss second

#define int long long int
#define FAST ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
 
#define N 200005
#define MOD 1000000007

signed main()
{
      FAST;
      
      ll n;
      cin>>n;
      
      ll a[7];
      
      for(int i=1;i<=6;i++)
            a[i]=21-i;
      
      while(n--)
      {
            ll n;
            cin>>n;
            
            bool flag=false;
            for(int i=1;i<=6;i++)
            {
                  ll tt=n-a[i];
                  if(tt%14==0)
                        flag=true;
            }
            
            if(n<15 || !flag)
            {
                  cout<<"NO\n";
            }
            else
            {
                  cout<<"YES\n";
            }
            
      }
      
      

}