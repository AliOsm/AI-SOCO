#include <stack>
#include <deque>
#include <cassert>
#include <iomanip>
#include <random>

using namespace std;



#define ll long long
#define ld long double
#define null NULL
#define prev prev228
#define index index228
#define left left228
#define right right228
#define hash hash228



signed main() {
	int t;
	scanf("%d", &t);
	for (int it = 0; it < t; ++it) {
		int n;
		cin >> n;
		bool good = false;
		for (int a = 0; a <= n / 3; ++a) {
			if ((n - a * 3) % 7 == 0) {
				good = true;
			}
		}
		if (good) {
			cout << "YES\n"
			;
		} else {
			cout << "NO\n";
		}
	}
	return 0;
}






