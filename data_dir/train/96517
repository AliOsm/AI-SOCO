//In God We Trust !
#include<bits/stdc++.h>
#define ll long long
#define ld long double
using namespace std;
int main(){
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    ll a,b,c; cin>>a>>b>>c;
    if(c==0){
        if(a==b){cout<<"YES";}else cout<<"NO";
        return 0;
    }
    if((b-a)%c ==0  && (b-a)/c >=0)cout<<"YES"; else cout<<"NO";
}
