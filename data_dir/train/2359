#include <bits/stdc++.h>
using namespace std;

const int N = 1e6 + 100;

int main() {
    // freopen("in.txt", "r", stdin);
    ios::sync_with_stdio(0);
    cin.tie(0);

    int tc;
    cin >> tc;
    while(tc--) {
        int n, k1, k2;
        cin >> n >> k1 >> k2;
        bool f = 0;
        for(int i = 1; i <= k1; i++) {
            int x;
            cin >> x;
            if(x == n) f = 1;
        }
        while(k2--) cin >> k1;
        if(f) cout << "YES" << endl;
        else cout << "NO" << endl;
    }

    return 0;
}

