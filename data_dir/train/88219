#include<bits/stdc++.h>
#define ll long long
using namespace std;
const int N = 1e5 + 5;
int main()
{
    ios_base ::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
    double n;
    cin>>n;
    double x,s=0;
    int temp=n;
    while(n--)
    {
        cin>>x;
        s+=x;
    }
    cout<<fixed<<setprecision(15)<<(double)(s/temp);
return 0;
}
