#include<bits/stdc++.h>
#define fi first
#define se second
#define FOR(a) for(int i=0;i<a;i++)
using namespace std;

typedef long long ll;
typedef pair<int,int> P;
typedef pair<P,int> LP;
const int inf=0x3f3f3f3f;
const int N=5010;

vector<P> v;
map<int,int> mp;
struct node
{
    int u,v,w;
}p[N];
int n,m,k,b[N],a[N],f[N],pre[N][N],num[N],dp[N][N];
bool did[N];
stack<int> s;
int s1=0,s2=0,cnt=0,ans=0,flag=0,tot,t=0;


int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);

    int A,B,C;
    cin>>A>>B>>C>>n;
    cnt=A+B-C;
    if((n-cnt)<=0||C>A||C>B) cout<<-1<<endl;
    else
    {
        cout<<n-cnt;
    }


}
