inline int ReadInt ()
{
int t ;
scanf ("%d", &t) ; 
return t ;
}
inline void Print (int t )
{
printf("%d\n",t); 
}
ll fact (ll n )
{
	if (n==1)return 1 ; 
	return n * fact(n-1); 
}
int main ()
{
	ll a,b ; 
	cin >> a >> b ; 
	cout << fact(min(a,b)) <<"\n";
	    
	return 0; 
}
