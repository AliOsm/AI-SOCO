#include <bits/stdc++.h>
#define ll long long int
#define pb push_back
#pragma GCC optimize ("O3")
using namespace std;
int main () {
    #ifdef LOCAL
        freopen ("input.txt", "r", stdin);
        freopen ("output.txt", "w", stdout);
        freopen ("error.txt", "w", stderr);
    #else
        ios::sync_with_stdio(false);
        cin.tie (NULL);
    #endif

    int t; cin >> t;
    while (t--) {
        int a, b; cin >> a >> b;
        cout << (__gcd(a, b) == 1 ? "Finite\n" : "Infinite\n");
    }

    return 0;
}
