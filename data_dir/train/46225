#include <iostream>
#include <fstream>
#include <string>
#define cin fin
#define cout fout

using namespace std;

ifstream fin ("input.txt");
ofstream fout("output.txt");

int main ()
{
    string s;
    cin >> s;
    int a;
    cin >> a;
    if (s == "front")
    {
        if (a == 1)
            cout << "L" << endl;
        else
            cout << "R" << endl;
    }
    else
        if (a == 1)
            cout << "R" << endl;
        else
            cout << "L" << endl;
}