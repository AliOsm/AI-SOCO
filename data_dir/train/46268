#include <bits/stdc++.h>
using namespace std;

int main() {
  int n, m, c, a = 0, b = 0;
  cin >> n;
  for (int i = 0; i < n; ++i) {
    cin >> m >> c;
    if (m < c) ++b;
    if (m > c) ++a;
  }
  if (a < b) cout << "Chris";
  else if (a > b) cout << "Mishka";
  else cout << "Friendship is magic!^^";
  return 0;
}