#include<iostream>
#include<stdio.h>
#include<algorithm>
#include<string>
#include<math.h>
using namespace std;
long long a,s,d[100002],f,g,h,j,k,l=1000,i,n,m;
main(){
cin>>n>>m;
if(m<n/2 || (n==1 && m!=0)) {cout<<-1;return 0;}
for(i=1;i<n;i+=2){
if(i+2>=n) {cout<<n+m-n%m<<" "<<n+m*2-n%m<<" ";} else {
cout<<i<<" "<<i+1<<" ";
m--;}
}
if(n%2==1) cout<<1000000000LL;
//system("pause");
}
