#include <bits/stdc++.h>

using namespace std;
typedef long long ll;
typedef pair<int, int> pi;


int main() {
  ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
  vector<ll> d(4);
  for (int i = 0; i < 4; i++)
    cin >> d[i];
  sort(d.begin(), d.end());
  for (int i = 0; i < 3; i++)
    cout << d[3]-d[i] << " ";
  cout << endl;
  return 0;
}

