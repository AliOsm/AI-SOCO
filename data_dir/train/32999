#include<bits/stdc++.h>
using namespace std;
char ch[4][4];
int i,j;
int main()
{
    for(i=1;i<=3;i++)
        for(j=1;j<=3;j++)
            cin>>ch[i][j];
    if(ch[1][1]!=ch[3][3] || ch[1][3]!=ch[3][1] || ch[1][2]!=ch[3][2] || ch[2][1]!=ch[2][3])
        return cout<<"NO",0;
    cout<<"YES";
}