#include<bits/stdc++.h>
#define show(a) cout<<a<<endl;
#define fi first;
#define se second;
using namespace std;
const int inf=0x3f3f3f3f;
const int N=2e5+10;


string s;
int a[N],b[N],ans,m,n,sum,cnt;

int main()
{
    cin>>n>>m;
    int x=0;
    for(int i=1;i<=n;i++)
    {
        cin>>x;
        sum+=x;
        cout<<sum/m<<" ";
        sum%=m;
    }


}
