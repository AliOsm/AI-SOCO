#include<bits/stdc++.h>
using namespace std;

int main(){
    string s;
    cin>>s;

    int Count=0;
    for(int i=s.size()-1;i>=0;i--){
        if(s[i]=='0') Count++;
        else{
            if(Count>=6) {printf("yes\n"); return 0;}
        }
    }
    printf("no\n");
}
/*
100100000
*/
