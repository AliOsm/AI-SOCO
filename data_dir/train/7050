#include<bits/stdc++.h>
using namespace std;
int main(){
    double t,a,y=0;
    cin>>t;
    while(t--){
        y=0;
        cin>>a;
        for(double i=3;i<10000;i++){
            if(180*(i-2)/i==a){
                cout<<"YES"<<endl;
                y++;
                break;
            }
        }
        if(y==0)cout<<"NO"<<endl;
    }

    return 0;
}
