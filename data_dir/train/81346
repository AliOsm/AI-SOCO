#include <bits/stdc++.h>
using namespace std;
const int MAX = 1e5 + 10;

int main()
{
    int n, q;
    cin >> n >> q;
    set<int> S;
    for(int i = 0; i < n; i++)
    {
        int x, y;
        cin >> y;
        while(y--)
        {
            cin >> x;
            S.insert(x);
        }
    }   
    if(S.size() == q) puts("YES");
    else puts("NO");

    return 0;
}