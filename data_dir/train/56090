#include <bits/stdc++.h>

using namespace std;

template<class T1, class T2>
istream & operator>> (istream& is, pair<T1,T2> &p){
    return is >> p.first >> p.second;
}
template<class T1, class T2>
ostream & operator<< (ostream &os, pair<T1,T2> &p){
    return os << p.first << " " << p.second;
}


signed main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
	int n;
	cin >> n;
	vector<int> v(n);
	int a;
	cin >> a;
	while(a--){
        int x;
        cin >> x;
        v[x-1]=1;
	}
	cin >> a;
	while(a--){
        int x;
        cin >> x;
        v[x-1]=1;
	}
	for(int i=0; i<n; i++){
        if(!v[i])return cout << "Oh, my keyboard!", 0;
	}
	cout << "I become the guy.";
}
