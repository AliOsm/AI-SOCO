#pragma GCC optimize ("O3")
#include <bits/stdc++.h>
#define abdelrahman010 ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
typedef long long ll;
using namespace std;
const int N = 2e5 + 5;
int t , a , b;
int main() {
    abdelrahman010
    cin >> t;
    while(t--) {
        cin >> a >> b;
        int ans = max(a * 2 , b);
        ans = min(ans , max(b * 2 , a));
        cout << ans * ans << '\n';
    }
    return 0;
}