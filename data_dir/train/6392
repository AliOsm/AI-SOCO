#include<bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;

    while (t--) {
        int n;
        cin>>n;

        for (int i=3; ;i++) {
            if ((i*n)%180)  continue;
            int k = (i*n)/180;
            if (k>=i-1) continue;
            cout<<i<<endl;
            break;
        }
    }
}

