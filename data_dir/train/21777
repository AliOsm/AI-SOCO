/**/

int n, l, r;
string s;

int32_t main() {
    kchay;
    cin >> n;
    
    for(int i = 0; i < n; i++){
        cin >> s;
        cin >> s;
        if(s == "hard")l++; else r++;
    }
    
    for(int i = 0; i <= 100; i++){
        int now = i * i;
        if(now/2+(now&1) >= max(l, r) && now/2 >= min(l, r))return cout << i, 0;
    }
    
    
    return 0;
}