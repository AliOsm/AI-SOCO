#include <bits/stdc++.h>
typedef long long ll ;
typedef unsigned long long ull ;
using namespace std;

int main()
{
     ios::sync_with_stdio(false);

     string s ;

     cin >> s;


     printf("%s",s.c_str());
     for(int i = s.size() - 1 ; i >= 0 ; i--) printf("%c",s[i]);




    return 0;
}
