#include <bits/stdc++.h>
#define intt long long
#define pb push_back
#define mk make_pair
#define ii pair<int,int>
#define sc second
#define fr first
using namespace std;

int main()
{
    int n,m;
    cin>>n>>m;
    int i,j;
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            char f;
            cin>>f;
            if(f=='C'||f=='M'||f=='Y')
            {
                cout<<"#Color";
                return 0;
            }
        }
    }
    cout<<"#Black&White";

    return 0;
}
