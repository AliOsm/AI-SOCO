// rc.cpp : Defines the entry point for the console application.
//

#include <math.h>

#include <set>
#include <list>
#include <cmath>
#include <ctime>
#include <map>
#include <deque>
#include <queue>
#include <stack>
#include <string>
#include <bitset>
#include <cstdio>
#include <limits>
#include <vector>
#include <climits>
#include <cstring>
#include <cstdlib>
#include <fstream>
#include <numeric>
#include <sstream>
#include <iostream>
#include <algorithm>
#include <unordered_map>
#include <functional>
#include <iomanip>
#include <iterator>
#include <regex>

using namespace std;

#define PI 3.14159265358979323846
#define EPS 1.E-8

#define FIN(a, n) { for (ul _ = 0; _ < n; ++_) cin >> (a)[_]; }
#define FOUT(a, n) { cout << a[0]; for (ul _ = 1; _ < n; ++_) cout << ' ' << a[_]; cout << '\n'; }

typedef unsigned char uc;
typedef unsigned int ui;
typedef unsigned long long ul;
typedef long long sl;

ul a[100001];

int main(int argc, char* argv[])
{
    ios_base::sync_with_stdio(false);

    ul n;
    cin >> n;

    FIN(a, n);

    sort(a, a + n);

    ul r = 0,
       cr = 1;
    for (ul i = 1; r < 2 && i < n; ++i)
    {
        if (a[i] == a[i - 1])
            ++cr;
        else
        {
            if (cr > 1)
                ++r;
            if (cr > 2)
                ++r;

            cr = 1;
        }
    }
    if (cr > 1)
        ++r;
    if (cr > 2)
        ++r;

    bool fl = r > 1;

    if (!fl) {
        if (n > 1 && a[0] == 0 && a[1] == 0)
            fl = true;
    }
    
    if (!fl && n > 1)
    {
        fl = true;

        ul p = a[0] - 2;
        for (ul i = 0; fl && i < n; ++i)
        {
            if (a[i] && a[i] - 1 != p)
            {
                if (r == 1)
                {
                    if (a[i] == a[i + 1])
                        fl = false;
                }
                else
                    fl = false;
            }

            p = a[i];
        }
    }

    if (!fl) {
        ul s = accumulate(a, a + n, 0) - n * (n - 1) / 2;
        if (s % 2 == 0)
            fl = true;
    }

    cout << (fl ? "cslnb" : "sjfnb") << endl;
    return 0;
}
