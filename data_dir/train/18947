#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define f first
#define s second
#define bp push_back
#define mp make_pair
const int mx=1e5+10;
const double eps=1e-9;
const int inf=1e9;
int a[mx];
main(){
    int n;
    scanf("%d",&n);
    for(int i=1;i<=n;i++)scanf("%d",a+i);
    for(int i=1;i<=n;i++){
        int tmp=i;
        for(int j=1;j<=n;j++){
            if(tmp==a[j]){
                printf("%d ",j);
                break;
            }
        }
    }



}
