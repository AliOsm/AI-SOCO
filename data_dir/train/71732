#include<bits/stdc++.h>
using namespace std;
main() {

    int n, m;
    scanf("%d%d", &n, &m);
    m--;
    vector<char> u(100);

    for (int i = n - 1; i >= 0; i--)
        if (~m & 1ll << i - 1) {
            printf("%d ", n - i);
            u[n - i] = true;
        }

    for (; n; n--)
        if (!u[n]) {
            printf("%d ", n);
        }

    return 0;
}
