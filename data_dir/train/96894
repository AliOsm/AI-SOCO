#include<bits/stdc++.h>
using namespace std;
int l[5],r[5],p[5],s[5];
int main()
{
    for(int i=1;i<=4;++i)
        cin>>l[i]>>s[i]>>r[i]>>p[i];
    if((l[1] && (p[1] || p[4])) || (s[1] && (p[1] || p[3])) || (r[1] && (p[1] || p[2])) || (l[2] && (p[1] || p[2])) || (s[2] && (p[2] || p[4])) || (r[2] && (p[2] || p[3])) || (l[3] && (p[2] || p[3])) || (s[3] && (p[1] || p[3])) || (r[3] && (p[3] || p[4])) || (l[4] && (p[3] || p[4])) || (s[4] && (p[2] || p[4])) || (r[4] && (p[1] || p[4])))
        return cout<<"YES",0;
    cout<<"NO";
}