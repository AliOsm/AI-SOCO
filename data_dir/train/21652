#include <bits/stdc++.h>                                                                                                         
#define ll long long
#define norm variable_also
#define ld long double
#define ull unsigned ll
#define mp make_pair
#define pb push_back
#define F first
#define S second
#define dec variable
#define left variable_too

using namespace std;

const int MIN = 2e3 + 2;
const int MXN = 3e5 + 2;
const ll INF = 1e9 + 7;
const ll MOD = (1LL << 32);
const ll LINF = 1e18 + 15;
const double EPS = 1e-9;
                                              
int v1, v2, v3, vm;

int main(){
	ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    #ifdef Pissoir
    	//freopen ("input.txt", "r", stdin);
    	//freopen ("output.txt", "w", stdout);
    #else
//		freopen ("A.in", "r", stdin);
  //  	freopen ("A.out", "w", stdout);    	
    #endif
    cin >> v1 >> v2 >> v3 >> vm;
    if (vm >= v1 || vm >= v2 || vm > v3 * 2 || vm * 2 < v3){
		cout << -1;
		return 0;    
    }
    cout << v1 * 2 << " " << v2 * 2 << " ";
    if (vm > v3){
    	cout << v3 * 2;
    }
    else {
    	cout << vm * 2;
    }
	return 0;                                          
}                          	