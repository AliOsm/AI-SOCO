#include <stdio.h>
#include <iostream>
#include <algorithm>
#include <queue>
#include <string>
using namespace std;
 
 
int main() {
	//freopen("input.txt", "r", stdin);
	//freopen("output.txt", "w", stdout);
 
	int n;
	string s;
	cin >> n >> s;
 
	int k8 = count(s.begin(), s.end(), '8');
	int ans = min(n / 11, k8);
 
	cout << ans;
}