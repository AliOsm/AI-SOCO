#include <bits/stdc++.h>

using namespace std;

int n, i;

int main()
{
	scanf("%d", &n);
	printf("%d ", n);
	for(i = 1; i < n; i += 2)
		printf("%d ", i);
	if(n & 1)
		printf("%d ", i);
	for(i -= 2; i >= 1; i -= 2)
		printf("%d ", i);
	for(i = 2; i < n; i += 2)
		printf("%d ", i);
	if((n & 1) == 0)
		printf("%d ", i);
	for(i -= 2; i >= 1; i -= 2)
		printf("%d ", i);
}