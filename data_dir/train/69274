#include<bits/stdc++.h>
using namespace std;
#define rep(i,a,b) for(int i=a;i<b;i++)
#define repr(i,a,b) for(int i=a;i>=b;i--)
#define mod 1000000007
#define pb push_back
#define mp make_pair
#define fs first
#define ss second
typedef long long ll;
typedef long double ld;
int main()
{
    ios_base::sync_with_stdio(0);
    int l,r;
    cin>>l>>r;
    if(l==r) cout<<l;
    else cout<<2;
}