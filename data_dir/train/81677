#include <bits/stdc++.h>
using namespace std ;

int n  ;

int main()
{
    cin >> n ;

    if( n == 2) cout << "-1" ;

    else if( n == 3) cout << "6\n10\n15" ;
    else
    {
        cout << "6\n10\n12\n15" ;

        n -= 4 ;

        for( int i = 0 ; i < n ; i++ )cout << "\n" << (6 * (i+3))  ;


    }
    return 0 ;
}
