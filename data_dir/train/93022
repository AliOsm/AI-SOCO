#include <bits/stdc++.h>

using namespace std;

#define ff first
#define ss second
#define pb push_back
using ll = long long;
using ii = pair<int,int>;
#define eps 1e-9

int main(){
	int a, b, c, n;
	scanf("%d %d %d %d", &a, &b, &c, &n);
	if(a + b - c >= n || c > a || c > b){
		printf("-1\n");
	}
	else
		printf("%d\n",n - (a + b - c));
	return 0;
}
