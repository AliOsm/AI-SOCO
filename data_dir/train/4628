#include<bits/stdc++.h>

using namespace std;

const int N = 2000000;

long long b[N],c[N],n, a[N],x,k,m;
double d[N];
pair<long long, long long> p[N];
vector<long long> v;
string s;

int main(){
    cin>>n;
    cin>>s>>s;
    if(s[0]=='w') {
        if(n<5 || n==7) cout<<52; else cout<<53;
    } else {
        if(n==30) cout<<11; else
        if(n==31) cout<<7; else cout<<12;
    }
    return 0;
}