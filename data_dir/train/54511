#include<iostream>
#include<bits/stdc++.h>
#include<math.h>
using namespace std;
int main()
{
    
    long long int b,e=0;
    cin>>b;
    for(int i=1;i<=sqrt(b);i++)
    {
        if(b%i==0)
         {
             if(b/i==i)
              e++;
             else
             e=e+2;
         }
    }
    cout<<e;}
    