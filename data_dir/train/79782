#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
typedef pair<int, int> pp;
double PI=acos(-1);
int const N = 3e5 + 10,M=5e4+5, oo = 1e9;
const int mod = oo+7;
const ll  OO = 2e18;

int t,n,v,l,r;

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    cin>>t;
    while(t--){
        cin>>n>>v>>l>>r;
        int an=l/v-(l%v==0);
        an+=n/v;
        an-=r/v;
        cout<<an<<endl;
    }
    return 0;
}



