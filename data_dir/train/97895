#include <bits/stdc++.h>
using namespace std;
int main()
{
    freopen("input.txt","r",stdin);
    freopen("output.txt","w",stdout);
    string s;
    int n;
    cin>>s>>n;
    if(s[0]=='f'&&n==1)
    {
        cout<<"L\n";
    }
    else if(s[0]=='b'&&n==2)
    {
        cout<<"L\n";
    }
    else cout<<"R\n";

}