#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
template <class T> int size(const T &x) { return x.size(); }
#define rep(i,a,b) for(int i = (a); i < (b); i++)
#define iter(it,c) for(auto it = (c).begin(); it != (c).end();++it)
#define pb push_back
#define fs first
#define sc second
typedef pair<int,int> ii;
typedef vector<int> vi;
typedef vector<ii> vii;

const int INF = ~(1<<31);
const double pi = acos(-1);
const double EPS = 1e-9;

int main() {
    cin.sync_with_stdio(false);
	string s;
	cin >> s;
	int cnt = 0;
	rep(i,0,s.size()) {
		if(s[i] == '1') {
			cnt++;
		} else if(s[i] == '3') {
			cnt++;
		} else if(s[i] == '5') {
			cnt++;
		} else if(s[i] == '7') {
			cnt++;
		} else if(s[i] == '9') {
			cnt++;
		} else if(s[i] == 'a') {
			cnt++;
		} else if(s[i] == 'e') {
			cnt++;
		} else if(s[i] == 'i') {
			cnt++;
		} else if(s[i] == 'o') {
			cnt++;
		} else if(s[i] == 'u') {
			cnt++;
		}
		
	
	}
	cout << cnt << endl;
    return 0;
}


