#include<bits/stdc++.h>
using namespace std;

#define ll long long

ll ax,ay,bx,by,cx,cy;

int main()
{
//    freopen("a.in","r",stdin);
    cin>>ax>>ay>>bx>>by>>cx>>cy;
    if((ax-bx)*(ax-bx)+(ay-by)*(ay-by)==(bx-cx)*(bx-cx)+(by-cy)*(by-cy) and (cy-by)*(bx-ax)!=(by-ay)*(cx-bx))
        cout<<"YES";
    else
        cout<<"NO";
}

