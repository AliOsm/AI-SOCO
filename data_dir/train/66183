#include <bits/stdc++.h>

using namespace std;
const int N = 400004;
int n,k,arr[N];
queue <int> nextt[N];
bool inSet[N];
int main()
{
    int currInSet = 0;
    scanf("%d %d",&n,&k);
    for(int i = 0;i<n;i++)
        scanf("%d",&arr[i]),nextt[arr[i]].push(i);
    set<int> currSet;
    priority_queue <pair<int,int> >pq;
    for(int i = 1;i<=n;i++)
        nextt[i].push(n);
    int ans = 0;
    for(int i = 0;i<n;i++) {
        nextt[arr[i]].pop();
        if(inSet[arr[i]]) {
            pq.push(make_pair(nextt[arr[i]].front(),arr[i]));
            continue;
        }
        if(currInSet < k) {
            inSet[arr[i]] = true;
            currInSet++;
        } else {
            pair<int,int> topp = pq.top();
            pq.pop();
            inSet[topp.second] = false;
            inSet[arr[i]] = true;
        }
        ans++;
        pq.push(make_pair(nextt[arr[i]].front(),arr[i]));
    }
    printf("%d",ans);

    return 0;
}
