using namespace std;

const int N = 100005;
const int inf = 1000 * 1000 * 1000;
const int mod = 998244353;

int t;
int x , y;

unordered_map <int,int> mp;

int main(){
    cin >> t;
    while(t--){
        cin >> x >> y;
        if(x == 1 && y > 1){
            cout << "NO" << endl;
            continue;
        }
        if(x == 2 && y > 3){
            cout << "NO" << endl;
            continue;
        }
        if(x == 3 && y > 3){
            cout << "NO" << endl;
            continue;
        }
        cout << "YES" << endl;
    }
    return 0;
}
