#include <bits/stdc++.h>

using namespace std;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
    long long a,b,cnt;
    cin>>a>>b;
    cnt=a;
    cout<<"YES"<<endl;
    while(cnt<=b)
    {
        cout<<cnt<<" "<<cnt+1<<endl;
        cnt+=2;
    }
    return 0;
}
