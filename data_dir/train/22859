/*
# include <stdio.h>
# include <string.h>
*/
# include <queue>
# include <iostream>
# include <algorithm>

using namespace std;

pair <int, int> pii;
priority_queue <pair <int, int> > pq;

int n, m, k, n2;
int maxk, maxc, maxs;

int main ()
{
# ifndef ONLINE_JUDGE
   freopen ("D.txt", "rt", stdin);
   freopen ("D.out", "wt", stdout);
# endif

   cin >> n; n2 = (n+1)/2;
   for ( int i=0, a, b; i < n; i++ )
   {
      cin >> a >> b;
      pii.first = a, pii.second = 1;
      pq.push (pii);
      if ( a != b )
      {
         pii.first = b, pii.second = 2;
         pq.push (pii);
      }
   }

/*
   for ( ; ! pq.empty (); pq.pop () )
      cout << pq.top ().first << " " << pq.top ().second << endl;
*/
   pii = pq.top ();
   if ( pii.second == 1 ) k = 1, m = 0; else k = 0, m = 1;
   maxk = 0, maxs = -1;
# ifndef ONLINE_JUDGE
   cout << "n=" << n << " n2=" << n2 << endl << endl;
   cout << pq.top ().first << " " << pq.top ().second << endl;
   cout << "k=" << k << " m=" << m << " maxk=" << maxk << " maxs=" << maxs << endl;
# endif

   for ( pq.pop (); ! pq.empty (); pq.pop () )
   {
# ifndef ONLINE_JUDGE
   cout << pq.top ().first << " " << pq.top ().second << endl;
# endif

      if ( pq.top ().first == pii.first )
         { if ( pq.top ().second == 1 ) k++; else m++; }
      else
      {
         if ( k >= maxk && k+m >= n2 ) maxk = k, maxs = m;
         pii = pq.top ();
         if ( pii.second == 1 ) k = 1, m = 0; else k = 0, m = 1;
      }
# ifndef ONLINE_JUDGE
   cout << "k=" << k << " m=" << m << " maxk=" << maxk << " maxs=" << maxs << endl;
# endif
   }

   if ( k >= maxk && k+m >= n2 ) maxk = k, maxs = m;

# ifndef ONLINE_JUDGE
   cout << "k=" << k << " m=" << m << " maxk=" << maxk << " maxs=" << maxs << endl;
# endif

   if ( maxs >= 0 )
   {
      maxc = n2 - maxk;
      if ( maxc < 0 ) maxc = 0;

      cout << maxc;
   }
   else
      cout << "-1";

   return 0;
}
