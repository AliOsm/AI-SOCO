#define pb push_back
#define mp make_pair

using namespace std;

int x;

int main(){
 for(int i=1; i<6; i++)
  for(int j=1; j<6; j++){
   cin>>x;
   if(x==1) {
    cout<<abs(i-3)+abs(j-3);
    break;
   }
  }
 return 0;
}