#include <iostream>

using namespace std;

int main()
{
    int a,b;
    cin>>a>>b;
    for(int i=1;i<=10;i++){
        if((a*i)%10==0){
            cout<<i<<endl;
            return 0;
        }
        else if(((a*i)-b)%10==0){
            cout<<i<<endl;
            return 0;
        }
    }
    return 0;
}
