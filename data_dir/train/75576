#include <iostream>
#include<cstdio>

using namespace std;

int main()
{
    //freopen("a.in","r",stdin);
    int a[1000]={0},i,k=0;
    bool b;
    unsigned long s;

    while (cin>>s){
            b=true;
            for(i=0;i<k;i++)
                if(s==a[i])
                    b=false;
            if(b){
                a[k++]=s;

            }
    }
    cout<<4-k;


    return 0;
}
