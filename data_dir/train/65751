#include <bits/stdc++.h>
using namespace std;
int main(){
	string a;
	getline(cin, a);
	for(int i = a.size() - 1; i >= 0; i --){
		if(a[i] >= 65 && a[i] <= 90 || a[i] >= 97 && a[i] <= 122){
			if(a[i] == 'A' || a[i] == 'a' || a[i] == 'E' || a[i] == 'e' || a[i] == 'i' || a[i] == 'I' || a[i] == 'O' || a[i] == 'o' || a[i] == 'Y' || a[i] == 'y' || a[i] == 'u' || a[i] == 'U'){
				cout << "YES";
				return 0;
			}
			else{
				cout << "NO";
				return 0;
			}
		}
	}
}
