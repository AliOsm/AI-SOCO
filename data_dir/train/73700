#include <bits/stdc++.h>

using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr); cout.tie(nullptr);

    int q;
    cin >> q;

    while(q--){
        int a[3];
        cin >> a[0] >> a[1] >> a[2];

        sort(a, a + 3);
        if(a[2] - a[1] <= 1 || a[2] - (a[0] + a[1]) <= 1){
            cout << "YES" << endl;
        }
        else{
            cout << "NO" << endl;
        }
    }
}
