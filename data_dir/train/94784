using namespace std;
#define MAX 2002
int main(){
    int n, k;
    scanf("%d%d", &n, &k);
    int a = n*(n - 1)/2;
    if (a <= k){
        puts("no solution");
    }
    else{
        for (int i = 0; i < n; i++){
            printf("%d %d\n", 0, i+1);
        }
    }
    return 0;
}