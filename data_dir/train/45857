#include<bits/stdc++.h>
#define MAX 300005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

i64 N, sum, Q;
i64 a[MAX];

int main()
{
    optimiza_io
    cin >> N;
    for( i64 i = 1; i <= N; i ++ )
        cin >> a[i], sum += a[i];
        
    sort( a + 1 , a + N + 1 );
    
    cin >> Q;
    while( Q -- )
    {
        i64 x;
        cin >> x;
        cout << sum - a[N - x + 1] << "\n";
    }
    return 0;
}

