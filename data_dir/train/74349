#include<iostream>
#include<cmath>
#include<algorithm>
using namespace std;
int main(){

    long long n, i=0, j, count=0, x, k, temp=0;
    cin>>n;
    temp=1;
    if(n>6){
    while(temp<=n){

        temp=temp+5*round(pow(2,i));
        i++;

    }
        i--;
        temp-=5*round(pow(2,i));
    }



    //cout<<i<<endl;
    //return 0;
    long long helik;
    helik=i;

    for(i=temp;;){
        //cout<<i<<endl;
        x=pow(2,helik);
        for(j=0;j<5;++j){

            i+=x-1;

                //cout<<i<<endl;

            if(i>=n){

            if(j==0)
                cout<<"Sheldon"<<endl;
            else if(j==1)
                cout<<"Leonard"<<endl;
            else if(j==2)
                cout<<"Penny"<<endl;
            else if(j==3)
                cout<<"Rajesh"<<endl;
            else if(j==4)
                cout<<"Howard"<<endl;
            return 0;
                }
            i++;




        //cout<<j<<endl;





        }
        //cout<<i<<endl;
        temp++;
        //cout<<temp<<endl;








    }






    return 0;

}
