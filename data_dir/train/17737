#include<bits/stdc++.h>
#define int long long
using namespace std;
typedef long long ll;
typedef pair<int,int> PI;

main(){
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin>>n;
	cout<<n+n/2<<'\n';
	for(int i=2;i<=n;i+=2)
		cout<<i<<' ';
	for(int i=1;i<=n;i+=2)
		cout<<i<<' ';
	for(int i=2;i<=n;i+=2)
		cout<<i<<' ';
	return 0;
}