#include <cstdio>
#include <cstring>
#include <cmath>
#include <algorithm>
using namespace std;
typedef long long LL;
#define INF 0x3f3f3f3f
#define eps 1e-8

int a, b, r; 

int main() {
	scanf("%d%d%d", &a, &b, &r);
	if (a >= 2*r && b >= 2*r) printf("First\n");
	else printf("Second\n");
	return 0;
}
