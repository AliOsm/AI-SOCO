#define ll          long long
#define pb          push_back
#define mp          make_pair
#define pii         pair<int,int>
#define vi          vector<int>
#define all(a)      (a).begin(),(a).end()
#define F           first
#define S           second
#define sz(x)       (int)x.size()
#define hell        1000000007
#define endl        '\n'
#define rep(i,a,b)	for(int i=a;i<b;i++)
using namespace std;

void solve(){
	int n,m;
	cin>>n>>m;
	vi p(n);
	rep(i,0,n)cin>>p[i];
	while(m--){
		int l,r,x;
		cin>>l>>r>>x;
		l--;
		r--;
		x--;
		int cursmall=x-l;
		for(int i=l;i<=r;i++){
			if(p[i]<p[x])cursmall--;
		}
		cout<<(cursmall?"No\n":"Yes\n");
	}
}

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	int t=1;
//	cin>>t;
	while(t--){
		solve();
	}
	return 0;
}