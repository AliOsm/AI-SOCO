#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define fr first
#define sc second
#define r_0 return 0
using namespace std;

const ll N = 1e5 + 1 ;

ll n , m , k , l , x , y  , z , cnt , ans , sum , mn = 1e18 , mx = -1e18 ;

ll a[N];
string s , t[10] ;

int main()
{
    //freopen( "input.txt", "r" , stdin );
    //freopen( "output.txt", "w" , stdout );
    ios_base::sync_with_stdio(0);

    cin >> s ;
    for ( int i = 0; i < 10 ; i ++ )
        cin >> t[i];
    for ( int i = 0 ; i < 90; i += 10 ){
        string a = s.substr( i , 10 );
        for ( int j = 0 ; j < 10 ; j ++ ){
            if ( a == t[j] ){
                cout << j ;
                break;
            }
        }
    }

}


















