#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define sz size()
#define sc second
#define fr first
#define r return
#define TIME ios_base::sync_with_stdio(0);
using namespace std;

    const ll N = 1e4+1;

ll n , m , k , l , cnt , ans , x , y , mx , mn;
vector < char > v , d;
pair < ll , ll > p [ N ];
string s , t ;
map < ll , ll > mp;
ll a[N];

int main()
{
    TIME
    cin >> n >> s ;
    for ( ll i = 0 ; i< n ; i ++ ){
            if ( s [ i ] == '1' )
        v.pb ( s[ i ] ) ;
    else
        d.pb ( s[ i ] );
    }
    if ( v.sz >= d.sz )
        cout << v.sz - d.sz;
    else
        cout << d.sz - v.sz;
}




