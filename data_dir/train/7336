#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

int main()
{
    int a[11111], s = 0;
    for(int i = 1; i <= 4; i++)
    {
        cin >> a[i];
    }
    sort(a+1, a+1+4);
    for(int i = 1; i <= 4; i++)
    {
        if(a[i] == a[i+1])
        {
            s++;
        }
    }
    cout<<s;
    return 0;
}

