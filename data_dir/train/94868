#include <bits/stdc++.h>

using namespace std;

#define x first
#define y second
#define SZ(x) ((int)((x).size()))
#define PB(x) push_back(x)
#define INF (0x3f3f3f3f)
#define MEMSET(x,v) memset(x,v,sizeof(x))

typedef long long LL;
typedef pair<int, int> PII; typedef pair<PII, int> PII2;

int main() {
    int sum = 0;
    int a;
    for (int i = 0; i < 5; i++) {
        cin >> a;
        sum += a;
    }
    if (sum % 5 == 0 && sum) {
        printf("%d\n", sum / 5);
    } else {
        printf("-1\n");
    }
    return 0;
}
