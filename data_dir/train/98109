#include <bits/stdc++.h>
#define f first
#define s second
#define mp make_pair
#define pb push_back
#define readi(i) scanf("%d",&i)
#define read2i(i,j) scanf("%d%d",&i,&j)
using namespace std;
typedef long long ll;
const int N = 200002;
int main()
{
    int n;
    cin>>n;
    if(n % 10 == 0)
        cout<<n;
    else {
        int x = n % 10;
        if(x >= 5)
            n += (10-x);
        else
            n-= x;
        cout<<n;
    }

    return 0;
}
