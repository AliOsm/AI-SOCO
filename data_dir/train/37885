#include<bits/stdc++.h>
using namespace std;

#define fi(i,a,b)   for(long long i=a;i<=b;i++)
#define fd(i,a,b)   for(long long i=b;i>=a;i--)
#define clr(x)      memset(x,0,sizeof(x))
#define __          printf(" ")
#define _           printf("\n")



int main(){
    int n,t;
    cin>>n>>t;
    if(t==10){
        if(n==1){
            cout<<"-1";return 0;
        }
        cout<<'1';
       fi(i,2,n) cout<<'0';
    }
    else fi(i,1,n) cout<<t;
    return 0;
}