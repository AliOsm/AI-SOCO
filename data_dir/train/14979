#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define sz size()
#define sc second
#define fr first
#define r_0 return 0
#define TIME ios_base::sync_with_stdio(0);
using namespace std;
ll n , m , l , k ,cnt , x , y , ans ;
map < ll , ll > mp;
set < ll > st;
ll a[ 10001 ];
int main()
{
TIME
    cin >> n >> m;
    for ( ll i = 0 ; i < n ;  i++ ){
        cin >> l;
        cnt += l;
        ans = max ( ans , l );
    }
    if ( cnt - ans <= m )
    cout << "YES";
    else
    cout << "NO";

}




