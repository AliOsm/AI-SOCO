#include <bits/stdc++.h>
using namespace std ;

int a[105] ;

int y , x, z , n, m ;

int main()
{
    cin >> n >> m ;

    for( int i = 0 ; i < n; i++ )
    {
        cin >> x ;
        while(x-- )
        {
            scanf("%d", &y ) ;
            a[y] = 1 ;
        }
    }

    for( int i = 1 ; i <= m ; i++ )
    {
        if( a[i] == 0 )
        {
            cout << "NO" ;
            return 0 ;
        }
    }
    cout <<"YES" ;
 return 0 ;
 }
