#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define N 500500



int main()
{
    //freopen("in.in", "r", stdin);
    ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

    int t; cin >> t;
    while(t--){
        ll x, y; cin >> x >> y;
        if(x - y == 1) cout << "NO" << endl;
        else cout << "YES" << endl;
    }

    return 0;
}