#include <bits/stdc++.h>
#define mk make_pair
#define fs first
#define sc second
using namespace std;
typedef long long ll;
typedef long double ld;
int a[100010];
int main(){
    int n, t;
    cin>>t;
    while(t--){
    	string a, b, c;
    	cin>>a>>b>>c;
    	n=a.size();
    	bool l=1;
        for(int i=0; i<n; ++i){
        	if(c[i]!=b[i] && c[i]!=a[i]){
        		l=0;
        		break;
        	}
        }
        if(l){
    	    cout<<"YES"<<endl;
	       }
        else{
        	cout<<"NO"<<endl;
        }
    }
  return 0;
}


