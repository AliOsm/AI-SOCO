LL a[100010] ;


int main()
{
    LL t , i , k , n , m , j , A , B ;

    SL3( n , A , B ) ;

    a[n] ++ , a[A] ++ , a[B] ++ ;
    if( a[1] >= 1 || a[2] >= 2 || a[3] == 3 || ( a[2] == 1 && a[4] == 2 ) )
    {
        puts("YES") ;
        return 0 ;
    }
    puts("NO") ;

    return 0;
}
