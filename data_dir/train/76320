#include<bits/stdc++.h>
using namespace std;
#define forr(i,a,n) for(int i=a; i<n; i++)
#define loop(i,a,n)	for(int i=a; i>=n; i--)
#define conti continue
#define brk break
#define pb push_back
#define SPEED ios_base :: sync_with_stdio(false), cin.tie(NULL), cout.tie(0)
#define in cin>>
#define out cout<<
#define endl <<"\n"
#define ll long long 
#define remove(a,b) a.erase(std::remove(a.begin(),a.end(),b),a.end())
#define runn(it,a) for(auto it = a.begin(); it!= a.end(); it++) 
#define fi first
#define se second

int main() 
{	
	ll int a,b; in a>>b;
	ll int k,m,l=0,flag=0; in k>>m; 
	ll int c[a],d[b];
	forr(i,0,a)	in c[i];
	forr(i,0,b)	in d[i];
	if(c[k-1]<d[b-m])	out "YES";
	else out "NO";
	
	return 0;
}