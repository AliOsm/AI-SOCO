#include <bits/stdc++.h>

using namespace std;
typedef long long ll;
typedef pair<int, int> pi;


int main() {
  ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
  string a, b;
  cin >> a >> b;
  if (a.size() == b.size()) {
    if (a == b) cout << -1 <<endl;
    else cout <<a.size() << endl;
  }
  else cout << max(a.size(), b.size())<<endl;
  return 0;
}

