#include <bits/stdc++.h>

using namespace std;

#define add push_back
#define all(x) x.begin(), x.end()

int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0); cout.tie(0);

    int x[4];
    for (int i = 0; i < 4; ++i) {
        cin >> x[i];
    }
    sort(x, x + 4);
    cout << x[3] - x[2] << ' ' << x[3] - x[1] << ' ' << x[3] - x[0];

    return 0;
}