#include <bits/stdc++.h>
using namespace std;
vector<int>ans;
int main(){
	//freopen("readin.txt","r",stdin);
	int n;
	cin >> n;
	for(int i = 2;i <= n;i+=2)ans.push_back(i);
	for(int i = 1;i <= n;i+=2)ans.push_back(i);
	for(int i = 2;i <= n;i+=2)ans.push_back(i);
	printf("%d\n",ans.size());
	for(auto i : ans)printf("%d ",i);
}