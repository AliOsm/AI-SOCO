/* winners never quit and quitters never win.
      #swap                                 */
#include <bits/stdc++.h>
using namespace std;
typedef long long int ll;
 
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(0);cout.tie(0);
    ll n;
    cin>>n;
    string s;
    cin>>s;
    ll cnt=0;
    for(ll a=0;a<=n-11;a++)
        if(s[a]=='8')
            cnt++;
            
    if(cnt>(n-11)/2)
        cout<<"YES\n";
    else
        cout<<"NO\n";
	return 0;
}