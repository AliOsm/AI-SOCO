#include <bits/stdc++.h>

#define F first
#define S second
#define pb push_back
#define ll long long

using namespace std;

const ll inf=1e18;
const int mod=1e9+7;

int main() {
    ios_base::sync_with_stdio(0);
    //freopen("input.txt", "r", stdin);
   // freopen("output.txt", "w", stdout);
    int n;
    cin >> n;
    string s;
    while (s.size() < n){
        s+="aabb";
    }
    for (int i = 0; i<n; i++){
        cout << s[i];
    }
}
