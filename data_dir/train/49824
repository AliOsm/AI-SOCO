#include<bits/stdc++.h>
 

#define ll long long
#define ull unsigned long long
#define vi vector<int>
#define pii pair<int, int>
#define vvi vector<vector<int>>
#define I "%d"
#define L "%I64d"

#define over999 1234567890
#define pb push_back
#define mp make_pair
#define fi first
#define se second
#define Pi 3.1415926535897932384626433
#define files freopen("roads.in","r",stdin);freopen("roads.out","w",stdout); 

#define elif else if
#define yes "Yes\n"
#define no "No\n"
#define space <<" "<<
#define next lucky_guy
#define time goose
#define endl "\n"


using namespace std;
char x, y;
int ans;

int main() 
{   
    cin >> x >> y;
    
    if(x+1<='h')ans++;
    //cout << ans << endl;
    if(x-1>='a')ans++;
    //cout << ans << endl;
    if(y+1<='8')ans++;
    //cout << ans << endl;
    if(y-1>='1')ans++;
    //cout << ans << endl;
    if(x+1<='h' && y+1<='8')ans++;
    //cout << ans << endl;
    if(x+1<='h' && y-1>='1')ans++;
    //cout << ans << endl;
    if(x-1>='a' && y+1<='8')ans++;
    //cout << ans << endl;
    if(x-1>='a' && y-1>='1')ans++;
    
    cout << ans;
    return 0;
}