void brainfuck()
{
    cin>>d>>e;
    cin>>a>>b>>c;
    ll lol = 0;

    if( d >= ( a * 2 ) )d -= a * 2;
    else lol = a * 2 - d , d = 0;

    if ( d >= b ) d -= b;
    else lol += b - d , d = 0;

    if ( e >= b ) e -= b;
    else lol += b - e , e = 0;

    if ( e < c * 3 )lol += c * 3 - e;
    cout<<lol;
}

