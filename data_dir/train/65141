#include <bits/stdc++.h>
using namespace std;
int main(){
int n,a[100000],i;
int m[100000];
   cin >> n;
    for(i=0;i<n;i++){
        cin>>a[i];
        m[a[i]]++;
    }
    for(i=0;i<n;i++){
        if(m[a[i]]%2==1){
            cout<<"Conan";
          goto End;
        }
    }
    cout<<"Agasa";
End:return 0;}
