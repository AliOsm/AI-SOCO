#include <bits/stdc++.h>

using namespace std;

#define endl '\n'

typedef long long ll;
typedef pair<ll, ll> pii;



int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);


    int a, b;
    cin >> a >> b;

    if(a == 9 && b == 1){
    	cout << "9 10" << endl;
    	return 0;
    }

    if(a == b){
    	cout << a << "0 " << b << "1" << endl;
    	return 0;
    }

    if(a + 1 == b){
    	cout << a << "9 " << b << "0" << endl;
    	return 0;
    }

    cout << -1 << endl;

    return 0;
}