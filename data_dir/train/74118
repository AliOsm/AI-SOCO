#include<bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);

    int n, t;
    cin>>n>>t;

    int len = 0;
    for (int i=1; i<=n; i++) {
        int x;
        cin>>x;
        len += x+1;
    }
    len--;

    if (len == t)   cout<<"YES"<<endl;
    else            cout<<"NO"<<endl;
}

