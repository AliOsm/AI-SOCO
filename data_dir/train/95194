#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c,d,e,i,j,k,n;
    cin>>a>>b>>c>>n;
    if(n==0)
    {
        cout<<-1<<endl;
        return 0;
    }
    if(c>a || c>b)
    {
        cout<<-1<<endl;
        return 0;
    }
    if(a>=n || b>=n || c>=n)
    {
        cout<<-1<<endl;
        return 0;
    }
    a=a+b-c;
    if(a>=n)
    {
        cout<<-1<<endl;
        return 0;
    }
    else
    {
        cout<<n-a<<endl;
        return 0;
    }
    return 0;
}
