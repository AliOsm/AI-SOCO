using namespace std;
typedef long long LL;
typedef unsigned long long ULL;
#define MEM(a,b) memset((a),(b),sizeof(a))
const LL INF = 1e9 + 7;
const int N = 1e5 + 10;
int main()
{
	//freopen("input.txt", "r", stdin);
	//freopen("output.txt", "w", stdout);
	int n, m;
	cin >> n >> m;
	if (m%n)
	{
		puts("-1");
		return 0;
	}
	m /= n;
	int tot = 0;
	while (m % 3 == 0) m /= 3, tot++;
	while (m % 2 == 0) m /= 2, tot++;
	if (m == 1) cout << tot << endl;
	else cout << -1 << endl;
	return 0;
}