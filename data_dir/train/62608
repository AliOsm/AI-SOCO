#include <iostream>
#include <string>
using namespace std;

string a[4];
char b[]={'A','B','C','D'};
int s[4],ans=2,f=0;
int main() {
	cin >> a[0] >> a[1] >> a[2] >> a[3];
	for(int i = 0 ; i < 4 ; i++)s[i]=a[i].size()-2;
	if((s[0]<=(s[1]/2))&&(s[0]<=(s[3]/2))&&(s[0]<=(s[2]/2))){ans=0;f++;}
	if((s[0]>=(s[1]*2))&&(s[0]>=(s[3]*2))&&(s[0]>=(s[2]*2))){ans=0;f++;}
	
	if((s[1]<=(s[0]/2))&&(s[1]<=(s[3]/2))&&(s[1]<=(s[2]/2))){ans=1;f++;}
	if((s[1]>=(s[0]*2))&&(s[1]>=(s[3]*2))&&(s[1]>=(s[2]*2))){ans=1;f++;}
	
	if((s[2]<=(s[0]/2))&&(s[2]<=(s[3]/2))&&(s[2]<=(s[1]/2))){ans=2;f++;}
	if((s[2]>=(s[0]*2))&&(s[2]>=(s[3]*2))&&(s[2]>=(s[1]*2))){ans=2;f++;}
	
	if((s[3]<=(s[1]/2))&&(s[3]<=(s[0]/2))&&(s[3]<=(s[2]/2))){ans=3;f++;}
	if((s[3]>=(s[1]*2))&&(s[3]>=(s[0]*2))&&(s[3]>=(s[2]*2))){ans=3;f++;}
	if(f>1)ans=2;
	
	cout << b[ans] << endl;
	return 0;
}