    /// إِنَّ مَعَ الْعُسْرِ يُسْرًا
    /// لَا أَبْرَحُ حَتَّىٰ أَبْلُغَ


    #include <bits/stdc++.h>
    #define LL long long
    #define LD long double
    #define S second
    #define IO ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    #define F first
    #define PI acos(-1)
    #define MAX 200005
    using namespace std;


    int main()
    {
        IO
         LL n,k=0,a=0,x,last=0;
        cin>>n;
        while(n--)
        {
            cin>>x;
            if(x>=last)
                a+=(x-last);
            else
                a+=(last-x);
                last=x;
        }
        cout<<a;
        return 0;
    }

