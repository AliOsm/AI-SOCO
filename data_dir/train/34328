#include<bits/stdc++.h>
using namespace std;
#define forr(i,a,n) for(int i=a; i<n; i++)
#define loop(i,a,n)	for(int i=a; i>=n; i--)
#define conti continue
#define brk break
#define pb push_back
#define SPEED ios_base :: sync_with_stdio(false), cin.tie(NULL), cout.tie(0)
#define in cin>>
#define out cout<<
#define endl <<"\n"
#define ll long long 
#define remove(a,b) a.erase(std::remove(a.begin(),a.end(),b),a.end())
#define runn(it,a) for(auto it = a.begin(); it!= a.end(); it++) 

int main() 
{
	SPEED;	
    
    	int a;
    	in a;
    	int b[a],c;
    	forr(i,0,a)	in b[i];
    	in c;
    	int d[c];
    	forr(i,0,c)	in d[i];
	sort(b,b+a);
	sort(d,d+c);
	if(b[a-1]>d[c-1])	{out b[a-1]<<' '<<d[0];}
	else out b[0]<<' '<<d[c-1];
	
	
	return 0;
}