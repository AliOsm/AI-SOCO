#pragma GCC optimize("Ofast")
#pragma GCC optimize("unroll-loops")
#pragma GCC target("sse,sse2,sse3,ssse3,sse4,popcnt,abm,mmx,avx,tune=native")
#include <bits/stdc++.h>
using namespace std;
int main()
{
	ios::sync_with_stdio(false);
	int n; cin >> n;
	set<int> s;
	for (int i = 0; i < n; i++)
	{
		int a; cin >> a;
		if (a != 0)
			s.insert(a);
	}
	cout << s.size() << endl;
}