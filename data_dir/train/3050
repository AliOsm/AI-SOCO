#include <bits/stdc++.h>
using namespace std;
const int N = 1e6 + 6;
int n;

int main() {
    scanf("%d", &n);
    bool check = true;
    for (int i = 0; i < n; i++) {
        int x; scanf("%d", &x);
        if (x & 1) {
            check = false;
            break;
        }
    }
    if (check) cout << "Second" << endl;
    else cout << "First" << endl;
}
