#include <cstdio>
#include <iostream>
#include <algorithm>
#include <map>
#include <set>
#include <vector>
#include <deque>
#include <string>
#include <cmath>
#include <cctype>
#include <locale>
#include <queue>
#include <utility>

#define forn(n) for(int i = 0; i < n; i++)
#define fornm(n, m) for(int i = 0; i < n; i++) for(int j = 0; j < m; j++)
#define pb push_back
#define pf push_front
#define ll long long
#define ld long double
#define pi 3.141592653589793238462643383

using namespace std;

int main() 
{
   int n; cin >> n;
   set<int> s;
   forn(n)
   {
      int v; cin >> v;
      if(v <= n) s.insert(v);
   }
   cout << (n-s.size()) << endl;
   return 0;
}
