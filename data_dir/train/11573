#include <iostream>
#include <vector>
#include <algorithm>
#include <map>
#include <cstdlib>
#include <cassert>
#include <cmath>
#include <string>
#include <queue>
#include <stack>
#include <iomanip>
#include <set>
#include <deque>
#include <fstream>

using namespace std;

#define forn(i,n) for (int i = int(0); i < int(n); i++)
#define forb(i,n) for (int i = int(n-1); i >= int(0); i--)
#define for1(i,n) for (int i = int(1); i <= int(n); i++)
#define fore(i,a,n) for (int i = int(a); i < int(n); i++)

#define pb push_back
#define mp make_pair
#define fr first
#define sc second
#define sz size()
#define sqr(x) ((x)*(x))
#define cube(x) ((x)*(x)*(x))
#define all(x) x.begin(), x.end()
#define rall(x) x.rbegin(), x.rend()
#define min2(a, b) a < b ? a : b
#define max2(a, b) a > b ? a : b
#define min3(a, b, c) min(a, min(b, c))
#define max3(a, b, c) max(a, max(b, c))
#define dist(x1, y1, x2, y2) sqrt(double (sqr(x1 - x2) + sqr(y1 - y2)))
  
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
 
const int maxn = 1005;
const int MAXN = 2000000;
const int logn = 12;
const int inf = int (2e9);
const int alpha = 26;
const int my_hash = 31;
const long long base = ll (1e18);
const long long my_mod = 2674894331;
const long long l_inf = ll (4e18);
const long long mod = ll (1e9+7);
const double pi = acos(-1.0);
const double eps = 1e-10;

int main() {
	ios_base::sync_with_stdio(0);
	int n, ans = 0;
	cin >> n;
	vector<pair<int, int> > list(n);
	for (int i = 0; i < n; i++)
		cin >> list[i].fr >> list[i].sc;
	for (int i = 0; i < n; i++) {
		bool l = 0, r = 0, u = 0, d = 0;
		for (int j = 0; j < n; j++) {
			if (list[j].fr < list[i].fr && list[j].sc == list[i].sc)
				l = 1;
			if (list[j].fr > list[i].fr && list[j].sc == list[i].sc)
				r = 1;
			if (list[j].fr == list[i].fr && list[j].sc < list[i].sc)
				u = 1;
			if (list[j].fr == list[i].fr && list[j].sc > list[i].sc)
				d = 1;
		}
		ans += l & r & u & d;
	}
	cout << ans;
}