#include <iomanip>
#include <iostream>
#include <vector>
#include <set>
#include <map>
#include <algorithm>
#include <ctime>
#include <cstdio>
#include <cmath>
#include <queue>
#include <bitset>
#include <string>
 
#define ll long long
#define mp make_pair
#define pb push_back
 
using namespace std;

const int MOD = 1e9+7;
const int N = 1000010;

int n;
string s;
 
int main() {
    cin >> n;
    cin >> s;
    int l = 0, r = 0;
    for (int i = 0; i < n; i++) {
        if (s[i] == 'R') r++;
        else l++;
    }
    cout << r + l + 1;
	return 0;
}