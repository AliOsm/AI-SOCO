#include <bits/stdc++.h>
using namespace std;
int main()
{
	int n, m;
	int k, l;
	cin>>n>>m>>k>>l;
	vector<long long>A;
	vector<long long>B;
	for (int i=0; i<n; i++)
	{
		int c;
		cin>>c;
		A.push_back(c);
	}
	for (int i=0; i<m; i++)
	{
		int d;
		cin>>d;
		B.push_back(d);
	}
	reverse(B.begin(), B.end());
	if (A[k-1]<B[l-1])
	{
		cout<<"YES";
		return 0;
	}
	else
	{
		cout<<"NO";
		return 0;
	}
}