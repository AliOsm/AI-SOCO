#include <bits/stdc++.h>
#define fi first
#define se second
#define lo long long 
#define inf 1000000000
#define md 1000000007
#define li 100005
#define mp make_pair
#define pb push_back
using namespace std;
int x,y,z,t1,t2,t3;
int main(){
	scanf("%d %d %d %d %d %d",&x,&y,&z,&t1,&t2,&t3);
	if(abs(z-x)*t2+t3*3+abs(x-y)*t2<=abs(x-y)*t1){
		printf("YES\n");
	}
	else printf("NO\n");
	return 0;
}
