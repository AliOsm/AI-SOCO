#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(NULL);
    int n,count=0;;
    cin>>n;
    unordered_set<int>s;
    for(int i=0;i<n;++i)
    {
        int a;
        cin>>a;
        if(a!=0)
            s.insert(a);
    }
    cout<<s.size();
}