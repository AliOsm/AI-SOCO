#include <bits/stdc++.h>

using namespace std;

int main(){
    int n, k;
    cin >> n;
    int ans = 1;
    for (int i = 0; i < n; i++){
        cin >> k;
        while (k != 0){
            if (k % 2 == 1){
                ans *= 2;
            }
            k /= 2;
        }
        cout << ans << endl;
        ans = 1;
        
    }
    return 0;
}