void brainfuck()
{
    cin>>n;
    map < int , int > mp;
    while( mp.find( n ) == mp.end() )
    {

        mp [ n ] = 1;
        n++;
        while( n % 10 == 0 )n /= 10;
    }
    cout<<mp.size();
}
