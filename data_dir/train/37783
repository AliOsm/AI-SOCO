#include <iostream>
#include <stdio.h>
#include <string>
#include <memory.h>
#include <set>
#include <map>
#include <vector>
#include <queue>
#include <time.h>
#include <assert.h>
#include <cmath>
#include <stack>
#include <string.h>
#include <sstream>
#include <functional>
#include <algorithm>
using namespace std;
typedef long long ll;
int main() {
	int b, k;
	cin >> b >> k;
	int x = 0;
	for (int i = 0; i < k; ++i) {
		int w;
		cin >> w;
		x = (x*b + w) % 2;
	}
	if (x)
		puts("odd");
	else
		puts("even");
	return 0;
}