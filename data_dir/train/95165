#include <bits/stdc++.h>

using namespace std;
const int mod = 1e9 + 7;

int arr[300011][10];

bool isPossible(int num){


}

int main()
{
    int a,b;
    int t;
    cin>>t;
    while(t--){
    cin>>a>>b;

    if(a==b){
        cout<<"0"<<endl;
    }
    else if(a>b){
        if( abs(a-b)%2 ){
            cout<<"2"<<endl;
        }
        else cout<<"1"<<endl;
    }
    else{
        if( abs(a-b)%2 ){
            cout<<"1"<<endl;
        }
        else cout<<"2"<<endl;
    }

    }
    return 0;
}
