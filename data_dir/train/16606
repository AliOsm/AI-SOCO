#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define ld long double
#define oo 666666666

int main()
{
    ios::sync_with_stdio(0);
    int n,x;
    cin>>n;
    map<int,int>mp;
    while(n--)
        cin>>x,mp[x]++;

    int ats = 0;

    for(auto&p:mp)
        ats+=p.second/2;

    cout<<ats/2;
}
