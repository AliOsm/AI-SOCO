#include <iostream>

using namespace std;

int main()
{

    long long x ;
    cin>>x;
    int sum=0;
    while(x>0)
    {
        if(x%10==4 ||x%10==7)sum++;
        x/=10;
    }
    if(sum==7 || sum==4)
    {

        cout<<"YES";

    }
    else cout<<"NO";
    return 0;
}
