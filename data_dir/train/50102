//In the name of God
#include <iostream>
#include <map>
using namespace std;
map <string ,int> m;
int main(){
  int n;
  cin >> n;
  for(int i = 0 ; i < n ; i ++){
    string s;
    cin >> s;
    if(m[s]<=0){
      m[s]=1;
      cout << "OK" << endl;
      continue;
    }
    cout << s<<m[s] <<endl;
    m[s]++;
  }
  return 0;
}
