#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

int main()
{
    int a, b, c, d, e, n;
    cin >>a >> b >> c >> d >> e;
    n = a + b + c + d + e;
    if(n < 5 )
    {
        cout<<-1;
    }
    else if(n % 5 == 0 )
    {
        cout<<n/5;
    }
    else
    {
        cout<<-1;
    }
    return 0;
}
