#include <bits/stdc++.h>
using namespace std;
const long long md=1e9+7;
long long a[1000005],n;

int main(){
	#ifdef ONLINE_JUDGE
	ios_base::sync_with_stdio(0);
	cin.tie(0);cout.tie(0);
	#endif
	long long q,m,k,h,ans=1e9,sum=0,p,x,y;
	cin>>n>>k;
	for(int i=0;i<n;++i){
		cin>>a[i];
		if(k%a[i]==0) ans=min(ans,k/a[i]);
	}
	cout<<ans;
}