#include <bits/stdc++.h>
using namespace std;

int n;
vector<int> arr, l, r, ans;

int main (int argc, char *argv[])
{
	int m;
	scanf("%d%d", &n, &m);	
	arr.resize(n);
	for(int i = 0; i < n; ++i) {
		scanf("%d", &arr[i]);
	}
	l.resize(n);
	l[0] = 0;
	for(int i = 1; i < n; ++i) {
		if(arr[i] >= arr[i - 1]) {
			l[i] = l[i - 1] + 1;
		} else {
			l[i] = 0;
		}
	}
	r.resize(n);
	r[n - 1] = 0;
	for(int i = n - 2; i >= 0; --i) {
		if(arr[i] >= arr[i + 1]) {
			r[i] = r[i + 1] + 1;
		} else {
			r[i] = 0;
		}
	}
	ans.resize(n, 0);
	for(int i = 0; i < n; ++i) { 
		ans[i + r[i]] = max(ans[i + r[i]], r[i] + l[i] + 1);
	}
	int mx = ans[n - 1];
	for(int i = n - 2; i >= 0; --i) {
		ans[i] = max(ans[i], mx - 1);
		mx = ans[i];
	}

	while(m--) {
		int l, r;
		scanf("%d%d", &l, &r);
		--r; --l;
		if(ans[r] >= r - l + 1) {
			printf("Yes\n");
		} else {
			printf("No\n");
		}
	}

	return 0;
}

