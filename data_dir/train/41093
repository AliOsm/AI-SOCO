#include <iostream>
#include <stdio.h>
#include <algorithm>
#include <vector>
using namespace std;
int gl,gr,br,bl;
int main() {
    cin>>gl>>gr>>bl>>br;
    if(bl>=gr-1){
        bl-=gr-1;
        if(bl<=gr+3){cout<<"YES";return 0;}
    
    }
    if(br>=gl-1){
        br-=gl-1;
        if(br<=gl+3){cout<<"YES";return 0;}
        
    }
    cout<<"NO"<<endl;
    return 0;
}
