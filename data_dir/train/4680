#include <iostream>
#include <algorithm>
#include <vector>
#include <math.h>
#include <set>
using namespace std;
int main(){
    string a0,a1;
    cin>>a0>>a1;
    if (a1 == a0)cout<<"YES";
    else if(a0.size()!=a1.size())cout<<"NO";
    else {
        int a11 = 0,a01 = 0;
        for (int  i = 0;i<a1.size();i++){
                if (a1[i] == '1')a11++;
                if(a0[i] == '1')a01++;
            }
        if (a01 == 0 && a11 != 0)cout<<"NO";
        else if(a01 != 0 && a11 == 0)cout<<"NO";
        else cout<<"YES";
    }
    }
















