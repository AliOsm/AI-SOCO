#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define ld long double
#define oo 666666666

int main()
{
    ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    int n;
    string s;
    cin>>n>>s;
    int ats = 0;
    for(int i=0; i<n; i++)
        if(s[i]%2==0)
        ats+=i+1;
    cout<<ats;
}
