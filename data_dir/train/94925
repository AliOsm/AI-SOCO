#include<bits/stdc++.h>
#define st first
#define nd second
#define pb push_back
#define ppb pop_back
#define umax(x,y) x=max(x,y)
#define umin(x,y) x=min(x,y)
#define ll long long
#define ii pair<int,int>
#define iii pair<ii,int>
#define iiii pair<ii,ii>
#define sz(x) ((int) x.size())
#define orta ((bas+son)>>1)
#define all(x) x.begin(),x.end()
#define dbgs(x) cerr<<(#x)<<" --> "<<(x)<<" "
#define dbg(x) cerr<<(#x)<<" --> "<<(x)<<endl;getchar()
#define pw(x) (1<<(x))
#define inf 2000000000
#define MOD 1000000007
#define N 100005
#define MAX 5000000
#define LOG 100
#define KOK 333
using namespace std;

int main() {
	char s[105];
	int n,t8=0,to=0;


	//freopen("input.txt","r",stdin);

	scanf("%d",&n);

	scanf("%s",s+1);

	for(int i=1;i<=n;i++) {

		t8+=s[i]=='8';
		to+=s[i]!='8';

	}

	int ans=0;

	for(int i=1;i<=n;i++) {

		if(i>t8) break ;

		int tut=to+t8-i;

		if(tut/10>=i) ans=i;

	} 

	printf("%d",ans);

}