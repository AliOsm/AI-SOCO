#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define all(x) x.begin(),x.end()

int const N = 200001;

int main(){
    #ifndef ONLINE_JUDGE
    freopen("read.txt","r",stdin);
    #endif
    int t, n;
    scanf("%d", &t);
    while(t--){
        scanf("%d", &n);
        ll an = (1ll * n / 0.9);
        printf("%lld\n", an);
    }
} 