#include <bits/stdc++.h>
typedef long long ll ;
using namespace std;

int main()
{
    int k = 0 , r = 0 ;

    cin >> k >> r ;


    int i = 1 , c = k ;

    while(k % 10 && (k % 10) != r )
    {
       k = c * i ;
       i++;
    }
    if(i > 1)
        i-- ;

    cout << i << endl ;

    return 0;
}
