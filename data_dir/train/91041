#include <bits/stdc++.h>
using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(nullptr);
	int t;
	cin >> t;
	while (t--) {
		int n;
		cin >> n;
		if (n == 1) {
			cout << "-1\n";
		} else {
			if ((n - 1) % 3) {
				for (int i = 1; i < n; i++) {
					cout << '2';
				}
				cout << "3\n";
			} else {
				for (int i = 2; i < n; i++) {
					cout << '2';
				}
				cout << "43\n";
			}
		}
	}
	return 0;
}
