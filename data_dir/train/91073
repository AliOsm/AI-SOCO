#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

string s, t;
inline int val( char x ) { return ( x == 'a' or x == 'e' or x == 'i' or x == 'o' or x == 'u' ); }


int main()
{
    optimiza_io
    cin >> s >> t;
    if( s.size() != t.size() )
    {
        cout << "NO\n";
        return 0;
    }
    
    for( int i = 0; i < s.size(); i ++ )
        if( val( s[i] ) != val( t[i] ) )
        {
            cout << "NO\n";
            return 0;
        }
    cout << "YES\n";
    return 0;
}

