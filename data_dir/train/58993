#include <iostream>
#include <cstdio>
#include <algorithm>
#include <string.h>
using namespace std;

const char s[] = {'R', 'O', 'Y', 'G', 'B', 'I', 'V' };
int n;

int main() {
    scanf("%d", &n);
    for(int i = 0; i < n - 3; i++) {
            printf("%c", s[i % 4]);
    }
    printf("BIV");
    
    
    return 0;
}
