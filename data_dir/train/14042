#include<bits/stdc++.h>

using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);

    int n, m, k;
    cin >> n >> m >> k;
    int a = (k + 1) / 2 % m;
    cout << ((k + 1) / 2 - 1) / m + 1 << ' ' << (a ? a : m) << ' ' << "RL"[k % 2] << '\n';

    return 0;
}
