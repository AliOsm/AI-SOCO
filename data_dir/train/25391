#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define f first
#define s second
#define bp push_back
#define mp make_pair
const int mx=1e5+10;
const double eps=1e-9;
const int inf=1e9;
int a[mx];
main(){
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int q;
    scanf("%d",&q);
    while(q--){
        int l,r,d;
        int ans=inf;
        scanf("%d%d%d",&l,&r,&d);
        if(d<l|| d>r)printf("%d\n",d);
        else printf("%d\n",(r/d+1)*d);
    }



}
