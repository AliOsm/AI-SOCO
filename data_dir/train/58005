# include <stdio.h>
# include <string.h>
# include <math.h>


# define iabs(x) ( (x) < 0 ? (-(x)) : (x) )
# define imax (a,b) ( (a) > (b) ? (a) : (b) )
# define imin (a,b) ( (a) < (b) ? (a) : (b) )
# define isalpha (c) ( (c) >= 'a' && (c) <= 'z' )
# define isAlpha (c) ( (c) >= 'A' && (c) <= 'Z' )
# define isdigit (c) ( (c) >= '0' && (c) <= '9' )


const int N = 100010;


int n;
int m, k, a, b, c, r;
bool ok;

void Print ();

int main ()
{
# ifndef ONLINE_JUDGE
   freopen ("A.txt", "rt", stdin);
   freopen ("A.out", "wt", stdout);
# endif

   scanf ("%d %d", &n, &a);
   m=a, k=1, r=1;

   for ( int i=2; i <= n; i++ )
   {
      scanf ("%d", &a);
      if ( a < m )
         m = a, k = 1, r = i;
      else if ( a == m )
         k++;
   }

   if ( k == 1 )
      printf ("%d", r);
   else
      printf ("Still Rozdil");

   return 0;
}

void Print ()
{

}
