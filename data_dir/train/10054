#include <bits/stdc++.h>
#define ll long long
#define MOD 1000000007

using namespace std;

const double pi = acos(-1);

string s;

int main()
{
  for(int i=0; i<8; i++){
    cin >> s;
    for(int j=0; j<8; j++){
      if(s[j] == s[(j + 1) % 8]){
        puts("NO");
        return 0;
      }
    }
  }
  puts("YES");
  return 0;
}
