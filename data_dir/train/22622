#include <stdio.h>
#include <string.h>
#include <iostream>
#include <string.h>
#include <vector>
#include <algorithm>

const int inf=0x3f3f3f3f;

using namespace std;

int n,a[4001];

int main()
{
	scanf("%d",&n);
	for(int i=0;i<n;i++)
	{
		int x;
		scanf("%d",&x);
		a[x]=1;
	}
	for(int i=1;1;i++)
	{
		if(a[i]) continue;
		printf("%d\n",i);
		return 0;
	}
	return 0;
}
