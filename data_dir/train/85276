int p[MAXN];
void solution()
{
        cin >> n;
        n *= 2;
        for( int i = 1; i <= n; i++ ) cin >> a[i];
        sort( a + 1, a + 1 + n );
        if( a[1] == a[n] ){
                cout << "-1" << endl;
                return;
        }
        for( int i = 1; i <= n; i++ ) cout << a[i] << " ";



}
/*
