    #include <bits/stdc++.h>
    #define LL long long
    #define LD long double

    #define IQ ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    #define S second
    #define F first
    #define PI acos(-1)
    #define MAX 5000005
    #define file freopen("input.txt","r",stdin); freopen("output.txt","w",stdout);
    #define inf 1000000

    using namespace std;


    int main()
    {
        IQ
        LL n;
        cin>>n;
        int c=0;
        set<int>st;
        while(n>0 && c<1e7)
        {
            st.insert(n);
            n++;
            c++;
            while(n%10==0)
                n/=10;
        }
        cout<<st.size();





            return 0;
    }




