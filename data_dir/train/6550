#define mendl "\n"
#define N 104//10000001
#define M N*4//(1<<19) + 100 10000001
#define P 2
#define K 32
#define MOD 1000000007//1e9 + 7   ((1LL << 30) + 1LL)//1000000007LL//1000000007LL  
#define MOD1 1000000000000007LL//1000000007LL  
#define INF 1000000000000000001LL//18
#define msq(px) (p2(px.first) + p2(px.second)) 
#define PI 3.141592653589793238462643383279 
#define EPS 1e-5

#define Mod(a) ((a) % MOD)

mt n;
mt cnt[N], t;
bool ok;
int main()
{
#ifdef _DEBUG	
	freopen("in.txt", "r", stdin);
#endif
	//std::ios::sync_with_stdio(false);
	scanf("%lld", &n);
	rep(0, n, i)
		scanf("%lld", &t), ++cnt[t];
	rep(0, N, i)
		rep(i + 1, N, j)
		if (cnt[i] == cnt[j] && cnt[i] == (n >> 1))
		{
			printf("YES\n%lld %lld\n", i, j);
			ok = true;
		}
	if (!ok)
		puts("NO");
	return 0;
}