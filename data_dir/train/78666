#include<bits/stdc++.h>
#define ll long long
#define f first
#define s second
#define pb push_back
using namespace std;

int main(){
    ios::sync_with_stdio(false);
    string s;
    ll n,a=0,b=0;
    cin >> n;
    cin >> s;
    
    for(int i=0; i<s.size(); i++){
        if(s[i] == 'n')a++;
        if(s[i] == 'z')b++;
    }
    for(int i=1; i<=a; i++)cout<<"1 ";
    for(int i=1; i<=b; i++)cout<<"0 ";
    
    
    
    
    return 0;
}
