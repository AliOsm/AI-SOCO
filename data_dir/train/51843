#include <bits/stdc++.h>

using namespace std;

int main(){
    char s[100+5];
    scanf("%s",s);

    for(int i=0,len=strlen(s);i<len;i++){
        if(s[i]!='a' && s[i]!='e' && s[i]!='i' && s[i]!='o' && s[i]!='u'){
            if(s[i]!='n') {
                if(s[i+1]!='a' && s[i+1]!='e' && s[i+1]!='i' && s[i+1]!='o' && s[i+1]!='u'){
                    printf("NO\n");
                    return 0;
                }
            }
        }
    }
    printf("YES\n");

    return 0;
}
