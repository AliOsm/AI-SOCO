#include <bits/stdc++.h>
#include <iostream>
using namespace std;

int main () 
{   
    ios_base::sync_with_stdio(false);
    long long a,b,n;
    cin>>a>>b>>n;
    bool bb = false;
        for(int i = 0; i <= 9; ++i){
            if((a * 10 + i) % b == 0){
                cout<<a * 10 + i;
                for(int i = 0; i < n - 1; ++i){
                    cout<<"0";
                }
                bb = true;
                break;
            }
        }
        if(bb == false){
            cout<<-1<<endl;
            return 0;
    }
       
        return 0;
}