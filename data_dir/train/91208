#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define pb push_back
#define mp make_pair
#define F first
#define S second
const ll N = 200005;
void go() {
	ll n, i, x;
	bool odd, even;
	cin >> n;
	odd = even = false;
	while(n--) {
		cin >> x;
		odd |= (x % 2 == 1);
		even |= (x % 2 == 0);
	}
	if(odd && even) cout << "NO\n";
	else cout << "YES\n";
	return;
}
int main() {
	ll t;
	cin >> t;
	while(t--)
		go();
	return 0;
}
