#include <cctype>
#include <string>
#include <iostream>
#include <algorithm>
#include <vector>
#include <set>
#include <cmath>
#include <map>
#include <queue>
#include <functional>
#include <iomanip>

void solve() {
  int l, r;
  std::cin >> l >> r;
  if (l == r) {
    std::cout << r;
  } else {
    std::cout << 2;
  }
}

int main () {
    std::ios_base::sync_with_stdio(false);
    std::cin.tie(nullptr);
    solve();
    return 0;
}

