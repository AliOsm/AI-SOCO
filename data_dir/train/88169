typedef long long LL;
typedef struct data sd;
int gcd(int a, int b)
{
    return b==0?a:gcd(b, a%b);
}
int main()
{
    //use %I64d
    //read;
    int p, q, f, s, i, j, k, t, r;
    in2(p, q);
    in(f);
    for (i=0; i<p; i++) in(t);
    in(s);
    for (i=0; i<q; i++) in(t);


    if (p<q)
    {
        printf("0/1\n");
    }
    else if (p>q)
    {
        f*s>=0?printf("Infinity\n"):printf("-Infinity\n");
    }
    else
    {
        k=gcd(f, s);
        f*s>=0?printf("%d/%d\n", abs(f/k), abs(s/k)):printf("-%d/%d\n", abs(f/k), abs(s/k));
    }

//file?
return 0;
}
