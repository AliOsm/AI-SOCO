/// I won’t run away anymore…
/// I won’t go back on my word…
/// That is my ninja way!

#include<bits/stdc++.h>
using namespace std ;
const int N = 3e5+5 ;
int t ,n ,k1 ,k2 ,mx1 ,mx2 ,x ;
int main(){
    scanf("%d",&t);
    while(t--){
        mx1 = mx2 = 0 ;
        scanf("%d%d%d",&n,&k1,&k2);
        for(int i=0;i<k1;++i){
            scanf("%d",&x);
            mx1 = max(mx1,x);
        }
        for(int i=0;i<k2;++i){
            scanf("%d",&x);
            mx2 = max(mx2,x);
        }
        puts(mx1>mx2?"YES":"NO");
    }
    return 0;
}
