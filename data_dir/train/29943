#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
#define ss second
#define ff first
#define pb push_back
#define mp make_pair

int main(){
    ios_base::sync_with_stdio(0);
    int t;
    cin>>t;
    while(t--){
        ll x;
        cin>>x;
        ll a=0;
        ll o=1;
        while(o<=x)o*=10;
        o/=10;
        while(x>0){
            x-=o;
            a+=o;
            x+=o/10;
            if(o>x)o/=10;
            
            
        }
        cout<<a<<endl;
    }
    return 0;
}
