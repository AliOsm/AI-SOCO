#include <bits/stdc++.h>

using namespace std;

typedef long long intt;

int main()
{
    int n;
    cin>>n;
    int a[9];
    int i;
    for(i=1;i<=7;i++)
        cin>>a[i];
        int sum=0;
        int day=0;
        i=1;
        while(sum<n)
        {
            if(day==7)
            {
                day=0;
                i=1;
            }
            sum+=a[i];
            i++;
            day++;

        }
        cout<<day;






    return 0;
}
