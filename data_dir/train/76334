# include <iostream>

using namespace std;

int m, n;

int main ()
{
   cin >> n;

   if ( n % 2 == 1 )
	   m = 3;
   else
      if ( n >= 900 )
         m = n - 2;
      else
         m = n + 2;

   cout << m;
/*
   cout << endl << n*m+1 << endl;
   system ("pause");
*/
   return 0;
}
