int a[111111],x[111111],y[111111],z[111111];

int j,i,n,m=0,k,t;
 int ans=0; 
 
int po(int x,int y,int t) {
  if (y==0) return t; else return po(y,x%y,t+x/y);
              
    
} 
int main() {
// freopen("input.txt","r",stdin);
// freopen("output.txt","w",stdout);
 cin>>n;
 
 for (i=0;i<n;i++) {
     int l,r;
     cin>>l>>r;
     cout<<po(l,r,0)<<endl;
     
     
     } 
  
 // system("pause");  
}
