#include <bits/stdc++.h>

using namespace std;
typedef long long ll;
typedef pair<int, int> pi;


int main() {
  ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
  int q;
  cin >> q;
  while (q--) {
    ll l, r, d;
    cin >> l >> r >> d;
    if (d < l) cout << d << "\n";
    else cout << d*((r+1+d-1)/d) << "\n";
  }
  return 0;
}

