typedef long long ll;
typedef unsigned long long ull;
typedef long double ld;
typedef vector<int> vi;
typedef pair<ll, ll> pll;
typedef pair<int, int> pii;

string itosm(ll x){
    if(x == 0)
        return "0";
    string ans = "";
    while(x > 0){
        ans +=((x%10) + '0');
        x/=10;
    }
    reverse(ans.begin(), ans.end());
    return ans;
}

ll stoim(string str){
    ll ans = 0;
    ll k = 1;
    for(int i = str.length()-1; i >= 0; i--){
        ans+=(str[i]-'0')*k;
        k*=10;
    }
    return ans;
}

const ll infll = 1e18 + 3;
const int inf = 1009000999;
const double eps = 1e-7;
const int maxn = 1e6 + 77;
const int baseint = 1000200013;
const ll basell = 1e18 + 3;
const ld PI = acos(-1.0);
const ll mod = 1e9 + 7;

int a[maxn];

void write(int x){
    if(x & 1)
        cout << "odd\n";
    else
        cout << "even\n";
}

void solve(){
    int n;
    cin >> n;
    for(int i = 0; i < n; i++)
        cin >> a[i];
    int cnt = 0;
    for(int i = 0; i < n; i++)
        for(int j = 0; j < i; j++)
            if(a[j] > a[i])
                cnt++;
    int m;
    cin >> m;
    while(m--){
        int l, r;
        cin >> l >> r;
        int ll = r - l + 1;
        ll = (ll - 1) * ll /2;
        if(ll & 1)
            cnt = cnt + 1;
        else
            cnt = cnt + 0;
        write(cnt);
    }
}

