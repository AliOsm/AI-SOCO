#include <cstdio>

int main(void) {
    int n, t;
    scanf("%d%d", &n, &t);
    if(t==10 && n==1) {
        puts("-1");
        return 0;
    }
    if(t==10) {
        printf("1");
        for(int i=1;i<n;i++) printf("0");
        puts("");
    } else {
        for(int i=0;i<n;i++) printf("%d", t);
        puts("");
    }
    return 0;
}