#include <bits/stdc++.h>

using namespace std;

int t, x, y;

int main()
{
  cin >> t;
  while (t--) {
    cin >> x >> y;
    if (x == y) {
      cout << "YES\n";
    }
    else if (x > 3) {
      cout << "YES\n";
    }
    else if (x == 3 && (y == 1 || y == 2)) {
      cout << "YES\n";
    }
    else if (x == 2 && (y == 1 || y == 3)) {
      cout << "YES\n";
    }
    else if (x == 1 && y == 1) {
      cout << "YES\n";
    }
    else
      cout << "NO\n";
  }
  return 0;
}
