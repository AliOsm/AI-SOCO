#include <bits/stdc++.h>
using namespace std;

#define F first
#define S second
#define ll long long
#define ld long double
#define ull unsigned long long
#define sz(v)  ((int)((v).size()))
#define all(v) ((v).begin()),((v).end())
#define db(x) cout << " WHY " <<  x << '\n'
#define MEM(dp,i) memset( dp ,i, sizeof (dp))
#define forr(c,x,n)   for(ll c=x;c<=(ll)n; ++c)
#define forrr(c,x,n)  for(ll c=x;c>=( ll )n ;--c)
#define ICPC ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);


int main()
{ ICPC;
string s ;int n ;
cin >> n ;
cin >> s ;

int a[500] = {0} , cnt = 0;

forr ( i , 0 , s.size()-1 )
{
    a[s[i]]++;
}
forr ( i , 0 , 499 )
{
    if ( a[i] >=  2 || s.size() == 1  )return cout <<"YES" , 0;

}
cout << "NO";

}
