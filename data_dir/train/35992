string YN[] = {"NO", "YES"};


#define gcd(a, b) __gcd(a, b)
#define lcm(a, b) ((a)*((b)/gcd(a,b)))


inline int gcd_e( int a, int b, int & x, int & y )
{
        if ( !a ) {
                x = 0;
                y = 1;
                return b;
        }
        int x1, y1;
        int d = gcd_e ( b % a, a, x1, y1 );
        x = y1 - ( b / a ) * x1;
        y = x1;
        return d;
}


inline int div_mod( int a, int b, int m = mod )
{
        int x, y;
        int g = gcd_e( b, m, x, y );
        x = ( x % m + m ) % m;
        return ( a * x ) % m;
}


int pow_mod ( int a, int n, int m = mod )
{
        int res = 1;
        while ( n ) {
                if ( n & 1 )
                        res = ( res * a ) % m;
                a = ( a * a ) % m;
                n >>= 1;
        }
        return res;
}




