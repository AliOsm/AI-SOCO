#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin >> n;
    int left  = 0, rgt = 0;
    for(int i = 0; i < n; i++){
        int x,y;
        cin >> x >> y;
        if(x < 0)left++;
    }
    if(left == n || left == 1 || left == 0 || left == n-1)cout << "Yes\n";
    else cout << "No\n";
    return 0;
}
