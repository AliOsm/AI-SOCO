#include<bits/stdc++.h>

using namespace std;

typedef long long ll;

ll solve(int n)
{

}

int main()
{
    int n,k;
    cin>>n>>k;
    int tot=0;
    for(int i=0;i<n;i++)
        for(int j=i+1;j<n;j++)
        tot++;
    if(tot<=k)
        cout<<"no solution"<<endl;
    else
    for(int i=0;i<n;i++)
        cout<<0<<" "<<i<<endl;
    return 0;
}

