#include <cstdio>
#include <cstdlib>
#include <cstring>
#include <cmath>
#include <vector>
#include <algorithm>
#define SZ(x) ((int)(x).size())
#define FOR(it, c) for(__typeof((c).begin()) it = (c).begin(); it != (c).end(); ++it)
using namespace std;
typedef long long LL;

int main(void) {
  int sum=0;
  int x;
  for(int i=0;i<5;i++) { scanf("%d", &x); sum += x; }
  if (sum==0 || sum%5) puts("-1");
  else printf("%d\n", sum/5);
  return 0;
}
