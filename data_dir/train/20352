#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define pb push_back
#define mk make_pair
#define lw lower_bound
ll a[1234567],b[1234567],c,d,e,i,j,n,l,r;
vector<ll> v;
map<ll,ll> m;
pair<ll,ll> p[1234567];
string s,s1;
int main(){
	ios_base::sync_with_stdio(NULL);
	cin.tie(NULL);
	cout.tie(NULL);
	cin>>n;
	while(n--){
		cin>>c>>d>>e;
		j=c+d+e;
		l=max(c,max(d,e));
		if(2*(j-l)+1>=j){
			cout<<"Yes";
		}
		else cout<<"No";
		cout<<endl;
	}
}