#include <stdio.h>

#include<iostream>

#include<string>

#include<cstring>

using namespace std;

int main()
{
    string str;
    getline(cin, str);
    string al="abcdefghijklmnopqrstyvwxuz";
    int k=0;
    int kol=0;
    while(k!=26)
    {
        
        if(str.find(al[k])!=-1)
        {
            kol++;
        }
        k++;
    }
    cout<<kol;
    return 0;
}