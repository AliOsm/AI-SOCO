int a[100 + 228];

int main() {
	//files;
	SWS;
	int n;
	cin >> n;
	for (int i = 1; i < n; ++i) {
        cin >> a[i];
	}
	int x, y, ans = 0;
	cin >> x >> y;
	for (int i = x; i < y; ++i) {
        ans += a[i];
	}
	cout << ans;
	return 0;
}
