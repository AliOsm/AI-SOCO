#include<bits/stdc++.h>
using namespace std;

#define F first
#define S second
#define ii pair < int , int >
#define LS pair < ll , string >
typedef long long ll;
const int inf = 1<<30;
const int N =100100;
int n  , q ;
int a , b ;
int main ( ){
 cin >> a >> b ;
 int ans = min  (a  ,b );
 ll fact = 1 ;
 for ( int i = 1 ; i <= ans ; i++ )
     fact *= i ;
 cout << fact ;
return 0 ;
}
