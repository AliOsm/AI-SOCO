#include <bits/stdc++.h>
using namespace std;
#define pb push_back
#define ulli unsigned long long int
#define LL long long int
#define FOR(i,begin,end) for (LL i=begin;i<=end;i++)
#define rep(i,t) for (LL i=0;i<t;i++)
const int MAX=500007,N=117;
ifstream myfile("input.txt");
ofstream mine("output.txt");
string x;
LL n;
int main()
{
  myfile>>x>>n;
  if(x=="front")
  {
    if(n==1)mine<<"L\n";
    else{mine<<"R\n";}
  }
  else
  {
    if(n==1)mine<<"R\n";
    else{mine<<"L\n";}
  }
  return 0;
}