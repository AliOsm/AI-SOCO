#include <algorithm>
#include <iostream>
#include <vector>
using namespace std;

int main() {
    int t; cin >> t;
    while ( t-- ) {
        int n; cin >> n;
        vector<int> numbers(n);
        for ( auto& x: numbers ) cin >> x;
        sort(numbers.rbegin(), numbers.rend());
        for ( auto x : numbers ) cout << x << " ";cout << endl;
    }
    return 0;
}
