//++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
//++--++--++--++--++--++--++--++--++--+--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
const int MAXN = 5001;
const int inf = 1000000000;
const ld INF = 1.0 / 0.0;
const ld EPS = 1e-7;
const int mod = inf + 7;
bool check(vector<int> cnt,  int mid, int n)
{
	int now = 0;
	for (int i = 0; i < 101; ++i)
	{
		now += cnt[i] / mid;
	}
	return now >= n;
}
signed main()
{
	fastio();
	int n, m;
	cin >> n >> m;
	vector<int> a(m);
	vin(a);
	sort(all(a));
	vector<int> cnt(101);
	for (int i : a) cnt[i] ++;
	int l =0, r = m;
	while (r - l > 1)
	{
		int mid = (l + r) / 2;
		if (check(cnt, mid, n))l = mid;
		else r = mid;
	}
	if (!check(cnt, r, n))
		r--;
	cout << r;
	//sp;
}
