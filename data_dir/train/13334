#include <bits/stdc++.h>

using namespace std;


int main(){
  ios::sync_with_stdio(false);
  cin.tie(0);
  cout.tie(0);
  int n;
  string s;
  cin >> n >> s;
  int d = 0 , a = 0;
  for(int i = 0 ; i < n ; i++){
    if(s[i] == 'A'){
      a++;
    }
    if(s[i] == 'D'){
      d++;
    }
  }
  if(a > d){
    cout << "Anton" << endl;
  }else{
    if(d > a){
      cout << "Danik" << endl;
    }else{
      cout << "Friendship" << endl;
    }
  }
  return 0;
}