#include<bits/stdc++.h>

using namespace std;

int main(){

    int n;
    cin>>n;

    int ax,ay,bx,by,cx,cy;
    cin>>ax>>ay;
    cin>>bx>>by;
    cin>>cx>>cy;

    if((bx>ax && cx<ax) || (bx<ax && cx>ax) ||(by>ay && cy<ay) || (by<ay && cy>ay)){
        cout<<"NO";
    }
    else{
        cout<<"YES";
    }
}
