#include <iostream>
#include <sstream>
#include <string>
#include <vector>
#include <deque>
#include <queue>
#include <set>
#include <map>
#include <algorithm>
#include <functional>
#include <utility>
#include <cmath>
#include <cstdlib>
#include <ctime>
#include <cstdio>

using namespace std;

#define REP(i,n) for((i)=0;(i)<(int)(n);(i)++)
#define foreach(c,itr) for(__typeof((c).begin()) itr=(c).begin();itr!=(c).end();itr++)

int main(void){
    int i,j,n;
    string s;
    
    cin >> s;
    n = s.length();
    
    string t = "hello";
    j = 0;
    REP(i,n) if(j < 5 && s[i] == t[j]) j++;
    
    cout << ((j == 5) ? "YES" : "NO") << endl;
    
    return 0;
}
