#include <bits/stdc++.h>
using namespace std;

const int N = 3e5 + 5;

int l, r, n;
string s;
pair<char, int> c[N];

int main(){



    cin >> n >> s;
    for (int i = 0; i < s.size() - 1; ++i){
        if (s[i] > s[i + 1]){
            cout << "YES" << '\n';
            cout << i + 1 << ' ' << i + 2;
            return 0;
        }
    }
    cout << "NO";


}
