#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,m,a=0,b=0,c,d,i,j,k,l;
    cin>>n;
    string nm;
    cin>>nm;
    for(i=0;i<n;i++)
    {
        if(nm[i]=='0')
            a=a+1;
        if(nm[i]=='1')
            b=b+1;

    }
    c=a;
    if(c>b)
        c=b;
    n=n-(c*2);
    cout<<n;
    return 0;
}
