// rc.cpp : Defines the entry point for the console application.
//

#include <math.h>

#include <set>
#include <list>
#include <cmath>
#include <ctime>
#include <map>
#include <deque>
#include <queue>
#include <stack>
#include <string>
#include <bitset>
#include <cstdio>
#include <limits>
#include <vector>
#include <climits>
#include <cstring>
#include <cstdlib>
#include <fstream>
#include <numeric>
#include <sstream>
#include <iostream>
#include <algorithm>
#include <unordered_map>
#include <functional>
#include <iomanip>
#include <iterator>
#include <regex>

#include <intrin.h>

using namespace std;

#define PI 3.14159265358979323846
#define EPS 1.E-8

#define FIN(a, n) { for (ul _ = 0; _ < n; ++_) cin >> (a)[_]; }
#define FOUT(a, n) { cout << a[0]; for (ul _ = 1; _ < n; ++_) cout << ' ' << a[_]; cout << '\n'; }

typedef unsigned char uc;
typedef unsigned int ui;
typedef unsigned long long ul;
typedef long long sl;

int main(int argc, char* argv[])
{
    int r, c;
    cin >> r >> c;

    if (r == 1 && c == 1)
        cout << "0\n";
    else
    {
        if (c == 1)
        {
            for (int i = 0; i < r; ++i)
                cout << i + 2 << '\n';
        }
        else
        {
            for (int i = 0; i < c; ++i)
                cout << i + 2 << ' ';
            cout << '\n';

            for (int i = 1; i < r; ++i)
            {
                for (int j = 0; j < c; ++j)
                {
                    cout << (j + 2) * (i + c + 1) << ' ';
                }
                cout << '\n';
            }
        }
    }
    return 0;
}