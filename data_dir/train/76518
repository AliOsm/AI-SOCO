#include<bits/stdc++.h>
using namespace std;
#define F first
#define S second
#define ll long long
#define pb push_back
#define pf push_front
#define pi pair<int,int>
#define pii pair<int,pi>
#define go ios::sync_with_stdio(0); cin.tie(0); cout.tie(0)
int t;
int main()
{
    go;
    cin>>t;
    while(t--)
    {
        ll a,b,c,d;
        cin>>a>>b>>c>>d;
        ll x=b;
        ll y=c;
        ll z=c;
        if(x+y>z)
        cout<<x<<" "<<y<<" "<<z<<endl;
        else
            assert(0);
    }
    return 0;
}
