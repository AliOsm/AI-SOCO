#include<bits/stdc++.h>
using namespace std;
#define ll long long
ll i,j;
int main()
{
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    ll n,k;
    cin>>n>>k;
    for(i=1;i<=50;i++)
    {
        ll tmp=1;
        for(j=1;j<=i;j++)
            tmp*=2;
        ll tmp2=tmp/2;
        if((k-tmp2)%tmp==0)
        {
            cout<<i<<endl;
            return 0;
        }
    }
}

