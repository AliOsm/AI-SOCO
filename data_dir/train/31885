int k, h, m, n, a [N], h1, m1, h2, m2, t1, t2, t;
char s [N], c;
bool ok, z1, z2;


int F ();


int main ()
{
# ifndef ONLINE_JUDGE
   freopen ("A.TXT", "r", stdin);
   freopen ("A.OUT", "w", stdout);
# endif

   ios_base::sync_with_stdio (false);
   cin.tie (NULL);

   cin >> h1 >> c >> m1;
   cin >> h2 >> c >> m2;

# ifndef ONLINE_JUDGE
   cout << "*** t1 = " << h1 << ":" << m1 << endl;
   cout << "*** t2 = " << h2 << ":" << m2 << endl;
   cout << endl;
# endif

   t1 = h1 * 60 + m1;
   t2 = h2 * 60 + m2;
   t = (t1 + t2) / 2;

   h = t / 60, m = t % 60;

   z1 = h < 10, z2 = m < 10;

   cout << ( z1 ? "0" : "" ) << h << ":" << ( z2 ? "0" : "" ) << m << endl;

//   system ("pause");
   return 0;
}

