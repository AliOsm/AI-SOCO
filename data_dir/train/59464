typedef long long ll;

using namespace std;
int n,a[111111];
int main()
{
	cin>>n;
	for(int i=0; i<n; i++)
		a[i]=i%4;
	for(int i=0; i<n; i++)	
		cout<<(char)(a[i]+'a');
	return 0;
}
