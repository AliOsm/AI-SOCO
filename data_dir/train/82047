#include<bits/stdc++.h>
using namespace std;
#define forr(i,a,n) for(int i=a; i<n; i++)
#define loop(i,a,n)	for(int i=a; i>=n; i--)
#define conti continue
#define brk break
#define pb push_back
#define SPEED ios_base :: sync_with_stdio(false), cin.tie(NULL), cout.tie(0)
#define in cin>>
#define out cout<<
#define endl <<"\n"
#define ll long long 
#define remove(a,b) a.erase(std::remove(a.begin(),a.end(),b),a.end())
#define runn(it,a) for(auto it = a.begin(); it!= a.end(); it++) 
#define fi first
#define se second

int main() 
{	
	int m; in m; while(m--){int flag=0;
	string a,b,c; in a>>b>>c;
	forr(i,0,a.size())
	{
		if(a[i]==c[i]) conti;
		else if(b[i]==c[i])	conti;
		else {out "NO" endl; flag=1; brk;}
	}if(flag==0){
	out "YES" endl;}}
	return 0;	
}