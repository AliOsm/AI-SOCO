#include <bits/stdc++.h>
using namespace std;

typedef long long ll;


int main()
{
  string str;
  int n, m;
  cin >> n >> m;
  cin >> str;
  for (int i = 0; i < m; ++i)
    {
      int l, r;
      char c1, c2;
      scanf("%d %d %c %c", &l, &r, &c1, &c2);
      for (int i = l-1; i <= r-1; ++i)
	if (str[i] == c1)
	  str[i] = c2;
    }
  cout << str << endl;

}  
