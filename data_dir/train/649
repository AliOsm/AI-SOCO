#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <map>

using namespace std;

long long i,j,k,l,m,n,s,t,a[12345],b[12345];
string p,q,r,w;
map <string, string> x,y;
int main() {
    cin >>n>>m;
    for (i = 0; i<n; i ++){
        cin >> p>>q;
        q = q + ";";
        x[q] = p;
    }
    for (i = 0; i<m; i ++){
        cin >> p>>q;
        
        cout << p<<' '<<q<<' '<<'#'<<x[q]<<endl;
    }
    return 0;
}
