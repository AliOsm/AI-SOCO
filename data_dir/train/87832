#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int a,b,c,d,e,i,j,k,l,n,m,x,y,t,p;
    scanf("%lld",&t);
    for(p=1;p<=t;p++)
    {
        //vector<char>vec;
        scanf("%lld%lld",&n,&k);
        char ch='a';
        for(i=1;i<=n;i++)
        {
            a=i%k;
            printf("%c",(ch+a));
        }
        printf("\n");

    }
}
