#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main()
{
    int n;
    cin >> n;
    int k;
    cin >> k;
    int t;
    cin >> t;
    int res;
    if (t<=k)
    {res=t;}
    else if (t>=n)
    {res=k-(t-n);}
    else res=k;
    cout << res;
    return 0;
}
