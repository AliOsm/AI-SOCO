#include <cstdio>
#include <algorithm>
#include <vector>
#include <cmath>
#include <cstring>
#include <string>
#include <iostream>
#include <stack>
#include <queue>
#include <deque>
#include <set>
#include <map>

using namespace std;

typedef long long LL;
typedef long double LD;
typedef pair<int, int> PII;

int n, k;

void solve()
{
	scanf("%d%d", &n, &k);
	n /= k;
	n *= k;
	n += k;
	printf("%d\n", n);
}

int main()
{
	solve();
	return 0;
}
