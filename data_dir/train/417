#include<bits/stdc++.h>
using namespace std;
int main(){
    string s;
    cin>>s;

    int len=s.size();
    int ans=1;
    for(int i=0;i<=len/2;i++){
        int flag=0;
        if(s[i]=='b' && s[len-i-1]=='d') flag=1;
        else if(s[i]=='d' && s[len-i-1]=='b') flag=1;
        else if(s[i]=='o' && s[len-i-1]=='o') flag=1;
        else if(s[i]=='p' && s[len-i-1]=='q') flag=1;
        else if(s[i]=='q' && s[len-i-1]=='p') flag=1;
        else if(s[i]=='v' && s[len-i-1]=='v') flag=1;
        else if(s[i]=='w' && s[len-i-1]=='w') flag=1;
        else if(s[i]=='x' && s[len-i-1]=='x') flag=1;
        else if(s[i]=='A' && s[len-i-1]=='A') flag=1;
        else if(s[i]=='A' && s[len-i-1]=='A') flag=1;
        else if(s[i]=='H' && s[len-i-1]=='H') flag=1;
        else if(s[i]=='I' && s[len-i-1]=='I') flag=1;
        else if(s[i]=='M' && s[len-i-1]=='M') flag=1;
        else if(s[i]=='O' && s[len-i-1]=='O') flag=1;
        else if(s[i]=='T' && s[len-i-1]=='T') flag=1;
        else if(s[i]=='U' && s[len-i-1]=='U') flag=1;
        else if(s[i]=='V' && s[len-i-1]=='V') flag=1;
        else if(s[i]=='W' && s[len-i-1]=='W') flag=1;
        else if(s[i]=='X' && s[len-i-1]=='X') flag=1;
        else if(s[i]=='Y' && s[len-i-1]=='Y') flag=1;

        if(flag==0){ans=0;break;}
    }
    if(ans==1) printf("TAK");
    else printf("NIE");
}
