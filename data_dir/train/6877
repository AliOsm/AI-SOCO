#include <bits/stdc++.h>
#define pb push_back
#define mp make_pair
#define pp pair<int, int>
#define ppl pair<int, ll>
#define ppll pair<ll, ll>
#define ppx pair<pp, int>
#define fi first
#define se second
#define esp 1e-9
#define MAXN 1000000001
#define mod 1000000007
typedef long long ll;
using namespace std;
int n;
int d[123];
int c[123];

int main()
{
    ios::sync_with_stdio(false);
    cin>>n;
    for (int i = 1; i <= n * n; i++)
    {
        int x, y;
        cin>>x>>y;
        if (d[x] == 0 && c[y] == 0) {cout<<i<<" ";

            d[x] = 1;
            c[y] = 1;
        }

    }
    return 0;
}
