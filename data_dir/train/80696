#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef vector<int> vi;
typedef vector<vi> vvi;
typedef vector<ll> vll;
typedef pair<int, int> pii;
typedef vector<pii> vii;
typedef vector<bool> vb;
typedef vector<string> vs;

int main() {
	ios::sync_with_stdio(false), cin.tie(0);
	int n; cin >> n;
	string s; cin >> s;
	string mns = "";
	if (n == 12)
	{
		int mn = 50;
		string ts = "";
		for (int i = 1; i <= 12; i++)
		{
			for (int j = 0; j < 60; j++)
			{
				ts = (i < 10 ? "0" + string(1, '0' + i) : to_string(i)) + ":";
				ts += (j < 10 ? "0" + string(1, '0' + j) : to_string(j));
				int c = 0;
				for (int k = 0; k < s.length(); k++)
					c += s[k] != ts[k];
				if (c < mn)
					mn = c, mns = ts;
			}
		}
	}
	else if (n == 24)
	{
		int mn = 50;
		string ts = "";
		for (int i = 0; i < 24; i++)
		{
			for (int j = 0; j < 60; j++)
			{
				ts = (i < 10 ? "0" + string(1, '0' + i) : to_string(i)) + ":";
				ts += (j < 10 ? "0" + string(1, '0' + j) : to_string(j));
				int c = 0;
				for (int k = 0; k < s.length(); k++)
					c += s[k] != ts[k];
				if (c < mn)
					mn = c, mns = ts;
			}
		}
	}

	cout << mns << endl;
	//cin.ignore(), cin.get();
}