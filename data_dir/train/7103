#include <iostream>
#include <bits/stdc++.h>
using namespace std;

#define pb push_back
#define ll long long
#define F first
#define S second
typedef pair<int,int>ii;
typedef pair<vector<int>,int>vii;
const int N=100005;

vector<int>v[30];
int main()
{
    //freopen("test.in","r",stdin);
   int n,x,sum=0;
   cin>>n>>x;
   for(int i=0;i<n;i++){
        int a;
        cin>>a;
        sum+=a;
   }
   if(sum+n-1==x)cout<<"YES";
   else cout<<"NO";
   return 0;
}
