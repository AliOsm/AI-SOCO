#include<bits/stdc++.h>
using namespace std;

int main()
{
    int f,m,s,M;
    cin >> f >> m >> s >> M;
    if(M*2 < s || M > 2*s || M >= f || M >= m)cout<<"-1"<<endl;
    else
        {
            cout<<min(200, 2*f)<<endl;
            cout<<min(199, 2*m)<<endl;
            cout<<max(s,M)<<endl;
        }
    return 0;
}
