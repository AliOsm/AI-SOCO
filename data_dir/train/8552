#include <bits/stdc++.h>
#define ll long long
#define mp make_pair
#define pb push_back
#define fi first
#define se second
#define over999 1234567890


using namespace std;


int main()
{
        ll a=0,b=0,ans=0,k,y,m,n;
        string s,x;
        cin >> s;
        cin >> x;
        for(int i=0;i<s.length();i++){
                if(s[i]<=90)s[i]=char(s[i]+32);
                if(x[i]<=90)x[i]=char(x[i]+32);
        }
        if(s>x)cout << 1; else if(s==x)cout << 0; else cout << -1;
        return 0;
}
