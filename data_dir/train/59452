using namespace std;

typedef int integer;
typedef array<int, 2> hv;
#define int long long
#define double long double
#define make(type, x) type x; cin>>x
#define make2(type, x, y) type x, y; cin>>x>>y
#define make3(type, x, y, z) type x, y, z; cin>>x>>y>>z
#define make4(type, x, y, z, q) type x, y, z, q; cin>>x>>y>>z>>q
#define ALL(x) (x).begin(), (x).end()
#define FORIT(p) for(auto it = p.begin(); it != p.end(); it++)
#define FOR(b) for(int i = 0; i <(b); i++)
#define VI vector<int>
#define VVI vector<vector<int>>
#define PII pair<int, int>
#define PPII pair<PII, int>
#define MOD7 1000000007
#define MOD9 1000000009




void Solve() {
	make(int, n);
	vector<double> a(n), b(n), ansA(n), ansB(n);
	FOR(n) cin >> a[i];
	FOR(n) cin >> b[i];
	for (int i = 1; i < n; i++) a[i] += a[i - 1];
	for (int i = n - 2; i >= 0; i--) b[i] += b[i + 1];
	double tsumx = 0, tsumy = 0;
	for (int i = 0; i + 1 < n; ++i) {
		double k = 1 + a[i] - b[i + 1];
		double sr = sqrt(fabs(k*k - 4 * a[i]));
		ansA[i] = (k - sr) / 2 - tsumx;
		ansB[i] = (k + sr) / 2 - tsumy;
		tsumx += ansA[i];
		tsumy += ansB[i];
	}
	ansA[n - 1] = 1 - tsumx;
	ansB[n - 1] = 1 - tsumy;
	FOR(n) cout << setprecision(10) << fixed << ansA[i] << ' ';
	cout << endl;
	FOR(n)  cout << setprecision(10) << fixed << ansB[i] << ' ';
}


signed main() {
	ios_base::sync_with_stdio(0);
	cin.tie();
#if  _DEBUG
	freopen("input.txt", "r", stdin);
	freopen("output.txt", "w", stdout);
	Solve();
	while (!cin.eof()) {
		string s = "_";
		do getline(cin, s);
		while (!cin.eof() && !s.empty());
		if (cin.eof()) return 0;
		FOR(3) cout << endl;
		Solve();
	}
#else
	Solve();
#endif
	return 0;
}