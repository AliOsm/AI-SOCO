#include <bits/stdc++.h>
using namespace std;
int main() {
    int n;
    cin >> n;
    map <string, bool> m;
    for (int i = 1; i <= n; i++) {
        string a;
        cin >> a;
        if (!m.count(a)) {
                m[a] = 1;
            cout << "NO\n";
        } else {
            cout << "YES\n";
        }
    }
}
