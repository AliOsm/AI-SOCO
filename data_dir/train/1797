#include<bits/stdc++.h>
using namespace std;
int n , m , a , b , c;
set < int > st;
int main()
{
    scanf("%d%d",&n,&m);
    while ( n-- )
    {
        scanf("%d",&a);
        for ( int i = 1; i <= a; i++ )
        {
            scanf("%d",&b);
            st.insert( b );
        }
    }
    if ( st.size() == m )printf("YES");
    else printf("NO");
    return 0;
}
