#include<bits/stdc++.h>

using namespace std;

int main(){
	
	int t;
	cin >> t;
	
	while(t --){
		int n;
		cin >> n;
		
		if(n == 1){
			cout << -1 << endl;
			continue;
		}
		
		if((n - 1) % 3 != 0){
			for(int i = 1;i < n;i++) cout << 5;
			cout << 3 << endl;
			continue; 
		}
		
		for(int i = 1;i < n - 1;i++) cout << 5;
		cout << 33 << endl;
	}
}