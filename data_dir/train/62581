using namespace std;
#define inf 1e9
const int mod = 1000000007;
const int INF = 1000000000;
const int N = 100005;
int a[26];
string s;
int main()
{
    int n;
    getline(cin, s);
    n = s.size();
    for(int i =  0; i < n; ++i){
        if((s[i] >= 'a') && (s[i] <= 'z'))
        {
            a[s[i] - 'a'] = 1;
        }
    }
    int ans = 0;
    rep(i, 26) ans += a[i];
    cout << ans << endl;
}