const int M=100010;

map<int, int>cnt;

int main()
{
#ifndef ONLINE_JUDGE
    freopen("/home/riaz/Documents/in.txt", "r", stdin);
#endif
    ios_base::sync_with_stdio(false);

    int i, j, k, n, cur = inf;

    in(n);
    for (i = 0; i < n; i++) in(k), cur = min(cur, k), cnt[k]++;

    while (n--)
    {
        if (cnt[cur+1] > 0) cnt[cur+1]--, cur++;
        else if (cnt[cur-1] > 0) cnt[cur-1]--, cur--;
        else
        {
            printf("NO\n");
            return 0;
        }
    }

    printf("YES\n");

return 0;
}

