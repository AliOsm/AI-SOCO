#include <bits/stdc++.h>
 
#define f first
#define s second
#define ll long long
#define ull unsigned long long
#define mp make_pair
#define pb push_back
#define vi vector <int>
#define ld long double
#define pii pair<int, int>
#define y1 sda
 
using namespace std;    
const int N = int(3e5) + 12, mod = int(1e9)  + 7; 

int t, n, a[N];

int main () {
	scanf("%d", &t);
	while(t--){
		scanf("%d", &n);

		ll x = 0, y = 0;

		ll r = 0, s = 0;

		for(int i = 1; i <= n; i++){
			scanf("%d", &a[i]);
			x += a[i];
			y = y ^ a[i];
		}

		cout << 3 << " " << y << " " << (x + y) << " " << 0 << endl;
	}
	
 
return 0;
}