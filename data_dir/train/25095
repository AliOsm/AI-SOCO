#include<bits/stdc++.h>
using namespace std;
#define optimize ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
int main()
{
    optimize
    int n,i,a=0,d=0;
    cin>>n;
    string s;
    cin>>s;
    for(i=0;i<s.length();i++)
    {
        if(s[i]=='A')
            a++;
        else
            d++;
    }
    if(a>d)
        cout<<"Anton";
    else if(d>a)
        cout<<"Danik";
    else
        cout<<"Friendship";
}
