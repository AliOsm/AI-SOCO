#include<bits/stdc++.h>
using namespace std;
 
typedef long long ll;
#define Mehrkish ios::sync_with_stdio(false),	cin.tie(0),		cout.tie(0)
 
ll a, b, ans;
 
void Read_Input(){
		cin >> a >> b;
}
 
void Solve(){
		ans = 6 - max( a, b ) + 1;
		if ( ans == 0 ) cout << 0 << "/" << 1 << endl;
		if ( ans == 1 ) cout << 1 << "/" << 6 << endl;
		if ( ans == 2 ) cout << 1 << "/" << 3 << endl;
		if ( ans == 3 ) cout << 1 << "/" << 2 << endl;
		if ( ans == 4 ) cout << 2 << "/" << 3 << endl;
		if ( ans == 5 ) cout << 5 << "/" << 6 << endl;
		if ( ans == 6 ) cout << 1 << "/" << 1 << endl;
}
 
int main(){
		Mehrkish;
		Read_Input(),		Solve();
		return 0;
}