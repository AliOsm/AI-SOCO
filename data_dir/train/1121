/*jai mata di
let's rock*/
#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long l,r;
    scanf("%I64d %I64d",&l,&r);
    vector<long long int>vec1;
    vector<long long int>vec2;
    for(long long i=l;i<=r;i++)
    {
        if(i%2==0)
            vec1.push_back(i);
        else vec2.push_back(i);
    }
    if(vec1.size()==vec2.size())
    {
        printf("YES\n");
        for(int i=0;i<vec1.size();i++)
        {
            printf("%I64d %I64d\n",vec1[i],vec2[i]);
        }
    }
    else
      printf("NO\n");
    return 0;
}
