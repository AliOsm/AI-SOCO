#include<bits/stdc++.h>
using namespace std;
const int MXN = 1e6 + 10, NX = 1e9 + 7;
int n, r, t, p, c, Mx = -1e9, A[MXN];
int main()
{
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &r);
        if (ceil(sqrt(r)) != floor(sqrt(r)))
            Mx = max(Mx, r);
    }
    printf("%d", Mx);
    return (0);
}
