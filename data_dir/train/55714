#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <set>
#include <map>
#include <string>
using namespace std;
#define inf 0x3f3f3f3f
 long long i,j,k,l,m,n,a[2203456],b[5555],c[22222],d,e,f,s,t;
pair <long long, long long> x[123456];
map <long long, long long> y;
string p;
string q,r[1111];
int main() {
    cin >> d>>e;
    while (d >0 && e > 0 && (d >= 2*e || e >= 2*d)){
        if (d >= 2*e){
            d = d % (2*e);
        }
        else {
            e = e % (2*d);
        }
    }
    cout << d << ' '<<e;
    return 0;
}