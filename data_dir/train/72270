// In the name of Allah the Lord of the Worlds.

#include<bits/stdc++.h>
using namespace std;

typedef long long ll;

int main(void)
{
    ios::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);

    int t;
    cin >> t;
    while(t--){

        ll L , v , l , r;
        cin >> L >> v >> l >> r;

        cout << L/v -(r/v - (l-1)/v) << endl;
    }

    return 0;
}
