#include<iostream>
#include<string>
#include<vector>
#include<cmath>
#include<algorithm>
#include<fstream>
#include<map>

using namespace std;

int main()
{
 //ifstream fin("input.txt");
 int n,m,k,t,ans,i=0,j=0;
 cin>>n;
 vector<int>v(n);
 for(i=0;i<n;i++)
 {
	 cin>>v[i];
 }
 sort(v.begin(),v.end());
 for(i=0;i<n;i++)
	 cout<<v[i]<<" ";
 cout<<endl;
	 return 0;
}