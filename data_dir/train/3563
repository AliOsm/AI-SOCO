/*long long gcd(long long a, long long b)
{
if (b == 0)return a; return gcd(b, a%b);
}
long long lcm(long long a, long long b) {
return  a * b / gcd(a, b);
}
*/
map<int, int>mp1;
map<int, int>mp2;
int main()
{
	int n,x,y;
	sci(n);
	int ans = 2e9;
	for (int i = 0; i < n; ++i) {
		sci(x); sci(y);
		++mp1[x]; if (x != y)++mp2[y];
	}
	int k = (n + 1) / 2;
	for (auto v : mp1)
	{
		if (v.second >= k)return puts("0");
		else {
			int temp = k - v.second;
			if (mp2.count(v.first) > 0 && mp2[v.first] >= temp)ans = min(ans, temp);
		}
	}
	for (auto v : mp2) {
		if (v.second >= k)ans = min(ans, k);
	}
	if (ans == 2e9)printf("-1\n");
	else pr(ans);
}