#include<bits/stdc++.h>
using namespace std;

#define scl(x) scanf("%lld",&x)
#define sc(x)  scanf("%d",&x)
#define ll long long
#define lop(i,n) for(int i=0;i<n;++i)
typedef pair<int, int> ii;
typedef pair<ll, ll> pll;


int a,b,c;

int main(){
#ifndef ONLINE_JUDGE
	freopen("i.txt","r",stdin);
#endif
	cin>>a>>b>>c;
	if(!c)puts(a==b?"YES":"NO");
	else if((b-a)%c==0&&(b-a)/c>=0)puts("YES");
	else puts("NO");
}
