#include <bits/stdc++.h>

typedef  long long ll ;
typedef long double ld ;
using namespace std;

int main()
{
    std::cin.tie(0);
    std::ios::sync_with_stdio(false);
    //freopen("codecoder.in", "r", stdin);
    //freopen("codecoder.out", "w", stdout);
    int xq,yq,xt,yt,xk,yk;
    int n;
    cin >> n;
    cin >> xq>>yq>>xk>>yk>>xt>>yt;
    if(xk<xq&&yk<yq&&xt<xq&&yt<yq)
    {
        cout << "Yes"<< endl;
    }
    else if (xk<xq&&yk>yq&&xt<xq&&yt>yq)
    {
        cout << "Yes"<< endl;
    }
    else if (xk>xq&&yk>yq&&xt>xq&&yt>yq)
    {
        cout << "Yes"<< endl;
    }
    else if (xk>xq&&yk<yq&&xt>xq&&yt<yq)
    {
        cout << "Yes"<< endl;
    }
    else
    {
        cout << "NO" << endl;
    }
    return 0;
}
