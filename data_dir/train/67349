#include <bits/stdc++.h>
#define MAX (ll)(1e5+7)
#define MOD (ll)(998244353)
#define INF (ll)(1e18)

using namespace std;
using ll = long long;
using ld = long double;

ll mult (ll a, ll b) {
	return (a  * b) % MOD;
}

ll add (ll a, ll b) {
	return (a + b) % MOD;
}

ll pw (ll a, ll b) {
	ll res = 1;
	while (b) {
		if (b & 1) {
			res = mult (res, a);
		}
		b >>= 1;
		a = mult (a, a);
	}
	return res;
}

int main () {
	ll n;
	cin >> n;
	ll ar[n], su = 0;
	ll len[11] = {}, lt[11] = {};
	for (ll i = 0; i < n; i++) {
		cin >> ar[i];
		len[to_string (ar[i]).size ()]++;
	}
	for (int i = 0; i < n; i++) {
		ll j = 1;
		while (ar[i]) {
			ll x = ar[i] % 10;
			for (ll y = 1; y <= 10; y++) {
				su = add (su, mult (x, mult (pw (10LL, j  + min (j, y)  - 1), len[y])));
			}
			for (ll y = 1; y <= 10; y++) {
				if (y > j - 1) {
					su = add (su, mult (x, mult (pw (10LL, j  + min (j, y)  - 2), len[y])));
				}
				else {
					su = add (su, mult (x, mult (pw (10LL, j  + y - 1), len[y])));
				}
			}
			j++;
			ar[i] /= 10;
		}
	}
	cout << su << "\n";
}
