#include <algorithm>
#include <iostream>
#include <cstring>
#include <cstdlib>
#include <cassert>
#include <cstdio>
#include <vector>
#include <cmath>
#include <ctime>
#include <set>
#include <map>


typedef long long ll;

using namespace std;
int t,n,p;
int main()
{
	cin>>t;
	while(t--){
		cin>>n>>p;
		for(int i=1; i<n; i++)
		for(int j=i+1; j<=n; j++)
		if(2*n+p>0){
			cout<<i<<" "<<j<<endl;
			p--;
		}
	}

	
	return 0;
}
