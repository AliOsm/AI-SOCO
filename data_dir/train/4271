int dp[N][4][2];


struct DP {
    vi digits;
    int max_dig;
    int base;

    DP() {
        digits.clear();
        base = 10;
        max_dig = base - 1;
        memset(dp, -1, sizeof dp);
    }

    DP(int X, int X_base = 10) {
        base = X_base;
        max_dig = X_base - 1;
        digits.clear();
        memset(dp, -1, sizeof dp);
        while(X > 0) {
            digits.pb(X % base);
            X /= base;
        }
        reverse(all(digits));
    }

    DP(string X, int X_base = 10) {
        base = X_base;
        digits.clear();
        max_dig = X_base - 1;
        memset(dp, -1, sizeof dp);
        for(auto ele: X) {
            digits.pb(ele - '0');
        }
    }
    
    void print_dig() {
        for(auto ele: digits) {
            cout << ele << ' ';
        }
    }

    //// Problem Specific //////////

    int f(int i, int msum, int fl) {
        if(msum > 3) return 0;
        if(i == digits.size()) {
            return (msum <= 3);
        }
        if(dp[i][msum][fl] != -1) {
            return dp[i][msum][fl];
        }
        int res = 0;
        int ub = max_dig;
        if(!fl) {
            ub = digits[i];
        }
        for(int dig = 0; dig <= ub; dig++) {
            int nfl = fl;
            int nmsum = msum;
            if((!fl) && (dig < ub)) {
                nfl = 1;
            }
            nmsum = (nmsum + (dig != 0));
            res = add(res, f(i + 1, nmsum, nfl));
        }
        return dp[i][msum][fl] = res;
    }
};

 
