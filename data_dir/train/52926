#include<bits/stdc++.h>
using namespace std;

#define ll long long

int main()
{
    ll n, k; scanf("%I64d%I64d", &n, &k);
    ll red = n * 2;
    ll green = n * 5;
    ll blue = n * 8;

    ll tot = (red / k) + (red % k > 0) + (green / k) + (green % k > 0) + (blue / k) + (blue % k > 0);
    printf("%I64d\n", tot);
    return 0;
}
