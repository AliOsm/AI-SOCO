#include <bits/stdc++.h>

using namespace std;
int n,x,l[1000],r[1000],ans;
int main()
{
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>l[i]>>r[i];
    }
    cin>>x;
    for(int i=0;i<n;i++)
    {
        if(r[i]<x)
            ans++;
        else break;
    }
    cout<<n-ans;
}
