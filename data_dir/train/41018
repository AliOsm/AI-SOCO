#include <string>
using namespace std;
 
#define MK make_pair
#define FORN(i,n) for(int i=0;i< (n);i++)
#define FOR(i,a,b) for(int i=a;i<=b;i++)
#define FORA(i,a,b,h) for(int i=a;i<=b;i+=h)
#define FOR1(i,a,b) for(int i=a;i>=b;i--)
#define sc scanf
#define scp() scanf("\n")

const int MOD=1e9+7;
const int N=1010;
const int V=5010;
const int E=4*1e6;
const long long inf=1e18;

int main(){
	int a,b,c,d;
	cin>>a>>b>>c>>d;

	int kolm=max(3*a/10,a-a*c/250);
	int kolv=max(3*b/10,b-b*d/250);

	if(kolm>kolv) puts("Misha");
	else if(kolm<kolv) puts("Vasya");
	else puts("Tie");
	return 0;
}