const double EPS = 1e-9;
const double PI = acos(-1.);
const int MOD = 1e9 + 7;
const int MAXN = 120001;
const int __MAX = __INT64_MAX__;
int MAX = 1e-9;
int MIN = 1e9;
string YN[] = {"No", "Yes"};
 
bool isPrime(int n)
{
     if (n <= 1)  {
          return false;
     }
     if (n <= 3)  {
          return true;
     }
 
     if (n%2 == 0 || n%3 == 0) {
          return false;
     }
 
     for (int i=5; i*i<=n; i=i+6)
          if (n%i == 0 || n%(i+2) == 0) {
               return false;
          }
 
     return true;
}
void input(int *a, int size)
{
     for(int i = 0; i < size; ++i) {
          cin >> a[i];
     }
}
 
void output(int *a, int size)
{
     for(int i = 0; i < size; ++i) {
          cout << a[i] << " ";
     }
     cout<<endl;
}
int n, A[MAXN], m, B[MAXN], cnt, res;
void solution()
{
     cin >> n >> cnt;
     while(cnt--) {
          if(n % 10)
               n--;
          else
               n /= 10;
          }
          cout << n;
     
}
__INT16_C() main()
{
     int t = 1;
     // cin >> t;
     while(t--) {
          solution();
     }
}