#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n;
    cin >> n;
    unordered_set<string>seta;
    for(int i = 1;i <= n;i++){
        string str;
        cin >> str;
        if(seta.find(str) == seta.end())
            cout << "NO" << "\n";
        else cout << "YES" << "\n";
        seta.insert(str);
    }
}
