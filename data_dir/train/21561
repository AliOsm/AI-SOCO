#include<bits/stdc++.h>

using namespace std;

int ara[105];

int main()
{
    int n,i,j,sum,tc;
    cin>>tc;
    while(tc--)
    {
        cin>>n;
        sum=0;
        for(i=1; i<=n; i++)
        {
            cin>>ara[i];
            sum+=ara[i];
        }
        int ans=sum/n;
        if(sum%n) ans++;
        cout<<ans<<endl;
    }
}
