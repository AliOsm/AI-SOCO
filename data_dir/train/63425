# include <iostream>
# include <cstring>

using namespace std;

const int N = 111;
const int ABC = 26;

int n, m, k;
char s [N][N];

int main ()
{
# ifndef ONLINE_JUDGE
   freopen ("A.TXT", "r", stdin);
   freopen ("A.OUT", "w", stdout);
# endif

	ios_base::sync_with_stdio (false);
	cin.tie (NULL);

	cin >> n >> m;

	for ( int i (0); i < n; i++ )
	{
		cin >> s [i];

# ifndef ONLINE_JUDGE
       cout << s [i] << endl;
# endif

	    for ( int j (0); j < m; j++ )
	       if ( s [i][j] == '.' )
              if ( (i+j) % 2 == 0 )
                 s [i][j] = 'W';
              else
                 s [i][j] = 'B';
	}

	for ( int i (0); i < n; i++ )
       cout << s [i] << endl;

	return 0;
}
