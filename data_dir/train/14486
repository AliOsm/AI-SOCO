#include <bits/stdc++.h>
using namespace std;
int main(){
  ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(0);
  int n,m,res = 0,a,b;
  cin>>n>>m;
    for(int i=0;i<n;i++){
      for(int j=0;j<m;j++){
        cin>>a>>b;
        if (a+b != 0)res++;
      }
    }
  cout<<res<<"\n";
  return 0;
}

