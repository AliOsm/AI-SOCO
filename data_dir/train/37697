#include<bits/stdc++.h>
#define MAX 5005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

int N, M, ans;
int a[MAX];
int pre[MAX];
unordered_map < int , bool > bad;
unordered_map < int , int > v;
unordered_map < int , bool > vis;

int val( int x )
{
    if( vis[x] )
        return v[x];
    int rx = x;
    int tmp = 0;
    for( int i = 2; i <= sqrt( x ); i ++ )
        while( x % i == 0 )
        {
            if( bad[i] )
                tmp--;
            else
                tmp++;
            x /= i;
        }
    if( bad[x] )
        tmp--;
    else if( x != 1 )
        tmp++;
    
    vis[rx] = 1;
    return v[rx] = tmp;
}

int main()
{
    optimiza_io
    cin >> N >> M;
    for( int i = 1; i <= N; i ++ )
    {
        cin >> a[i];
        pre[i] = __gcd( pre[i - 1] , a[i] );
    }
    while( M -- )
    {
        int x;
        cin >> x;
        bad[x] = 1;
    }
    
    for( int i = N; i >= 1; i -- )
    {
        if( pre[i] == 1 )
        {
            ans += val( a[i] );
            continue;
        }
        if( val( pre[i] ) < 0 )
        {
            for( int j = 1; j < i; j ++ )
            {
                pre[j] /= pre[i];
                a[j] /= pre[i];
            }
            a[i] /= pre[i];
        }
        ans += val( a[i] );
    }
    
    cout << ans;
    return 0;
}

