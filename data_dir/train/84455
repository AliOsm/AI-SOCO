#include <bits/stdc++.h>
using namespace std;
long long  i, j, n, c[223456],a[211234], b[225678],d[423456], l,tt,l0,l1,l2,r, s, t, k, m,x,z;
long long to[67],y;
vector<long long> p[456478],q,qq;
vector <long long> u,uu;
map <long long,long long> mm;
int main() {
    cin >> tt;
    while (tt --){
        cin >> n>>l>>r>>x>>y;
        if ((l-r)*n>(x+y)||(l+r)*n<(x-y))
            cout << "No"<<endl;
        else 
        cout<<"Yes"<<endl;
    }
    return 0;
}
