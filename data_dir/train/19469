#include <cstdio>
#include <cstring>
#include <cmath>
#include <algorithm>
using namespace std;
typedef long long LL;
#define INF 0x3f3f3f3f
#define eps 1e-8

int n, a, b, aa, bb;

int main()
{
	scanf("%d", &n);
	aa = bb = 0;
	for (int i = 0; i < n; i++)
	{
		scanf("%d", &a);
		aa |= a;
	}
	for (int i = 0; i < n; i++)
	{
		scanf("%d", &b);
		bb |= b;
	}
	printf("%d\n", aa+bb);
	return 0;
}
