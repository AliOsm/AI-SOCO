#include<bits/stdc++.h>
using namespace std;



int main() {
    ios::sync_with_stdio(0);
    cin.tie(0); cout.tie(0);
    long long ans  = 0;
    long long x = 0;
    int n;
    cin >> n;
    for(int i = 0; i < n; i++) {
            int j;
        cin >> j;
        if(j == 1) {
            x++;
        } else ans += x;
    }
    cout <<ans;
    return 0;
}
