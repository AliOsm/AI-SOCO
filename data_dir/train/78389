#include<bits/stdc++.h>

using namespace std;
#define ll long long
#define dbug printf("I am here\n");
#define fast ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
const int maxn = 2e5+100;

const ll inf = 1e18;
int a[maxn] , b[maxn];

int main()
{
    fast;
    int test;
    cin >> test;
    while(test--){
        int n , k1, k2;
        cin >> n >> k1 >> k2;
        bool f = false;
        for(int i = 1; i<=k1; i++){
            int p; cin >> p;
            if(p == n)f = true;
        }
        for(int i = 1; i<=k2; i++){
            int p; cin >> p;
        }
        if(f){
            cout << "YES" << endl;
        }
        else cout << "NO" << endl;
    }
    return 0;
}
