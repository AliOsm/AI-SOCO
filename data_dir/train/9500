#define pb push_back
#define ll long long
#define pf(x) cout<<x<<endl
#define MX 1e18
#define ff first
#define ss second
#define for1(i,n) for(int i=1;i<=n;i++)
#define for0(i,n) for(int i=0;i<n;i++)
#include <bits/stdc++.h>
using namespace std;

ll a[1000006];

int main(int argc, char const *argv[]) {
  ll n,k;
  cin>>n>>k;
  while(k--){
    ll temp = n%10;
    if(!temp){
      n/=10;
    }
    else{
      n--;
    }
  }
  cout<<n<<endl;
  return 0;
}
