#include<bits/stdc++.h>
#define ff first
#define ss second
#define pb push_back
#define mp make_pair
#define ll long long
#define all(a) a.begin(),a.end()
#define endl '\n'
using namespace std;

const int N=51;

int n;

main()
{
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    cin>>n;
    if(n==2)
    {
        cout<<-1;
        return 0;
    }
    int vg=0;
    for(int i=1;i<n-1;++i)
    {
        vg++;
        while(vg%5==0)vg++;
        cout<<2*3*vg<<endl;
    }
    cout<<2*5<<endl;
    cout<<3*5<<endl;
}
