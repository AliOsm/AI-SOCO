#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

int N, sum;
int a[MAX];

bool ok( int s )
{
    double grd = ( double ) s / ( double ) N;
    return grd >= 4.5;
}

int main()
{
    optimiza_io
    cin >> N;
    for( int i = 1; i <= N; i ++ )
    {
        cin >> a[i];
        sum += a[i];
    }
    sort( a + 1 , a + N + 1 );
    if( ok( sum ) )
    {
        cout << "0\n";
        return 0;
    }
    for( int i = 1; i <= N; i ++ )
    {
        sum -= a[i];
        sum += 5;
        if( ok( sum ) )
        {
            cout << i;
            return 0;
        }
    }
    return 0;
}
