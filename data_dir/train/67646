                               //----------------kokomade tenpure------------
int ko[11]={};

vector<int> p[11][11],q[11][11];
int ju[40];
int solve(pair<string,int> X){
	string s=X.first;
	reverse(s.begin(),s.end());
	int l=X.second;
	int ans=0;
	for(int i=1;i<=10;i++){
	//	cout<<i<<endl;
		int e=0;
		for(int j=0;j<l;j++){
			e+=ju[q[i][l][j]]*(s[j]-'0')%mod;
			e+=ju[p[l][i][j]]*(s[j]-'0')%mod;
			e%=mod;
		}
		e*=ko[i];
		e%=mod;
		ans+=e;
		ans%=mod;
	}
	return ans;
	
}
 signed main(){
 	
 
    	       cin.tie(0);
   		ios::sync_with_stdio(false);

 	int n;
 	cin>>n;
 	mod= 998244353;
 	vector<pair<string,int>> ve;
 	
 	ju[0]=1;
 	for(int i=1;i<40;i++){
 		ju[i]=ju[i-1]*10%mod;
 	}
 	
 	for(int l1=1;l1<=10;l1++)for(int r1=1;r1<=10;r1++){
 		
 		int f=0;
 		for(int i=0;;i++){
 			if(i>=l1 && i>=r1) break;
 			if(i<l1){
 				p[l1][r1].pb(f);
 				f++;
 			}
 			if(i<r1){
 				q[l1][r1].pb(f);
 				f++;
 			}
 		}
 		
 	}
 	
 	
 	for(int i=0;i<n;i++){
 		int y;
 		cin>>y;
 		string s=itos(y);
 		ve.pb(mp(s,s.length()));
 		ko[s.length()]++;
 	}
 
 	int ans=0;
 	for(auto v:ve){
 		int r=solve(v);
 		ans+=r;
 		ans%=mod;
 	}
 	cout<<ans<<endl;
 		
 	
 	return 0;

 	
 	
 }