#include <algorithm>
#include <iostream>
#include <string>
#include <vector>
#include <unordered_set>

using namespace std;

int main() {
    int n, m;
    cin >> n >> m;

    string s1 = "5", s2 = "5";
    int as = 5, bs = 5;
    while (true) {
        if (as >= n && bs >= n)
            break;
        s1 = "4" + s1;
        s2 = "5" + s2;
        as += 4;
        bs += 5;
    }
    cout << s1 << " " << s2 << endl;
}
