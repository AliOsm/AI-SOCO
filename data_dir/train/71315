#define MX   150005
#define inf  100000009
#define eps  1e-9
#define MD   1000000007

int main(){
    int n,i,j,k;
    string s;
    cin>>n;
    cin>>s;

    fr(i,0,n-1) {
       // bool f=1;
        if( s[i]=='.')continue;
        fr(j,1,n-1) {
            int cnt=0;
            //bool can=1;
            for(k=i+j;k<n;k+=j) {
                if( s[k]=='*') cnt++;
                else break;
            }
            if(cnt>=4) {
                cout<< "yes\n";
                return 0;
            }
        }
    }
    cout<< "no\n";
}
