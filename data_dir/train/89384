#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
#define se second
#define fi first
#define pb push_back
const int N = 1e5 +5;

int main(){

//	freopen("in.txt","r",stdin);

	ll n, m;
	cin >>n >>m;
	ll temp =1;
	while (n && temp <=m){
		temp *=2;
		n--;
	}
	cout <<m%temp <<endl;


#ifndef ONLINE_JUDGE
	cout << "Time elapsed: " << 1.0 * clock() / CLOCKS_PER_SEC << " s.\n";
#endif
	return 0;

}
