#define ph(a)           push(a)
#define pb(a)           push_back(a)
#define scd(a)          scanf("%d",&a)
#define scdd(a,b)       scanf("%d %d",&a,&b)
#define ll              long long int
#define scl(w)          scanf("%I64d",&w)
#define scll(a,b)       scanf("%I64d %I64d",&a,&b)
#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n,m,a,b,cnt=0;
    cin >> n >> m;
    for (int i=0;i<=m;i++){
        a=i,b=m-i;
        b=sqrt(b);
        if (a+b*b==m && a*a+b==n)cnt++;
    }
    cout << cnt << endl;

    return 0;
}


