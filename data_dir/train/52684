#include <bits/stdc++.h>

using namespace std;
typedef long long ll;
typedef pair<int, int> pi;


int main() {
  ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
  int n, k;
  cin >> n >> k;
  int l = 1;
  while ((5*l*(l+1))/2 <= 240-k) l++;
  cout << min(n, l-1)<<endl;

  return 0;
}

