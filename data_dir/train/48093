//basma taala
#include<bits/stdc++.h>
using namespace std;
long double pi = 3.14159265358979323846;
int main()
{
	long double d, h, v, e, s;
	cin >> d >> h >> v >> e;
	s = pi*(d/2)*(d/2)*e;
	if(s >= v)
	{
		cout << "NO"	;
		return 0;
	}
	v -= s;
	cout << "YES" << endl << fixed << setprecision(6) << pi*h*(d/2)*(d/2)/v;
}