#include <bits/stdc++.h>

using namespace std;

inline void prep()
{
    cin.tie(0);
    cin.sync_with_stdio(0);
};

#define X first
#define Y second
#define endl '\n'
#define ll long long
#define inf (0x7fffffffffffffff)
#define all(x) x.begin(), x.end()
#define debug(x) cerr << #x << " = " << (x) << "\n";
#define ZXC cerr << clock() * 1.0 / CLOCKS_PER_SEC << endl;

// const int mod = (int)1e9 + 7;
const int mod = 998244353;
const int MX = (int)3e5 + 10;


int main()
{     
    prep();

    int n;
    cin >> n;

    for(int i = 0, x; i < n; ++i){
        cin >> x;
        if(x) return puts("HARD"), 0;
    }

    cout << "EASY" << endl;

    // ZXC
    return 0;
}