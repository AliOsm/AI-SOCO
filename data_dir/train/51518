
#include<iostream>
#include <bits/stdc++.h>
#include <math.h>
#define ll long long
#define ld long double
using namespace std;
ll mode=1000000007;
ll gcd (ll a,ll b)
{
    if(b==0)
        return a;
    else
        return gcd(b,a%b);
}
int main()
{
 ll n;
 cin>>n;
 if(n%2==1)
    cout<<"-1";
 else
 {
     for(ll i=1;i<=n;i=i+2)
         cout<<i+1<<" "<<i<<" ";
 }
 return 0;
}





