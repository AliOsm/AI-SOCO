#include <bits/stdc++.h>
#define pb push_back
#define mp make_pair
using namespace std;

int main()
{
	int n,m;
	bool ok=0;
	char c;
	scanf("%d%d",&n,&m);
	n*=m;
	while(n--)
	{
		cin >> c;
		if(c !='W' && c !='B' && c != 'G')
			ok=1;
	}
	if(ok)
		printf("#Color");
	else
		printf("#Black&White");
}
