#include<bits/stdc++.h>
using namespace std;
const int N = 1e5 +10, Mod = 1e9 + 7;
int n, m, r, t, a, b, c, A[N];
int main()
{
    string s;
    cin >> s;
    n = s.size();
    for (int i = 0; i < n; i++)
    {
        if (s[i] > r + 'a')
            return !printf("NO");
        if (s[i] == r + 'a')
            r ++;
    }
    printf("YES");
    return (0);
}
