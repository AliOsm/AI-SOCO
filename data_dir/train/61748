#include <bits/stdc++.h>

using namespace std;

int n;

int main() 
{
  cin >> n;
  if (n < 6)
    cout << -1 << "\n";
  else {
    cout << "1 2\n";
    cout << "1 3\n";
    cout << "1 4\n";
    for (int i = 5; i <= n; i++)
      cout << "3 " << i << "\n";
  }
  for (int i = 2; i <= n; i++)
    cout << "1 " << i << "\n";
  return 0;
}