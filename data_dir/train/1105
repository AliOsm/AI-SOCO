#include <bits/stdc++.h>
#define ll long long
#define ull unsigned long long
#define pb push_back
#define forq(i , a , b) for (int i = (a); i <= (b); ++i)
#define forr(i , b) forq( i , 0 , b - 1 )
#define carr( n , array ) forr(i , n) cin >> array[i]
#define F first
#define S second
#define IF ->first
#define IS ->second
#define qqmemset(array , val) memset (array , val , sizeof(array))

const ll max9 = 10 + 1e9 ,  max6 = 10 + 1e6 , max12 = 10 + 1e12 , max15 = 10 + 1e15 ;
const ll min6 = -1 * max6 , min9 = -1 * max9 , min12 = -1 *max12 , min15 = -1 * max15 ;
const ll R = 7 + 1e9 , NN = 10 + 1e5 ;
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false); cin.tie(NULL);
    int n , k ; cin >> n >> k ;
    map <int , set <int> > pos ; vector <int> ans ;
    forr (i , n) {
        int x; cin >> x ;
        pos [x].insert(i + 1) ;
    }
    int sum = 0 ;
    for (auto it : pos) {
        for (auto ANS : it.S ) {
            if (sum + it.F > k) break ;
            sum += it.F ;
            ans.pb(ANS) ;
        }
    }
    cout << ans.size() << endl ;
    for (auto it : ans) cout << it << " " ;
    return 0;
}
