#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

int N, M, ans;
int id[MAX];

int find( int x ) {
    if( x == id[x] )
        return x;
    return id[x] = find( id[x] );
}

int main()
{
    optimiza_io
    cin >> N >> M;
    for( int i = 1; i <= N; i ++ )
        id[i] = i;
    
    while( M -- ) {
        int u, v;
        cin >> u >> v;
        u = find( u );
        v = find( v );
        if( u != v )
            id[u] = id[v];
        else
            ans++;
    }
    cout << ans << "\n";
    return 0;
}