#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

int N, M;
bool vis[MAX];
queue < int > q;

int main()
{
    optimiza_io
    cin >> N >> M;
    while( N -- )
    {
        int a, b;
        cin >> a >> b;
        while( a <= b )
            vis[a++] = true;
    }
    for( int i = 1; i <= M; i ++ )
        if( ! vis[i] )
            q.push( i );
    cout << q.size() << "\n";
    while( ! q.empty() )
        cout << q.front() << " ", q.pop();
    return 0;
}
