const D pie = acos(-1);
const D hpie = asin(1);
const D qpie = atan2(1,1);
const D tpie = 2*pie;

string vow = "aeiou";

void solve( int test_case )
{
    string s;
    cin>>s;

    int n = s.size();

    int i = 0;
    char m = 'a';

    for ( ; i<n ; i++ )
    {
        if ( s[i]<=m )
        {
            s[i] = m;
            m++;
            if ( m>'z' )
            {
                cout << s;
                R;
            }
        }
    }

    cout << -1;

}

int main()
{
    int t = 1;
    //cin>>t;

    for ( int i=0 ; i<t ; i++ ) solve(i);
    return 0;
}
