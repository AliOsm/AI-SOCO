#include <bits\stdc++.h>
#ifdef lyosha
#define files freopen("input.txt", "r", stdin);
#else
#define files //freopen("howmany.in","r",stdin); freopen("howmany.out","w",stdout);
#endif
using namespace std;
typedef long long ll;
const int N = 100005;
const int mod = 1e9 + 7;
int n;
string s;
int main(){
    files;
    cin >> n >> s >> s;
    if(s == "week"){
        int ans = 52;
        if(n == 5 || n == 6) ans ++;
        cout << ans << endl;
    }
    else{
        if(n == 30) cout << 11 << endl;
        if(n <= 29) cout << 12 << endl;
        if(n == 31) cout << 7 << endl;
    }
    return 0;
}
