#include <stdio.h>
#include<iostream>
#include<string>
#include<cstring>
#include<vector>
using namespace std;

int main()
{
    long long  n;
    cin>>n;
    long long s=0;
    int p;
    for(int a=0; a<n; a++)
    {
        cin>>p;
        s=s+p;
        
    }
    s=s*1000000;
    int gg=(s/(n*100));
    if(gg-(gg/10000)*10000>=1000)
     cout<<gg/10000<<'.'<<gg-(gg/10000)*10000;
    else
    cout<<gg/10000<<".0"<<gg-(gg/10000)*10000;
 return 0;

}