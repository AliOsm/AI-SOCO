//1-2
#include "bits/stdc++.h"
using namespace std;

int main() {

	long long n;
	cin >> n;

	bool found = false;

	for(long long i=1;i<=n;i++){

		long long a;
		cin >> a;

		if(a == 1LL) {
			found = true;
		}
	}

	if(found){
		cout << "HARD\n";
	} else {
		cout << "EASY\n";
	}
}