#include <bits/stdc++.h>
#define ll long long
#define ull unsigned long long
#define pb push_back
#define fo(i , a , b) for (int i = (a); i < (b); ++i)
#define forr(i , b) fo( i , 0 , b )

const int max9 = 1000000009 , max6 = 1000006 , min6 = -1000006 , min9 = -1000000009 ;
const ll max12 = 1000000000012 , max15 = 1000000000000015 ;
const ll min12 = -1000000000012 , min15 = -1000000000000015 ;

using namespace std;

int main()
{
    ios_base::sync_with_stdio(false); cin.tie(NULL);
    string s ; cin >> s ;
    int a[200] ; memset(a , 0 , sizeof(a)) ;
    forr(i , s.size())a[s[i]] ++ ; if (a['n'] != 0) a['n'] -- ;
    int a1 = a['n'] / 2 , a2 = a['i'] , a3 = a['e'] / 3 , a4 = a['t'] ; cout << min(a1 , min(a2 , min(a3 , a4))) ;
    return 0;
}
