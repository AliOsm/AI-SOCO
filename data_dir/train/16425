#include <bits/stdc++.h>
using namespace std;

#define IOS ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define endl "\n"
#define long long long 

int main(){
   IOS;

   long int t;
   cin>>t;

   while(t--){
    long int x,y;
    cin>>x>>y;

    if(x - y == 1)
      cout<<"NO\n";
    else
      cout<<"YES\n";
   }  

   return 0;
}