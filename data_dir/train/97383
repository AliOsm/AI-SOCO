#pragma GCC optimize ("O3")
#include <bits/stdc++.h>
#define ll long long
#define ld long double
using namespace std;
ll l, r;
int main()
{
    ios::sync_with_stdio(0);
    cin >> l >> r;
    if(l == r)
        return cout << 0,0;
    cout << (1ll << ((ll)log2(l ^ r) + 1)) - 1 << endl;
    return 0;
}
