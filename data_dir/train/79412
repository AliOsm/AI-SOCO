main(){
	ios::sync_with_stdio(0) , cin.tie(0) , cout.tie(0);
	int n,m;
	cin>>n>>m;
	if(m!=0 && n==0)
        return cout<<"Impossible",0;
    cout<<n+max(m-n,0)<<" ";
    cout<<n+max(m-1,0);
}
