#include <bits/stdc++.h>
#define s() t=c,c=_n-r+1,r=t,t=_n,_n=_m,_m=t
int main() {
	int n, m, x, y, z, p, t, r, c;
	scanf("%d%d%d%d%d%d", &n, &m, &x, &y, &z, &p);
	x %= 4, y %= 2, z %= 4;
	for(; p--?scanf("%d%d", &r, &c):0; printf("%d %d\n", r, c)) {
		int _n=n, _m=m;
		for (int i=0; i<x; i++) s();
		c=y?_m-c+1:c;
		for (int i=0; i<4-z; i++) s();
	}
}
