#include<bits/stdc++.h>
using namespace std;
#define Jewel ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

int main()
{
    Jewel;
    int i, j, k=0;
    string s, c = "hello";
    cin>>s;
    for(i=0; i<s.size(); i++)
    {
        if(s[i]==c[k])
        {
            k++;
        }
        if(k==5)
            break;
    }
    if(k==5)
        cout<<"YES";
    else
        cout<<"NO";
    return 0;
}


