#include <bits/stdc++.h>
using namespace std;
#define oo 1000000010
#define mod 1000000007
const int N = 100010;
int n;
char s[N];
int l , r;
int main(){
	scanf("%d",&n);
	scanf("%s",s);
	for(int i = 0 ;i < n;i++){
		if(s[i] == 'L') l++; else r++;
	}
	cout << r + l + 1 << endl;
	return 0;
}