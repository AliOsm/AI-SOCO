#include <bits/stdc++.h>
#pragma GCC optimization ("unroll-loops")
#pragma GCC optimization ("O2")
#pragma GCC target("popcnt")
#pragma GCC target ("avx2")
using namespace std;

int main(){
	int n;
	cin >> n;
	if(n == 0){
		cout << 1 << endl;
	}else if(n%4 == 1){
		cout << 8 << endl;
	}else if(n%4 == 2){
		cout << 4 << endl;
	}else if(n%4 == 3){
		cout << 2 << endl;
	}else{
		cout << 6 << endl;
	}
}