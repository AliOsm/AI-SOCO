#include <bits/stdc++.h>

using namespace std;
#define ll long long
int arr[106];
int main(){
    set<int>st;
    int n;
    scanf("%d", &n);

    for(int i=1; i<=n; i++)scanf("%d", &arr[i]);

   // sort(arr+1, arr+n+1);

    int ans=0;
    for(int i=1; i<=n; i++){
        if(arr[i])st.insert(arr[i]);

    }
    cout<<st.size()<<endl;

}
