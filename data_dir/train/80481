#define _CRT_SECURE_NO_WARNINGS

#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <iterator>
#include <set>
#include <cmath>
#include <ctime>
#include <stack>

using namespace std;
typedef unsigned long long ull;
typedef long long ll;
#define all(a)  a.begin(), a.end()
#define Pause   cout << "\n\n"; system("pause")

void solve()
{
    int n, k, l, c, d, p, nl, np;
    cin >> n >> k >> l >> c >> d >> p >> nl >> np;

    l *= k;
    c *= d;

    int maxNL = l / nl,
        maxNP = p / np;

    int countAll = min(min(maxNL, maxNP), c);

    cout << countAll / n;
}

int main()
{
    solve();
//  Pause;
    return 0;
}