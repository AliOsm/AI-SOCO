#include<iostream>
#include<cstdio>
#include<math.h>
#include<cmath>
#include<set>
#include<algorithm>
#include<stack>
#include <vector>
#include<queue>
#include<map>
#include <sstream>
using namespace std ;
typedef long long ll ;
// double cnt = 0 ;
int main(void)
{
//freopen ("input.txt","r",stdin);
int n ;
cin >> n ;
int arr[101] , freq[101] = {} , cnt = 0 , mx = -1 ;
for(int i = 0 ; i < n ; ++i)
{
	cin >> arr[i];
	++freq[arr[i]];
    mx = max(mx , freq[arr[i]]);
}
cout << mx << endl;
}