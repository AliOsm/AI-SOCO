#include "bits/stdc++.h"
using namespace std;

#define  F  first
#define  S  second
typedef long long int ll;
const int N = 2e3+5,Mod = 1e9+7;
int main()
{
    int n;cin>>n;
    map<int,int>M;
    int m=n,MX = 0;
    while(m--)
    {
        int x;cin>>x;
        M[x]++;
        MX = max(MX,M[x]);
    }
    cout<<n-MX<< "\n";


    return 0;
}
