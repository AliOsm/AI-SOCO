#include <bits/stdc++.h>
using namespace std;

void solve() {
    string a, b, c;
    cin >> a >> b >> c;
    int len = a.size();
    for(int i = 0; i < len; i++) {
        if(c[i] != a[i] && c[i] != b[i]) {
            cout << "NO" <<endl;
            return ;
        }
    }
    cout << "YES" <<endl;
}

int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    // freopen("input.txt","r",stdin);
    // freopen("output.txt","w",stdout);
    int t;
    cin >> t;
    while (t--)
    {
        /* code */
        solve();
    }
    
    return 0;
}