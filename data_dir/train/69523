#include<bits/stdc++.h>
#define ll long long
#define inf 1000000000000
#define eps 0.00000001
using namespace std;

int main(){
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);
	ll a, b, ans = 1;
	cin>>a>>b;
	for (ll i = 2; i <= min(a, b); ++i){
		ans *= i;
	}
	cout<<ans;
}
