#include<bits/stdc++.h>
using namespace std;


void f(){
    long long x,y;cin>>x>>y;
    
    if(x-1==y){
        cout<<"NO"<<"\n";
    }else{
        cout<<"YES"<<"\n";
    }
}
 
int main(int argc, char *argv[]){
    
    int n;
    cin>>n;
    
    for(int i = 0;i<n;i++){
        f();
    }
    
    return 0;
}