#include <bits/stdc++.h>
#define mk make_pair
#define fs first
#define sc second
using namespace std;
typedef long long ll;
typedef long double ld;
int main(){
    int n, t;
    cin>>t;
    while(t--){
        ll x, y, a, b;
        cin>>y>>x;
        a=y;
        b=x/y*y;
        if(y==1)
            cout<<1<<" "<<2<<endl;
        else
            cout<<a<<" "<<b<<endl;
    }
  return 0;
}


