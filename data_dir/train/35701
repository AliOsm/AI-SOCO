typedef long long ll;
typedef unsigned long long ull;
typedef long double ld;
typedef vector<int> vi;
typedef pair<ll, ll> pll;
typedef pair<int, int> pii;

string itosm(ll x){
    if(x == 0)
        return "0";
    string ans = "";
    while(x > 0){
        ans +=((x%10) + '0');
        x/=10;
    }
    reverse(ans.begin(), ans.end());
    return ans;
}

ll stoim(string str){
    ll ans = 0;
    ll k = 1;
    for(int i = str.length()-1; i >= 0; i--){
        ans+=(str[i]-'0')*k;
        k*=10;
    }
    return ans;
}

const ll infll = 1e18 + 3;
const int inf = 1009000999;
const double eps = 1e-7;
const int maxn = 1e6 + 46;
const int baseint = 1000200013;
const ll basell = 1e18 + 3;
const ld PI = acos(-1.0);
const ll mod = 1e9 + 7;

bool use0[10];
int cnt[10][10];

void solve(){
    int n;
    cin >> n;
    for(int i = 0; i < 10; i++)
        use0[i] = 1;
    for(int i = 0; i < n; i++){
        string s;
        cin >> s;
        use0[s[0] - 'a'] = 0;
        int st = 1;
        for(int g = 0; g < s.size(); g++){
            cnt[s[s.size() - g - 1] - 'a'][g] += st;
            st *= 10;
        }
    }
    vector<int> f;
    for(int i = -1; i < 10; i++)
        f.pb(i);
    ll ans = infll;
    while(f[0] == -1){
        ll sum = 0;
        if(use0[f[1]]){
            for(int i = 1; i <= 10; i++){
                for(int j = 0; j < 9; j++){
                    sum += cnt[f[i]][j] * (i - 1);
                }
            }
            ans = min(ans, sum);
        }
        next_permutation(f.begin(), f.end());
    }
    cout << ans;
}

