#include <bits/stdc++.h>

using namespace std;

int main() {
	int n, m; scanf("%d %d", &n, &m);


	for(int i = 0; i < n; i++){
		for(int j = 0; j < m; j++) {
			int e; scanf("%d", &e);
			if (e == 1 && (i == 0 || i == n - 1 || j == 0 || j == m - 1)) {
				puts("2");
				return 0;
			}
		}
	}
	puts("4");
	return 0;

}
