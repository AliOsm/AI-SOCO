#include <functional>
#include <algorithm>
using namespace std;
typedef long long ll;
int main()
{
	string s;
	cin >> s;
	for (int i = 0; i < 5; ++i) {
		string x;
		cin >> x;
		if (x[0] == s[0] || x[1] == s[1]) {
			puts("YES");
			return 0;
		}
	}
	puts("NO");
	return 0;
}