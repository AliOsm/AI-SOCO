// In the name of Allah the Most Merciful.

#include<bits/stdc++.h>
using namespace std;

typedef long long ll;

int ar[700];

int main(void)
{
    memset(ar , 0 , sizeof(ar));

    int n;
    scanf("%d",&n);

    for(int i=0;i<n;i++){

        int in;
        scanf("%d",&in);
        if(in==0)continue;
        ar[in] = 1;
    }

    int counter = 0;

    for(int i=0;i<700;i++){

        if(ar[i]==1)counter++;
    }
    printf("%d\n",counter);

    return 0;
}
