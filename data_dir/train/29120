	#define lld I64d
#endif

int main() {
	cin.sync_with_stdio(0);
	cin.tie(0);
	cout << fixed << setprecision(10);
	int N;
	cin >> N;
	vector<int> A(N);
	for(int i = 0; i < N; i++) cin >> A[i];
	for(int k = 4; k >= 0; k--) for(int i = 0; i <= N-(1<<k); i += (1<<k)) {
		bool sorted = true;
		for(int j = i+1; j < i+(1<<k); j++) if(A[j-1] > A[j]) sorted = false;
		if(!sorted) continue;
		cout << (1<<k) << "\n";
		return 0;
	}
	return 0;
}

