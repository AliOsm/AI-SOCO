#include<bits/stdc++.h>
using namespace std;

int i, j, k, l, x, y, z, m, n, ans, p, q, r, s;
int main()
{
    cin >> n;
    for(i = 0; i < n; i++){
        cin >> x >> y;

        if(x == 0) p++;
        else q++;

        if(y == 0) r++;
        else s++;
    }

    ans += min(p, q) + min(s, r);

    cout << ans;

    return 0;
}
