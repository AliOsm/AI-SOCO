#include <bits/stdc++.h>

using namespace std;

int have[3];

int main() {
  int n;
  scanf("%d", &n);
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < 3; j++) {
      int a;
      scanf("%d", &a);
      have[j] += a;
    }
  }
  if (have[0] == 0 && have[1] == 0 && have[2] == 0) puts("YES");
  else puts("NO");
  return 0;
}