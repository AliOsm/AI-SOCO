#include <bits/stdc++.h>
#include <ext/pb_ds/assoc_container.hpp>
#include <ext/pb_ds/tree_policy.hpp>
using namespace __gnu_pbds;
using namespace std;
int32_t main() {
    int q;
    cin >> q;
    while (q--) {
        int a, b;
        cin >> a >> b;
        if (a % b) {
            cout << "NO\n";
        } else {
            cout << "YES\n";
        }
    }
}