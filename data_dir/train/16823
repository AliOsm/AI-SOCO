#include <stdio.h>
#include <stdlib.h>
#include <algorithm>
#include <string.h>
#include <queue>
#include <iostream>

using namespace std;

const int N=400040;

int a[N];

int main()
{
	int n,k;
	scanf("%d%d",&n,&k);
	for(int i=1;i<n-k;i++)
		cout<<i<<" ";
	for(int i=n;i>=n-k;i--)
		cout<<i<<" ";
	cout<<endl;
	return 0;
}