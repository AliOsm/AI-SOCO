#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define N 500500



int main()
{
   // freopen("in.in", "r", stdin);
    ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

    int t; cin >> t;
    while(t--){
        int n; cin >> n;
        ll sum = 0;
        for(int i = 0; i < n; i++){
            ll a; cin >> a;
            sum += a;
        }
        ll ans = sum / n;
        if(sum % n) ans++;
        cout << ans << endl;
    }

    return 0;
}