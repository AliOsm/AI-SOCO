#include <bits/stdc++.h>
using namespace std; 
#define pb push_back
#define mp make_pair
#define endl '\n'
typedef long long ll;
typedef pair <int,int> pii;

int main()
{
    ios_base::sync_with_stdio(false);
    int n,h;
    cin >> n >> h;
    int res = 0;
    for (int i = 0 ; i < n ; i++)
    {
        int a;
        cin >> a;
        if (a > h)
            res += 2;
        else
            res++;
    }    
    cout << res << endl;
    return 0;
}