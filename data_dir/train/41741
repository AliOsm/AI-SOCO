class TaskC {
public:
    vector<int> Next, Prev;
    vector<Fenwick<ll>> F;
    int C, S;

    void remove(int u, int v, int w) {
        if (u != -1) {
            Next[u] = w;
            F[u/S].add(v/S, u-v);
        }
        if (w != -1) {
            Prev[w] = u;
            F[v/S].add(w/S, v-w);
        }
        if (u != -1 && w != -1) {
            F[u/S].add(w/S, w-u);
        }
    }

    void add(int u, int v, int w) {
        Prev[v] = u;
        Next[v] = w;

        if (u != -1) {
            Next[u] = v;
            F[u/S].add(w/S, u-w);
            F[u/S].add(v/S, v-u);
        }
        if (w != -1) {
            Prev[w] = v;
            F[v/S].add(w/S, w-v);
        }
    }

    void solve(istream& cin, ostream& cout) {
        int N, M; cin >> N >> M;
        C = max(1, (int)sqrt(N));
        S = (N-1)/C+1;
        vector<int> A(N); cin >> A;
        for (int&a:A) --a;

        F = vector<Fenwick<ll>>(C, Fenwick<ll>(S, 0));
        Next = Prev = vector<int> (N, -1);
        vector<set<int>> Pos(N);
        for (int i = 0; i < N; ++i) {
            Pos[A[i]].insert(i);
        }
        for (int i = 0; i < N; ++i) {
            int prev = -1;
            for (const int&x:Pos[i]) {
                if (prev != -1) { F[prev/S].add(x/S, x-prev); Next[prev] = x; Prev[x] = prev; }
                prev = x;
            }
        }

        for (int i = 0; i < M; ++i) {
            int t,a,b; cin >> t >>  a >> b;
            --a; --b;
            if (t==1) {
                int p = A[a]; //prev value
                if (p == b) continue;

                auto it = Pos[p].lower_bound(a);
                int q = it == Pos[p].begin() ? -1 : *prev(it);
                int r = next(it) == Pos[p].end() ? -1 : *next(it);
                remove(q, a, r);

                it = Pos[b].lower_bound(a);
                q = it == Pos[b].begin() ? -1 : *prev(it);
                r = it == Pos[b].end() ? -1 : *it;
                add(q, a, r);

                Pos[p].erase(a);
                Pos[b].insert(a);
                A[a] = b;
            } else {
                int c1 = a / S, c2 = b / S;
                ll ans = 0;
                for (int c = c1+1; c < c2; ++c) {
                    ans += F[c].range(c, c2);
                }

                if (c1 != c2) {
                    for (int x = a; x < (c1+1)*S; ++x) {
                        if (Next[x] <= b && Next[x] != -1) ans += Next[x] - x;
                    }

                    for (int x = c2*S; x <= b; ++x) {
                        if (Prev[x] >= (c1+1)*S) ans += x - Prev[x];
                    }
                } else {
                    for (int x = a; x <= b; ++x) {
                        if (Next[x] <= b && Next[x] != -1) ans += Next[x] - x;
                    }
                }
                cout << ans << '\n';
            }
        }
    }
};


int main() {
	ios_base::sync_with_stdio(false); cin.tie(nullptr); cout.tie(nullptr);
	TaskC solver;
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
    return 0;
