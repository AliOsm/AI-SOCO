#include<bits/stdc++.h>
using namespace std;

#define end '\n'
#define D(x) cout << #x << " = " << (x) << endl;

int main() {
  int n;
  while (cin >> n) {
    int a, b;
    while (n --> 0) {
      cin >> a >> b;

      int tot = 60 - b + (60 * (23 - a));
      cout << tot << endl;
    }
  }
  return 0;
}