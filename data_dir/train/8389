#include<bits/stdc++.h>
using namespace std;

#define F first
#define S second
#define ii pair<int,int>

typedef long double ldbl;
typedef long long ll;
typedef double dbl;

const ldbl pi = acos(-1);
const ll inf = 1LL<<30;
const dbl eps = 1e-9;
const ll mod = 1e9+7;

/** /////////// ** Code ** /////////// **/

const int N=0;

int main(){
    int t;
    scanf("%d",&t);
    while(t--){
        int n;
        scanf("%d",&n);
        if(n == 1){
            printf("-1\n");
            continue;
        }

        string ans="";
        int x=3;
        for(int i=0 ; i<n-1 ; i++){
            ans+='2';
            x+=2;
        }

        if(x%3 == 0){
            ans.pop_back();
            ans+='4';
        }

        printf("%s",ans.c_str());
        printf("3\n");

    }
}
