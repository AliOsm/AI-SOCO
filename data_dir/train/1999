#include <iostream>
#include <string>
using namespace std;
int main(){
    int n;
    cin>>n;
    for (int i=0; i<n; i++){
        string s;
        cin>>s;
        int a, b;
        cin>>a>>b;
        if (a >= 2400 && b > a){
            cout<<"YES";
            return 0;
        }
    }
    cout<<"NO";
}
