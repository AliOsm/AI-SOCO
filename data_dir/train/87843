#include <bits/stdc++.h>
using namespace std;

const int N = 1e6 + 100;

int main() {
    // freopen("in.txt", "r", stdin);
    ios::sync_with_stdio(0);
    cin.tie(0);

    int tc;
    cin >> tc;
    while(tc--) {
        int a, b, c, d, k;
        cin >> a >> b >> c >> d >> k;
        int x = (a+c-1)/c, y = (b+d-1)/d;
        if(x+y <= k) cout << x << " " << y << endl;
        else cout << -1 << endl;
    }

    return 0;
}

