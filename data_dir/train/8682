#include <bits/stdc++.h>
using namespace std;

int n;
string s,t;

int main(){





    cin >> n >> s;
    sort(s.rbegin(),s.rend());

    for (int i = 0;i < s.size();i++)
        if (s[i] == '1'){

            if (t.size() == 0 && t[t.size() - 1] != '1') t += '1';
            else{
                continue;
            }
        }
        else t += s[i];

    cout << t;



}
