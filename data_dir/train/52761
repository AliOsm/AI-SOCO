#include <bits/stdc++.h>
using namespace std;

#define elif else if
#define rep(i,a,n) for (ll i=a;i<n;i++)
#define per(i,a,n) for (ll i=a-1;i>=n;i--)
#define pf(x) printf("%.15f\n", x)
#define fix(x,y) cout<<fixed<<setprecision(y)<<x;
#define pb push_back
#define IOS ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define files freopen("input.in","r",stdin);freopen("output.out","w",stdout)
#define bs(s,n) binary_search(s.begin(),s.end(),n)
#define be(s) s.begin(),s.end()
#define sz(s) s.size()
#define pii pair<long long,long long>
#define in insert
#define er erase
#define lb lower_bound
#define ub upper_bound
#define fi first
#define se second

typedef long long ll;
typedef long double ld;
const ld pi = 3.1415926535897932384626433;
const ll M = 1e9;

int main(){
    IOS;
    ll n,k,a=0,b=0,c=0,x=0,y=0,z=0;
    cin>>n>>k;
    a=2*n;
    b=5*n;
    c=8*n;
    x=a/k;
    if((a%k)!=0)
        x++;
    y=b/k;
    if((b%k)!=0)
        y++;
    z=c/k;
    if((c%k)!=0)
        z++;
    cout<<x+y+z;
}
