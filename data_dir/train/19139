#include<bits/stdc++.h>
#define ll long long
using namespace std;
int a;
ll work(int x){
    int cg=0;
    for(int i=0;i<=30;i++){
        if(x&(1LL<<i))++cg;
    }
    return 1LL<<cg;
}
int w33ha(){
    scanf("%d",&a);
    printf("%lld\n",work(a));
    return 0;
}
int main(){
    int T;scanf("%d",&T);
    while(T--)w33ha();
    return 0;
}
