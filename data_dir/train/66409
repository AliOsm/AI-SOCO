#include <iostream>
#include <cstdio>
#include <fstream>
#include <cstring>
#include <algorithm>
#include <cmath>
#include <vector>
using namespace std;
#define pii pair<int,int>
#define mp make_pair
int const N=1e5+5;
int md=1e9+1;
int i,j,n,m,k,z,t,x,y,ans;
int a[5555];



int main()
{
cin>>n;
for (int i=0;i<=n/1234567;i++)
 for (int j=0;j<=(n-i*1234567)/123456;j++)
   {
    int k=n-i*1234567-j*123456;
    if (k%1234==0) {
        cout<<"YES";
        return 0;

    }
   }
cout<<"NO";
return 0;
}
