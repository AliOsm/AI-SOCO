#include<bits/stdc++.h>
#define int long long
using namespace std;
int32_t main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
    int n;cin>>n;
    int a[n];
    int c[2]={0};
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        c[a[i]]=i+1;
    }
    cout<<min(c[0],c[1]);
}