#include <bits/stdc++.h>
#define pb push_back
#define F first
#define S second
#define all(x) x.begin(), x.end()
using namespace std;
typedef long long ll ; 
typedef long double ld ;
typedef pair<ll , ll> pll ;

const ll N = 2e6 + 30 , Mod = 1e9 + 7 ;
const ll SQ = 330 ;


int main () {
    ios::sync_with_stdio(0), cin.tie(0) ;
    ll n , x = 0 , y = 0 ;
    cin >> n ;
    for (; n ; n --) {
    	ll d , f ;
    	cin >> d >> f ;
    	if (d > 0) x ++ ;
    	if (d < 0) y ++ ;
	}
	if (min(x , y) <= 1) cout << "YES" ;
	else cout << "NO" ;
    
    return (0) ;
}
