#include <iostream>
using namespace std;

int main() {
int x,z,g=0,w=0,b=0,n;
cin>>x>>z;
char a;
for (int i=0;i<x;i++)
{
    for (int j=0;j<z;j++)
    {
        cin>>a;
        if (a=='B'){b++;}
        if (a=='W'){w++;}
        if (a=='G'){g++;}
    }
}
n=x*z;
if (b+w+g==n)
{cout << "#Black&White";}
else
cout << "#Color";
    return 0;
}