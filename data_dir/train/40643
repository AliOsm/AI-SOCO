#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

int main()
{
	ios::sync_with_stdio(false);

	
	int q;
	cin >> q;
	while(q--)
	{
		int l, r, L, R;
		cin >> l >> r >> L >> R;
		if (l > L)
			cout << r << " " << L << "\n";
		else
			cout << l << " " << R << "\n";
	}

	return 0;
}
