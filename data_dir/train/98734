bool comp (int x , int y)
{
    return x > y;
}
bool isPal(string x)
{   string temp=x;
    reverse(x.begin(),x.end());
    return temp==x;
}
int main()
{
    string line; int k; cin >> line>>k;
    int len = line.length();
    if(len%k!=0){cout<<"NO";return 0;}
    forpos(i,len,len/k)
    {
        string temp=line.substr(i,len/k);
        if(!isPal(temp)){cout<<"NO";return 0;}
    }
    cout<<"YES";
    return 0;

}
