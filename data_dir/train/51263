#include <bits/stdc++.h>

using namespace std;

int main()
{
    int a ,b ,c , n;
    cin >> a >> b >> c >> n;
    if(c > min(a ,b))
    {
        cout << -1;
        return 0;
    }
    if(a > n || b > n || c > n)
    {
        cout << -1;
        return 0;
    }


       if( (n+c-a-b) <=0)
         cout << -1;
         else
        cout << n - (a+b) + c ;


    return 0;
}
