inline void randomizer3000 () {
  	unsigned int seed;
  	asm ("rdtsc" : "=A"(seed));
  	srand (seed);
}

int main () {
	int x1, x2, x3, x4;
	cin >> x1 >> x2 >>x3 >> x4;
	vector < int > v;
	v.pb(x4);
	v.pb(x2);
	v.pb(x3);
	v.pb(x1);
	sort(every(v));
	cout << v[3] - v[2] << ' ' << v[3] - v[1] << ' ' << v[3] - v[0] << '\n';
	return Accepted;
}

