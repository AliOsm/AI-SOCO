#include <bits/stdc++.h>

#define ll long long
#define pb push_back
#define pii pair<int,int>
#define pll pair<ll,ll>
#define ff first
#define ss second

using namespace std;

const double pi = acos(-1);
const int N = 1e6 + 6, mod = 998244353, M = 1e7 + 7;
const ll MAX = 5e18;

int T, n, a[N];

int main()
{
    scanf("%d", &T);
    while(T--){
        scanf("%d", &n);
        for(int i = 0; i < n; i++)
            scanf("%d", a + i);
        sort(a, a + n, greater<int>());
        for(int i = 0; i < n; i++)
            printf("%d ", a[i]);
        puts("");
    }

    return 0;
}
