#include <bits/stdc++.h>
using namespace std;

void solve() {
    int h, m;
    cin >> h >> m;
    int a = h*60+m;
    int ans = 24*60 - a;
    cout << ans <<endl;
}

int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    // freopen("input.txt","r",stdin);
    // freopen("output.txt","w",stdout);
    int t;
    cin >> t;
    while (t--)
    {
        /* code */
        solve();
    }
    
    
    return 0;
}