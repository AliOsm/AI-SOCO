/*


*/

int n, a[3000100], lst, fr;

int32_t main() {
    kchay;
    
    cin >> n;
    
    for(int i = 1; i <= n; i++) {
        cin >> a[i];
        if(a[i] != a[1])lst = i;
    }
    
    for(int i = 1; i <= n; i++) {
        if(a[i] != a[n]) {
            fr = i;
            break;
        }
    }
    
    cout << max(n - fr, lst - 1);
    
    
    return 0;
}

