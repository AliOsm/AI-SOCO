#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int a,b,c,d,e,i,j,k,l,n,m,x,y,t,p;
    string nm,ab;
    cin>>nm;
    cin>>ab;
    if(nm.size()!=ab.size())
    {
        printf("No\n");
        return 0;
    }
    l=nm.size();
    for(i=0;i<l;i++)
    {
        a=0;
        b=0;
        if(nm[i]=='a' || nm[i]=='e' || nm[i]=='i' || nm[i]=='o' || nm[i]=='u')
        {
            a=1;
        }
        if(ab[i]=='a' || ab[i]=='e' || ab[i]=='i' || ab[i]=='o' || ab[i]=='u')
        {
            b=1;
        }
        if((a+b)==1)
        {
            printf("No\n");
            return 0;
        }
    }
    printf("Yes\n");
}
