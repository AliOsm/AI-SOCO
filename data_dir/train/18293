#include <iostream>
#include <cstdio>
#include <algorithm>
#include <vector>
#include <list>
#include <map>
#include <set>
#include <queue>
#include <cmath>
#define rank krevedka
#define left trulala
#define next ttrulala
#define rigth trulalo
#define ran rank
using namespace std;

# define M_PI		3.14159265358979323846	/* pi */

int a[100500];

int main()
{
    int n;
    int res = 0;
    scanf("%d",&n);
    for(int i=0;i<n-1;i++)
    {
        int u,v;
        scanf("%d%d",&u,&v);
        res += a[u]++  + a[v]++;
    }
    cout <<res <<endl;

    return 0;
}

