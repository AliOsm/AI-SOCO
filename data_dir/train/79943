#include <bits/stdc++.h>

using namespace std;

string s,q;
int i,j;

int main() {
    cin>>s;
    q="heidi";
    while (i < q.size() && j < s.size()) {
        if (q[i] == s[j]) {i++; j++; continue;}
        j++;
    }
    if (i == q.size()) cout<<"YES"<<endl; else
        cout<<"NO"<<endl;
}
