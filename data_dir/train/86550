#include<bits/stdc++.h>
using namespace std;
int main()
{
   long long int a,b,c,d,e,i,j,k,l,n,m,x,y,t,p,xx,yy;
   cin>>n>>x>>y;
   xx=min(x-1,y-1);
   yy=max(x-1,y-1);
   a=xx+(yy-xx);
   xx=min(n-x,n-y);
   yy=max(n-x,n-y);
   b=xx+(yy-xx);
   if(a<=b)
   {
       printf("White\n");
   }
   else
   {
       printf("Black\n");
   }

}
