#include <iostream>
using namespace std;

int main() {
	int cnt1, cnt2, cnt3, cnt4;
	cin >> cnt1 >> cnt2 >> cnt3 >> cnt4;
	/*
	cnt1 ((
	cnt2 ()
	cnt3 )(
	cnt4 ))
	*/
	if(cnt3 > 0) {
		cout << (cnt1 > 0 && cnt4 > 0 && cnt1 == cnt4 ? 1 : 0) << endl;
	} else {
		cout << (cnt1 == cnt4 ? 1 : 0) << endl; 
	}
	return 0;
}
