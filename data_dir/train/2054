typedef pair <int,int> pii ;
typedef pair <intl,intl> pll ;
//typedef tree < pii, null_type ,less<pii>,rb_tree_tag,tree_order_statistics_node_update > ordered_set;

/***************************************************************************************************/

vector <int> g[1005] ;

int n;

main () {

    cin >> n ;
    for (int i = 2 ; i <= n ; i++) {
        int x ; cin >> x ;
        g[x].pb(i) ;
    }

    for (int i = 1 ; i <= n ; i++) {
        if (g[i].size() == 0) continue ;
        int cnt = 0 ;
        for (auto v : g[i]) {
            if (g[v].size() == 0) cnt++ ;
        }

        if (cnt < 3) {
            cout << "No" ;
            return 0 ;
        }

    }

    cout << "Yes" ;
}
