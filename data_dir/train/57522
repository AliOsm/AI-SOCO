#include<bits/stdc++.h>
using namespace std;

#define ll           long long
#define pii          pair<ll,ll>
#define bug(a)       cerr << #a << " : " << a << endl;
#define FastRead     ios_base::sync_with_stdio(false);cin.tie(NULL);

const int MAX = 1e6;

int main()
{
    ll a,b,c,d;

    cin >> a >> b >> c >> d;

    if(a != d || (c && !d))
        cout << 0 << endl;
    else
        cout << 1 << endl;
}