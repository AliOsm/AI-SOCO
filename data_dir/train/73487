int main() {
	cin.sync_with_stdio(false);
	cin.tie(0);
	//freopen("input.txt", "r", stdin);freopen("output.txt", "w", stdout);
	//freopen("ufo.in", "r", stdin);freopen("ufo.out", "w", stdout);
	ll n;
	ll ans = 0;
	cin >> n;
	ll cnt = 1;
	for (ll i = 1; i <= n; i *= 10, ++cnt) {
		ll j = min(i * 10 - 1, n);
		ans += (j - i + 1) * cnt;
	}
	cout << ans;

	return 0;
}