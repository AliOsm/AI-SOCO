#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
#include <string>

using namespace std;

#define all(a) a.begin(), a.end()

void solve()
{
    int a[4];
    
    for (int i(0); i < 4; i++)
        cin >> a[i];

    string s;
    cin >> s;

    int ans(0);

    for (int i(0); i < s.size(); i++)
    {
        switch (s[i])
        {
        case '1': ans += a[0]; break;
        case '2': ans += a[1]; break;
        case '3': ans += a[2]; break;
        case '4': ans += a[3]; break;
        }
    }

    cout << ans;
}

int main()
{
    solve();
//  system("pause");
    return 0;
}