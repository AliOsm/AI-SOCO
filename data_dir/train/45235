#include <bits/stdc++.h>

using namespace std;

#define pb push_back

const int N = 1e5;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int n;
    cin >> n;

    map < string, bool > m;
    while(n --> 0){
        string s; cin >> s;
        if(m[s]) cout << "YES" << '\n';
        else cout << "NO" << '\n';
        m[s] = true;
    }
}
