/*jai mata di
let's rock*/
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int L,v,l,r,ans;
        scanf("%d %d %d %d",&L,&v,&l,&r);
        int idx1,idx2;
        idx2=r/v;
        idx1=l/v;
        if(idx1*v==l)
        {
            ans=idx2-idx1+1;
        }
        else ans=idx2-idx1;
        printf("%d\n",L/v-ans);
    }
    return 0;
}
