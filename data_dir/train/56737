#include <bits/stdc++.h>
#define ll long long
#define OO LONG_LONG_MAX
#define oo 1e9
#define yalla ios::sync_with_stdio(0),cin.tie(0),cout.tie(0);
#define sz  100005
#define re return
#define mod  1000000007
#define pi acos(-1)
using namespace std;
int n; ll t,maxx_t,sum_items,mini=OO;
int main()
{yalla;
 cin>>n>>t;maxx_t=t; ll arr[n+2]={};

 for(int i=0;i<n;i++)
   cin>>arr[i],mini=min(arr[i],mini);

 while(t>=mini)
  {ll temp=0,mines=0;

   for(int i=0;i<n;i++)
    {
     if(t-arr[i]>=0)
       temp++,t-=arr[i],mines+=arr[i];
    }
    sum_items+=temp;

    ll down=(maxx_t-t);
    if(down==0)down=1;

    sum_items+=( ((maxx_t/down)-1) * temp );
    t-=(  mines* ((maxx_t/down)-1) );

    maxx_t=t;
  }

  cout<<sum_items<<endl;
 return 0;
}

