#include<bits/stdc++.h>

using namespace std;

int main()
{
    int x1, y1,  x2 , y2;

    cin>>x1>>y1>>x2>>y2;

    int d2 = max(x2,y2);
    int d1 = x1 + y1;

    if((x1<=x2 && y1<=y2)||d1 <=d2) {cout<<"Polycarp"<<endl; return 0;}
//    if( x2<=x1 && y2<=y1 )
        {cout<<"Vasiliy"<<endl; return 0;}


    return 0;
}

