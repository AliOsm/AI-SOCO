#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
typedef pair<int, int> ii;
typedef vector<ii> vii;
typedef vector<int> vi; 

#define xx first
#define yy second
#define mp make_pair
#define pb push_back
#define PI acos(-1.0)
#define INF 1000000000
#define all(c) c.begin(), c.end()



int main(){
	int n, k, tot = 0;
	cin >> n >> k;
	for( int i=0;i<n;i++)
	for( int j=i+1;j<n;j++)
		tot++;
	if( tot <= k ){
		cout << "no solution" << endl;
	}	
	else {
		for( int i=0;i<n;i++)
			printf("0 %d\n",i);
	}
	return 0;
}