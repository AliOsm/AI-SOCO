#include<bits/stdc++.h>
#define pb push_back
#define mp make_pair
const int MAXN = 1e5+20;
const int INF = 1e9;
using namespace std;
 



int t;
long long  a,b,n;


int main() 
{
 
	ios_base::sync_with_stdio(0);
//	freopen("name.in", "r", stdin);
//	freopen("name.out", "w", stdout);

	cin>>t;
	while (t>0)
	{
		t--;
		cin>>a>>b>>n;
		n = n % 3;
		long long f =a^b;
		if (n==0) cout<<a<<endl;
		if (n==1) cout<<b<<endl;
		if (n==2) cout<<f<<endl;
	}
}

