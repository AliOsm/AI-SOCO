#include <bits/stdc++.h>
#define LL long long
#define pb push_back
using namespace std;

int a, f, i;

int main() {
	string s;
	cin>>s>>s;
	for(char c : s) {
		if(c == 'A') a++;
		else if(c == 'I') i++;
		else f++;
	}
	if(i > 1) cout<<0<<endl;
	else if(i == 1) cout<<1<<endl;
	else if(i == 0) cout<<a<<endl;
	return 0;
}