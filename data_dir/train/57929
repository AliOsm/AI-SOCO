#include<bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define int long long int
using namespace std;
int mod = 1e9+7;
signed main()
{
	fast;
	string s;
	cin>>s;
	int n = s.size();
	int p = 0;
	for(int i=0;i<n;i++)
	{
		if(s[i]=='o')p++;
	}
	int l = n-p;
	if(p==0)
		cout<<"YES";
	else if(l%p==0)
		cout<<"YES";
	else cout<<"NO";

}