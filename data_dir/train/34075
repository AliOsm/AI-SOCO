#include <bits/stdc++.h>

using namespace std;

void solve(const int &test_id)
{
 int b,g,n;
 cin >> b >> g >> n;
 cout << 1+min(b,n)-max(0,n-g) << endl;
}

void solve_cases()
{
 int test_cases=1;
 //cin >> test_cases;
 for(int i=1;i<=test_cases;i++) solve(i);
}

void fast_io()
{
 ios::sync_with_stdio(false);
 cin.tie(0);
 cout.tie(0);
 cout << fixed << setprecision(15);
 cerr << fixed << setprecision(15);
}

int main()
{
 fast_io();
 solve_cases();
 return EXIT_SUCCESS;
}
