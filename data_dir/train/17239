#include<iostream>
#include<string>
#include<algorithm>
#include<vector>
#include<cmath>
#include<map>
#include<set>
#pragma comment(linker, "/STACK:167767260")
typedef long long ll;
const int INF = 1000000000;
#define FOR(i,a,b) for(i=a;i<=b;i++)
#define pb push_back 
#define mp make_pair
using namespace std;
int x,y,z,i;
int main()
{
  // freopen("input.txt","r",stdin);
   cin>>x>>y>>z;
   for(i=1;;i++)
   {
	   if(i%3==1)
	   {
		   x-=2;
	   }else
	   if(i%3==2)
	   {
		   y-=2;
	   }else
	   {
		   z-=2;
	   }
	   if(x<=0 && y<=0 && z<=0)break;
   }

   cout<<i+29<<endl;
}