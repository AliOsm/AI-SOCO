#include <algorithm>
#include <cstdio>
#include <cstdlib>
#include <cstring>
#include <iostream>
using namespace std;

int main(void)
{
    int n, m;
    int k;
    int i;

    scanf("%d %d", &n, &m);

    for(i = 0, k = (n - 1) / 9 + 1; i < k; i ++)
        putchar('9');
    for(i = 1; i < k; i ++)
        putchar('0');
    puts("1");

    for(i = 0; i < k; i ++)
        putchar('9');
    
    return 0;
}