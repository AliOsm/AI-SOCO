// Debugger.begin()

#define error(args...) { vector<string> _v = split(#args, ','); err(_v.begin(), args); pn;}
vector<string> split(const string& s, char c) {
	vector<string> v;
	stringstream ss(s);
	string x;
	while (getline(ss, x, c))
		v.emplace_back(x);
	return move(v);
}

void err(vector<string>::iterator it) {}
template<typename T, typename... Args>
void err(vector<string>::iterator it, T a, Args... args) {
	cerr << it -> substr((*it)[0] == ' ', it -> length()) << " = " << a << '\n';
	err(++it, args...);
}

// Debugger.end()

typedef long long LL;
static constexpr int inf = std::numeric_limits<int >::max();
//int  dx[]={1,0,-1,0};int dy[]={0,1,0,-1}; //4 Direction
//int  dx[]={1,1,0,-1,-1,-1,0,1};int dy[]={0,1,1,1,0,-1,-1,-1};//8 direction
//int  dx[]={2,1,-1,-2,-2,-1,1,2};int dy[]={1,2,2,1,-1,-2,-2,-1};//Knight Direction
//bool check(int n, int pos) {return (n & (1<<pos))>>pos;} //typecast 1 in case of int
//int  on(int n, int pos) {return n | (1<<pos);} //typecast 1 in case of int
//int  off(int n, int pos) {return n & ~(1<<pos);} //typecast 1 in case of int
//bool operator < (const data &d) const{return cost<d.cost;} //reverse in priority queue

const int M=5000, N=100000;
int arr[M], t[N], blue[M], id=0, seen[M][M], visited[M], matched_with[M];
vi dv[M], A[M], G[M], p;
map<pii, int>mapped;
void find_primes()
{
    for (LL i=0; i<N; i++) t[i]=true;
    for (LL i=2; i<N; i++)
    {
        if (t[i])
        {
            p.pb(i);
            for (LL j=i*i; j<N; j+=i) t[j]=false;
        }
    }
}
void find_divisors(int x, int n)
{
    int i=0;
    while (i<p.sz)
    {
        while (n%p[i]==0)
        {
            dv[x].pb(p[i]);
            n/=p[i];
        }
        i++;
    }
    if (n>1) dv[x].pb(n);
}
int divide(int n, int p)
{
    int sum=0;
    while (n%p==0)
    {
        sum++;
        n/=p;
    }
    return sum;
}
int node(int u, int p)
{
    if (mapped[MP(u, p)]==0) mapped[MP(u, p)]=++id;
    return mapped[MP(u, p)];
}
bool find_new_match(int u)
{
    //augmenting path
    int i, m=A[u].sz, v;
    for (i=0; i<m; i++)
    {
        v=A[u][i];
        if (visited[v]==true) continue;

        if (matched_with[v]==-1)
        {
            matched_with[v]=u;
            matched_with[u]=v;
            return true;
        }
        else if (matched_with[v]!=u)
        {
            visited[v]=true;
            if (find_new_match(matched_with[v]))
            {
                matched_with[v]=u;
                matched_with[u]=v;

                return true;
            }
        }
    }
return false;
}
int _max_match()
{
    int i, _match=0; //here m is number of members in one set and w is other
    MEM(matched_with, -1);

    _match=0;
    for (i=1; i<id; i++)
    {
        if (blue[i]==true&&matched_with[i]==-1)
        {
            MEM(visited, false);
            if (find_new_match(i))
            {
                _match++;
            }
        }
    }
return _match;
}
int main()
{
#ifndef ONLINE_JUDGE
    read;
#endif
    int i, j, k, n, m;

    find_primes();
    in2(n, m);
    for (i=1; i<=n; i++)
    {
        in(arr[i]);
        find_divisors(i, arr[i]);
    }
    while (m--)
    {
        in2(i, j);
        if (i%2==1) G[i].pb(j);
        else G[j].pb(i);
    }

    MEM(blue, false);
    MEM(seen, false);
    for (int u=1; u<=n; u+=2)
    {
        for (i=0; i<G[u].sz; i++)
        {
            int v=G[u][i];

            for (j=0; j<dv[u].sz; j++)
            {
                for (k=0; k<dv[v].sz; k++)
                {
                    if (dv[u][j]==dv[v][k])
                    {
//                        printf("(%d, %d) --> (%d, %d)\n", u, j, v, k);
                        A[node(u, j)].pb(node(v, k));
                        A[node(v, k)].pb(node(u, j));
                        blue[node(u, j)]=true;
//                        seen[node(v, k)][u]=true;
                    }
                }
            }
        }
    }

    id++;
    pr(_max_match());

return 0;
}
