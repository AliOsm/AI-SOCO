#include <bits/stdc++.h>
typedef long long ll ;
using namespace std;

int main()
{
    ios::sync_with_stdio(false);

    int a = 0 , b = 0 ;

    cin >> a >> b ;

    for(int i = 1 ;  ; i++){
           if(i & 1) {
                a -= i ;
                if(a < 0){
                      cout << "Vladik" << endl;
                      return 0 ;
                }
           }
           else {
               b -= i ;
               if(b < 0){
                  cout << "Valera" << endl ;
                  return 0 ;
               }
           }
    }

    return 0;
}
