#include<iostream>
#include<string>
#include <cstdio>
//#include<bits/stdc++.h>
#include<cmath>
#include<queue>
#include<algorithm>
#include<map>
#include<set>
#include <stdio.h>
using namespace std;
int main()
{
	int n;
	scanf("%d", &n);
	vector<int>v(n + 1);
	int mx = 0;
	for (int i = 1; i <= n; i++){
		scanf("%d", &v[i]);
		mx = max(v[i], mx);
	}
	printf("%d", mx);
	return 0;
}