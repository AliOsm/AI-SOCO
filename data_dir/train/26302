#include<bits/stdc++.h>
using namespace std; 
#define mp make_pair
#define pb push_back
#define int long long
#define pii pair<ll,ll>
#define F first
#define S second
int const M=4e5+10,M2=(1<<20)+10,mod=1e9+7,inf=1e9+10;
int32_t main()
{
	int n;
	cin>>n;
	int cnt1=0,cnt0=0;
	string x;
	cin>>x;
	for(int i=0;i<x.size();i++)
	{
		if(x[i]=='z')cnt0++;
		if(x[i]=='n')cnt1++;
	}
	for(int i=1;i<=cnt1;i++)cout<<1<<" ";
	for(int i=1;i<=cnt0;i++)cout<<0<<" ";
}		
