#pragma GCC optimize ("O3")
#include <bits/stdc++.h>
#define abdelrahman001 ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define pi acos(-1)
typedef long long ll;
typedef long double ld;
using namespace std;

int main()
{
    abdelrahman001
    ll h,l;
    cin >> h >> l;
    ld ang = atan(l*1.0/h*1.0) ,ang1 = 90 - (ang*180)/pi,ang2 = (ang*180)/pi-ang1;
    cout << fixed << setprecision(9) << l*1.0*tan((ang2*pi)/180);
    return 0;
}
