#include <iostream>
#include <map>
#include <vector>
#include <set>
#include <algorithm>
#include <cmath>
#include <assert.h>
#include <queue>
using namespace std;

#define all(a) (a).begin(), (a).end()

int main() {
    // freopen("in.txt", "rt", stdin);
    ios_base::sync_with_stdio(0);
    cin.tie(0);

    int n;
    cin >> n;

    for (int i = 0; i < n; ++i) {
        long long l, r, d;
        cin >> l >> r >> d;

        if (d < l) {
            cout << d << "\n";
        } else {
            cout << r / d * d + d << "\n";
        }
    }
}
