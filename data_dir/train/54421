		in << endl;
		auto _____ = std::stringstream(in.str());
		solveSlow(_____, ___);
	}
	static void solve(std::istream& in, std::ostream& out) {
		auto solve = [&] (vector<int> aa) -> int {
			vector<pair<int, int>> a;
			for (int val : aa) {
				if (!a.empty() && a.back().first == val) {
					a.back().second++;
				} else {
					a.emplace_back(val, 1);
				}
			}
			int res = 0;
			map<int, int> left, right;
			for (auto&[val, count] : a) right[val] += count;
			for (auto&[val, count] : right) res = max(res, count); // single value
			for (int i : range(a.size())) {
				auto mem_right = right;
				auto& [original_val, __] = a[i];
				int original_count = 0;

				for (int mid_right = i; mid_right < a.size(); mid_right++) {
					auto& [val, count] = a[mid_right];
					mem_right[val] -= count;
					if (val == original_val) {
						original_count += count;
					}

					int other_max = 0;
					for (auto&[other_val, other_count] : left) {
						other_max = max(other_max, min(other_count, mem_right[other_val]));
					}

					res = max(res, 2 * other_max + original_count);
				}

				right[original_val] -= __;
				left[original_val] += __;
			}

			return res;
		};

		int tests; in >> tests;
		for (int test : range(tests)) {
			int n; in >> n;
			vector<int> a(n); for (auto& val : a) in >> val;
			out << solve(a) << endl;
		}
	}
};


