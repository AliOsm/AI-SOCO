class E1ThreeBlocksPalindromeEasyVersion {
public:
	static void solveSlow(std::stringstream& in, std::ostream& out) {
		auto is_pal = [] (vector<int> a) -> bool {
			set<int> s(a.begin(), a.end());
			if (s.size() > 2) return false;
			int tran = 0;
			for (int i = 0; i < a.size() - 1; i++) {
				if (a[i] != a[i + 1]) {
					tran++;
				}
			}
			if (tran > 2) return false;
			auto rev = a; reverse(rev.begin(), rev.end());
			for (int i : range(a.size())) if (a[i] != rev[i]) return false;
			return true;
		};

		auto solve = [&] (vector<int> a) -> int {
			int res = 0;
			for (int mask = 1; mask < (1 << a.size()); mask++) {
				vector<int> temp;
				for (int i : range(a.size())) {
					if (((mask >> i) & 1) == 1) {
						temp.push_back(a[i]);
					}
				}
				if (is_pal(temp)) {
					res = max<int>(res, temp.size());
				}
			}
			return res;
		};

		int tests; in >> tests;
		for (int test : range(tests)) {
			int n; in >> n;
			vector<int> a(n); for (auto& val : a) in >> val;
			out << solve(a) << endl;
		}
	}
	static constexpr int kStressIterations = 1000;
	static void generateTest(std::ostringstream& in, std::ostringstream& ___) {
		in << 1 << endl;
