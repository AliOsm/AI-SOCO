#include <bits/stdc++.h>
#define SZ(x) (int)x.size()
typedef long long ll;
const int SZ=1e5+5;
const double PI=acos(-1);
using namespace std;

int main()
{
    double d,h,v,e;
    cin>>d>>h>>v>>e;
    
    double incUp=PI*d*d/4*e;
    double cur=PI*d*d/4*h;
    
    if(incUp>=v) cout<<"NO";
    else cout<<"YES\n"<<cur/(v-incUp);
}