#include<bits/stdc++.h>
using namespace std;

#define ll long long

#define mkp make_pair 
#define pb push_back
#define speed   ios::sync_with_stdio(false), cin.tie(0) , cout.tie(0)

 

main(){
  speed;
  int n;
  cin>>n;
  n = n+n;
  int a[n];
  for(int i =0 ; i < n; i++)
  {
  	cin>>a[i];
  }
  sort(a,a+n);
 	bool pos = true;
  for(int i = 0 ; i < n/2 ;i ++)
  {
  	if(a[i] >= a[(n/2)])
  		pos = false;
  }
  if(pos)
  	cout<<"YES"<<endl;
  else
  	cout<<"NO"<<endl;
 
}//main