#include <bits/stdc++.h>
using namespace std;
#define F first
#define S second
#define PI acos(-1)
#define X real()
#define Y imag()

typedef long long lli;
typedef complex<lli> P;
const lli MaX = 100005 , infi =1110111110111 , mod = 998244353 ;
typedef pair<int,int> pii;
const int MX = 1000005;

lli n,k,i;

int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
    cin>>n;
    while(n-- && cin>>k>>i)
        cout<<9*(k-1)+i<<"\n";
    return 0;
}
