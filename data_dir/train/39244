#include <cstdio>
using namespace std;

int main(int argc, char **argv)
{
#ifndef ONLINE_JUDGE
  freopen("input.txt", "r", stdin);
  freopen("output.txt", "w", stdout);
#endif

  int a, b, r;
  scanf("%d %d %d", &a, &b, &r);
  r += r;
  puts(r <= a && r <= b ? "First" : "Second");

  return 0;
}