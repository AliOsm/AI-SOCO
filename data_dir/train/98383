#include <algorithm>
#include <bitset>
#include <cmath>
#include <cstdio>
#include <cstdlib>
#include <cstring>
#include <iomanip>
#include <iostream>
#include <map>
#include <queue>
#include <set>
#include <stack>
#include <string>
#include <utility>
#include <vector>
using namespace std;
#define sc(a) scanf("%d", &a)
#define sc2(a, b) scanf("%d%d", &a, &b)
#define sc3(a, b, c) scanf("%d%d%d", &a, &b, &c)
#define scs(a) scanf("%s", a)
#define pri(x) printf("%d\n", x)
#define prie(x) printf("%d ", x)
#define mp make_pair
#define pb push_back
#define BUFF ios::sync_with_stdio(false);
#define db(x) cerr << #x << " == " << x << endl
typedef long long int ll;
typedef long double ld;
typedef pair<int, int> ii;
typedef vector<int> vi;
typedef vector<vector<int> > vvi;
typedef vector<ii> vii;
typedef vector< vii> vvii;
const int INF = 0x3f3f3f3f;
const ll LINF = 0x3f3f3f3f3f3f3f3fll;
const ld pi = acos(-1);
const int MOD = 1e9 + 7;
int main()
{
	int n;
	sc(n);
	int dupla=2*n;
	int especial=n;
	int k;
	sc(k);
	vi resto;
	while(k--){
		int x;
		sc(x);
		int res=x%4;
		if(especial>=x/4){
			especial-=x/4;
		}else{
			res+=4*(x/4-especial);
			especial=0;
		}
		if(res>0)
			resto.pb(res);
	}
	dupla+=especial;
	int solitaria=especial;
	for(int x: resto){
		if(x%2 and solitaria>0) {
			solitaria--;
			x--;
		}
		int cara= (x+1)/2;
		if(dupla>=cara){
			dupla-=cara;
		}
		else{
			int difi=cara-dupla;
			dupla=0;
			solitaria-=difi*2;
			if(solitaria<0){
				puts("NO");
				return 0;
			}
		}
	}
	puts("YES");
	return 0;
}
