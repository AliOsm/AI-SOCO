#include <iostream>
#include <fstream>
#include <map>
#include <set>
#include <queue>
#include <string>
#include <cmath>
#include <algorithm>
#include <assert.h>
#include <vector>
#include <cstring>
#include <bitset>
#include <random>

#define all(a) (a).begin(), (a).end()
#define sz(a) (int)(a).size()
#define pb push_back

using namespace std;

typedef long long ll;
//typedef __int128 ll;
typedef pair<int, int> pii;
typedef pair<ll, ll> pll;
typedef double ld;

int main() {

    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);

    int n;
    cin >> n;

    map<int, int> a, b;
    for (int i = 0; i < n; ++i) {
        int w;
        cin >> w;
        a[w] = i;
    }

    vector<int> ans;
    for (int i = 0; i < 2 * n; ++i) {
        char c;
        cin >> c;
        if (c == '0') {
            map<int, int>::iterator it = a.begin();
            ans.pb(it->second);

            b.insert({-it->first, it->second});
            a.erase(it);
        } else {
            map<int, int>::iterator it = b.begin();
            ans.pb(it->second);
            b.erase(it);
        }
    }

    for (int x : ans) {
        cout << x + 1 << " ";
    }
    cout << "\n";

}