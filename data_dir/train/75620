#include <bits/stdc++.h>
#define ll long long
using namespace std;
bool sl[101];
int main()
{
    //ifstream cin("in.in");
    ll a, b, aa = 0, n = 0, bb = 0;
    cin >> a >> b;
    for(ll i = 1;i <= 6;i++)
    {
        if(abs(i-a) < abs(i-b))aa++;
        else if(abs(i-a) > abs(i-b))bb++;
        else n++;
    }
    cout << aa << " " << n << " " << bb;
    return 0;
}
