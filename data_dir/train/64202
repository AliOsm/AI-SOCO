#include<bits/stdc++.h>

using namespace std;

int main()
{
    string s ;
    cin >> s >> s ;
    for (int i = s.size() -1  ; i >= 0 ; i-- )
    {
        if (s[i] == '1' && s[i + 1] == '0' || s[i] =='0' && s[i + 1] == '1')
        {
            s.erase(i,2) ;
        }
    }
    cout << s.size() ;


}
