#include <bits/stdc++.h>
 
using namespace std;
 
#define maxn 100010
#define INF 1e15
#define ff first
#define ss second
#define endl '\n'
typedef long long ll;

int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(0);
  int n;
  cin >> n;
  if (n & 1) cout << "0" << endl;
  else cout << (1LL << (n / 2)) << endl;
}