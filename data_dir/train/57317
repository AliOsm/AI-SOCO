//1923C
#include <iostream>
#include <string>

using namespace std;

int n, k, c[2], i;
string s;

int main()
{
	cin >> n >> k >> s;
	c[0] = k / 2;
	c[1] = c[0];

	while (c[0] > 0 || c[1] > 0)
	{
		if (c[s[i]==')'] > 0)
		{
			cout << s[i];
			--c[s[i] == ')'];
		}
		++i;
	}

	getchar();getchar();
	return 0;
}