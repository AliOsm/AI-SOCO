#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define fr first
#define sc second
#define r_0 return 0
#define fin freopen("input.txt", "r", stdin)
#define fout freopen("output.txt", "w", stdout)
#define TIME ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
using namespace std;
    const ll N = 2e5 + 7 ;
    const ll inf = 1e18;
float n , x , y , z ;
char c ;
map < float , int > mp ;
vector < float > v ;
int main(){
TIME
    cin >> n ;
    for ( int i = 1; i <= n ; i ++ ) {
        cin >> c >> x >> c >> y >> c >> c >> z  ;
        x += y ;
        x /= z ;
        mp [ x ] ++ ;
        v.pb ( x );
    }
    for ( auto i : v ){
        cout << mp [ i ] << ' ';
    }

}
