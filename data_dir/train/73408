#include <iostream>
#include <cstdio>
#include <cmath>
#include <vector>
#include <algorithm>

using namespace std;

int main()
{
    //freopen("input.txt", "r", stdin);
    ios_base::sync_with_stdio(0);

    int a, b;

    cin >> a >> b;

    a = min(a, b);

    if (a % 2 == 0) cout << "Malvika\n";
    else cout << "Akshat\n";

    return 0;
}
