#include<bits/stdc++.h>
#define ll long long
#define ld long double
#define OO 5e18
#define oo 2e9
#define yalla ios::sync_with_stdio(0),cin.tie(0),cout.tie(0);
#define FILES freopen("input.txt", "r", stdin); freopen("output.txt", "w", stdout);
#define sz  100005
#define re return
#define mod  1000000007
#define pi acos(-1)
using namespace std;
ll n,q;
int main()
{yalla;
    cin>>q;
    while(q--)
    { cin>>n; ll cnt=0;
      while(n!=1)
      { if(n%2==0)
         n/=2,cnt++;
        else if(n%3==0)
         n=(2*n)/3,cnt++;
        else if(n%5==0)
         n=(4*n)/5,cnt++;
        else
          break;
      }


     n==1 ? cout<<cnt<<endl : cout<<-1<<endl;
    }
    re 0;
}



