ll t,n,m,p,k,x;

int main()
{
    IOS;
    cin>>t;
    while(t--){
        cin>>n>>m>>k;
        p=0;
        rep(i,1,n){
            cin>>x;
            p+=x;
        }

        k=min(m,k+p);
        cout<<k<<endl;

