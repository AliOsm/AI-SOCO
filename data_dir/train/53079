#include <iostream>
#include <cstdio>
#include <cstdlib>
#include <cmath>
#include <string>
#include <map>
#include <set>
#include <algorithm>

using namespace std;
#define forn(i,a,b) for(i=(a);i<=(b);++i)
#define ford(i,a,b) for(i=(b);i>=(a);--i)
#define ll long long
int i,x,y,s=0,n;
int main(){
  cin>>n;
  forn(i,1,n) 
   {
     cin>>x>>y;
     if (s+x<=500) {
      cout<<"A";
      s+=x;
     }
     else {
      s-=y;
      cout<<"G";
     }
   }
}
