#include <bits/stdc++.h>
using namespace std;
 
int main()
{
	int t;
	scanf(" %d", &t);
	while(t--)
	{
		long long a, b;
		scanf(" %lld %lld", &a, &b);
		long long c = round(cbrt(a * b));
		if(c * c * c == a * b and a % c == 0 and b % c == 0)
			puts("Yes");
		else 
			puts("No");
	}

    return 0;
}