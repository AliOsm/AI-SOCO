#include <bits/stdc++.h>
using namespace std;

int n, d;
int s = 0;

int main (int argc, char *argv[])
{
	scanf("%d%d", &n, &d);
	for(int i = 0; i < n; ++i) {
		int a;
		scanf("%d", &a);
		s += a;
	}
	if(s + 10 * (n - 1) > d) {
		printf("-1");
		return 0;
	}
	printf("%d", (n-1) * 2 + (d - s - 10 * (n-1))/5);
	return 0;
}

