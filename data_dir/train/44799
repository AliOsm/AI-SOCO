int di[] = { 0, 1, 0, -1 };
int dj[] = { 1, 0, -1, 0 };
int dik[] = { 1, 1, 2, 2, -1, -1, -2, -2 };
int djk[] = { 2, -2, 1, -1, -2, 2, -1, 1 };

ll gcd(ll x, ll y){ return !y ? x : gcd(y, x%y); }

int main(){
    /*freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);*/
    int n, m, hope = 0;
    cin >> n >> m;
    char ans[100][100];
    mem(ans, '.');
    rep(i, n){
        if (i % 2 == 0) rep(j, m) ans[i][j] = '#';
        else if (!hope) ans[i][m - 1] = '#', hope = 1;
        else ans[i][0] = '#', hope = 0;

    }
    rep(i, n){
        rep(j, m) cout << ans[i][j];
        cout << endl;
    }
    return 0;
}
