#include<bits/stdc++.h>

#define ll long long
#define dl double

using namespace std;

int a[110];

int main()
{
    int n;
    cin >> n;

    int p, pp;
    cin >> p;

    for(int i = 0; i < p; i++){
        cin >> pp;
        a[pp]++;
    }

    int q, qq;
    cin >> q;

    for(int i = 0; i < q; i++){
        cin >> qq;
        a[qq]++;
    }

    for(int i = 1; i <= n; i++){
        if(a[i] == 0){
            cout << "Oh, my keyboard!\n";
            return 0;
        }
    }

    cout << "I become the guy.\n";
    return 0;
}
