#include<bits/stdc++.h>
typedef long long LL;
using namespace std;

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);

    int t;
    cin>>t;

    while (t--) {
        int n;
        cin>>n;
        string s;
        cin>>s;

        while (s.size() && s[0] != '8') s.erase(s.begin());
        assert(s.empty() || s[0] == '8');
        cout<<(s.size()>=11 ? "YES" : "NO")<<endl;
    }
}
