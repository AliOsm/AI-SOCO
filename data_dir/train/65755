#include <cstdio>
typedef long long ll;
typedef long double d;
typedef unsigned long long ull;
#define  mod 1000000007
using namespace std;

int main()
{
    cin.tie(0);
    std::ios::sync_with_stdio(false);
    int x,n,p;
    vector <ll> a;
    cin >> n>>p;
    for(int k=n;k>n-p;k--)
        cout << k << " " ;
    for(int k=1;k<=n-p;k++)
        cout << k<< " ";
    cout << endl;
    return 0;
}
