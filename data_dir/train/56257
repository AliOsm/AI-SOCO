#include<bits/stdc++.h>
using namespace std;
int main () {    
	int a, b,c = -1;
	cin >> a >> b;
	while(a--)  {
	int x, y;
	   cin >> x >> y;
        if (y == 0) y = 100;
        if (x < b || (x == b && y == 100)) 
        c = max(c, 100 - y);
        }        
        cout << c;

return 0;	
}
