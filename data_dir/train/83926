//ITNOA
#include <bits/stdc++.h>
using namespace std;
#define F first
#define S second
#define pb push_back
const long long INF=1e9+10,MAX=2e5+1e4,MOD=1e9+7;
typedef long long ll;
typedef pair<ll,ll> pii;
typedef pair<pii,ll> piii;
void OUT(long double o,int x){
	 cout<<fixed<<setprecision(x)<<o;
	 return;
}
ll a[MAX];
int main(){
	ios::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);
	int n,k;
	cin>>n>>k;
	for(int i=n;i>k+1;i--)
		cout<<i<<" ";
	int x=k+1,y=1;
	for(int i=k+1;i>0;i--){
		if(i%2==(k+1)%2){
			cout<<x<<" ";
			x--;
		}
		else{
			cout<<y<<" ";
			y++;
		}
	}
	return 0;
}
