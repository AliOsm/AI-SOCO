//In The Name Of God
#include<bits/stdc++.h>
#define ll long long
#define pb push_back
using namespace std;

const int MAXN = 1e5+22 ;

int n,m,z,res;

int main()
{
	ios_base::sync_with_stdio(false);
	cin>>n>>m>>z;
	for(int i=1;i<=z;i++)
	{
		if(i%n==0 and i%m==0)
			res++;
	}
	cout<<res;
	return 0;
}
