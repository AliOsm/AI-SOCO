#include <bits/stdc++.h>
#define VI vector<int>
#define pb push_back
#define mp make_pair
#define pii pair<int,int>
#define INF 0x3f3f3f3f
#define LL long long
#define ULL unsigned long long
using namespace std;
int A[] = {8,4,2,6};
int main(){
	int n;
	scanf("%d", &n);
	if(n == 0) printf("1\n");
	else printf("%d\n", A[(n-1)%4]);
	return 0;
}