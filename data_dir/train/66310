#include <bits/stdc++.h>
#define INT long long int
#define oo 987654321
using namespace std;
INT T, n, m, a[100010];
INT f(INT x)
{
    if( x == 1 ) return 2;
    return 3*(x*(x-1))/2+2*x;
}
int main()
{
    ios::sync_with_stdio(false);
    cin>>T;
    while( T-- )
    {
        cin>>n>>m;
        if( n == 1 or m == 1 ) cout<<"YES\n";
        else if( n < 3 and m < 3 ) cout<<"YES\n";
        else cout<<"NO\n";
    }
}

