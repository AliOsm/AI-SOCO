#include<bits/stdc++.h>
using namespace std;
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define pb push_back 
#define all(a) a.begin(),a.end()
#define lb lower_bound
#define ub upper_bound
#define pii pair<int,int>
#define ld long double
#define int long long int 
#define F first 
#define S second
signed main()
{
    fast;
    int a,b;
    cin>>a>>b;
    if(a == 9 && b == 1)
    {
        cout<<9<<" "<<10;
        return 0;
    }
    if(b-a<0 || b-a>=2)
        cout<<-1;
    else
    {
        if(b-a == 1)
        {
            cout<<a<<" "<<b;
        }
        else
        {
            cout<<a<<"1"<<" "<<b<<"2";
        }
    }
}

