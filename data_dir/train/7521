#include <bits/stdc++.h>
 using namespace std;
  int main()
  {
    int n; cin>>n;
    int a[100];
     for (int i=0;i<n;i++)
     {
         cin>>a[i];
     }
     sort(a,a+n);
     int i,f=0;
      for (i=1;i<n;i++)
      {
          if (a[i]>a[0]) {
                          cout<<a[i];
                          return 0;
                      }
      }
      cout<<"NO";
    }

