#include <bits/stdc++.h>
#define ll long long
using namespace std;
int main()
{
    //ifstream cin("in.in");
    string v, mek = "hello";
    ll poi = 0;
    cin >> v;
    for(ll i = 0;i < v.size();i++)
    {
        if(v[i] == mek[poi])poi++;
    }
    if(poi > 4)cout << "YES";
    else cout << "NO";
    //cin.close();
    return 0;
}
