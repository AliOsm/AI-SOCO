// start + (end - start) / 2;

#define OO ll(1e18)
#define mod ll(1000000007)
char a[1000006];
int main() {
	scanf("%s", a);
	int n = strlen(a);
	priority_queue<int>q;
	for (int i = 0; i < n; i++) {
		if (a[i] == 'l')q.push(i + 1);
		else printf("%d\n", i + 1);
	}
	for (; q.size(); q.pop())printf("%d\n", q.top());
}