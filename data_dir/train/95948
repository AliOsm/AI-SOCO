#include<bits/stdc++.h>

using namespace std;

int main() {
	
	ios_base::sync_with_stdio(0);
	cin.tie(NULL);
	
	string s;
	cin>>s;
	bool flag = true;
	char ch = 'a';
	for(auto i : s) {
		if(i > ch) { flag = false;
			break;
		}
		if(i == ch) ch++;
	}
	if(flag) cout<<"YES";
	else cout<<"NO";

	return 0;
}