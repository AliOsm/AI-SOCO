ll x,y;
int main()
{
    //cout<<Pow((ll)1000000,(ll)110);
//freopen("hotel.in","r",stdin);
//freopen("math.in","w",stdout);
//printf("%s %.4f\n",k.c_str(),g);
cin>>x>>y;
if(x==y)
    cout<<"=";
else if(x<y)
{
    if(x==1)
        cout<<"<";
    else if(x==2&&y==3)
        cout<<"<";
    else if(x==2&&y==4)
        cout<<"=";
    else
        cout<<">";
}
else if(y<x)
{
    if(y==1)
        cout<<">";
    else if(y==2&&x==3)
        cout<<">";
    else if(y==2&&x==4)
        cout<<"=";
    else
        cout<<"<";
}
return 0;
}
