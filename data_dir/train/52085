#include <iostream>
#include <fstream>

#include <bits/stdc++.h>

using namespace std;

class CDiverseMatrix {
public:
    void solve(std::istream &in, std::ostream &out) {
        int r, c;
        in >> r >> c;

        if (max(r, c) == 1) {
            out << 0 << endl;
            return;
        }

        if (c == 1) {
            for (int i = 0; i < r; ++i) {
                out << i + 2 << endl;
            }
            return;
        }

        for (int i = 0; i < r; ++i) {
            for (int j = 0; j < c; ++j) {
                int a = i == 0 ? 1 : c + i + 1;
                int b = j + 2;
                out << a * b << " ";
            }
            out << endl;
        }
    }
};


int main() {
    CDiverseMatrix solver;
    std::istream &in(std::cin);
    std::ostream &out(std::cout);
    solver.solve(in, out);
    return 0;
}
