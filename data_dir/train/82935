#include <bits/stdc++.h>

using namespace std;
#define ll long long
#define go ios::sync_with_stdio(0)

int main()
{
    go;
    int a,b;
    cin>>a>>b;
    for(int i=1; ;i++)
    {
        if(i&1)
        {
            a-=i;
        if(a<0)
        {
            cout<<"Vladik";
            return 0;
        }
        }
        else
        {
            b-=i;
        if(b<0)
        {
            cout<<"Valera";
            return 0;
        }
        }
    }
    return 0;
}
