#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long ll;

ll T, N, K, a[MAX];
bool cmp( ll a , ll b ) { return a > b; }

void solve() {
    cin >> N >> K;
    for( ll i = 1; i <= N; i ++ )
        cin >> a[i];
    sort( a + 1 , a + N + 1 , cmp );
    ll ans = 0, sum = 0;
    for( ll i = 1; i <= N; i ++ ) {
        sum += a[i];
        if( K * i <= sum )
            ans = i;
    }
    cout << ans << "\n";
}

int main()
{
    optimiza_io
    cin >> T;
    while( T -- ) 
        solve();
    return 0;
}

