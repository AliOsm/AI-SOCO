// in the name of Allah

#include <bits/stdc++.h>

#define endl '\n'
#define sz(x) ((int) (x.size()))

using namespace std;

int main() {
	ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
	int n, m;
	cin >> n >> m;
	for(int i=0; i<n; i++) cout << 1; cout << endl;
	for(int i=0; i<n-1; i++) cout << 8; cout << 9 << endl;
	return 0;
}