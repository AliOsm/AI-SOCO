using namespace std;

#define REP(i,n) for((i)=0;(i)<(int)(n);(i)++)
#define snuke(c,itr) for(__typeof((c).begin()) itr=(c).begin();itr!=(c).end();itr++)

int main(void){
	int ans=0,i,j;
	
	string s;
	cin >> s;
	
	string t = "aiueo13579";
	
	REP(i,s.length()) REP(j,10) if(s[i] == t[j]) ans++;
	
	cout << ans << endl;
	
	return 0;
}
