#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef pair<int, int> ii;
typedef pair<ll, ll> l4;
//typedef pair<double, double> dd;
#define mp make_pair
#define pb push_back
#define debug(x) cerr << #x << " = " << x << " "



int main()
{
  int T; scanf("%d", &T);
  for (int kase = 1; kase <= T; ++kase)
    {
      int n; scanf("%d", &n);
      set<int> st;
      for (int i = 0; i < n; ++i)
	{
	  int x; scanf("%d", &x); st.insert(x);
	}
      printf("%d\n", (int) st.size());
    }
}
