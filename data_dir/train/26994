//ITNOG
#include<bits/stdc++.h>

using namespace std;

const int maxN = 1000*1000 + 5;
const int inf = 1000*1000*1000;
int fib[26];

main()
{
    ios::sync_with_stdio(0); cin.tie(0);
    int n; cin >> n;
    int ans = 0;
    for(int i = 0; i < n; i ++)
    {
	  int x; cin >> x;
	  ans += x * (i + 1);
    }
    cout << ans << endl;
    return 0;
}
//end
