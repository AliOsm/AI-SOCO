const int maxn = 1e1 + 100  ;
const int inf = 1e18 + 1 ; 
int ans ; 
S s , s1 ; 

main ( ) 
{
  cin >> s >> s1 ; 
  for ( int i = 0 ; i < s . size ( ) ; i ++ ) 
    if ( s . substr ( i , s1 . size ( ) ) == s1 ) 
      s [ i + s1 . size ( ) - 1 ] = '#' , ans ++ ;
  cout << ans ;
}
