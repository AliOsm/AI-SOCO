#include <bits/stdc++.h>
#define pb push_back
#define inf 0x3f3f3f3f
#define all(v) (v).begin() , (v).end()
#define ones(n) __builtin_popcount(n)
using namespace std;

typedef vector<int> vi;
typedef long long ll;

int di[] = {0 , 0 , 1 , -1 , 1 , 1 , -1 , -1};
int dj[] = {1 ,-1 , 0 , 0 , -1 , 1 ,  1 , -1};

int main(){
    int n;
    scanf("%d",&n);

    int x , ans = 0;
    for(int i = 0 ;i < n ;i++){
        scanf("%d",&x);
        ans += abs(x);
    }

    cout << ans;
}
