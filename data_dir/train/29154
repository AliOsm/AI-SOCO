#include <bits/stdc++.h>
#define LL long long
#define pb push_back
#define fi first
#define sc second
#define mp make_pair
using namespace std;

LL n, a[1000100], sum;

int main() {
	ios::sync_with_stdio(0);
	cin.tie();
	cin>>n;
	for(int i = 0; i < n; i++) {
		cin>>a[i];
		sum += a[i];
		if(sum%2==1) {
			cout<<"First"<<endl;
			return 0;
		}
	}
	cout<<"Second"<<endl;
	return 0;
}