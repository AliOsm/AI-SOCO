#include<bits/stdc++.h>
#define ll long long 
#define scn(n) scanf("%d",&n)
#define lscn(n) scanf("%lld",&n)
#define lpri(n) printf("%lld",n)
#define pri(n) printf("%d",n)
#define pln() printf("\n")
#define priln(n) printf("%d\n",n)
#define lpriln(n) printf("%lld\n",n)
#define rep(i,init,n) for(int i=init;i<n;i++)
#define pb push_back
#define mp make_pair
#define F first
#define S second
#define gcd __gcd
#define inf INT_MAX
#define ninf INT_MIN
using namespace std;
const ll mod=1e9+7;
const int N=1e4+4;
int main()
{
    int a,b,c,d;
    cin>>a>>b>>c>>d;
    if(a==d)
    {
        if(c==0||(c>0&&a>0))
        {
            cout<<1;
            return 0;
        }
    }
    cout<<0;
    return 0;
}