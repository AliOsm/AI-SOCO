#include<iostream>
#include<string>
#include<cmath>
#include<math.h>
#include<set>
using namespace std;
int main()
{
    int size;
    set<char>s;
    char temp;
    cin >> size;
    for (int i = 0;i<size; i++)
    {
        cin >> temp;
        temp=tolower(temp);
        s.insert(temp);
    }
    if (s.size() >= 26)
        cout << "YES" << endl;
    else
        cout << "NO" << endl;
    return 0;
}