#include<bits/stdc++.h>
#define ll long long
#define ld long double
#define PB push_back
#define MP make_pair
#define INF 1e18
#define inf 1e9
#define y1 dasdsvcvd
using namespace std;
int h[105], n, x, ctr, t;
int main()
{
ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
cin >> n >> x;
for(int i = 0; i < n; i++)
{
    cin >> t;
    h[t]++;
}
for(int i = 0; i < x; i++)
{
    if(!h[i])
        ctr++;
}
if(h[x])
    ctr++;
cout << ctr;
return 0;
}
