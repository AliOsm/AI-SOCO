int main()
{
	int t;
	cin>>t;
	while (t--)
	{
		long long x,y;
		cin>>x>>y;
		if (x < y) swap(x,y);
		if (x > y)
		{
			long long rs = 2*y+1;
			x-= y+1;
			if (x%2==0) rs += x*2;
			else
				rs += 2*x+1;
			cout<<rs<<endl;
		}
		else
			cout<<x+y<<endl;
			
	}
}


