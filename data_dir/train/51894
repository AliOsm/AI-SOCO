int main()
{
	int n,m;
	while (cin>>n>>m)
	{
		vector<long long>a(n);
		long long mx = 0;
		for (int i = 0; i < n; i++) {cin>>a[i];
		mx = max(a[i], mx);
		}
		long long lo = mx;
		long long hi = 2000000000;
		while (lo <= hi)
		{
			long long md = (lo+hi) / 2;
			int rs = 0;
			long long rm = 0;
			for (int i = 0; i < n; i++)
			{
				if (a[i] > rm)
				{
					rm = md;
					rs++;
					rm-= a[i];
				}
				else
					rm-=a[i];
			}
			if (rs > m)
				lo = md+1;
			else
				hi = md-1;
		}
		cout<<lo<<endl;
	}
}


set<pair<int,int> > p;
vector<vector<int> > g;
vector<int> c;
int dfs(int i, int rm)
{
	if (rm==0) return 0;
	p.insert(make_pair(c[i], i));
	int rs =100000000;
	for (int j = 0; j < g[i].size(); j++)
	{
		pair<int,int> v = make_pair(c[g[i][j]], g[i][j]);
		if (p.count(v) > 0) continue;
		v.second = 10000;
		int inv = distance(p.lower_bound(v), p.end());
		rs = min(rs, inv + dfs(g[i][j], rm-1));
	}
	p.erase(make_pair(c[i], i));
	return rs;
}

int f(vector<int> a, vector<int> b, vector<int> C, k)
{
	c = C;
	g = vector<vector<int> > (a.size());
	for (int i = 0; i < a.size(); i++)
	{
		g[a[i]].push_back(b[i]);
		g[b[i]].push_back(a[i]);
	}
	int rs = 10000000;
	for (int i = 0; i < a.size(); i++) rs = min(rs, dfs(i, k-1));
	if (rs >= 10000000) return -1;
	return rs;
}


int main()
{
	int n;
	cin>>n;
	vector<long long> a(n);
	for (int i = 0; i < n; i++)
	{
		cin>>a[i];
	}
	vector<long long> l(n,1);
	for (int i = n-2; i >=0; i--)
	{
		if (a[i] < a[i+1]) l[i] = l[i+1]+1;
	}
	vector<long long> r(n,1);
	for (int i = 1; i <n; i++)
	{
		if (a[i] > a[i-1]) r[i] = r[i-1]+1;
	}
	int mx = 0;
	for (int i = 0; i < n; i++)
	{
		int ri = 0;
		if (i+1 < n) ri = l[i+1];
		int li = 0;
		if (i > 0) li = r[i-1];
		if (i+1 < n && i > 0)
		{
			if (a[i+1]-a[i-1] >= 2) mx = max(mx, ri+li+1);
			else mx = max(mx, max(ri+1, li+1));
		}
		else
		{
			mx = max(mx, max(ri+1, li+1));
		}
	}
	cout<<mx<<endl;
}


int main()
{
	long long n,m,k,p;
	cin>>n>>m>>k>>p;
	vector<vector<long long> > a(n, vector<long long> (m));
	for (int i =0; i < n; i++)
	{
		for (int j = 0; j < m; j++)
			cin>>a[i][j];
	}
	vector<long long> r(k);
	vector<long long> c(k);
	priority_queue<long long> qr;
	priority_queue<long long> qc;
	for (int i = 0; i < n; i++)
	{
		long long sm = 0;
		for (int j =0; j < m; j++)
			sm += a[i][j];
		qr.push(sm);
	}
	for (int i = 0; i < m; i++)
	{
		long long sm = 0;
		for (int j =0; j < n; j++)
			sm += a[j][i];
		qc.push(sm);
	}
	for (int i = 0; i < k; i++)
	{
		long long tp = qr.top();
		qr.pop();
		r[i] = tp;
		tp -= m*p;
		qr.push(tp);
		
		tp = qc.top();
		qc.pop();
		c[i] = tp;
		tp -= n*p;
		qc.push(tp);
	}
	for (int i = 1; i < k; i++)
	{
		r[i] += r[i-1];
		c[i] += c[i-1];
	}
	long long mx = -100000000000000ll;
	for (long long i = 0; i <= k; i++)
	{
		long long ri = 0;
		long long ci = 0;
		if (i > 0)
		{
			ri = r[i-1];
		}
		if (i < k)
			ci = c[k-i-1];
		long long rs = ri+ci-(p * i*(k-i));
		mx = max(mx, rs);
	}
	cout<<mx<<endl;
}


