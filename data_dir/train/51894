vector<vector<int> > g;
vector<bool> tk;
int dfs(int i)
{
	if (tk[i]) return 0;
	tk[i] = true;
	int r = 1;
	for (int j = 0; j < g[i].size(); j++) r += dfs(g[i][j]);
	return r;
}

int main()
{
	int n;
	cin>>n;
	g = vector<vector<int> >(n);
	tk = vector<bool>(n);
	int m;
	cin>>m;
	while (m--)
	{
		int x,y;
		cin>>x>>y;
		g[x-1].push_back(y-1);
		g[y-1].push_back(x-1);
	}
	long long r = 1;
	for (int i = 0; i < n; i++) if (!tk[i]) r *= (1ll<<(dfs(i)-1));
	cout<<r<<endl;
}


int main()
{
