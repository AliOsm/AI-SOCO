{
	
}




int main()
{
	int a[5][5] = {0};
	int n;
	cin>>n;
	
	for (int i = 0; i < n; i++)
	{
		string s;
		cin>>s;
		int x = 0;
		int y =s[1]-'0'-1;
		if (s[0] == 'R')
			x = 0;
		if (s[0] == 'G')
			x = 1;
		if (s[0] == 'B')
			x = 2;
		if (s[0] == 'Y')
			x = 3;
		if (s[0] == 'W')
			x = 4;
		a[x][y]=1;
	}
	vector<pair<int,int> > aa;
	for (int i = 0; i < 5; i++)
	{
		for (int j = 0; j < 5; j++)
		{
			if (a[i][j]) aa.push_back(make_pair(i,j));
		}
	}
	int mn = 10;
	for (int i =0; i < (1<<5); i++)
	{
		for (int j =0; j < (1<<5); j++)
		{
			bool ok = true;
			int cnt = 0;
			for (int x = 0; x < aa.size(); x++)
			{
				
				for (int y = x+1; y < aa.size(); y++)
				{
					bool ok2 = false;
					if (aa[x].first != aa[y].first)
					{
						if (((1<<aa[x].first)&i) > 0 || ((1<<aa[y].first)&i) > 0) ok2 = true;
					}
					if (aa[x].second != aa[y].second)
					{
						if (((1<<aa[x].second)&j) > 0 || ((1<<aa[y].second)&j) > 0) ok2 = true;
					}
					ok = ok && ok2;
				}
			}
			if (ok)
			{
				for (int x = 0; x < 5; x++)
				{
					if (((1<<x)&i) > 0) cnt++;
				}
				for (int x = 0; x < 5; x++)
				{
					if (((1<<x)&j) > 0) cnt++;
				}
				mn = min(mn, cnt);
			}
		}
	}
	cout<<mn<<endl;
}


pair<double, double> dp[105];
vector<double> p;
pair<double, double> sol(int i)
{
	if (i==p.size()) return make_pair(0,1);
	if (dp[i].first != -1) return dp[i];
	pair<double, double> r = sol(i+1);
	if (r.first*(1-p[i])+r.second*p[i] > r.first)
	{
		r.first = r.first*(1-p[i])+r.second*p[i];
		r.second *= (1-p[i]);
		return dp[i] = r;
	}
	else
		return dp[i] = r;
}

int main()
{
	int n;
	cin>>n;
	p = vector<double>(n);
	sort(p.begin(), p.end());
	for (int i = 0; i < n; i++)
	{
		dp[i].first = -1;
		cin>>p[i];
	}
	cout<<fixed<<setprecision(15)<<sol(0).first<<endl;
	
}


int main()
{
	int n,x;
	cin>>n>>x;
	vector<pair<int,int> > a[2];
	vector<pair<int,int> > b[2];
	for (int i = 0; i < n; i++)
	{
		int t,h,m;
		cin>>t>>h>>m;
		a[t].push_back(make_pair(h, m));
		b[t].push_back(make_pair(h, m));
	}
	sort(a[0].begin(), a[0].end());
	sort(a[1].begin(), a[1].end());
	int mx = 0;
	int cnt = 0;
	int h = x;
	int i = 0;
	for (i = 0; i < n; i++)
	{
		int mx2 = -1;
		int mxi = -1;
		for (int j = 0; j < a[i%2].size(); j++)
		{
			if (a[i%2][j].first <= h)
				if (a[i%2][j].second > mx2)
				{
					mx2 = max(mx2, a[i%2][j].second);
					mxi = j;
				}
		}
		if (mx2 == -1)
		{
		break;
		}
		else
		{
			a[i%2][mxi].first = 100000000;
			h += mx2;
		}
	}
	mx = i;
	h = x;
	for (i = 1; i <= n; i++)
	{
		int mx2 = -1;
		int mxi = -1;
		for (int j = 0; j < b[i%2].size(); j++)
		{
			if (b[i%2][j].first <= h)
				if (b[i%2][j].second > mx2)
				{
					mx2 = max(mx2, b[i%2][j].second);
					mxi = j;
				}
		}
		if (mx2 == -1)
		{
			break;
		}
		else
		{
			b[i%2][mxi].first = 100000000;
			h += mx2;
		}
	}
	mx = max(mx, i-1);
	cout<<mx<<endl;
}


vector<string> b;
int sp(int i, int j, char ch)
{
	if (i < 0 || j < 0 || i >= b.size() || j >= b[0].size()) return 0;
	if (b[i][j] == ch) return 1;
	return 0;
}
int main()
{
	int n,m,k;
	cin>>n>>m>>k;
	b = vector<string>(n);
	for (int i = 0; i < n; i++) cin>>b[i];
	for (int i = 0; i < m; i++)
	{
		int cnt = 0;
		for (int j = 1; j < n; j++)
		{
			cnt += sp(j+j, i, 'U');
			cnt += sp(j, i+j, 'L');
			cnt += sp(j, i-j, 'R');
		}
		cout<<cnt<<" ";
	}
}



int dp[50][50][50];
vector<vector<string> > a;
int sol(int i, int j, int k)
{
	if (i > a.size()) return 100;
	if (j == k) return i;
	if (dp[k][i][j] != -1) return dp[k][i][j];
	int mn = 100;
	for (int x = 0; x< a[j].size(); x++)
	{
		int mx = 0;
		for (int y = 0; y < a[j][x].size(); y++)
		{
			if (a[j][x][y]-'a' == j)
			{
				mx = 100;
				break;
			}
			mx = max(mx, sol(i+1, a[j][x][y]-'a', k));
		}
		mn = min(mn, mx);
	}
	return dp[k][i][j] = mn;
}

int main()
{
	ios_base::sync_with_stdio(false);
	int n;
	cin>>n;
	a = vector<vector<string> > (n);
	for (int i = 0; i < n; i++)
	{
		int m;
		cin>>m;
		for (int j = 0; j < m; j++)
		{
			string s;
			cin>>s;
			a[i].push_back(s);
		}
	}
	for (int i = 0; i < 50; i++) for (int j = 0; j < 50; j++) for (int k = 0; k < 50; k++)
	{
		dp[i][j][k] = -1;
	}
	for (int i = 0; i < n; i++)
	{
		for (int j =0; j < n; j++)
		{
			int r = sol(0,i,j);
			if (r ==100) cout<<-1<<" ";
			else cout<<r<<" ";
		}
		cout<<endl;
	}
}



vector<pair<int, pair<int,int> > >edges;
int n,m,k,w;
vector<bool> tk;
vector<int> p;
vector<vector<int> > g;
int find(int i)
{
	if (p[i] == -1) return i;
	return p[i] = find(p[i]);
}
int un(int i, int j)
{
	int a,b;
	a = find(i);
	b = find(j);
	p[a] = b;
}
bool con(int i, int j)
{
	return find(i) == find(j);
}
vector<pair<int,int> > res;
void dfs(int i, int par)
{
	if (tk[i]) return;
	tk[i] = true;
	if (par==-1) res.push_back(make_pair(i+1,0));
	else res.push_back(make_pair(i+1,par+1));
	for (int j = 0; j < g[i].size(); j++) dfs(g[i][j], i);
}
int main()
{
	cin>>n>>m>>k>>w;
	p = vector<int> (k,-1);
	vector<vector<string> > a(k, vector<string> (n));
	for (int i = 0; i < k; i++)
	{
		for (int j = 0; j < n; j++)
		{
			cin>>a[i][j];
		}
	}
	for (int i = 0; i < k; i++)
	{
		for (int j = i+1; j < k; j++)
		{
			int cnt = 0;
			for (int x = 0; x < n; x++)
			{
				for (int y = 0;y < m; y++)
				{
					if (a[i][x][y] != a[j][x][y]) cnt++;
				}
			}
			edges.push_back(make_pair(cnt, make_pair(i, j)));
		}
	}
	sort(edges.begin(), edges.end());
	g=vector<vector<int> > (k);
	int r = 0;
	for (int i = 0; i < edges.size(); i++)
	{
		if (edges[i].first*w >= n*m) break;
		if (con(edges[i].second.first, edges[i].second.second)) continue;
		r += edges[i].first*w;
		un(edges[i].second.first, edges[i].second.second);
		g[(edges[i].second.first)].push_back(edges[i].second.second);
		g[(edges[i].second.second)].push_back(edges[i].second.first);
	}
	tk = vector<bool>(k, false);
	
	for (int i = 0; i < k; i++)
	{
		if (tk[i] == false) r += n*m;
		dfs(i,-1);
	}
	cout<<r<<endl;
	for (int i = 0; i < res.size(); i++)
		cout<<res[i].first<<" "<<res[i].second<<endl;
}


vector<int> dig(int i)
{
	int mul = 1;
	vector<int> f(10,0);
	int rem = i;
	while (rem > 0)
	{
		for (int j = 0; j < 10; j++)
		{
			f[j] += (rem/10) *mul; 
			if (j < rem%10)
			{
				f[j] += mul;
			}
			if (j == rem%10)
			{
				f[j] += (i%mul)+1;
			}
		}
		f[0] -= mul;
		rem/=10;
		mul*=10;
	}
	return f;	
}

int main()
{
	int a,b;
	cin>>a>>b;
	while (a !=0 || b != 0)
	{
		vector<int> bb = dig(b);
		vector<int> aa = dig(a-1);
		cout<<bb[0]-aa[0];
		for (int i = 1; i < 10; i++)
		{
			cout<<" "<<bb[i]-aa[i];
		}
		cout<<endl;
		cin>>a>>b;
	}
}


int main()
{
	int t;
	cin>>t;
	while (t--)
	{
		int a[5][5];
		for (int i = 0; i < 5; i++)
		{
			for (int j = 0; j <=i; j++)
			{
				cin>>a[i][j];
			}
		}
		int b[9][9];
		int c[4] = {0};
		for (int i = 0; i < 4; i++)
		{
			c[i] = (a[3][i]-a[4][i]-a[4][i+1])/2;
		}
		for (int i = 0; i < 9; i++)
		{
			if (i%2==0)
			{
				b[8][i] = a[4][i/2];
			}
			else
			{
				b[8][i] = c[i/2];
			}
		}
		for (int i = 7; i>=0; i--)
		{
			for (int j = 0; j<=i; j++)
			{
				b[i][j] = b[i+1][j]+b[i+1][j+1];
			}
		}
		for (int i = 0; i < 9; i++)
		{
			cout<<b[i][0];
			for (int j = 1; j <=i; j++)
			{
				cout<<" "<<b[i][j];
			}
			cout<<endl;
		}
	}
}


