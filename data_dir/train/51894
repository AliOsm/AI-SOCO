	int cup = (a+4)/5;
	if (med+cup <= n) cout<<"YES"<<endl;
	else cout<<"NO"<<endl;
}


int main()
{
	string s,t;
	cin>>s>>t;
	vector<int> a[26];
	vector<int> b[26];
	for (int i = 0; i < s.size(); i++) a[s[i]-'a'].push_back(i);
	for (int i = 0; i < t.size(); i++) b[t[i]-'a'].push_back(i);
	bool arr = true;
	bool ntree = false;
	
	for (int i = 0; i < 26; i++)
	{
		if (a[i].size() < b[i].size()) ntree = true;
		if (a[i].size() != b[i].size())
		{
			arr = false;
		}
	}
	if (ntree)
	{
		cout<<"need tree"<<endl;
		return 0;
	}
	if (arr)
	{
		cout<<"array"<<endl;
		return 0;
	}
	int rch = -1;
	bool aut = true;
	for (int i = 0; i < t.size(); i++)
	{
		vector<int>::iterator it = upper_bound(a[t[i]-'a'].begin(), a[t[i]-'a'].end(), rch);
		if (it==a[t[i]-'a'].end())
		{
			aut = false;
		}
		else
			rch = (*it);
	}
	if (aut)
	{
		cout<<"automaton"<<endl;
	}
	else
	{
		cout<<"both"<<endl;
	}
}


vector<int> a;

long long sol(int i, int j)
{
	if (i > j) return 0;
	long long mn = j-i+1;
	int mnE = 1000000000;
	for (int k = i; k <= j; k++)
	{
		mnE = min(mnE, a[k]);
	}
	for (int k = i; k <= j; k++)
	{
		a[k]-= mnE;
	}
	int lst = i;
	long long rs = mnE;
	for (int k = i; k <= j; k++)
	{
		if (a[k] == 0)
		{
			rs += sol(lst, k-1);
			lst = k+1;
		}
	}
	rs += sol(lst, j);
	return min(rs, mn);
}

