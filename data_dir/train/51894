int main()
{
	int a[4][4];
	int t;
	cin>>t;
	while (t--)
	{
		for (int i = 0; i < 4; i++) for (int j = 0; j < 4; j++) cin>>a[i][j];
		int r = 0,l=0,u=0,d=0;
		bool win = false;
		for (int i = 0; i < 4; i++)
		{
			for (int j = 0; j < 4; j++)
			{
				if (a[i][j] == 2048) win = true;
				if (j < 3)
				{
					if (a[i][j] != 0 && (a[i][j+1] == a[i][j] || a[i][j+1] == 0)) r = 1;
				}
				if (j > 0)
				{
					if (a[i][j] != 0 && (a[i][j-1] == a[i][j] || a[i][j-1] == 0)) l = 1;
				}
				if (i < 3)
				{
					if (a[i][j] != 0 && (a[i+1][j] == a[i][j] || a[i+1][j] == 0)) d = 1;
				}
				if (i > 0)
				{
					if (a[i][j] != 0 && (a[i-1][j] == a[i][j] || a[i-1][j] == 0)) u = 1;
				}
			}
		}
		if (r+l+u+d == 0 || win)
		{
			cout<<"NONE"<<endl;
			continue;
		}
		string s = "";
		if (d) s+="DOWN ";
		if (l) s+="LEFT ";
		if (r) s+="RIGHT ";
		if (u) s+="UP ";
		cout<<s.substr(0,s.size()-1)<<endl;
	}
}


vector<pair<int, pair<int,int> > >edges;
long long n,m;
vector<int> p;
vector<long long> sz;
int find(int i)
{
	if (p[i] == i) return i;
	return p[i] = find(p[i]);
}
long long un(int i, int j)
{
	int a,b;
	a = find(i);
	b = find(j);
	long long r = sz[a]*sz[b];
	p[a] = b;
	sz[b] += sz[a];
	return r;
}
bool con(int i, int j)
{
	return find(i) == find(j);
}


int main()
{
	cin>>n>>m;
	vector<int> a(n);
	sz = vector<long long>(n,1);
	p = vector<int>(n,-1);
	for (int i = 0; i < n; i++){
		cin>>a[i];
		p[i] = i;
	}
	
	for (int i = 0; i < m; i++)
	{
		int x,y;
		cin>>x>>y;
		edges.push_back(make_pair(min(a[x-1],a[y-1]), make_pair(x-1,y-1)));
	}
	sort(edges.begin(), edges.end());
	double res = 0.0;
	for (int i = m-1; i>=0; i--)
	{
		if (con(edges[i].second.first, edges[i].second.second)) continue;
		res += ((2.0*edges[i].first)/(n*(n-1.0)))*un(edges[i].second.first, edges[i].second.second);
	}
	cout<<fixed<<setprecision(10)<<res<<endl;
	
}


int main()
{
	int t;
	cin>>t;
	while (t--)
	{
		long long x,y;
		cin>>x>>y;
		long long s = y-x;
		long long n = (-1 + sqrt(1-4*(-s))) / 2;
		long long sellem = n*(n+1);
		long long rs = 2*n;
		if (sellem + n+1 <= s)
		{
			sellem += n+1;
			rs++;
		}
		if (sellem == s)
		{
			cout<<rs<<endl;
		}
		else
		{
			cout<<rs+1<<endl;
		}
		
	}
}


