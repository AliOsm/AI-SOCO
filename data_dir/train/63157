#define pb push_back
#define ff first
#define ss second
#define ll long long
#define maxn 1000006
#include <bits/stdc++.h>
using namespace std;



int main() {

    int tc;
    cin >> tc;
    while(tc--){
        int n;
        cin >> n;
        string s;
        cin >> s;
        int pos = -1;
        for(int i=0;i<n;i++){
            if(s[i] == '8'){
                pos = i;
                break;
            }
        }
        if(pos == -1 || (n-pos)<11){
            cout << "NO" << endl;
        }
        else{
            cout << "YES" << endl;
        }
    }
    return 0;
}
