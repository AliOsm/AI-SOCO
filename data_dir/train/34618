#include <bits/stdc++.h>
#define LL long long
#define pb push_back
#define mp make_pair
#define fi first
#define sc second
using namespace std;

LL n,x,y,z;

int main() {
	cin>>n;
	x=2;
	for(LL i=1;i<=n;i++){
		y = i*(i+1);
		cout<<(i+1)*(i+1)*i-x/i<<endl;
		x = y;
	}
	return 0;
}