#include <bits/stdc++.h> 
using namespace std;
int main()
{
    int q,n,tmp;
    cin>>q;
    while(q--){
        cin>>n;
        tmp = n;
        while(tmp){
            if (tmp%3==2){
                n++;
                tmp = n;
            }
            tmp/=3;
        }
        cout<<n<<"\n";
    }
   
    return 0;
}