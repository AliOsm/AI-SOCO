#include <bits/stdc++.h>

using namespace std;

const int maxn = 2e5 + 10;

long long a[maxn];
long long f[maxn][2];
long long ans = 0;

int main()
{
	int n = 0;
	int k = 0;
	long long x;
	scanf("%d",&n);
cout << __builtin_popcount(n) << endl;
return 0;
	for (int i = 1; i <= n; i++)
		scanf("%I64d", &a[i]);
	for (int i = 1; i <= n; i++)
	{
		f[i][0] = f[i - 1][0] | a[i];
	}
	for (int i = n; i >= 1; i--)
		f[i][1] = f[i + 1][1] | a[i];
	for (int i = 1; i <= n; i++)
	{
		long long tmp = 1;
		for (int j = 0; j <= k; j++)
		{
			ans = max(ans, f[i - 1][0] | f[i + 1][1] | (a[i] * tmp));
			tmp *= x;
		}	
	}
	cout << ans << endl;
	return 0;
}