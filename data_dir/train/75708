#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define pb push_back
#define mp make_pair
int main()
{  
    std::ios::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    ll n;
    cin>>n;
    if(n%2==0)
        cout<<"white"<<endl<<"1 2"<<endl;
    else
        cout<<"black"<<endl;
    return 0;
}
