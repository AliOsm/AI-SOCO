#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
#include <set>
#include <queue>
#include <deque>
#include <map>
#include <stack>
#include<bitset>
#include<list>
#include<cassert>
#include<numeric>
 
using namespace std ;
// 8/ 4/ 2/ 6/ 8
int main() {
	int n, m;
	cin >> n >> m;
	int two = 1;
	for (int i = 0; i < n; i++)
	{
		two = two * 2;
		if (two > m)
		{
			cout << m << endl;
			exit(0);
		}
	}
	cout << m % two;
	

    
}