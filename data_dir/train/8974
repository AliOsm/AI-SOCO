LL maxx(LL a , LL b){ return a>b ?  a : b ;}
LL minn(LL a , LL b){ return a>b ?  b : a ;}

using namespace std;
int dx[] = {1 , 1 ,1 , 0 , 0  , -1 , -1  , -1  , 0 };
int dy[] = { -1 , 0  , 1 , 1 , -1 , 0 , 1 , -1 , 0 };
void boostIO()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);
}
LL gcd(LL a, LL b) {
    return b == 0 ? a : gcd(b, a % b);
}
LL lcm(LL a , LL b )
{
    return (a*b)/gcd(a , b);
}
LL poww(LL base, LL exp){
  base %= mod;
  LL result = 1;
  while (exp > 0) {
    if (exp & 1) result = (result * base) % mod;
    base = (base * base) % mod;
    exp >>= 1;
  }
  return result;
}
int main()
{
    string a , b ; cin >> a >> b ;
    string ans =  "" ; bool turn = 0 ;
    for(int  i = 0 ; i < a.size() ; i++){
        if(a[i] != b[i]){
            if(turn){
                ans = ans + b[i] ;
            }
            else
                ans = ans + a[i] ;
            turn ^= 1 ;
        }
        else
            ans = ans + a[i] ;
    }
    cout << (turn == 0 ? ans : "impossible") ;
}
