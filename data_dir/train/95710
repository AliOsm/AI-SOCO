//I_F_A
#include "bits/stdc++.h"
using namespace std;

int main(){
	
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	
	long long n;
	cin >> n;
	
  long long ans = 0;
	long long arr[n+1];
  map<long long,long long> mymap;
	for(long long i=1;i<=n;i++){
		
		cin >> arr[i];
      	mymap[arr[i]]++;
      ans = max(ans,mymap[arr[i]]);
	}	cout << ans << endl;
	
	
}