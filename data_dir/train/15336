#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    int n,m,res=0,i,j,w1,w2;
    cin>>n>>m;
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            cin>>w1>>w2;
            if(w1==1||w2==1)
                res++;
        }
    }
    cout<<res;
    return 0;
}
