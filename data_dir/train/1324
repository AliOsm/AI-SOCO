main ()
{
    ios;
    int t;
    cin>>t;
    for (int tt=1;tt<=t;++tt){
        for (int i=1;i<=9;++i){
            for (int j=1;j<=9;++j){
                cin>>a[i][j];
            }
        }
        int j=1;
        for (int i=1;i<=3;++i){
            if (a[i][j]=='1')a[i][j]='2';
            else a[i][j]='1';
            j+=3;
        }
        j=2;
        for (int i=4;i<=6;++i){
            if (a[i][j]=='1')a[i][j]='2';
            else a[i][j]='1';
            j+=3;
        }
        j=3;
         for (int i=7;i<=9;++i){
            if (a[i][j]=='1')a[i][j]='2';
            else a[i][j]='1';
            j+=3;
        }
        for (int i=1;i<=9;++i){
            for (int j=1;j<=9;++j){
                cout<<a[i][j];
            }
            cout<<endl;
        }
    }
