    #include<bits/stdc++.h>
    using namespace std;
    int main()
    {
    	int x,y;
    	cin>>x>>y;
    	if(y==(x+1))
    	{
    		cout<<x<<"9 "<<y<<"0";
     
    	}
    	else if(x==y)
    	{
    		cout<<x<<"1 "<<y<<"2";
     
    	}
    	else if(x==9&&y==1)
    	{
    		cout<<"99 100";
    	}
    	else
    	{
    		cout<<"-1";
    	}
    }