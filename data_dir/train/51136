#include <bits/stdc++.h>

using namespace std;


int main()
{
    long long  i,j,k,l,n,u,m,v,a,b,w,p,q;
    string s,s1;
    while(cin>>s>>s1)
    {
        if(s.compare(s1)==0)
        {
            cout<<s<<endl;
        }
        else
        {
            cout<<"1\n";
        }
    }

    return 0;
}
