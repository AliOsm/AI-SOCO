#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
#define endl '\n'
int main() {
    ios::sync_with_stdio(false);cin.tie(nullptr);ios_base::sync_with_stdio(false);
    int t,x , y, a, b, k;
    cin >> t;
    while (t--)
    {
        cin >> a >> b >> x >> y >> k;
        a = (a + x - 1) / x;
        b = (b + y - 1) / y;
        if (a + b <= k)
            cout << a << ' ' << b << endl;
        else
            cout << "-1\n";
    }
    return 0;
}