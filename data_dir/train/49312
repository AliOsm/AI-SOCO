#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define pb push_back
#define mem(a,b) memset(a,b,sizeof(a))

int main(){
	ios::sync_with_stdio(false);
	cin.tie(0);
	int k,mx=0,t;
	cin>>k;
	for(int i=0;i<k;i++){
		cin>>t;
		mx=max(t,mx);
	}
	if(mx>25)cout<<mx-25<<endl;
	else cout<<0<<endl;
	return 0;
}
