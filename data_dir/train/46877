#include<bits/stdc++.h>
using namespace std;
#define int long long 
#define mp make_pair
#define pb push_back
#define pii pair<int,int>
#define F first 
#define S second
int const M=2e5+10,M2=1e6+10,M3=1e3+10,inf=1e9+10,mod=1e9+7;
int a[M];
map<int,int>cnt;
main()
{	
	int n;
	cin>>n;
	while(n--)
	{
		int a;
		cin>>a;
		bool flag=0;
		for(int i=0;i<=a/3;i++)
		{
			if((a-(3*i))%7==0)
				flag=1;
		}
		if(flag)cout<<"YES";
		else cout<<"NO";
		cout<<"\n";
	}	
}
