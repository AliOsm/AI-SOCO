#include<bits/stdc++.h>
using namespace std;
long long int ab[105][105];
int main()
{
    long long int a,b,c,d,e,i,j,k,l,n,m,x,y;
    cin>>n>>m;
    char nm[105][105];
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
            cin>>nm[i][j];
        }
    }
    for(i=1;i<=n;i++)
    {
        b=n+1-m;
        for(j=1;j<=b;j++)
        {
            c=j+m;
            d=0;
            for(k=j;k<c;k++)
            {
                if(nm[i][k]!='.')
                {
                    d=1;
                    break;
                }

            }
            if(d==0)
            {
                for(k=j;k<c;k++)
                {
                    ab[i][k]++;
                }
            }
        }
    }
    a=n+1-m;
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=a;j++)
        {
            b=j+m;
            d=0;
            for(k=j;k<b;k++)
            {
                if(nm[k][i]!='.')
                {
                    d=1;
                    break;
                }
            }
            if(d==0)
            {
                for(k=j;k<b;k++)
                {
                    ab[k][i]++;
                }
            }
        }
    }
    a=-5;
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
            if(ab[i][j]>a)
            {
                a=ab[i][j];
                x=i;
                y=j;
            }
        }
    }
    printf("%lld %lld\n",x,y);
}
