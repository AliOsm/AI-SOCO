#include <bits/stdc++.h>
using namespace std;
int main ()
{
    int i, x;
    string a, b;
    cin>>a>>b;
    x=a.size();
    char D[x][2];
    for (i=0; i<x; i=i+1)
    {
        D[i][0]=a.at(i);
        D[i][1]=b.at(i);
    }
    for (i=0; i<x; i=i+1)
    {
        if (D[i][0]==D[i][1])
        {
            cout<<0;
        }
        else
        {
            cout<<1;
        }
    }
    return 0;
}