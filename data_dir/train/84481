#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;
typedef long double ld;
#define rep(i,a,b)  for(ll i=a;i<b;i++)
#define nl cout<<endl

#define pii pair<ll,ll>
#define vi  vector<ll>
#define vii vector<pii>
#define mi  map<ll,ll>
#define all(a)  (a).begin(),(a).end()

#define pb push_back
#define make make_pair
#define ff first
#define ss second
#define hell 1000000007

#define test4(x,y,z,a) cout<<"x is "<<x<<"		y is "<<y<<"		z is "<<z<<"		a is "<<a<<endl;
#define test3(x,y,z) cout<<"x is "<<x<<"		y is "<<y<<"		z is "<<z<<endl;
#define test2(x,y) cout<<"x is "<<x<<"		y is "<<y<<endl;
#define test1(x) cout<<"x is "<<x<<endl;
#define N 5010

ll power(ll a,ll b,ll m)
{
	ll ans=1;
	while(b)
	{
		if(b&1)
			ans=(ans*a)%m;
		b/=2;
		a=(a*a)%m;
	}
	return ans;
}
int main()
{	
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
        string s;cin>>s;
	ll n=s.length();
	ll a[N],b[N];
	a[0]=(s[0]=='a');
	b[0]=(s[0]=='b');
	rep(i,1,n)
	{
		if(s[i]=='a')
		{
			a[i]=a[i-1]+1;
			b[i]=b[i-1];
		}
		else
		{
			b[i]=b[i-1]+1;
			a[i]=a[i-1];
		}
	}
	ll val=-1;
	rep(i,0,n+1)
	{
		ll ans=0;ll c;
		if(i==0)
		c=0;
		else
		c=a[i-1];

		for(ll j=0;j<=(n-i);j++)
		{
			ans=c;
			if(j==0)
			{
				ans+=0;

				if(i==0)
				ans+=b[n-1];
				else
				ans+=b[n-1]-b[i-1];
			}
			else
			{
				ans+=a[n-1]-a[n-1-j];
				if(i!=0)
				ans+=b[n-1-j]-b[i-1];	
				else
				ans+=b[n-1-j];
			}
			val=max(val,ans);
		}
	}
	cout<<val<<endl;
}
