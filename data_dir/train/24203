#define upper(a) transform(a.begin(),a.end(),a.begin(),::toupper);

using namespace std;

lli modexp(lli a,lli n)
{
    lli r=1;
    while(n){
        if(n&1)
            r=(r*a)%mod;
        a=(a*a)%mod;
        n>>=1;
    }
    return r;
}

bool compare(pair<lli, lli> p,pair<lli, lli >  q){
    if(p.ff < q.ff)
        return true ;
    else if(p.ff == q.ff)
    {
        if(p.ss < q.ss)
            return true ;
        return false ;
    }
        return false ;
}

