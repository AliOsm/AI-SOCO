void solve()
{

    lli n;
    cin>>n;
    in(a, n);
    sort(a, a + n);
    f(i, 0, n - 2){
        if(a[i] + a[i + 1] > a[i + 2])
            {cout<<"YES";return ;}
    }
    cout<<"NO"<<endl;
    return ;
}


int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);

    lli t;
    //cin>>t;
    t=1;
    while(t--)
    {

        solve();
    }
    return 0;
}

