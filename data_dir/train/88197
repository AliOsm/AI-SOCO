#include <bits/stdc++.h>
using namespace std;
typedef long long ll;


int main()
{
    ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
    int x,y;
    cin >> x >> y;
    if(x==1){
        if(y>x)cout<<"<";
        else cout<<"=";
        return 0;
    }
    if(y==1){
        if(y<x)cout<<">";
        else cout<<"=";
        return 0;
    }
    if(x==2){
        if(y==2){
            cout<<"=";
        }
        else if(y==3){
            cout<<"<";
        }
        else if(y==4){
            cout<<"=";
        }
        else{
            cout<<">";
        }
        return 0;
    }
    if(y==2){
        if(x==2){
            cout<<"=";
        }
        else if(x==3){
            cout<<">";
        }
        else if(x==4){
            cout<<"=";
        }
        else{
            cout<<"<";
        }
        return 0;
    }
    if(x>y){
        cout<<"<";
    }
    else if(x<y){
        cout<<">";
    }
    else{
        cout<<"=";
    }

}
