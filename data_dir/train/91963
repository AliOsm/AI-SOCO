#include <string>
#include <cstring>
#include <algorithm>
#include <bitset>
#include <cmath>

using namespace std;

#define LL long long

int main()
{
	int k, d;
	cin >> k >> d;
	if (d == 0)
	{
		if (k == 1)
			cout << 0 << endl;
		else
			cout << "No solution" << endl;
	}
	else
	{
		cout << d;
		for(int i = 1; i < k; i++)
			cout << 0;
		cout << endl;
	}
	return 0;
}