#include <bits/stdc++.h>

using namespace std;

int main() {
    int test;
    cin >> test;
    while (test-- > 0) {
        int n;
        cin >> n;
        if (n == 1) {
            cout << -1 << "\n";
            continue;
        }
        for (int i = 0; i < n - 1; i++) cout << 3;
        cout << 4 << "\n";
    }
    return 0;
}
