// In the Name of Allah
#include <bits/stdc++.h>

using namespace std;

typedef long double LD;
typedef long long int LL;
typedef pair <int,int> pii;

#define L first
#define R second

int main() {
	ios_base::sync_with_stdio(false);
	LL l, r;
	cin >> l >> r;
	cout << "YES\n";
	for (LL i = l; i < r; i += 2)
		cout << i << ' ' << i + 1 << '\n';
	return 0;
}