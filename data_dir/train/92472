///-----------------------------START FROM HERE--------------------------------------///

string SS;

int main(){

    int n, m;
    scanf("%d %d", &n, &m);

    cin>>SS;
    char c1,c2;
    for(int q=0; q<m; q++){
        int l,r;
        scanf("%d %d", &l, &r);
        l--;r--;
        getchar();
        scanf("%c", &c1);
        getchar();
        scanf("%c", &c2);

        for(int i=l; i<=r; i++){

            if(SS[i]==c1)SS[i]=c2;
        }

    }
    cout<<SS;
}
