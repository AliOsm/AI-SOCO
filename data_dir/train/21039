#include <iostream>
#include<memory.h>
#include <map>
#include <string>
#include <iomanip>
#include <fstream>
#include <vector>
#include <algorithm>
#include <stack>
#include <queue>
#include <cmath>
#include <set>
using namespace std;


int main()
{
	int a, b;
	cin>>a>>b;
	if (a == 0)
	{
		for (int i = a+b+1; i>= 1; i--) cout<<i<<" ";
		return 0;
	}
	cout<<1<<" ";
	for (int i = b+2; i <= a+b+1; i++) cout<<i<<" ";
	for (int i = b+1; i > 1; i--) cout<<i<<" ";
	cout<<endl;
}