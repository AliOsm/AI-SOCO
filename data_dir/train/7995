#include <bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(NULL);cout.tie(NULL)
#define ll long long int
using namespace std;
const int N = 1e6 + 5;
const ll inf = -1e18;
const int MOD = 1e9 + 7;



int main(){
   fast;
   ll n, ct =0, ct1= 0;
   cin >> n;
   string s;
   cin >> s;
   for(int i = 0; i < n; i += 1){
   	if(s[i] == '1')
   		ct++;
   	else
   		ct1++;
   }
   if(ct <= 1)
   	return cout << s, 0;
   cout << 1;
   while(ct1--)
   	cout << 0;




   return 0;
} 