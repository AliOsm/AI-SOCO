#include <bits/stdc++.h>
using namespace std;

int main(){

//	freopen("readin.txt","r",stdin);

	int rows,lanes,k;
	scanf("%d%d%d", &lanes, &rows, &k);
	int x= ceil(k / (2 * rows + 0.0));
	cout << x << " ";
	int d= (k % (2 * rows));
	if(d == 0) d= 2 * rows;
	int b= ceil(d / (2 + 0.0));
	cout << b << " ";

	if(k % 2)
		cout << 'L' << endl;
	else
		cout << 'R' << endl;
}
