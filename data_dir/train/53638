#include <bits/stdc++.h>
#define ll long long int
#define mp make_pair
#define pb push_back
#define pii pair<int, int>
#define pll pair<ll, ll>
#pragma GCC optimize ("O3")
using namespace std;
int main () {
    ios::sync_with_stdio(false);
    cin.tie (0);
    int n, m; cin>>n>>m;
    for (int i=0;i<n;++i) {
        for (int j=0;j<m;++j) {
            char a; cin>>a;
            if (a=='-') cout<<'-';
            else if ((i+j)%2==1) cout<<'B';
            else if ((i+j)%2==0) cout<<'W';
        }
        cout<<'\n';
    }
    return 0;
}