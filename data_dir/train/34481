#include <bits/stdc++.h>
 
using namespace std;
 
#define maxn 100010
#define INF 1e15
#define ff first
#define ss second
#define endl '\n'
typedef long long ll;

int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(0);
  int tc;
  cin >> tc;
  while (tc--) {
    int a[4];
    for (int i= 0; i < 3; ++i) cin >> a[i];
    sort(a, a + 3);
    if (a[0] + a[1] + 1 < a[2]) {
      cout << "No" << endl;
    }
    else cout << "Yes" << endl;
  }
}
