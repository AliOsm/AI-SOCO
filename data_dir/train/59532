#include<bits/stdc++.h>
using namespace std;

int main()
{
    int na, nb, i, k, m, a, b, x;

    cin >> na >> nb >> k >> m;

    for(i = 1; i <= na; i++){
        cin >> x;

        if(i == k) a = x;
    }

    for(i = 1; i <= nb; i++){
        cin >> x;

        if((nb - i + 1) == m) b = x;
    }

    ///cout << a << ' ' << b << endl;

    if(a < b) cout << "YES" << endl;
    else cout << "NO" << endl;

    return 0;
}
