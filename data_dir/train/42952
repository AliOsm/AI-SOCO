# ifndef ONLINE_JUDGE
   cout << "*** a = " << a << endl;
   cout << "*** b = " << b << endl;
   cout << "*** c = " << c << endl;
   cout << "*** n = " << n << endl;
# endif

   if ( a < c || b < c || a + b - c > n )
      k = -1;
   else
   {
      k = n - (a + b - c);
      if ( k < 1 )
         k = -1;
   }

   cout << k;

//   system ("pause");
   return 0;
}

