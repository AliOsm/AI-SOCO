#include <bits/stdc++.h>
#define MAX (ll)(5e3 + 7)
#define INF (ll)(1e17)
#define MOD (int)(1e9 + 7)
#define PI (double)(3.14159265)
#define IO ios_base :: sync_with_stdio (false);
#define LOG 20

using namespace std;
using ll = long long;

int main () {
	int t;
	cin >> t;
	while (t--) {
		int n;
		cin >> n;
		set<int> se;
		for (int i = 0; i < n; i++) {
			int va;
			cin >> va;
			se.insert (va & 1);
		}
		if (se.size () == 1) {
			cout << "YES\n";
		}
		else {
			cout << "NO\n";
		}
	}
}
