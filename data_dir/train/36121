#include<bits/stdc++.h>
typedef long long ll;
using namespace std;

int main(){
    int n,m;
    while(cin >> n >> m){
        if( m != 10){
            for(int i = 0; i < n; i++) cout << m; cout << endl;
        }else{
            if( n < 2){ cout << -1 << endl;
            }else{
                cout << 1;
                for(int i = 1; i < n; i++) cout << 0;cout << endl;
            }
            
        }
    }
}