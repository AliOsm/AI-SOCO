/* In The Name Of God */
#include <bits/stdc++.h>

# define xx first
# define yy second
# define pb push_back
# define pp pop_back
# define eps 1e-9

using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
typedef vector<int> vint;
char ch[] = {'a','e','o','i','u'};
int main(){
	ios_base::sync_with_stdio (0);cin.tie(0);
	string s;
	int tmp = 0;
	getline(cin,s);
	for(int i=0 ; i<(int)s.size() ; i++)
		for(int j=0 ; j<5 ; j++)
			tmp += (s[i]==ch[j]);
	if(tmp!=5){
		cout<<"NO\n";
		return 0;
	}
	tmp = 0;
	getline(cin,s);
	for(int i=0 ; i<(int)s.size() ; i++)
		for(int j=0 ; j<5 ; j++)
			tmp += (s[i]==ch[j]);
	if(tmp!=7){
		cout<<"NO\n";
		return 0;
	}
	tmp = 0;
	getline(cin,s);
	for(int i=0 ; i<(int)s.size() ; i++)
		for(int j=0 ; j<5 ; j++)
			tmp += (s[i]==ch[j]);
	if(tmp!=5){
		cout<<"NO\n";
		return 0;
	}
	cout<<"YES\n";
	return 0;
}
