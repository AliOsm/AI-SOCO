#include <bits/stdc++.h>

using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);

    int n, m;
    bool color = false;
    cin >> n >> m;
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            char c;
            cin >> c;
            if (c == 'C' || c == 'M' || c == 'Y')
                color = true;
        }
    }

    if (color)
        cout << "#Color\n";
    else
        cout << "#Black&White\n";

    return 0;
}
