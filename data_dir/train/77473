#include<bits/stdc++.h>
#define ll long long int
#define getFaster ios_base::sync_with_stdio(false),cin.tie(NULL),cout.tie(NULL)
#define rep(i,init,n) for(int i=init;i<n;i++)
#define rev(i,n,init) for(int i=n;i>=init;i--)
#define MAXN 5005
using namespace std;
int main()
{
      string str;
      cin>>str;
      char ch[5]={'h','e','l','l','o'};
      int ptr=0;
      rep(i,0,str.length())
      {
         if(ptr>=5)
         break;   
         if(str[i]==ch[ptr])
         ptr++;  
      }
      if(ptr>=5)
      cout<<"YES"<<endl;
      else
      cout<<"NO"<<endl;  
      return 0;
}