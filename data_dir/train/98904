#include <bits/stdc++.h>
#define MAX (ll)(1e3 + 4)
#define MOD (ll)(1e9 + 7)
#define PI (double)(3.14159265)

using namespace std;
using ll = long long;
using ld = long double;

int main () {
	int n;
	cin >> n;
	string st;
	cin >> st;
	cout << count (st.begin (), st.end (), 'R') + count (st.begin (), st.end (), 'L')  + 1 << "\n";
}
