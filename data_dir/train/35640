#include <bits/stdc++.h>
using namespace std;
typedef long long intt;
bool com(pair<int,int> a ,pair <int,int> b)
{
    return a.second-a.first<b.second-b.first;
}
int main()
{

int n,m;
cin>>n>>m;
vector <pair<int,int> > opada;
vector <int> mx;
int i;
intt sum=0;
int a[n+6];
memset(a,-1,sizeof a);
int j;
for(i=0;i<m;i++)
{
    int x,y;
    cin>>x>>y;
    opada.push_back(make_pair(x,y));
}
for(i=1;i<=n;i++)
{
   if(i%2==0)
    cout<<1;
   else
    cout<<0;
}



return 0;
}
