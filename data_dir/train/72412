string s,ss,a[22],b[11];
ll dp[200005];
ll const inf=1e9+7;
map <ll,bool> us;
vector <string> v;
int main()
{
    cin>>n;
    a[0]="zero";
    a[1]="one";
    a[2]="two";
    a[3]="three";
    a[4]="four";
    a[5]="five";
    a[6]="six";
    a[7]="seven";
    a[8]="eight";
    a[9]="nine";
    a[10]="ten";
    a[11]="eleven";
    a[12]="twelve";
    a[13]="thirteen";
    a[14]="fourteen";
    a[15]="fifteen";
    a[16]="sixteen";
    a[17]="seventeen";
    a[18]="eighteen";
    a[19]="nineteen";
    b[2]="twenty";
    b[3]="thirty";
