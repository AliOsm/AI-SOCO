    b[5]="fifty";
    b[6]="sixty";
    b[7]="seventy";
    b[8]="eighty";
    b[9]="ninety";
    if (n<20) cout<<a[n]; else
    {
        cout<<b[n/10];
        if (n%10!=0) cout<<'-'<<a[n%10];
    }
    return 0;
}