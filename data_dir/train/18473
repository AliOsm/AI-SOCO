#include <bits/stdc++.h>
using namespace std;

char ch[201];
vector<string> ans;

void solve() {
	string str;
	for(int i = 0; ch[i]; ++i) {
		if(ch[i] == '@' && i + 1 == strlen(ch)) {
			printf("No solution");
			return;
		}
		if(ch[i] == '@' && !str.length()) {
			printf("No solution");
			return;
		}
		if(ch[i] != '@') {
			str += ch[i];
		} else {
			i++;
			if(ch[i] == '@') {
				printf("No solution");
				return;
			} else {
				str += '@';
				str += ch[i];
				ans.push_back(str);
				str.resize(0);
			}
		}
	}
	if(str.length()) {
		if(ans.size()) {
			string q = ans.back();
			ans.pop_back();
			q += str;
			ans.push_back(q);
		} else {
			printf("No solution");
			return;
		}
	}

	for(int i = 0; i < ans.size(); ++i) {
		if(i) {
			printf(",");
		}
		printf("%s", ans[i].c_str());
	}
}

int main (int argc, char *argv[])
{
	scanf("%s", &ch);
	solve();
	
	return 0;
}

