#include <stack>
#include <bitset>
#include <algorithm>
#include <functional>
#include <numeric>
#include <utility>
#include <sstream>
#include <iostream>
#include <iomanip>
#include <cstdio>
#include <cmath>
#include <cstdlib>
#include <ctime>
#include <vector>

#define pb push_back
#define mp make_pair

using namespace std;

int main(){
    int n,x=4,y;
    cin>>n;
    while(true){
        y=n-x;
        if((x%2==0 || x%3==0)&&(y%2==0 || y%3==0)) break;
        x++;
    }
    cout<<x<<" "<<y;
    return 0;
}