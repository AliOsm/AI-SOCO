#include <bits/stdc++.h>
#define ll long long

using namespace std;


int main()
{
    int t;  cin >> t;
    while(t--){
        int A,B;  cin >> A >> B;
        ll cnt = 0 , k = 9;
        while(k <= B){
            cnt++;
            k*=10;
            k+=9;
        }
        cout << A * cnt << '\n';
    }
    return 0;
}
