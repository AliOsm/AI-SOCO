#pragma GCC optimize ("O3")
#include <bits/stdc++.h>
#define abdelrahman001 ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
typedef long long ll;
using namespace std;

int main()
{
	abdelrahman001
	ll a,b;
	cin >> a >> b;
	if(a == b)
		return cout << a*10 << " " << b*10 + 1,0;
	if(b - 1 == a)
		return cout << a * 10 + 9 << " " << b*10,0;
	if(b == 1 && a == 9)
		return cout << "9 10",0;
	cout << -1;
	return 0;
}
