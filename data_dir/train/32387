/* In The Name Of God */
#include <bits/stdc++.h>

# define xx first
# define yy second
# define pb push_back
# define pp pop_back
# define eps 1e-9

using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
typedef vector<int> vint;
ll ans[100100];
int main(){
	ios_base::sync_with_stdio (0);
	ans[1] = 2;
	for(ll i=2 ; i<=100000 ; i++)
		ans[i] = i*i*i + 2*i*i + 1;
	int n;cin>>n;
	for(int i=1 ; i<=n ; i++)
		cout<<ans[i]<<endl; 
	return 0;
}
