#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef pair< int, int > pii;
typedef vector< int > vi;
typedef vector< vi > vvi;
typedef vector< pii > vp;
typedef vector< vp > vvp;
typedef vector< ll > vl;
typedef vector< vl > vvl;
typedef vector< double > vd;
typedef vector< vd > vvd;
typedef vector< string > vs;

template< class T1, class T2 >
int upmin( T1 &x, T2 v ){
  if( x <= v ){
    return 0;
  }
  x = v;
  return 1;
}

template< class T1, class T2 >
int upmax( T1 &x, T2 v ){
  if( x >= v ){
    return 0;
  }
  x = v;
  return 1;
}

void init(){
  int N, K; cin >> N >> K;
  if( K == 0 ){
    for( int i = 1; i <= N; ++i ){
      cout << i << " \n"[ i == N ];
    }
    exit( 0 );
  }
  for( int i = 0; i < K; ++i ){
    cout << N - i << " ";
  }
  for( int i = 1; i <= N - K; ++i ){
    cout << i << " ";
  }
  cout << endl;
}

void preprocess(){

}

void solve(){

}

signed main(){
  ios::sync_with_stdio( 0 );
  init();
  preprocess();
  solve();
  return 0;
}
