#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef unsigned long long llu;
int a[10];
int main()
{
    int ans;
    for(int i=0;i<6;i++){
        cin>>a[i];
    }
    ans=(a[0]+a[1]+a[2])*(a[0]+a[1]+a[2]);
    ans-=(a[0]*a[0])+(a[2]*a[2])+(a[4]*a[4]);
    cout<<ans;
    return 0;

}



