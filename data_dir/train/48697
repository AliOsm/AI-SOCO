#import<bits/stdc++.h>
using namespace std;typedef struct{long long a;long long x;}H;long long i,j,k,m,n,s;H w[200040];int E(H a,H b){return a.a<b.a|a.a==b.a&a.x<b.x;}main(){for(cin>>n;i<n;i++)cin>>w[i].a>>w[i].x;for(cin>>m;i<n+m;i++)cin>>w[i].a>>w[i].x;for(sort(w,w+i,E);j<i;j++)w[j].a^w[j+1].a?s+=w[j].x:0;cout<<s;}