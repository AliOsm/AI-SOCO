#include <bits/stdc++.h>
using namespace std;
#define ll long long
int main() 
{
     string a;
     cin>>a;
     ll n= a.size();
     ll temp = 1;
     for(ll i=0;i<n-1;i++)
     {
         if(temp==1 && a[i]=='0')
         {
             temp = 0;
         }
         else cout<<a[i];
     }
     if(temp==0)
     {
         cout<<a[n-1];
     }
}