#include <bits/stdc++.h>
using namespace std;
int main(){

//	freopen("readin.txt","r",stdin);

	int tt= 240;
	int n,k;
	cin >> n >> k;
	tt-= k;
	int ans= 0;
	for(int i= 1;i <= n;i++){

		if(tt - 5 * i >= 0){
			ans++;
			tt-= 5 * i;
		}
		else
			break;

	}
	cout << ans << endl;

	return 0;
}
