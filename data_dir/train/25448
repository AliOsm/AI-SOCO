#include <algorithm>
#include <iostream>
#include <cstdlib>
#include <iomanip>
#include <sstream>
#include <cstdio>
#include <vector>
#include <cmath>
#include <stack>
#include <set>
#include <map>
#include <bitset>
#include <cstring>
#include <cctype>
#include <queue>
#include <deque>

using namespace std;

#define fi first
#define se second
#define pf push_front
#define pb push_back
#define mk make_pair
#define all(c) (c).begin(), (c).end()
#define sz(x) (int)x.size()
#define SWS ios_base::sync_with_stdio(false)
#define rfile freopen("input.txt", "r", stdin)
#define wfile freopen("output.txt", "w", stdout)
#define files rfile; wfile

typedef long long ll;
typedef long double ld;

const int Z = (int)1e5 + 111;
const int inf = (int)1e9 + 111;
const ll llinf = (ll)1e18 + 5;
const int MOD = (int)1e9 + 7;

int main() {
	//files;
	SWS;
	string s;
	getline(cin, s);
	string cur = "";
	bool ok = false;
	s += ' ';
	for (int i = 0; i < sz(s); ++i) {
        if (s[i] == ' ' && ok) {
            cur += s[i];
            continue;
        }
        if (s[i] == ' ' && !ok) {
            if (cur != "")
            cout << '<' << cur << ">\n";
            cur = "";
            continue;
        }
        if (s[i] == '"' && ok) {
            cout << '<' << cur << ">\n";
            cur = "";
            ok = 0;
            continue;
        }
        if (s[i] == '"') {
            ok = 1;
            continue;
        }
        cur += s[i];
	}
	return 0;
}