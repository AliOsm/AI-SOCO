/// hate is heavy 
#include<bits/stdc++.h>
using namespace std;
#define F first
#define S second
#define pb push_back
#define mk make_pair
#define ll long long
const int MX=2e5+11;
int N, M, k;

void solve()
{
    scanf("%d%d", &N, &M);
    int p=6-max(N, M)+1;
    int q=6;
    int g=__gcd(p, q);
    p/=g;
    q/=g;
    cout<<p<<"/"<<q<<'\n';
}
int main()
{
    int T=1;
    /// cin>>T;
    while(T--) solve();
}
