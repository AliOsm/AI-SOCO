#include <bits/stdc++.h>
using namespace std;
#define int long long
typedef long long ll;

int32_t main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int n;
    cin >> n;
    for(int i = 0; i < n; ++i) {
        int num;
        cin >> num;
        if(num == 1) {
            cout << "hard\n";
            return 0;
        }
    }
    cout << "easy\n";
    return 0;
}































