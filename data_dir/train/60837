#include <bits/stdc++.h>
using namespace std;

int N, X;
char S[200005];

int main() {
    scanf("%d", &N); scanf("%s", S); X = N - 1;

    for (int i = 0; i < N - 1; i++) if (S[i] > S[i + 1]) {
        X = i; break;
    }

    for (int i = 0; i < N; i++) if (i != X) printf("%c", S[i]);



}
