const ll INF = 1e18;
const ll inf = 1e9;
const ll MOD = 1e9 + 7;
const ll nax = 1000000 + 10;

ll n, arr[nax];
ll a, b, c;
ll f(ll d)
{
    ll ans = abs(a + d - b) + abs(a + d + d - c);
    return ans;
}
