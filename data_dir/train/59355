using namespace std;

typedef unsigned long long ull;
typedef long long ll;
#define all(a) a.begin(),a.end()

void solve()
{
    string s1, s2;
    cin >> s1 >> s2;

    reverse(all(s1));

    cout << ((s1 == s2) ? "YES" : "NO");
}

int main()
{
    //SetConsoleCP(1251);
    //SetConsoleOutputCP(1251);

    solve();

    //cout << "\n\n"; system("pause");
    return 0;
}