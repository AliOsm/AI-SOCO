#define ll long long
#define clr(x) memset(x,0,sizeof(x))
#define _clr(x) memset(x,-1,sizeof(x))
#define fr(i,a,b) for(int i = a; i < b; ++i)
#define frr(i,a,b) for(int i = a; i > b; --i)
#define pb push_back
#define sf scanf

#define pf printf
#define mp make_pair

using namespace std;

bool same(string s1, string s2) {
	return s1[0]==s2[0] || s1[1] == s2[1];
}

int main() {
	string s1;
	cin>>s1;
	string ans = "NO";
	for(int i = 0; i < 5; ++i) {
		string s2;
		cin>>s2;
		if(same(s1,s2)) {
			ans = "YES";
		}
	}
	cout<<ans<<endl;
}