#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef pair<ll,ll> ii;

int v[200005];

int main(){
    
    int n; scanf("%d", &n);
    for(int i = 1; i <= n; i++){
        ll x, y; scanf("%lld %lld", &x, &y);
        if(x-y == 1) cout << "NO" << endl;
        else cout << "YES" << endl;
    }
    
	return 0;
}