#include <bits/stdc++.h>

using namespace std;

typedef long long            ll;
typedef long double          ld;
typedef string               str;
typedef pair < ll , ll >     pll;

#define X               first
#define Y               second
#define pb              push_back
#define sep             ' '
#define endl            '\n'
#define fast_io         ios::sync_with_stdio(false);cin.tie(0);cout.tie(0);
#define dbl_io(x)       cout << fixed << setprecision(x);

const int N = 1e5 + 10;
const ll mod = 1e9 + 7;
const ll inf = 1e18 + 10;

int main(){
    fast_io
    //dbl_io(10);
    ll q;
    cin >> q;
    while (q --)
    {
        ll n, a, b, x, f = 0;
        cin >> n >> a >> b;
        for (int i = 0; i < a; i ++){
            cin >> x;
            if (x == n)
                f = 1;
        }
        for (int i = 0; i < b; i ++){
            cin >> x;
        }
        cout << (f ? "YES\n" : "NO\n");
    }
    return 0;
}
