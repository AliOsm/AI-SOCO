#include <bits/stdc++.h>
using namespace std;
long long  i, j, n, c[223456],a[111][1234], b[1125678],d[123456], l,ll,r, s, t, k,x, y,y1,z,m;
string p[123456],q,qq;
map <long long , long long> mm,u;
int main() {
 
    cin >> n>>m;
    for (i = 0; i < n; i ++){
        cin >> p[i];
    }
    t = 0;
    for (i = n-1; i >=0; i --){
        if (p[i] == "half")
            t= t*2;
        else {
            t = (t+1)*2;
            x ++;
        }
        //cout <<t<<endl;
    }
   // cout << t;
    cout << (t-x) * (m/2);
    return 0;
}