#include <bits/stdc++.h>

using namespace std;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
    string s;
    int n;
    cin>>n;
    cin>>s;
    for(int i=0;i+1<s.size();i++)
    {
        if(s[i]!=s[i+1])
        {
            cout<<"YES"<<endl;
            cout<<s[i]<<s[i+1]<<endl;
            return 0;
        }
    }
    cout<<"NO"<<endl;
    return 0;
}
