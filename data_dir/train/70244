#include <iostream>
#include <vector>
#include <string>
#include <algorithm>

using namespace std;

int main()
{
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
    string s;
    cin >> s;
    int a;
    cin >> a;
    if (s == "front" && a == 1) {
        cout << "L" << endl;
    }
    else if (s == "front" && a == 2) {
        cout << "R" << endl;
    }
    else if (s == "back" && a == 1) {
        cout << "R" << endl;
    }
    else {
        cout << "L" << endl;
    }
    return 0;
}