#include<bits/stdc++.h>
using namespace std;
string s;
int main(){
    //freopen("out.txt","w",stdout);
    int n,i;
    scanf("%d",&n);
    if(n<=2){
        for(i=0;i<n;i++)cout<<"a";
        cout<<endl;
    }
    else{
        for(i=0;i<n;i++){
            if(i%4==0||i%4==1)s+='a';
            else s+='b';
        }
        cout<<s<<endl;
    }
    return 0;
}
