#include <bits/stdc++.h>
#define ll long long
#define fr first
#define sc second
#define mp make_pair
#define pii pair<int,int>
#define pll pair<ll,ll>

using namespace std;

int main(){
    ios::sync_with_stdio(0);
    int t;
    cin>>t;

    while(t--){
        int a,b;
        cin>>a>>b;

        if(a == b){
            cout<<"0\n";
            continue;
        }

        if(a > b){
            if((a-b)%2){
                cout<<"2\n";
            }
            else{
                cout<<"1\n";
            }
            continue;
        }
        if((b-a)%2 == 0){
            cout<<"2\n";
        }
        else{
            cout<<"1\n";
        }


    }


    return 0;
}
