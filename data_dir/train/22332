#include<bits/stdc++.h>
#define pb push_back
#define fr first
#define se second
#define ll long long
using namespace std;
    const ll N = 1e3 + 7;
ll used[N];
ll n , x , y , m ,a, b,  mx, cnt;
string s , s1 , s2;
vector<string>v;
int main(){
    cin >> s >> s1;
    s[s.size() - 1] +=1;
    for( int i = s.size() - 1;i >= 0; i -- ){
       if( s[i] > 'z' )
        s[i - 1] = char(s[i - 1] + 1) , s[i] = 'a';
    }
    if( s == s1 )
    cout << "No such string";
    else
        cout << s;
 }
