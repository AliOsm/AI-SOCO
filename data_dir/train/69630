#include <bits/stdc++.h>

//#define endl '\n'

//#define int long long

#define Vanya Unstoppable

using namespace std;

signed main(){
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr); cout.tie(nullptr);

    int q;
    cin >> q;

    while(q--){
        int n, m;
        cin >> n >> m;
        if((n >= 3 && m >= 2) || (m >= 3 && n >= 2)){
            cout << "NO" << endl;
        }
        else{
            cout << "YES" << endl;
        }
    }
}
