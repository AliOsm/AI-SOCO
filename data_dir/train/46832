#include <bits/stdc++.h>
 
#define ll long long
#define over999 1234567890
#define pb push_back
#define mp make_pair
#define fi first
#define se second
#define Pi 3.1415926535897932384626433
#define files freopen("input.txt","r",stdin);freopen("output.txt","w",stdout); 
#define clear fflush(stdout);
#define elif else if
#define yes "YES"
#define no "NO"
 
using namespace std;


string s;
int n, m;

int main() 
{

    cin >> s;
    for(int i=0;i<s.length();i++)
    {
        if(s[i]=='0')n++; else n=0;
        if(s[i]=='1')m++; else m=0;
        if(n==7 || m==7)return cout << yes,0;
    }
    cout << no;
    return 0;
}