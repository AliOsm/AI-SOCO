#include <iostream>
using namespace std;
int main(){
    int n, ct = 0; string s;
    cin >> n;
    while(n--){
        cin >> s;
        if(s[0]=='T')ct+=4;
        else if(s[0]=='C')ct+=6;
        else if(s[0]=='O')ct+=8;
        else if(s[0]=='D')ct+=12;
        else ct+=20;
    }
    cout << ct << endl;
}