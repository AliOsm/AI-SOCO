#include <bits/stdc++.h>
const bool unsyncedio=std::ios::sync_with_stdio(false);
using namespace std;
typedef unsigned int uint;
typedef long long ll;

int main() {
int n;
string s;
    cin>>n>>s;

    //int vMoves=(s.size()-11+1)/2;
    int pMoves=(s.size()-11)/2;

    int c8=0;
    for(uint i=0; i<s.size()-10; i++) {
        int c=s[i];
        if(c=='8')
            c8++;
    }

    if(c8<=pMoves) {
        cout<<"NO";
    } else {
        cout<<"YES";
    }
    cout<<endl;
}

