    #include <bits/stdc++.h>
     
    using namespace std;
        
    int main()
    {
        int q; cin>>q;
        while(q--)
        {
            int n; cin>>n;
            int minx = -100000, miny=-100000;
            int maxx = 100000, maxy = 100000;
            int x,y,f1,f2,f3,f4;
            for(int i=0;i<n;i++)
            {
                cin>>x>>y>>f1>>f2>>f3>>f4;
                if(!f1)
                    minx = max(minx, x);
                if(!f2)
                    maxy = min(maxy, y);
                if(!f3) 
                    maxx = min(maxx, x);
                if(!f4)
                    miny = max(miny, y);
            }
                if(minx<=maxx && miny<=maxy)
                    cout<<"1 "<<minx<<" "<<miny<<endl;
                else
                    cout<<"0"<<endl;		
        }
        return 0;
    } 