#include <bits/stdc++.h>
#define MOD (ll)(1e9 + 7)
#define MAX (ll)(2e5 + 4)
#define INF (ll)(1e9)
using namespace std;
using ll =  long long;
using ld = long double;

int main () {
	int t;
	cin >> t;
	while (t--) {
		int a, b, n, s;
		cin >> a >> b >> n >> s;
		int xa = min (a, s / n);
		int xb = min (b, s - xa * n);
		if (xa * n + xb == s) {
			cout << "YES\n";
		}
		else {
			cout << "NO\n";
		}
	}
}
