#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <iomanip>

using namespace std;

int main()
{
    long long n, m, a;
    cin >> n >> m;
    if (n > 30)
        cout << m << '\n';
    else
    {
        a = 1;
        for (int i = 0; i < n; ++i)
            a *= 2;
        cout << m % a << '\n';
    }
    return 0;
}