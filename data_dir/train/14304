#include<bits/stdc++.h>
#define ll long long
#define f first
#define s second
#define pb push_back
using namespace std;
ll n,b[2000005];
int main(){
    ios::sync_with_stdio(false);
    cin >> n;
    ll mx = 0;
    for(int i=1; i<=n; i++){
        cin >> b[i];
        ll a = b[i] + mx;
        mx = max(mx , a);
        cout << a << " ";
    }
    
    
    
    
    return 0;
}
