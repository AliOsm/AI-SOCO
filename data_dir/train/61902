#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int n,a=0,b=0,c=0,d=0,i,j,k,l,x,y;
    int mon1[48]={31,29,31,30,31,30,31,31,30,31,30,31,31,28,31,30,31,30,31,31,30,31,30,31,31,28,31,30,31,30,31,31,30,31,30,31,31,28,31,30,31,30,31,31,30,31,30,31};
    int mon2[48]={31,28,31,30,31,30,31,31,30,31,30,31,31,29,31,30,31,30,31,31,30,31,30,31,31,28,31,30,31,30,31,31,30,31,30,31,31,28,31,30,31,30,31,31,30,31,30,31};
    int mon3[48]={31,28,31,30,31,30,31,31,30,31,30,31,31,28,31,30,31,30,31,31,30,31,30,31,31,29,31,30,31,30,31,31,30,31,30,31,31,28,31,30,31,30,31,31,30,31,30,31};
    int mon4[48]={31,28,31,30,31,30,31,31,30,31,30,31,31,28,31,30,31,30,31,31,30,31,30,31,31,28,31,30,31,30,31,31,30,31,30,31,31,29,31,30,31,30,31,31,30,31,30,31};
    cin>>n;
    int mon[n];
    for(i=0;i<n;i++)
    {
        cin>>mon[i];
    }
    for(i=0;i<48-n+1;i++)
    {
        a=0;
        x=0;
        y=0;
        for(j=i;j<i+n;j++)
        {
            if(mon1[j]!=mon[y++])
            {
                x=1;
                break;
            }
        }
        if(x==0)
        {
            a=1;
            break;
        }
    }
    for(i=0;i<48-n+1;i++)
    {
        b=0;
        x=0;
        y=0;
        for(j=i;j<i+n;j++)
        {
            if(mon2[j]!=mon[y++])
            {
                x=1;
                break;
            }
        }
        if(x==0)
        {
            b=1;
            break;
        }
    }
    for(i=0;i<48-n+1;i++)
    {
        c=0;
        x=0;
        y=0;
        for(j=i;j<i+n;j++)
        {
            if(mon3[j]!=mon[y++])
            {
                x=1;
                break;
            }
        }
        if(x==0)
        {
            c=1;
            break;
        }
    }
    for(i=0;i<48-n+1;i++)
    {
        d=0;
        x=0;
        y=0;
        for(j=i;j<i+n;j++)
        {
            if(mon4[j]!=mon[y++])
            {
                x=1;
                break;
            }
        }
        if(x==0)
        {
            d=1;
            break;
        }
    }
    if(a==1 || b==1 || c==1 || d==1)
    {
        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"No"<<endl;
    }
    return 0;

}
