#include <iostream>

using namespace std;

int main()
{
    int a, b, c, d, k1, k2;
    cin >> a >> b >> c >> d;
    k1 = max((3 * a) / 10, a - ((a / 250) * c));
    k2 = max((3 * b) / 10, b - ((b / 250) * d));
    if (k1 > k2) cout << "Misha";
    else if (k1 == k2) cout << "Tie";
    else cout << "Vasya";
    return 0;
}