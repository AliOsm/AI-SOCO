//besmellah
#include <bits/stdc++.h>
using namespace std;

int main(){
	int a, b;
	cin >> a >> b;
	if (a == b){
		cout << a << "0 " << b << '1';
		return 0;
	}
	if (b == a + 1){
		cout << a << "9 " << b << '0';
		return 0;
	}
	if (b == 1 && a == 9){
		cout << "99 100";
		return 0;
	}
	cout << -1;
}
