#pragma GCC optimize ("O3")
#include <bits/stdc++.h>
#define re return
#define ll long long
#define ull unsigned ll
#define ld long double
#define f first
#define s second
#define pi acosl(-1)
#define oo (int)1e9
#define OO 1e18
#define EPS 1e-9
#define Endl '\n'
#define all(v) (v).begin(),(v).end()
#define FX(n) fixed<<setprecision(n)
#define mm(o,k) memset(o,k,sizeof o)
#define IO ios_base::sync_with_stdio(0),cin.tie(0),cout.tie(0);
using namespace std;
const int N = 2e5 + 5, M = 2 * N + 5;

int main()
{
    IO;
    int t;
    cin >> t;
    while(t--){
        ll a, b;
        int n;
        cin >> a >> b >> n;
        if(n % 3 == 0)cout << a << endl;
        else if(n % 3 == 1)cout << b << endl;
        else cout << (a ^ b) << endl;
    }
    re 0;
}