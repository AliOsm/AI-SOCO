#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

int main() {
    ios_base::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
    int test;
    cin >> test;
    while(test--) {
        int n;
        string s;
        cin >> n >> s;
        for(int i = 0; i < n; i++) {
            if(s[i] == '8' && n - i >= 11) {
                cout << "YES" << endl;
                goto endLoop;
            }
        }
        cout << "NO" << endl;
        endLoop: ;
    }
}

