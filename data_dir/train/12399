#include <bits/stdc++.h>

using namespace std;

#define int long long
#define double long double

main(){
    ios_base::sync_with_stdio(0);
    int n;
    int mx = 0;
    cin >> n;
    for(int i = 0; i < n; i++){
        int t;
        cin >> t;
        mx = max(mx, t);
    }
    cout << max(0ll, mx - 25) << endl;
}
