#include<iostream>
#include<algorithm>
#include<string>
#include<vector>
#include<fstream>
#include<map>
#include<set>
#include<cmath>
using namespace std;
int main()
{
    int a, b,sum1=0,sum2=0,sum3=0,i,j,k;
    cin >> a >> b;
    for (i = 1; i <=6; i++)
    {
        if (abs(a - i)<abs(b - i))
        {
            sum1++;
        }
        else if (abs(a - i) == abs(b - i))
        {
            sum2++;
        }
        else
        {
            sum3++;
        }

    }
    cout << sum1 << " " << sum2 << " " << sum3 << endl;
    
    return 0;
}
    
