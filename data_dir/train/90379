#include <bits/stdc++.h>

using namespace std;
#define ll long long
#define mp make_pair
#define pb push_back

int main(){
    ios_base::sync_with_stdio(false);
    ll t;
    cin >> t;
    for(int i=0;i<t;i++){
        double a;
        cin >> a;
        if (ceil(360/(180-a))==360/(180-a)){
            cout << "YES" << endl;
        }else{
            cout << "NO" << endl;
        }
    }
       
    return 0;
}