#include<bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef unsigned long long ull;
const ll prime = 998244353;

int main(){
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  int a, b, c, d;
  double v, m;
  cin >> a >> b >> c >> d;
  m = max(double(3*a)/10, double(a*(250-c))/250);
  v = max(double(3*b)/10, double(b*(250-d))/250);
  if(m > v){cout << "Misha";}
  else if(m < v){cout << "Vasya";}
  else{cout << "Tie";}
  return 0;
}