#include <bits/stdc++.h>
using namespace std;

int x1, y1, x2, y2, x, y;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(NULL);

    cin >> x1 >> y1 >> x2 >> y2 >> x >> y;
    int difx = abs(x1 - x2);
    int dify = abs(y1 - y2);
    cout << ((difx % x == 0 && dify % y == 0 && (difx / x) % 2 == (dify / y) % 2) ? "YES" : "NO") << endl;
    return 0;
}
