#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define f first
#define s second
#define bp push_back
#define mp make_pair
const int N=1e5+10;
const double eps=1e-9;
const int inf=1e9;
main(){

    int a,b;
    scanf("%d%d",&a,&b);
    if(a==b){
        printf("%d0 %d1",a,b);
        return 0;
    }
    
    if(a==9 && b==1){
        printf("9 10");
        return 0;
    }
    if(a>b || abs(a-b)!=1)printf("-1");
    else{
        printf("%d9 %d0",a,b);
    }



}
