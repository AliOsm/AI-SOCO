/**.................................... Code starts Here .......................................**/

LL n , m ;

string s ;
map< pair<LL,LL> , LL > ma ;

struct info{
    LL x , y ;
}a[200010];

int main()
{
    //InpOut;
    LL i , j , k , x , d , res = 0 , p , tot = 0 ;
    //clock_t startTime = clock() ;

    SL1( m ) ;
    LL I ;

    F( I , 1 , m )
    {
        cin >> s ;
        n = s.size() ;
        LL d = 0 ;
        LL val1 = 0 ;
        F( i , 1 , n - 1 )
        {
            if( s[i] == '+' ){ j = i + 1 ; break ; }
            d *= 10 ;
            d += ( s[i] - 48 ) ;
        }
        val1 += d ;
        d = 0 ;
        F( i , j , n - 1 )
        {
            if( s[i] == ')' ){ j = i + 2 ; break ; }
            d *= 10 ;
            d += ( s[i] - 48 ) ;
        }
        val1 += d ;
        LL val2 = 0 ;
        F( i , j , n - 1 )
        {
            val2 *= 10 ;
            val2 += ( s[i] - 48 ) ;
        }
        //printf("%lld %lld\n",val1 , val2) ;
        LL gcd = Gcd( val1 , val2 ) ;
        val1 /= gcd ;
        val2 /= gcd ;
        a[I].x = val1 , a[I].y = val2 ;
        ma[MP( val1 , val2 )] ++ ;
    }
    F( i , 1 , m )
    {
        if( i > 1 ) printf(" ") ;
        printf("%lld",ma[MP( a[i].x , a[i].y )]) ;
    }
    NL ;


    //cout << double( clock() - startTime ) / (double)CLOCKS_PER_SEC<< " seconds." << endl;


    return 0;
}
