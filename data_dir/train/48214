#include <bits/stdc++.h>
using namespace std;

#define ll long long
#define F first
#define S second
#define ii pair < int , int >
#define ever (;;)

int t,a1,a2,b1,b2;

int main()
{
    scanf("%d",&t);
    while(t--)
    {
        scanf("%d%d%d%d",&a1,&b1,&a2,&b2);

        int mn1 = min( a1 , b1 );
        int mn2 = min( a2 , b2 );

        if( mn1 + mn2 == max( a1 , b1 ) && max( a1 , b1 ) == max( a2 , b2 ) )
            printf("Yes\n");
        else
            printf("No\n");
    }
}
