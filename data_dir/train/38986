int main()
{
    /*
    freopen("input.txt","r",stdin);
    freopen("output.txt","w",stdout);
    */
    string s;cin>>s;
    int n=s.size();
    
    map<char,char> x;
    x['A']='A';
    x['H']='H';
    x['I']='I';
    x['M']='M';
    x['O']='O';
    x['T']='T';
    x['U']='U';
    x['V']='V';
    x['W']='W';
    x['X']='X';
    x['Y']='Y';
    
    for(int i=0;i<n;i++){
        if(x[s[i]]==0 || x[s[n-1-i]]==0 || x[s[i]]!=s[n-1-i]){
            cout<<"NO";
            return 0;
        }
    }
    
    cout<<"YES";
    return 0;   
}


