#include<bits/stdc++.h>

/*#noDefs*/

using namespace std;

int n, m;
char c;

int main() 
{   
    cin >> n >> m;
    
    for(int i=0;i<n*m;i++)
    {
        cin >> c;
        if(c!='B' && c!='W' && c!='G')return cout << "#Color", 0; 
    }
    
    cout << "#Black&White";
    return 0;
}