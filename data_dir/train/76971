//I_F_A
#include "bits/stdc++.h"
using namespace std;

int main(){

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	string S;
	long long ans = INT_MIN;

	long long n;
	cin >> n;

	while(n--){

		string s;
		cin >> s;

		long long A , B ,  C , D , E , F , G;
		cin >>A >> B >> C >> D >> E >> F >> G;

		long long lans = 100*A - 50*B + C + D + E +F + G;
		if(lans > ans){
			ans= lans;
			S = s;
		}
	}
	cout << S << endl;
}