#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef long double ld;


int main(){
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

    int tt;
    cin >> tt;
    while(tt--){
        int n, k;
        cin >> n >> k;
        for (int i = 0; i < n; i++)
            cout << (char)(i%k+'a');
        cout << "\n";
    }

}
