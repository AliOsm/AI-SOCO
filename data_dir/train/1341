#include <bits/stdc++.h>
#define fr first
#define sc scanf
#define pf printf
#define se second
#define sq(x) x * x
#define ll long long
#define pb push_back
#define mk make_pair
#define ld long double
#define pr pair<int,int>
#define sz(s) int(s.size())
#define piip pair< pair<ll,ll>,ll >
#define fin(s) freopen( s, "r", stdin )
#define fout(s) freopen( s, "w", stdout )
#define TIME ios_base::sync_with_stdio(0)
using namespace std;

const ll INF = 1e9;
const ll N = 1e5;
const ll mod = 1e9 + 7;
const long double eps = 1E-7;
ll n , mx , mn = 1e9,cnt , m;
int main()
{
        TIME;
      cin >> n >> m;
      while( n and m ){
        if( n >= m * 2 )
            n = n % (m * 2);
        else if( m >= n * 2 )
            m = m % (n * 2 ) ;
            else
                break;
      }
      cout <<n << " " << m;
}




