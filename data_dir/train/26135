#include<bits//stdc++.h>
using namespace std;
int main()
{
	int n,s=0;
	cin>>n;
	string str;
	for(int i=0;i<n;i++)
	{
		cin>>str;
		if(str=="Tetrahedron") s+=4;
		else if(str=="Cube") s+=6;
		else if(str=="Octahedron") s+=8;
		else if(str=="Dodecahedron") s+=12;
		else if(str=="Icosahedron") s+=20;
	}
	cout<<s;
}