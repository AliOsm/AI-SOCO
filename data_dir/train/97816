#include <bits/stdc++.h>

using namespace std  ;

int main(){
    ios_base :: sync_with_stdio(false) ;
    cin.tie(nullptr) ;
    int n ;
    cin >> n ;
    string s ;
    cin >> s ;
    int cnt = 0 ;
    for(;cnt < n && s[cnt] == '1' ; cnt++);
    cout << min(cnt + 1,n) << '\n' ;
    return 0 ;
}