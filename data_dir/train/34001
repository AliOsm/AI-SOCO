#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
map<int,int> cnt;
int main(){
	int n,a,max_cnt=0;
	scanf("%d",&n);
	for(int i=1;i<=n;i++){
		scanf("%d",&a);
		cnt[a]++;
		max_cnt=max(max_cnt,cnt[a]);
	}
	printf("%d\n",max_cnt);
	return 0;
}