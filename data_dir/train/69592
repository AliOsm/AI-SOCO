#include <bits/stdc++.h>
using namespace std;
 
#define IOS ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define endl "\n"
#define int long long

int32_t main()
{
	IOS;
	int t;
	cin >> t;
	while(t--)
	{
		int a, b, n;
		cin >> a >> b >> n;
		int f[3];
		f[0] = a;
		f[1] = b;
		f[2] = a ^ b;
		cout << f[n % 3] << endl;
	}
	return 0;
}
