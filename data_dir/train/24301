//++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
//++--++--++--++--++--++--++--++--++--+--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
const int MAXN = 100001;
const int inf = 1000000001;
const ld INF = 1.0 / 0.0;
const ld EPS = 1e-7;
const int mod = inf + 6;
const ld PI = acos(-1.0);
int get(int n)
{
	int ans = 0;
	while (n)
	{
		ans += n % 10;
		n /= 10;
	}
	return ans;
}
signed main()
{
	fastio();
	int a, b, c;
	cin >> a >> b >> c;
	vector<int> ans;
	for (int s = 1; s < 82; ++s)
	{
		ll fs = s;
		bool f = 0;
		for (int i = 1; i < a; ++i)
		{
			fs *= s;
			if (fs > 1e9)
			{

				f = 1;
				break;
			}
		}
		if (f)
		{
			break;
		}
		ll tx = 1ll * b * fs + c;
		if (tx <= 0 || tx >=1e9)continue;
		if (get(tx) == s)
		{
			ans.pbc(tx);
		}
	}
	cout << ans.size() << endl;
	vout(ans, ' ');
}