#include <iostream>
#include <iomanip>
#include <fstream>
#include <sstream>
#include <cstdio>
#include <cstdlib>
#include <ctime>
#include <cmath>
#include <cstring>
#include <string>
#include <algorithm>
#include <vector>
#include <set>
#include <queue>
#include <map>
#include <stack>
#include <complex>
#include <random>
using namespace std;
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;

string A, B;

int main()
{
	ios::sync_with_stdio(0);

    cin >> A >> B;
    if (A == B)
        cout << "-1\n";
    else
        cout << max(A.size(), B.size()) << "\n";

	return 0;
}
