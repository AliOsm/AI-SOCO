#include <bits/stdc++.h>
#include<string>
    
using namespace std;
            
    #define ll long long
    #define db long double
    #define ff first
    #define ss second
    #define pb push_back
    #define mp make_pair
    #define inf LONG_LONG_MAX
	#define ninf LONG_LONG_MIN 
    #define MOD 998244353
	#define N 1000010
    #define FAST ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);


void solve()
{
	ll n,m,i,j,k;
	cin>>n;
	cin>>k;
	cout<<k<<" ";
	ll mx=max(k,(ll)0);
	for(i=1;i<n;i++)
	{
		cin>>k;
		cout<<mx+k<<" " ;
		mx=max(mx,mx+k);
	}

}

int main()
{
    FAST
    ll q=1;
   //	cin>>q;
    while(q--)
    	solve();
 }





