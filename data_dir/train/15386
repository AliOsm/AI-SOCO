#include <bits/stdc++.h>
using namespace std;

#define F first
#define S second
#define ll long long
#define ld long double
#define vi   vector<int>
#define pii pair<int, int>
#define ull unsigned long long
#define sz(v)  ((int)((v).size()))
#define all(v) ((v).begin()),((v).end())
#define MEM(dp,i) memset(dp ,i, sizeof (dp))
#define db(x) cout << " LOOK " <<(#x) <<" = " <<  x << '\n'
#define ICPC ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define pri1D(a , n ) for(int i=0;i<n;i++)cout<<a[i]<<" ";cout<<endl;
#define pri2D(a, n , m) for(int i=0;i<n;i++){for(int j=0;j<m;j++)cout<<a[i][j] <<" ";cout<<endl;}

//freopen("input.txt", "r", stdin);
//freopen("output.txt", "w", stdout);

int main()
{
    ICPC;
    int t;
    cin >> t;
    while ( t-- )
    {
      int n ;
      cin >> n ;
      int f1 = 0 , f2 =  0 ;

      for ( int i = 0 ; i < n ; i++ )
	     {
	     	int x;
	     	cin >> x;
	     	if ( x &1 )
	     	f1 = 1;
	     	else
			f2 =1;

	     }
      if ( !f1 || !f2  )
        cout << "YES\n";
      else
        cout <<"NO\n";
    }

}
