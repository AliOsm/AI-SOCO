#include <iostream>
#include <string>
#include <cmath>
#include <vector>
#include <algorithm>
#include <iterator>
#include <cstdlib>
#include <cstdio>
#include <map>
#include <set>
#include <queue>
#include <deque>
#include <stack>
#include <list>

using namespace std;

typedef pair <int, int> pii;
typedef pair <long long, long long> pll;
typedef vector <int> vi;
typedef long long ll;

#define endl "\n"
#define mp make_pair

int n;
int result = 0;

int main()
{
	int ii;
	int a[4];

	ios::sync_with_stdio(false);
	cin.tie(NULL);
	
	cin >> n;

	if (n == 0)
		result = 1;
	else
	{
		a[0] = 8;
		a[1] = 4;
		a[2] = 2;
		a[3] = 6;

		result = a[(n - 1) % 4];
	}

	cout << result << endl;

	return 0;
}