	if (st=="0"){
		cout<<0<<endl;
		return 0;
	}
	int cnt=0;
	for (int i=0;i<st.size();i++){
		if (st[i]=='0')
			++cnt;
	}
	cout<<"1";
	for (int i=0;i<cnt;i++){
		cout<<"0";
	}
	cout<<endl;

//	cin.get(); cin.get();
