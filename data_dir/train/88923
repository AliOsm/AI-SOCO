#include <iostream>
#include <algorithm>
#include <vector>
#include <queue>
#include <string>
#include <set>
#include <iomanip>
using namespace std;



int main() {
	int a, b, c; cin >> a >> b >> c;
	if(c<0) {
	    c*=-1;
	    swap(a,b);
	}
	if(b-a>=0 && (b-a==0 || (c!=0 && (b-a)%c==0)))
	cout << "YES";
	else
	cout << "NO";
	return 0;
}