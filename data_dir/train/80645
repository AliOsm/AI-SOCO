#include<bits/stdc++.h>
using namespace std;
#define ll  long long int
int main()
{
   int n;
   cin>>n;
   int a[n],s=0;
   for(int i=0;i<n;i++)
   {
       cin>>a[i];
   }
   sort(a,a+n,greater<int>());
   for(int i=0;i<n;i++)
   {
      s=s+a[0]-a[i];
   }
   cout<<s;
   return 0;
}