#include <bits/stdc++.h>
using namespace std;
int main()
{
    int a, b, i, j;
    char c;
    cin>>a>>c>>b;
    for (j=b+1; j<60; j=j+1)
    {
        if (a/10==j%10 && a%10==j/10)
        {
            if (a>9 && j>9)
            {
                cout<<a<<":"<<j;
                return 0;
            }
            if (a<10 && j>9)
            {
                cout<<0<<a<<":"<<j;
                return 0;
            }
            if (a>9 && j<10)
            {
                cout<<a<<":"<<0<<j;
                return 0;
            }
            if (a<10 && j<10)
            {
                cout<<0<<a<<":"<<0<<j;
                return 0;
            }
        }
    }
    if (a<23)
    {
        for (i=a+1; i<24; i=i+1)
        {
            for (j=0; j<60; j=j+1)
            {
                if (i/10==j%10 && i%10==j/10)
                {
                        if (i>9 && j>9)
                {
                cout<<i<<":"<<j;
                return 0;
                }
            if (i<10 && j>9)
            {
                cout<<0<<i<<":"<<j;
                return 0;
            }
            if (i>9 && j<10)
            {
                cout<<i<<":"<<0<<j;
                return 0;
            }
            if (i<10 && j<10)
            {
                cout<<0<<i<<":"<<0<<j;
                return 0;
            }
                }
            }
        }
    }
    for (i=0; i<=a; i=i+1)
    {
        for (j=0; j<60; j=j+1)
        {
            if (i/10==j%10 && i%10==j/10)
                {
                        if (i>9 && j>9)
                {
                cout<<i<<":"<<j;
                return 0;
                }
            if (i<10 && j>9)
            {
                cout<<0<<i<<":"<<j;
                return 0;
            }
            if (i>9 && j<10)
            {
                cout<<i<<":"<<0<<j;
                return 0;
            }
            if (i<10 && j<10)
            {
                cout<<0<<i<<":"<<0<<j;
                return 0;
            }
                }
        }
    }
}