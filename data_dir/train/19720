#include <bits/stdc++.h>

using namespace std;

int main()
{
    int T;  cin >> T;

    while(T--)
    {
        int n;  scanf("%d", &n);

        string Ans = "-1";

        if(n > 1)
        {
            Ans = "2" + string(n - 1, '3');
        }

        puts(Ans.c_str());
    }
}
