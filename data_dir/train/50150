using namespace std;

#define task "a"

typedef long long ll;

const int INF = (int)1e9;
const ll LINF = (ll)1e18;

#define mp make_pair
#define fi first
#define se second
#define endl '\n'

int main() {
	//freopen(task".in", "r", stdin); freopen(task".out", "w", stdout);
	//ios_base::sync_with_stdio(0);
	string s;
	getline(cin, s);
	for (int i = 0; i < s.size(); i++) {
		if (s[i] == 'H' || s[i] == 'Q' || s[i] == '9') {
			cout << "YES" << endl;
			return 0;
		}
	}
	cout << "NO" << endl;
	return 0;
}

