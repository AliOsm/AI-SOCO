#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n;
    scanf("%d", &n);
    map<string, int> mp;
    for(int i=0; i<n; i++){
        string str;
        cin >> str;
        if(mp[str] == 0) mp[str] = 1;
        else mp[str]++;
    }
    int maxs = 0;
    map<string, int> ::iterator it;
    for(it = mp.begin(); it!=mp.end(); it++){
        int x = it->second;
        maxs = max(maxs, x);
    }
    cout << maxs;
}
