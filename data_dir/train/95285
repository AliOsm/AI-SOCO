#include <bits/stdc++.h>
using namespace std;

int main()
{	int t,ang;
	cin>>t;
	while (t--)
	
	{
		int n;
		cin>>n;
		for (int i=3; i<=180; i++)	if  (((n*i) % 180==0)&&(n<=(180*(i-2))/i) ) 
			{
				cout<<i<<endl;
				break;
			}
		if (n==179) cout<<360<<endl;
	}

	
	ios::sync_with_stdio(0); 
	cin.tie(0);
	
	return 0;
}