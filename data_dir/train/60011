#include <bits/stdc++.h>
#define ll long long

using namespace std;

int main()
{
    char c;
    ll h1,m1,h2,m2;   cin>>h1>>c>>m1>>h2>>c>>m2;

    ll d1 = h1*60 + m1;
    ll d2 = h2*60 + m2;

    ll Mid = (d1+d2)/2;

    ll h3 = Mid/60;
    ll m3 = Mid%60;


    if(h3<10)   cout<<0;cout<<h3<<":";
    if(m3<10)   cout<<0;cout<<m3;
}
