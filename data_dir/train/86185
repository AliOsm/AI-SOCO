#include <bits/stdc++.h>
#define ll long long

using namespace std;
int a[100100];

int main()
{
    int t;  cin >> t;
    while(t--){
        int n;  cin >> n;
        for(int i = 0;i<n;i++)  cin >> a[i];
        int Ans = 0;
        set < int > st;
        for(int i = 0;i<n;i++)  st.insert(a[i]);
        cout << st.size() << '\n';


    }


    return 0;
}
