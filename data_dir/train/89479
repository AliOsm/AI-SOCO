#include <bits/stdc++.h>
#define int long long
using namespace std;
int32_t main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
    int t;cin>>t;
    while(t--){
        int n;cin>>n;
        int a[n];
        for(int i=0;i<n;i++)
            cin>>a[i];
        set<int> s;
        for(int i=0;i<n;i++){
            s.insert(a[i]);
        }
        int ans=0;
        while(s.size()){
            int x=*s.rbegin();
            s.erase(--s.end());
            if(x%2)
                continue;
            s.insert(x/2);
            ans++;

        }
       cout<<ans<<"\n";
         
    }
    
}
