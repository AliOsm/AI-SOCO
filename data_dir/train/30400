#include <bits/stdc++.h>
#define ll long long
#define fr first
#define sc second
#define mp make_pair
#define ii pair<ll,ll>
using namespace std;
int a[500];
int main()
{
    ios_base::sync_with_stdio(false);
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        for(int i=0;i<n;i++)
            cin>>a[i];
        sort(a,a+n);
        for(int i=n-1;i>=0;i--)
            cout<<a[i]<<" ";
        cout<<endl;
    }
    return 0 ;
}
