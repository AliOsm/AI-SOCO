#include <bits/stdc++.h>
using namespace std;

int n, k;
int ans = 0;

int main (int argc, char *argv[])
{
	scanf("%d%d", &n, &k);
	for(int i = 0; i < n; ++i) {
		char ch[11];
		scanf("%s", &ch);
		if(strlen(ch) < k + 1) {
			continue;
		}
		bool cnt[10]; bool yes = true; memset(cnt, false, sizeof(cnt));
		int c = 0;
		for(int j = 0; ch[j]; ++j) {
			int q = (ch[j] - '0');
			if(!cnt[q]) {
				cnt[q] = true;
				c++;
			}
		}
		for(int j = 0; j <= k; ++j) {
			if(!cnt[j]) {
				yes = false;
			}
		}
		ans += yes;
	}
	printf("%d", ans);

	return 0;
}

