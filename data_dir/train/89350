#include <bits/stdc++.h>
using namespace std;
const int N = 1e5 + 10;
int n;
char cnt[10];
int main(){
	// freopen("input.txt", "r", stdin);
	// freopen("output.txt", "w", stdout);
	ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
	int n;
	cin>>n;
	int zero = 0, one = 0;
	for (int i=1; i<=n; i++){
		char cur;
		cin>>cur;
		if (cur == 'z'){	
			zero++;
		}
		if (cur == 'n'){
			one++;
		}
	}
	for (int i=1; i<=one; i++){
		cout<<1<<' ';
	}
	for (int i=1; i<=zero; i++){
		cout<<0<<' ';
	}
}