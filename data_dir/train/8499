#include<bits/stdc++.h>

using namespace std;

#define ll long long int

int main()
{
    ll n,q,i,j,l,r,d;

    cin>>q;

    while(q--)
    {
        cin>>l>>r>>d;

        if(d<l)
        {
            cout<<d<<endl;

            continue;
        }

        ll temp=(r+1)/d;

        if((r+1)%d) temp++;

        cout<<temp*d<<endl;


    }
}
