#include <bits/stdc++.h>
using namespace std;

const int N=1003;

int n,m;
long long a[N][N],dp[N][N][5];
long long ANS;

main () {
	scanf("%d %d",&n,&m);
	
	for (int i=1; i<=n; i++) 
		for (int j=1; j<=m; j++) 
			scanf("%I64d",&a[i][j]);
	
	for (int i=1; i<=n; i++) for (int j=1; j<=m; j++) dp[i][j][0]=max(dp[i-1][j][0]+a[i][j],dp[i][j-1][0]+a[i][j]);
	for (int i=n; i>=1; i--) for (int j=1; j<=m; j++) dp[i][j][1]=max(dp[i+1][j][1]+a[i][j],dp[i][j-1][1]+a[i][j]);	
	for (int i=1; i<=n; i++) for (int j=m; j>=1; j--) dp[i][j][2]=max(dp[i-1][j][2]+a[i][j],dp[i][j+1][2]+a[i][j]);
	for (int i=n; i>=1; i--) for (int j=m; j>=1; j--) dp[i][j][3]=max(dp[i+1][j][3]+a[i][j],dp[i][j+1][3]+a[i][j]);
		
	for (int i=2; i<n; i++) 
		for (int j=2; j<m; j++) {
			ANS=max(ANS,dp[i][j-1][0]+dp[i+1][j][1]+dp[i-1][j][2]+dp[i][j+1][3]);
			ANS=max(ANS,dp[i-1][j][0]+dp[i][j-1][1]+dp[i][j+1][2]+dp[i+1][j][3]);
		}
	
	printf("%I64d\n",ANS);
}