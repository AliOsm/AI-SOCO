# include <bits/stdc++.h>

using namespace std;

int x, y, z, t1, t2, t3;

int main(){
      cin >> x >> y >> z;
      cin >> t1 >> t2 >> t3;

      int lift = abs(x - z) * t2 + t3 + abs(x - y) * t2 + t3 + t3;
      int lest = abs(x - y) * t1;

      if(lift <= lest)
            puts("YES");
      else
            puts("NO");
}
