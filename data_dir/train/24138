 #include <iostream>
#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define inf 1000000000000
#define mod 1000000007
#define pb push_back
#define mp make_pair
#define all(v) v.begin(),v.end()
#define S second
#define F first
#define boost1 ios::sync_with_stdio(false);
#define boost2 cin.tie(0);
#define mem(a,val) memset(a,val,sizeof a)
#define endl "\n"
#define maxn 100001

int main()
{
	boost1;boost2;
	ll a,b,i,ans;
	cin>>a>>b;
	if(a>b)
	swap(a,b);
	ans=1;
	for(i=1;i<=a;i++)
	ans*=i;
	cout<<ans;
	return 0;
}
