#include <iostream>
#include <cstdio>

using namespace std;



int main()
{
    int n;
    cin >> n;
    for(int i=0;i<n;i++)
    {
        int a;
        cin >>a;
        while(a)
        {
            printf("P");
            a--;
            if(i!=n-1) printf("RL"); else printf("LR");
        }
        if(i!=n-1)printf("R");
    }
    return 0;
}
