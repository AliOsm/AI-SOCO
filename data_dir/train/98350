#define L long long int
#define LD long double
#define pi 3.141592653589793238462643383
#define M 1000000007

using namespace std;

int a[100005];

int main()
{
    int n,i,s;
    while(cin>>n)
    {
              s=0;
              for(i=0;i<n;i++)
              {
              cin>>a[i];
              s+=a[i];
              }
              
              if(s%n==0)
              cout<<n<<"\n";
              else
              cout<<n-1<<"\n";
              
              
              
    }
    return 0;
}
