#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define int long long
#define all(sdk) sdk.begin(),sdk.end()
#define mll map<int, int>
#define pb push_back
#define rep(i,a,b) for(int i=a;i<b;++i)
#define repd(i,a,b) for(int i=a;i>=b;--i)
#define mp make_pair
#define hell 1000000007
#define endl '\n'
#define vvll vector<vector<int> >
#define vll vector<int>
#define mint map<int,int> 
#define sz(x) (int)x.size()
#define sll set<int>
#define pll pair<int,int>
#define F first
#define S second
const long double PI = acos(-1.0);

int occ[35];
void solve(){  
	int n;
	memset(occ,0,sizeof occ);
	cin>>n;
	rep(i,0,n){
		int x;
		cin>>x;
		int cnt=0;
		while(x!=1){
			x/=2;
			++cnt;
		}
		occ[cnt]++;
	}  
	rep(i,0,11){
		occ[i+1]+=occ[i]/2;
		occ[i]%=2;
	}
	if(occ[11]){
		cout<<"YES"<<endl;
	}
	else cout<<"NO"<<endl;
}

signed main(){
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int t=1;
    cin>>t;
    while(t--){
        solve();
    }
    return 0;
}