#include <bits/stdc++.h>

using namespace std;

const int N = 1e7;

int n;
main(){
  cin >> n;

  vector < int > v;
  for (int i = 2; i <= n; i += 2){
      v.push_back(i);
  }

  for (int i = 1; i <= n; i += 2){
      v.push_back(i);
  }

  for (int i = 2; i <= n; i += 2){
      v.push_back(i);
  }

  cout << v.size() << "\n";

  for (int j : v){
      cout << j << " ";
  }


}
