int main() {
#ifndef ONLINE_JUDGE
	freopen("input.in", "r", stdin);
#endif

	int n, k;
	scanf("%d%d", &n, &k);
	while (k--) {
		if (n % 10 == 0) n /= 10;
		else n--;
	}

	printf("%d\n", n);
	return 0; 
