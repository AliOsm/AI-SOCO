#include <bits/stdc++.h>
using namespace std;
typedef long long int ll;
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll x1,x2,y1,y2;
    ll x,y;
    cin>>x1>>y1>>x2>>y2;
    cin>>x>>y;
    ll a=abs(x1-x2);
    ll b=abs(y1-y2);
    if(a%x==0 and b%y==0 and (a/x)%2==(b/y)%2)cout<<"YES";
    else cout<<"NO";
    return 0;
}