#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
typedef pair<ll, ll> pl;

const ll INF = 1e18;

ll n, a, b;
string line;
ll anw = INF;

int main() {
  ios::sync_with_stdio(0);
  cin >> n >> a >> b;
  a--; b--;
  cin >> line;
  if (line[a] == line[b]) {
    cout << 0 << endl;
  } else {
    cout << 1 << endl;
  }
}