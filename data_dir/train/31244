#include <bits/stdc++.h>

#define endl '\n'
#define fr first
#define se second
#define pb puch_back
#define sz(x) ((int)(x.size()))
#define all(x) x.begin(), x.end()
#define PI acos(-1.0)
#define max_n 

using namespace std;

int main() {
	ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
	string s;
	cin >> s;
	string f = s;
	reverse(all(s));
	cout << s + f << endl;
	return 0;
}