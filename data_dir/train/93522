#include<bits/stdc++.h>
using namespace std;
#define F first
#define S second
#define ll long long
#define pb push_back
#define pf push_front
#define Mid ((l+r)/2)
#define pi pair<int,int>
#define pii pair<pi,int>
#define go ios::sync_with_stdio(0); cin.tie(0); cout.tie(0)
int t;
int main()
{
    go;
    cin>>t;
    while(t--)
    {
        int a1,a2;
        int b1,b2;
        cin>>a1>>a2>>b1>>b2;
        if(a1>a2)
            swap(a1,a2);
        if(b1>b2)
            swap(b1,b2);
        if(b2!=a2)
            cout<<"NO\n";
        else if(a1+b1!=a2)
            cout<<"NO\n";
        else
            cout<<"YES\n";
    }
    return 0;
}
