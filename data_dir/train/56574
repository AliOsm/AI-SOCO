/* In The Name Of God */
#include <bits/stdc++.h>

# define xx first
# define yy second
# define pb push_back
# define pp pop_back
# define eps 1e-9

using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
typedef vector<int> vint;

int main(){
	ios_base::sync_with_stdio (0);
	int a,b,c;
	cin>>a>>b>>c;
	int n,ans=0;cin>>n;
	for(int i=1 ; i<=n ; i++){
		int x;cin>>x;
		ans += (x>b && x<c);
	}
	cout<<ans<<endl;
	return 0;
}

