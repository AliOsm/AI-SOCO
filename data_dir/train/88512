#include<bits/stdc++.h>
using namespace std;
int main(){

 int n, d;
 cin>>n>>d;
 int a[n];
 int i;
 int sum=0;
 for(i=0;i<n;i++){
    cin>>a[i];
    sum+=a[i];
 }
 int gap;
 gap=(n-1)*10;
 if(sum+gap>d){
    cout<<-1<<endl;
    return 0;
 }

 int jokes;
 jokes=(d-sum)/5;
 cout<<jokes<<endl;



 return 0;
}
