#include<bits/stdc++.h>
#define ll long long
#define OO 2e18
#define oo 1e9
#define yalla ios::sync_with_stdio(0),cin.tie(0),cout.tie(0);
#define sz  100005
#define re return
#define mod  1000000007
#define pi acos(-1)
using namespace std;
int n,m,x;
int main()
{yalla;
cin>>n; ll arr[n+1],tot=0;
for(int i=0;i<n;i++)
 cin>>arr[i],tot+=arr[i];
sort(arr,arr+(n));

cin>>m;
for(int i=0;i<m;i++)
{cin>>x;
 cout<<tot-arr[n-x]<<endl;
}
  return 0;
}
