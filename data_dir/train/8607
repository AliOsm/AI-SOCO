#include <iostream>
#include <algorithm>
#include <iomanip>
#include <set>

#define ll long long
#define ld long double

using namespace std;

int main()
{
	int n; cin >> n;
	bool flag = true;
	for(int i = 0; i < n; i++)
	{
		int a; cin >> a;
		if(a == 1) flag = false;
	}

	cout << (flag ? 1 : -1) << endl;
	return 0;
}
