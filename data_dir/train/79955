#include<bits/stdc++.h>
#define MAX 200005
#define optimiza_io cin.tie(0); ios_base::sync_with_stdio(0);
using namespace std;
typedef long long i64;

int T, B, P, C, P1, P2, ans;

int main()
{
    optimiza_io
    cin >> T;
    while( T -- ) {
        cin >> B >> P >> C;
        cin >> P1 >> P2;
        ans = 0;
        while( 1 ) {
            if( B < 2 or ( ! P && ! C ) ) 
                break;
            if( ( P && P1 > P2 ) or ( P && ! C ) ) 
                B -= 2, P--, ans += P1;
            else
                B -= 2, C--, ans += P2;
        }
        cout << ans << "\n";
    }
    return 0;
}