#include <bits/stdc++.h>

using namespace std;

int main()
{
    int t,x;
    scanf("%d",&t);
    while(t--) {
        scanf("%d",&x);
        if(x < 4 || x == 5 || x == 7 || x == 11)
            printf("-1\n");
        else
            printf("%d\n",(x/4) + (x%4%2?-1:0));
    }
    return 0;
}
