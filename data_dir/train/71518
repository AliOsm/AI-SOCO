{
  
}
int main()
{
 IO;
 OS;
 lli u,v;
 cin>>u>>v;
 if(u>v)
 return cout<<-1,0;
 if((v-u)&1)
 return cout<<-1,0;
 if(u==v)
 {
     if(u==0)
     return cout<<0,0;
     return cout<<1<<"\n"<<u,0;
 }
 lli val=(v-u)/2ll;
 bitset<64> bb(val);
 bitset<64> cc(u);
 lli flag=0;
 for(int j=0;j<64;j++)
 {
     if(bb[j])
     {
         if(cc[j])
         flag=1;
     }
 }
 if(flag)
 {
     cout<<3<<"\n";
     cout<<u<<" "<<val<<" "<<val;
 }
 else
 {
     cout<<2<<"\n";
     cout<<u+val<<" "<<val<<"\n";
 }
}