#include<bits/stdc++.h>
using namespace std;
#define ll long long
	ll a,b=INT_MAX,c[1234567],d,e,i,j,n;
int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(0);
	char ch;
	cin>>a>>ch>>d;
	e=a*60+d;
	cin>>a>>ch>>d;
	n=a*60+d;
	e+=n;
	e/=2;
	if(e/60>=10) cout<<e/60;
	else cout<<0<<e/60;
	cout<<":";
	if(e%60>=10) cout<<e%60;
	else cout<<0<<e%60;
}