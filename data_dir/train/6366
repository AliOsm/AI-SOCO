#include <bits/stdc++.h>

using namespace std;
using namespace __gnu_cxx;

typedef double db;
typedef long long ll;
typedef pair<db, db> pdd;
typedef pair<ll, ll> pll;
typedef pair<int, int> pii;
typedef unsigned long long ull;

#define F          first
#define S          second
#define pnl        printf("\n")
#define sz(x)      (int)x.size()
#define sf(x)      scanf("%d",&x)
#define pf(x)      printf("%d\n",x)
#define all(x)     x.begin(),x.end()
#define rall(x)    x.rbegin(),x.rend()
#define rep(i, n)  for(int i = 0; i < n; ++i)

const db eps = 1e-9;
const db pi = acos(-1);
const int INF = 0x3f3f3f3f;
const ll LL_INF = 0x3f3f3f3f3f3f3f3f;
const int mod = 1000 * 1000 * 1000 + 7;

int n;
vector<int> adj[1010];
int num[1010];
int leaf[1010];

int dfs(int u) {
	if (sz(adj[u]) == 0) leaf[u] = 1;
	for (int i = 0; i < sz(adj[u]); ++i)
		num[u] += dfs(adj[u][i]);
	return leaf[u];
}

int main() {
    scanf("%d", &n);
    for (int i = 2; i <= n; ++i) {
    	int u;
    	scanf("%d", &u);
    	adj[u].push_back(i);
    }
    dfs(1);
    int can = 1;
    for (int i = 1; i <= n; ++i)
    	if (leaf[i] == 0 && num[i] < 3)
    		can = 0;
    if (can) cout << "YES\n";
    else cout << "NO\n";
    return 0;
}
