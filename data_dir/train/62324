#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define MAX_INT 1000000000
int main()
{
    int n;
    cin>>n;
    int m=0,c=0;
    int i,j,k;
    for(i=0;i<n;i++){
        cin>>j>>k;
        if(j>k){
            m++;
        }else if(k>j){
            c++;
        }
    }
    if(m>c)cout<<"Mishka"<<endl;
    else if(c>m)cout<<"Chris"<<endl;
    else cout<<"Friendship is magic!^^"<<endl;
    return 0;
}