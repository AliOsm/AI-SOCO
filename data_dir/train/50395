long long int N, M, K, H, W, L, R;


int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);

	cin >> N >> M;
	string s, t;
	for (int i = 0; i < N; i++) {
		s.push_back('5');
		t.push_back('4');
	}
	t.push_back('5');
	s.push_back('5');
	cout << s << endl << t << endl;
	return 0;
}