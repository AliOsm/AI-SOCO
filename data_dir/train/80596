int  powermod(int  x, int  y, int p )
{
    int res = 1;
    while (y > 0)
    {
        if (y & 1)
            res = (res * x) % p;
        y = y >> 1;
        x = (x * x) % p;
    }
    return res % p;
}
int logg(int  a, int b)
{
    int  x = 0;
    while (a > 1)
    {
        x++;
        a /= b;
    }
    return x;
}
int  gcd(int  a, int   b)
{
    if (a == 0)
        return b;
    return gcd(b % a, a);
}
int ceiling (int  a, int  b)
{
    if (a % b == 0)
        return a / b;
    else
        return a / b + 1;
}
int modinv(int x)
{
    return powermod(x, mod - 2, mod);
}

void input(int *arr, int n)
{
    for (int i = 0; i < n; i++)
        cin >> arr[i];
}

bool isperfectsquare(int n)
{
    double x = sqrt(n);
    return ((x - floor(x)) == 0);
}

int sumofdigits(int n)
{
    int ans = 0;
    while (n)
        ans += n % 10, n /= 10;
    return ans;
}

int cntdigits(int n)
{
    int cnt = 0;
    while (n)
        cnt++, n /= 10;
    return cnt;
}

bool prime[100010];

void seive()
{
    int n = 100009;
    MSET(prime, true);
    prime[0] = 0, prime[1] = 0;
    for (int i = 2; i <= n; i++)
        if (prime[i])
            for (int j = 2 * i; j <= n; j += i)
                prime[j] = false;
}
  
int32_t main()
{
    fast;
    tc
    {
        string s;cin>>s;
        int n=s.size();
        sort(s.begin(),s.end());
        if(s[0]==s[n-1])
            cout<<-1<<endl;
        else
            cout<<s<<endl;
    }
}