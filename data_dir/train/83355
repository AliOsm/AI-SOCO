#include <bits/stdc++.h>
using namespace std;

int n, m;
string arr[100];
int num[100];
bool use[100];

void solve() {
	for(int i = 1; i < m; ++i) {
		if(arr[num[i]].length() != arr[num[0]].length()) {
			cout << "No";
			return;
		}
	}
	string ans;
	ans.resize(arr[num[0]].length());
	for(int i = 0; arr[num[0]][i]; ++i) {
		bool fi = false;
		for(int j = 1; j < m; ++j) {
			if(arr[num[j]][i] != arr[num[0]][i]) {
				fi = true;
				break;
			}
		}
		if(fi) {
			ans[i] = '?';
		} else {
			ans[i] = arr[num[0]][i];
		}
	}
	for(int i = 0; i < n; ++i) {
		if(!use[i] && arr[i].length() == ans.length()) {
			bool fi = true;
			for(int j = 0; arr[i][j]; ++j) {
				if(ans[j] == '?') {
					continue;
				}
				if(ans[j] != arr[i][j]) {
					fi = false;
					break;
				}
			}
			if(fi) {
				cout << "No";
				return;
			}
		}
	}
	cout << "Yes" << endl; 
	cout << ans;
}

int main (int argc, char *argv[])
{
	memset(use, false, sizeof(use));
	cin >> n >> m;
	for(int i = 0; i < n; ++i) {
		cin >> arr[i];
	}
	for(int i = 0; i < m; ++i) {
		cin >> num[i];
		num[i]--;
		use[num[i]] = true;
	}
	solve();

	return 0;
}

