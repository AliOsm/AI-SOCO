#include <bits/stdc++.h>
#define FOR(i,begin,end) for (int i=begin;i<=end;i++)
#define rep(i,t) for (int i=0;i<t;i++)
#define pb push_back
#define mp make_pair
using namespace std;
typedef unsigned long long ll;
typedef pair <int,int> pii;


int32_t main()
{
    ios_base::sync_with_stdio(false); cin.tie(0);
    ll n;
    cin >> n;
    cout << 2 << endl;
    ll m = 2;
    for (ll i = 2 ; i <= n ; i++)
    {
        printf("%I64d\n", (i * (i + 1) * (i + 1)) - m / i);
        m = i * (i + 1);
    }
    return 0;
}