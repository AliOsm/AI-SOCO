#include <bits/stdc++.h>
using namespace std;
long n , i , a[1000] , l , r;
int main()
{
    cin >> n;
    for (i = 0; i < n; i++)
        cin >> a[i];
    sort(a , a + n);
    l = -1; r = n;
    while (l < r){
        l++;
        r--;
    }
    if (n % 2 != 0)
    cout << a[l];
    else
    cout << a[r];
}
