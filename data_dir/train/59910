#include <bits/stdc++.h>
using namespace std;

#define all(x) begin(x), end(x)
#define sz(x) (int)(x).size()

using ll = long long;
using ld = long double;

constexpr ll inf = 0x3FFFFFFF;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    ll a, b;
    cin >> a >> b;
    if (a == 9 && b == 1) {
        cout << "9 10" << endl;
    } else if (b < a) {
        cout << -1 << endl;
    } else if (b == a) {
        cout << a << "0 " << b << "1" << endl;
    } else if (a + 1 == b) {
        cout << a << "9 " << b << "0" << endl;
    } else {
        cout << -1 << endl;
    }
}
