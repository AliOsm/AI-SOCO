#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

const ll N=101;

map<ll,ll> cnt;

int main(){
	ll a,b,c;
	cin>>a>>b>>c;
	ll mx=0;
	mx=max(mx,a*b*c);
	mx=max(mx,a+b*c);
	mx=max(mx,(a+b)*c);
	mx=max(mx,a*b+c);
	mx=max(mx,a*(b+c));
	mx=max(mx,a+b+c);
	cout<<mx<<endl;
	return 0;
}
