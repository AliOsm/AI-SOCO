#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;
int main(){
    ll t,n;
    cin>>t;
    for(ll i=0;i<t;i++){
        cin>>n;
        ll cnt=0;
        while(n!=0){
            if(n&1)
            cnt++;
            n>>=1;
        }
        cout<<(ll)pow(2,cnt)<<endl;
    }
}