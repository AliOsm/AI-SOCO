#include <bits/stdc++.h>
using namespace std;
int n,k,d;
string st;
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cin>>n>>k>>st;
    for(int i=0;i<n;++i){
        if(st[i]=='.'){
            d = 0;
        }else{
            ++d;
        }
        if(d==k){
            cout<<"NO";
            return 0;
        }
    }
    cout<<"YES";
}