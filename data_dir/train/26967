void brainfuck()
{
    cin>>t;
    while( t-- )
    {
        cin>>ar [ 1 ]>>ar [ 2 ]>>ar [ 3 ];
        sort( ar + 1 , ar + 4 );
        if ( ar [ 1 ] + ar [ 2 ] + 1 >= ar [ 3 ] )cout<<"Yes\n";
        else cout<<"No\n";
    }
}
