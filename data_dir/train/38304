#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n, a, b, x;
	cin >> n;
	while(n--)
	{
		cin >> x;
		a = 0;
		for(int i = 0; i <= 100; i++)
		for(int j = 0; j <= 100; j++)
		{
			if(i*7 + j*3 == x)
			a = 1;
		}
		if(a)
		{
			cout << "YES" << endl;
		}
		else
		cout << "NO" << endl;
	}
}