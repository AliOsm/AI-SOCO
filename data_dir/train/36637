#include <iostream>

using namespace std;

int main() {
    long long a, b;
    cin >> a >> b;
    
    for (int year = 1; ; ++year) {
        a *= 3;
        b *= 2;
        
        if (a > b) {
            cout << year << endl;
            break;
        }
    }
    
    return 0;
}