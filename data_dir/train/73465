#include<bits/stdc++.h>

using namespace std;

int main(){
	
	ios :: sync_with_stdio(false); cin.tie(nullptr); cout.tie(nullptr);
	
	int n;
	
	cin>>n;
	
	string s;
	
	cin>>s;
	
	map <char,int> count;
	
	for(auto &i : s){
		
		count[i]++;
	}
	
	if(count['A'] > count['D']){
		
		cout<<"Anton";
	}
	
	else if(count['A'] < count['D']){
		
		cout<<"Danik";
	}
	
	else{
		
		cout<<"Friendship";
	}
}
