#include<bits/stdc++.h>
using namespace std;

int solve() {
    int cnt1,cnt2,cnt3,cnt4;
    cin>>cnt1>>cnt2>>cnt3>>cnt4;
    if(cnt1 == cnt4 && (cnt3 == 0 || cnt1 > 0))
        cout<<1;
    else
        cout<<0;
    
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    solve();
}