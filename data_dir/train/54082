#include <bits/stdc++.h>
using namespace std;
int arr[4];
int main()
{

    int x,k,t;
    cin>>x>>k;
    arr[0]=x;
    while(2*arr[0]+3*arr[1]+4*arr[2]+5*arr[3]<k){
        if(arr[0]!=0){
            arr[0]--;
            arr[1]++;
        }
        else break;
    }

cout<<arr[0];
    return 0;
}
