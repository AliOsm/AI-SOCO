#include <bits/stdc++.h>
using namespace std;

#define    ll    long long int

int cnt[28];
int ase[11];
vector<char> v;

int main( )
{
    string s;
    int i, j, k, l, pa, pb;
    cin >> s;
    sort(s.rbegin( ), s.rend( ));
    cout << s[0];
    for (i = 1; i < s.size( ); i++)
    {
        if (s[i - 1] == s[i])
            cout << s[i];
        else
            break;
    }
    cout << "\n";
    return 0;
}
