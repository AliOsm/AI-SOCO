#include<bits/stdc++.h>
#define pb push_back
#define mp make_pair
const int MAXN = 1e6+20000;
const int INF = 1e9+7;
const int base1 = 67;
const int base2 = 79;
const int MAXNk = 1e4+20;
const double eps = 1e-9;
const long long inf = 1e18;
using namespace std;
 
 

 int t;
 int n,x;
 long long sum;
int main(){
	ios::sync_with_stdio(NULL), cin.tie(0), cout.tie(0);
	cout.setf(ios::fixed), cout.precision(8);
//	freopen("kotlin.in", "r", stdin);
//	freopen("kotlin.out", "w", stdout);
 
	cin>>t;
	while (t>0)
	{
		t--;
		cin>>n;
		sum = 0;
		for (int i=1; i<=n; i++) 
		{
			cin>>x;
			sum+=x;
		}
		if (sum % n == 0) cout<<sum/n<<endl;
		else
		cout<<sum/n+1<<endl;
	}

}
