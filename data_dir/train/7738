#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main()
{
    string s;
    cin >> s;
    int sevenfour = 0;
    for(int i = 0;i < s.length();i++)
        if(s[i] == '7' || s[i] == '4')
            sevenfour++;

    if(sevenfour == 7 || sevenfour == 4)
        cout << "YES";
    else
        cout << "NO";
    return 0;
}
