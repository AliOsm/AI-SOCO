#include <bits/stdc++.h>
#define mp make_pair
#define pb push_back
using namespace std;
typedef long long ll;
typedef pair<int,int> pii;
typedef pair<ll,ll> pll;
int a,b;

int main()
{
    //freopen("in.txt","r",stdin);
    //freopen("out.txt","w",stdout);
    cin>>a>>b;
    if(a==b) printf("%d1 %d2",a,b);
    else if(a+1==b) printf("%d9 %d0",a,b);
    else if(a==9&&b==1) printf("%d %d0",a,b);
    else printf("-1");
    return 0;
}
