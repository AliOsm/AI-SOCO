#include <bits/stdc++.h>

using namespace std;

int n,s,u,a,b,c,d,e,w=-10000000;
int main(){
    string t,x;
    cin >> n;
    while(n--){
    cin >> t >> s >> u >> a >> b >> c >> d >> e;
        if (s*100 - u*50 + a + b + c + d + e>w){
            w = s*100 - u*50 + a + b + c + d + e;
            x = t;
        }
    }
    cout << x;
}           