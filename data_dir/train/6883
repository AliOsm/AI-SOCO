#include<bits/stdc++.h>
using namespace std;


int n;

main()
{
    scanf("%d" , &n);

    if(n <= 2)printf("No\n");
    else {
        printf("Yes\n");
        printf("%d " , n/2);
        for(int i = 2 ; i <= n ; i += 2)printf("%d " , i);

        printf("\n%d " , (n+1)/2 );
        for(int i = 1 ; i <= n ; i += 2)printf("%d " , i);
    }
}
