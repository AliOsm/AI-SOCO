#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
#include <cstdio>
#include <map>
#include <queue>
#include <cstring>
#include <cstdlib>
#include <set>
using namespace std;
#define INF 2001001001
#define MOD 1000000007

int main()
{
  //ios_base::sync_with_stdio(0);cin.tie(0);
  int A,B,C,N;
  cin>>A>>B>>C>>N;
  if (C>A || C>B)
    cout<<-1<<endl;
  else if (A+B-C<N)
    cout<<N-(A+B-C)<<endl;
  else
    cout<<-1<<endl;
  return 0;
}
