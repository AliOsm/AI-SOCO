#include<bits/stdc++.h>
using namespace std;
main()
{
    long n;
    while(cin>>n)
    {
        long i;
        cout<<n+(n/2)<<endl;
        for(i=2;i<=n;i+=2)
        {
            cout<<i<<" ";
        }
        for(i=1;i<=n;i+=2)
        {
            cout<<i<<" ";
        }
        for(i=2;i<=n;i+=2)
        {
            cout<<i<<" ";
        }
        cout<<endl;
    }
}
