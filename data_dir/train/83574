#include <bits/stdc++.h>
typedef long long ll ;
using namespace std;

int a[100500],b[100500];
int main()
{
    ios::sync_with_stdio(false) ;

    int n , m , k1 , k2 ;
    cin >> n >> m >> k1 >> k2 ;

    for(int i = 1 ; i <= n ; i++) cin >> a[i] ;
    for(int i = 1 ; i <= m ; i++) cin >> b[i] ;

    if(a[k1] < b[m - k2 + 1]) cout << "YES" << endl ;

    else cout << "NO" << endl ;

    return 0;
}
