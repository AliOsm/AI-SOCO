#include <stdio.h>
#include <stdlib.h>
using namespace std;
int main(int argc, char **argv)
{
    int n, k, t;
    scanf("%d%d%d", &n, &k, &t);
    if (t <= k) {
        printf("%d", t);
    } else if (t > n) {
        printf("%d", k - t + n);
    } else {
        printf("%d", k);
    }
    return 0;
}
