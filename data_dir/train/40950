#include <bits/stdc++.h>
#define pii pair <int, int>
#define ll long long
#define pb push_back
#define ff first
#define ss second
#define endl '\n'
using namespace std;
ll x, y, z;
int main(){
  //ios::sync_with_stdio(0), cin.tie(0), cout.tie(0);
  cin >> x >> y;
  z = x - 1;
  while(z--) cout << 8;
  cout << 9 << endl;
  z = x;
  while(z--) cout << 1;
}