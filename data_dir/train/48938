#include <bits/stdc++.h>
using namespace std;
#define forr(i, a, b) for(int i = (a); i < (int) (b); i++)
#define forn(i, n) forr(i, 0, n)
#define db(v) cerr << #v << " = " << v << endl
#define pb push_back
#define sz(x) ((int)x.size())
#define fst first
#define snd second
typedef long long ll;
typedef pair<int, int> ii;
const int SZ=500;

int n,m;

int main(){
  while(scanf("%d%d",&n,&m)>=1){
    forn(i,SZ)printf("9");puts("");
    forn(i,SZ)printf("9");
    forn(i,SZ-1)printf("0");
    printf("1\n");
  }
	return 0;
}
