#include <bits/stdc++.h>

using namespace std;

int main() {
      int n;
      cin >> n;
      map <int, int> v;
      int mx = -1;
      for (int i = 0; i < n; ++i) {
            int a;
            cin >> a;
            ++v[a], mx = max(mx, v[a]);
      }
      cout << n - mx << '\n';
      return 0;
}