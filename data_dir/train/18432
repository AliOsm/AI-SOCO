using namespace std;

const ll N = 2e5 + 1 ;

string s , t ;
int a[N];
ll n , m , k , l , cnt , ans , sum , x , y  , z , mn = 1e18 , mx ;
int main()
{
    //freopen( "input.txt", "r" , stdin );
    //freopen( "output.txt", "w" , stdout );
    ios_base::sync_with_stdio( 0 );
    cin >> s;
    for ( int i = 0 ; i < s.size(); i ++ )
    {
        if ( s[i] == s[i + 1] )
        cnt ++ ;
    else
        ans ++ , cnt = 0 ;
    if ( cnt == 5 )
        ans ++ , cnt = 0 ;
    }
    cout << ans ;
















}




