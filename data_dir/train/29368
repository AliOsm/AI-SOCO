#include<iostream>
#include<stdio.h>
#include<algorithm>
using namespace std;
int a,s,d[5],f,g,h,j,k,l,i,n,m;
main(){
cin>>n>>m>>k;
d[0]=n;
d[1]=m;
d[2]=k;
sort(d,d+3);
if(d[2]%2!=(d[1]-d[0])%2 || d[1]+d[0]<d[2]) {cout<<"Impossible";return 0;} 
l=(n+m-k)/2;
cout<<l<<" "<<m-l<<" "<<l-m+k;
//system("pause");
}
