#include <bits/stdc++.h>
using namespace std;
long long  i, j, n, c[123456],a[1111234], b[1125678],d[123456], l,ll, s, t, k, m, xh, x, x1, y,z;
string p,q;
int main() {
    cin >> p>>q;
    n = p.size();
    if (n != q.size()){
        cout << "No";
        return 0;
    }
    for (i = 0; i < n; i ++){
        x = 0;
        y= 0;
        if (p[i] == 'a'||
            p[i] == 'e'||
            p[i] == 'i'||
            p[i] == 'o'||
            p[i] == 'u')
            x = 1;
        if (q[i] == 'a'||
            q[i] == 'e'||
            q[i] == 'i'||
            q[i] == 'o'||
            q[i] == 'u')
            y = 1;
        if (x!= y){
            cout << "No";
            return 0;
        }
    }
    cout << "Yes";
    return 0;
}