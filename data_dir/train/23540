#include<bits/stdc++.h>
#define pb push_back
#define adelkhah main
//#define int long long
using namespace std;
typedef long long ll;
typedef long double ld;
typedef pair<int,char> pii;
int32_t adelkhah()
{
    int t,b,k; cin >> b >> k; b %= 2;int ans = 0;for(int i = 1; i < k; i++) cin >> t, t %= 2,ans += b*t, ans %= 2; cin >> t; ans += t;ans %= 2; cout << (ans?"odd":"even");
}
