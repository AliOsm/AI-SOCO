int MOD = 1e9+7;
void ADD(LL& x,LL v){x=(x+v)%MOD;if(x<0)x+=MOD;}
/*}}}*/
const int SIZE = 1e5+10;
set<int>AA[SIZE];
VI e[SIZE];
int a[SIZE][3];
int n;
void dfs(VI& res,int x,int lt){
    if(x==1)return;
    res.PB(x);
    if(e[x][0]==lt)dfs(res,e[x][1],x);
    else dfs(res,e[x][0],x);
}
void go(){
    VI res2;
    VI pp;
    FOR(i,1,n){
        if(SZ(AA[i])==1)pp.PB(i);
    }
    while(SZ(res2)<n-2){
        int st=pp.back();
        pp.pop_back();
        if(SZ(AA[st])==0)continue;
        int st_id=*AA[st].begin();
        res2.PB(st_id);
        int now=st_id;
        REP(i,3){
            AA[a[now][i]].erase(now);
            if(SZ(AA[a[now][i]])==1)pp.PB(a[now][i]);
        }
    }
    FOR(i,1,n){
        sort(ALL(e[i]));
        int k=0;
        for(int j=0;j<SZ(e[i]);){
            if(j+1<SZ(e[i])&&e[i][j]==e[i][j+1]){
                j+=2;
            }
            else{
                e[i][k++]=e[i][j];
                j++;
            }
        }
        e[i].resize(k);
        //W(i,":",e[i]);
    }
    VI res{1};
    dfs(res,e[1][0],1);
    W(res);
    W(res2);
}
void solve(){
    R(n);
    FOR(i,1,n)AA[i].clear();
    FOR(i,1,n)e[i].clear();
    FOR(i,1,n-2){
        REP(j,3){
            R(a[i][j]);
            AA[a[i][j]].insert(i);
        }
        REP(j,3){
            e[a[i][j]].PB(a[i][(j+1)%3]);
            e[a[i][(j+1)%3]].PB(a[i][j]);
        }
    }
    go();
}
int main(){
    CASET{solve();}
    return 0;
}
