#include <bits/stdc++.h>
using namespace std;

int n, m;
vector<pair<int, int> > arr;
int yes = -1;

void check(int num) {
	for(int i = 0; i < m; ++i) {
		if(arr[i].first / num == arr[i].second) {
			continue;
		}
		return;
	}
	int cur_yes = n / num;
	if(yes == -1 || yes == cur_yes) {
		yes = cur_yes;
	} else {
		printf("-1");
		exit(0);
	}
}

int main (int argc, char *argv[])
{
	scanf("%d%d", &n, &m);
	--n;
	arr.resize(m);
	for(int i = 0; i < m; ++i) {	
		scanf("%d%d", &arr[i].first, &arr[i].second);
		--arr[i].first;
		--arr[i].second;
	}
	for(int i = 1; i <= 1000; ++i) {
		check(i);
	}
	printf("%d", yes + 1);

	return 0;
}

