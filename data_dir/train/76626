#include <bits/stdc++.h>
using namespace std;
int main()
{
    long long a,b;
    cin >>a>>b;
    for (int i=0;;i++)
    	{
        if (i%2==0)
            b-=i;
        else a-=i;

        if (a<0)
        {
            cout <<"Vladik";
            return 0;
        }
        if (b<0)
        {
            cout <<"Valera";
            return 0;
        }
    }
}