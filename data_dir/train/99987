#include  <bits/stdc++.h>
using namespace std;
#define ll long long
int a[201];
int main() {
	int n, m, z, kol =0, ans = 0;
	cin >> n >> m;
	for (int i =0; i < m; i++) {
			cin >> z;
			a[z]++;
	}
	if (m < n) {
			cout << 0;
			return 0;
	}
	for (int i = 1; i <= 100; i++) {
			kol = 0;
			for(int j = 1; j <= 100; j++){
					kol += a[j] / i;
			}
			if(kol >= n)
				ans= max(ans, i);
	}
	cout << ans;
}
