/// إِنَّ مَعَ الْعُسْرِ يُسْرًا

#include <bits/stdc++.h>
#define LL long long
#define LD long double
#define S second
#define F first
#define IO ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

using namespace std;

int main()
{
    IO
    int n;
    cin>>n;
    string s,v;
    cin>>s;
    if(s[0]=='0')
    {
        cout<<s;
        return 0;
    }
    sort(s.begin(),s.end());
    for(int i=0; i<s.size() ; i++)
    {   if(s[i]=='0')
        v+=s[i];
    }
    v+='1';
    reverse(v.begin(),v.end());
    cout<<v;

    return 0;
}
