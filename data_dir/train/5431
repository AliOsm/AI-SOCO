//#pragma GCC optimize("Ofast")
//#pragma GCC target("sse,sse2,sse3,ssse3,sse4")
#include<bits/stdc++.h>

#define sync ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define ll long long
#define pb push_back
#define mp make_pair
#define fi first
#define se second
#define pi 3.1415926535897932
#define eps 0.000001

//#define int ll

using namespace std;

int n, m, k;

int main()
{
    cin >> n >> m;
    vector<int> cnt(m + 1);
    for(int i = 1;i <= n;i++){
        int l ,r;
        cin >> l >> r;
        for(int j = l;j <= r;j++)
            cnt[j] ++;
}
    vector<int> x;
    for(int i = 1;i <= m;i++)
        if (cnt[i] == 0){
            x.pb(i);
        }

    cout << x.size() << endl;
    for(int i : x)
        cout << i << " ";

    return 0;
}



