#include <bits/stdc++.h>
using namespace std;

int a = 0,
	b = 0;

int main (int argc, char *argv[])
{
	int n;
	scanf("%d", &n);
	while(n--)
	{	
		int x, y;
		scanf("%d%d", &x, &y);
		if(x < 0)
		{
			++a;
		} else
		{
			++b;
		}
	}
	if(a <= 1 || b <= 1)
	{
		printf("Yes");
	} else
	{
		printf("No");
	}

	return 0;
}

