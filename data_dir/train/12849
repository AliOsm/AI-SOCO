#include<bits/stdc++.h>
using namespace std;

#define IO ios_base::sync_with_stdio(NULL); cin.tie(false);
#define endl '\n'
#define D(x) cout << #x << " = " << (x) << endl;

int main() {
  int n;
  cin >> n;

  int i = 1, j = n;
  int cnt = 0;
  while (cnt < n) {
    if (cnt % 2 == 0) {
      cout << i << " ";
      i ++;
    }
    else {
      cout << j << " ";
      j --;
    }
    cnt ++;
  }

  return 0;
}