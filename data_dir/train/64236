#include <iostream>
#include <algorithm>
#include <vector>
#include <set>
#include <stack>
#include <map>
#include <iomanip>
#include <cmath>
using namespace std;
int main() {

  ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

  int t = 0;

  cin >> t;

  while(t--) {
    int x = 0, y = 0;
    cin >> x >> y;
    if(x > 3 || (x == 1 && y == 1) || ((x == 2 || x == 3) && y < 4)) cout << "YES\n";
    else cout << "NO\n";
  }

  return 0;
}
