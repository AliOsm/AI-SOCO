const int maxn  = 5e5 + 10;
const int maxm = 4e6 + 10;
const int maxlg = 22;
const int base = 29;
const int mod = 1e9 + 7;
const ll inf = 1e18 + 10;
const double eps = 1e-4;
const int SQ = 317;
int a[maxn];
int cnt[maxn],res;
int32_t main()
{
    int n = in() , x = in();
    for (int i = 0 ; i < n ; i ++) a[i] = in();
    for (int i = 0 ; i < n ; i ++) res += cnt[a[i]] , cnt[a[i]^x] ++;
    cout << res << endl;
}