int main() {
  ios::sync_with_stdio(false), cin.tie(NULL), cout.tie(NULL);
//  freopen("test.in", "rt", stdin);
  int n, m;
  cin >> n >> m;
  if (n > 30)
    cout << m << endl;
  else
    cout << (m % (1 << n)) << endl;
  return 0;
}
