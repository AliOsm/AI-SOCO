#include <algorithm>
#include <iostream>
#include <cstring>
#include <cstdlib>
#include <cassert>
#include <cstdio>
#include <vector>
#include <cmath>
#include <ctime>
#include <set>
#include <map>


typedef long long ll;

using namespace std;
int s,x,y;
int main()
{

	cin>>s;
	x=s/10;
	y=s%10;
	if(x==1){
		if(!y)
			cout<<"ten";
		if(y==1)
			cout<<"eleven";
		if(y==2)
			cout<<"twelve";
		if(y==3)
			cout<<"thirteen";
		if(y==4)
			cout<<"fourteen";
		if(y==5)
			cout<<"fifteen";
		if(y==6)
			cout<<"sixteen";
		if(y==7)
			cout<<"seventeen";
		if(y==8)
			cout<<"eighteen";
		if(y==9)
			cout<<"nineteen";
	}
	if(x!=1){
		if(x>1){
		if(x==2)
			cout<<"twen";
		if(x==3)
			cout<<"thir";
		if(x==4)
			cout<<"for";
		if(x==5)
			cout<<"fif";
		if(x==6)
			cout<<"six";
		if(x==7)
			cout<<"seven";
		if(x==8){
			cout<<"eigh";
		}
		if(x==9)
			cout<<"nine";
			cout<<"ty";
			if(y)
				cout<<"-";				
		}
		if(!y&&!s)
			cout<<"zero";
		if(y==1)
			cout<<"one";
		if(y==2)
			cout<<"two";
		if(y==3)
			cout<<"three";
		if(y==4)
			cout<<"four";
		if(y==5)
			cout<<"five";
		if(y==6)
			cout<<"six";
		if(y==7)
			cout<<"seven";
		if(y==8)
			cout<<"eight";
		if(y==9)
			cout<<"nine";
	}	
	
	return 0;
}
