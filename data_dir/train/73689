#include<bits/stdc++.h>
using namespace std;
int main()
{

    string s ,abyte="127", ashort="32767", aint="2147483647",along="9223372036854775807";
    cin>>s;
    if(s.size()<=2 )puts("byte");
    else if(s<=abyte && s.size()==3 )puts("byte");
    else if(s.size()<=4)puts("short");
    else if(s<=ashort && s.size()==5)puts("short");
    else if(s.size()<=9)puts("int");
    else if(s<=aint && s.size()==10)puts("int");
    else if(s.size()<=18)puts("long");
    else if(s<=along && s.size()==19)puts("long");
    else puts("BigInteger");
    return 0;
}



