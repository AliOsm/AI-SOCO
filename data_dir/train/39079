#include <bits/stdc++.h>
using namespace std;

const int N = 1e5 + 5;

int main(){



    ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
    int t;
    cin >> t;
    while (t--){
        long long a, b, n, c;
        cin >> a >> b >> n;
        c = (a ^ b);
        n %= 3;
        if (n == 0) cout << a << '\n';
        if (n == 1) cout << b << '\n';
        if (n == 2) cout << c << '\n';
    }


}
