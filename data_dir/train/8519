#include<bits/stdc++.h>
#define int long long
using namespace std;
int32_t main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
    int n;cin>>n;
    string a;cin>>a;
    if(a=="1" || a=="0")
    return 	cout<<a,0;
	cout<<"1";
	for(int i=1;i<n;i++)
	{
		if(a[i]=='0')
		{
			cout<<"0";
		}
	}

}