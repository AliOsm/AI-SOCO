#define pb push_back


const long long MOD = 1e9 + 7;
const long long MAXN = 1e6 + 1;
using namespace std;

typedef long long ll;

long long readInt() {
    bool minus1 = false;
    long long result = 0;
    char ch;
    ch = getchar();
    while (true) {
        if (ch == '-') break;
        if (ch >= '0' && ch <= '9') break;
        ch = getchar();
    }
    if (ch == '-') minus1 = true; else result = ch-'0';
    while (true) {
        ch = getchar();
        if (ch < '0' || ch > '9') break;
        result = result*10 + (ch - '0');
    }
    if (minus1)
        return -result;
    else
        return result;
}


vector<int> z_function (string s) {
	int n = (int) s.length();
	vector<int> z (n);
	for (int i=1, l=0, r=0; i<n; ++i) {
		if (i <= r)
			z[i] = min (r-i+1, z[i-l]);
		while (i+z[i] < n && s[z[i]] == s[i+z[i]])
			++z[i];
		if (i+z[i]-1 > r)
			l = i,  r = i+z[i]-1;
	}
	return z;
}


string rev(string s) {
	reverse(s.begin(), s.end());
	return s;
}

int tmp[MAXN];

int main() {
	#ifdef IZI_KATKA
	assert(freopen("input", "r", stdin));
    assert(freopen("output", "w", stdout));
    #endif
    string S;
    cin >> S;
    int n = readInt();
    int ans = 0;
    for (int i = 1; i <= n; i++) {
    	string T;;
    	cin >> T;
    	if (T.size() == 1) continue;
		vector <int> Z1 = z_function(T + '@' + S);
		vector <int> Z2 = z_function(rev(T) + '@' + rev(S));
		memset(tmp, 0, sizeof(tmp));
		for(int i = T.size() + 1; i < S.size() + T.size() + 1; i++) {
        	int pos = S.size() - i + T.size() - Z2[i] + 1;
        	tmp[pos] = max(tmp[pos], Z2[i]);
      	}
      	for(int i = S.size() - 1; i >= 0; i--)
			tmp[i] = max(tmp[i], tmp[i + 1]);
      	bool ok = 0;
      	for(int i = T.size() + 1; i < S.size() + T.size() + 1; i++) {
        	int pos = i - T.size() - 1 + Z1[i];
        	ok |= (tmp[pos] + Z1[i] >= T.size());
      	}
      	ans += ok;		
    }
    cout << ans;
    return 0;
}

