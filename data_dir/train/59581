const int siz = 2e5 + 5, siz2 = 1049659, siz3 = 5032108;
ll l, r;
int main()
{
	scanf("%I64d%I64d", &l, &r);
	printf("YES\n");
	ll cur = l, lim = (r-l+1)/2;
	for (int i = 0; i < lim; i++) {
		printf("%I64d %I64d\n", cur, cur+1);
		cur += 2;
	}
	return 0;
}