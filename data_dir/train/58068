#include<stdlib.h>
#include<stdio.h>
#include<math.h>
#include<cstring>
#include<iostream>
#include<algorithm>
using namespace std;
int n,a,x,cnt;
long long res=1;
int main()
{
	scanf("%d",&n);
	for(int i=1;i<=n;i++)
	  {
	  scanf("%d",&a);
	  if(x)if(!a)cnt++;
	  else res*=cnt+1,cnt=0;x|=a;
	  }
	return printf("%I64d\n",x?res:0),0;
}
