#include<bits/stdc++.h>
#define ll long long
#define ld long double
#define pb push_back
#define x first
#define y second
#define fastread ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
#define PI (atan(1)*4)
#define mp make_pair
using namespace std;

int main()
{
    fastread;
    ll x, y, z, t1, t2, t3;
    cin >> x >> y >> z >> t1 >> t2 >> t3;
    ll stairs = abs(x - y) * t1;
    ll ele = (abs(z - x) + abs(x - y)) * t2 + 3 * t3;
    if (ele <= stairs)
        cout << "YES";
    else
        cout << "NO";
    return 0;
}