#include <bits/stdc++.h>

using namespace std  ;

int main(){
    ios_base :: sync_with_stdio(false) ;
    cin.tie(nullptr) ;
    string s ;
    int k ;cin >> k;
    for(int i = 1 ;s.size() < 1e4; i++) {
        s += to_string(i);
        if(s.size() >= 1e4) cout << s[k - 1] ; 
    }
    return 0 ;
}