#include<bits/stdc++.h>
using namespace std;

#define IO ios_base::sync_with_stdio(false),cin.tie(0);
#define ll long long

int main()
{
    IO
    double a, b, x, y;
    cin>>x>>y;
    if(x == y)
    {
        cout<<"=\n";
        return 0;
    }
    a = log(x)/log(y);
    b = x/y;
    if(a==b)
        cout<<"=\n";
    else if(a > b)
        cout<<">\n";
    else if(a < b)
        cout<<"<\n";

    return 0;
}
