#include <bits/stdc++.h>

using namespace std;

#define MOD 1000000007
#define ll long long int
#define ld long double
#define pb push_back
#define mkp make_pair
#define pii pair<int, int> 
#define pll pair<long long int, long long int>
#define sci(x) scanf("%d", &x)
#define scl(x) scanf("%lld", &x)
#define fi first
#define sc second
#define deb 0

int main()
{
	ios_base::sync_with_stdio(false);cout.tie(0);cin.tie(0);

	char s[2000];
	int i, j, k;
	gets(s);
	k = 0;
	for (i = 0; s[i]; ++i) {
		if (s[i] == 'a' || s[i] == 'e' || s[i] == 'i' || s[i] == 'o' || s[i] == 'u') k++;
	}
	if (k != 5) {
		cout << "NO" << endl;
		return 0;
	}
	gets(s);
	k = 0;
	for (i = 0; s[i]; ++i) {
		if (s[i] == 'a' || s[i] == 'e' || s[i] == 'i' || s[i] == 'o' || s[i] == 'u') k++;
	}
	if (k != 7) {
		cout << "NO" << endl;
		return 0;
	}
	gets(s);
	k = 0;
	for (i = 0; s[i]; ++i) {
		if (s[i] == 'a' || s[i] == 'e' || s[i] == 'i' || s[i] == 'o' || s[i] == 'u') k++;
	}
	if (k != 5) {
		cout << "NO" << endl;
		return 0;
	}
	cout << "YES" << endl;

	return 0;
}
