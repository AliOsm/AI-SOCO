#include <iostream>
#include <set>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int i,y;
    int a[3002]={0};
    //cout<<a[1000]<<endl;
    for(i=0;i<n;i++)
    {
        cin>>y;
        a[y]=1;
    }
    for(i=1;i<=3002;i++)
    {
       if(a[i]==0)
       {
         y=i;
         break;
       }
    }
    cout<<y;
    return 0;

}
