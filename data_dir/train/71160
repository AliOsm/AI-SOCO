#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
#include <cstdio>
#include <map>
#include <queue>
#include <cstring>
#include <cstdlib>
#include <set>
using namespace std;
#define INF 2001001001
#define MOD 1000000007
typedef long long ll;

int main()
{
  //ios_base::sync_with_stdio(0);cin.tie(0);
  ll N,X,Y;
  cin>>N>>X>>Y;
  ll white,black;
  white=(X-1)+(Y-1);
  black=(N-X)+(N-Y);
  if (white<=black)
    cout<<"White"<<endl;
  else
    cout<<"Black"<<endl;
  return 0;
}
