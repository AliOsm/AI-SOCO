#include <iostream>
using namespace std;

int main() {
	string s;
	cin >> s;
	char c = s[0];
	cout << "Mike" << endl;
	for(int i = 1; s[i]; ++i) {
		if(s[i] > c) {
			cout << "Ann" << endl;
		} else if(s[i] == c) {
			cout << "Mike" << endl;
		} else {
			cout << "Mike" << endl;
			c = s[i];
		}
	}
	return 0;
}