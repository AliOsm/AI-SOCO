#include <iostream>
#include <vector>
#include <cassert>
#include <map>
#include <cmath>
#include <set>
#include <algorithm>
using namespace std;
typedef long long Long;
#define MAXX 1000000LL
#define MAXY MAXX

int main() {

	int M = 0,C = 0;
	int N;
	cin >> N;
	for (int i = 0; i < N; ++i) {
		int m,c;
		cin >> m >> c;
		if(m > c){
			M++;
		}
		if(c > m){
			C++;
		}
	}
	if(M > C){
		cout << "Mishka" << endl;
	}else if(C > M){
		cout << "Chris" << endl;
	}else{
		cout << "Friendship is magic!^^" << endl;
	}
	
}


