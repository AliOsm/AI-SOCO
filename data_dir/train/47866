#include <iostream>

using namespace std;

int main (){
    int n, m;
    cin >>n>>m;
    for (int i=0;i<n;i++){
        for (int j=0;j<m;j++){
             char ch;
             cin >>ch;
             if ( ch == '-' ) cout <<'-';
             else {
                 if ( (i + j)&1 ) cout <<"B";
                 else cout <<"W";
             }
        }
        cout <<endl;
    }
    
    
}