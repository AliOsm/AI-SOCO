#include <bits/stdc++.h>
#define ll long long
#define ull unsigned ll
#define ld long double 
#define mp make_pair
#define pb push_back
#define F first
#define S second
#define y1 dgsdfgsfg

using namespace std;

const ll MIN = 1e3 + 2;
const ll MXN = 1e6 + 3;
const ll INF = 1e9 + 7;
const ll LINF = 1e18 + 15;
const ld EPS = 1e-9;

ll a[MXN];

int main(){
	cin >> a[1] >> a[2];
	sort (a + 1, a + 3);
	for (ll i = 1; i <= 3; i++){
		if (a[i] != i)
			{cout << i;
			return 0;
			}
	}
	return 0;
}