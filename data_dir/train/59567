#include <cstdio>
#include <cstring>
#include <cstdlib>
#include <algorithm>

using namespace std;

int main()
{
	long long l, r;
	int t;
	scanf("%d", &t);
	
	long long x, y;
	for (int ii = 0 ; ii < t ; ii++)
	{
		scanf("%I64d%I64d", &l, &r);
		
		x = l;
		y = x * 2;
		printf("%I64d %I64d\n", x, y);
	}
	
	return 0;
}

