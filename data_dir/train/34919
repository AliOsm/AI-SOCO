/*************************************************/
ll n,m,l,r,k,i,j,ans,lys;
struct mat
{
	ll a[2][2];
}a;
mat mul(mat x,mat y)
{
	ll i,j,k;
	mat z;
	memset(z.a,0,sizeof(z.a));
	forup(i,0,1) forup(j,0,1) forup(k,0,1) (z.a[i][j]+=x.a[i][k]*y.a[k][j])%=lys;
	return z;
}
mat qp(mat a,ll y)
{
	mat z;
	memset(z.a,0,sizeof(z.a));
	z.a[0][0]=z.a[1][1]=1;
	while (y){
		if (y&1) z=mul(z,a);
		y/=2;a=mul(a,a);
	}
	return z;
}
int main()
{
	cin>>lys>>l>>r>>k;
	for (i=1;i<=r;i=j+1){
		j=r/(r/i);
		if (i<l) j=min(j,(l-1)/((l-1)/i));
		if ((r/i-(l-1)/i)>=k) ans=j;
	}
	if (ans==0){
		if (m==1){
			puts("0");
		}else{
			puts("1");
		}
		return 0;
	}
	a.a[0][1]=a.a[1][0]=a.a[1][1]=1;
	a=qp(a,ans);
	cout<<a.a[1][0]<<endl;
	return 0;
}