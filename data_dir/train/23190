#include "bits/stdc++.h"
using namespace std;

typedef long long int ll;
const int N = 1e6+5;
int vis[1000];
signed main()
{
   ios_base::sync_with_stdio(0);
   cin.tie(0);

   int n;cin>>n;
   string s;cin>>s;
   for(int i=0;i<n;i++)
    vis[s[i]-'A']++;
   cout<<2*(min(vis['U'-'A'],vis['D'-'A'])+min(vis['L'-'A'],vis['R'-'A']))<< "\n";







}
