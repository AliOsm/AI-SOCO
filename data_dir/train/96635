#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
#include <string>
#include <set>
using namespace std;
int main()
{
   long long a,b,x,y;
   cin>>a>>b>>x>>y;
   long long m=x,n=y,c;
   while(n)
   {
   	c=m%n;
   	m=n;
   	n=c;
   }
   x=x/m;
   y=y/m;
 
   cout<<min(a/x,b/y);
}
