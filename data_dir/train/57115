#include <bits/stdc++.h>

using namespace std; 	
typedef long long ll;
typedef unsigned long long ull;
const int N = 1e5+50;
const int oo = 1e9;
const int mod = 1e9+7;

int main(){
	double a,b,c;
	cin>>a>>b>>c;
	double ans1 = (-b + sqrt(b*b - 4.0*a*c))/(2.0*a);
	double ans2 = (-b - sqrt(b*b - 4.0*a*c))/(2.0*a);
	printf("%6.12lf\n", max(ans1,ans2));
	printf("%6.12lf\n", min(ans1,ans2));
}