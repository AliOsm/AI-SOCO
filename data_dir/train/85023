#include<bits/stdc++.h>
using namespace std;
long long a,s,d,f,g,h,j,k,l,i,n,m;
string x;
main(){
    cin>>n;
    for(i=0;i<=8100;i++){
        for(a=0;a<=810;a++){
            if(n>=i*123456+a*1234567 && (n-i*123456-a*1234567)%1234==0) {cout<<"YES";return 0;}
        }
    }
    cout<<"NO";
}