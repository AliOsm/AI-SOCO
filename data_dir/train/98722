#include <bits/stdc++.h>
using namespace std;

const int N = 1e5 + 5;

int q, n, m;

int main(){



    ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
    cin >> q;
    while (q--){
        cin >> n >> m;
        cout << 'W';
        for (int i = 1; i < m; ++i) cout << 'B'; cout << '\n';
        for (int i = 1; i < n; ++i){
            for (int j = 1; j <= m; ++j) cout << 'B';
            cout << '\n';
        }
    }


}
