//++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
typedef long long ll;
typedef long double ld;
typedef unsigned long long ull;
//++--++--++--++--++--++--++--++--++--+--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++--++
const int MAXN = 200001;
const int inf = 1000000001;
const ld INF = 1.0 / 0.0;
const ld EPS = 1e-7;
const int mod = inf + 6;
const ld PI = acos(-1.0); 
#define int long long
int am(int n, int k)
{
	int res = 0;
	while (n)
	{
		n /= k;
		res += n;
	}
	return res;
}
signed main()
{
	fastio();
	int n, k;
	cin >> n >> k;
	map<int, int> cnt;
	int ans = 2e18;
	for (ll i = 2; i *i <= k; ++i)
	{
		if (k % i == 0)
		{
			cnt[i] ++;
			k /= i;
			i = 1;
		}
	}
	if (k > 1)
	{
		cnt[k] ++;
	}
	for (auto i : cnt)
	{
		ans = min(ans, am(n, i.first) / i.second);
	}
	cout << ans;
	//sp;
}