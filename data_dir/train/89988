#include <bits/stdc++.h>

using namespace std;

map<int,int>mp;

int main(){

    int n, mx=0;

    scanf("%d", &n);

    for(int i=1; i<=n; i++){
        int x;
        scanf("%d", &x);
        mp[x]++;
        int d=mp[x];
        mx=max(mx,d);
    }
    cout<<mx<<endl;


}
