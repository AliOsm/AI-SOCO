using namespace std;
#define mx 100002
ll dp[51],n,a[55],s=0;
ll sum(ll ind)
{
    if(ind>n)
        return 0;
        if(dp[ind]!=-1)
            return dp[ind];
    ll r;
    ll a1=abs(a[ind]-sum(ind+1));
    ll b=a[ind]+sum(ind+1);
    r=min(a1,b);
    dp[ind]=r;
    return r;
}
int main()
{
    ll i,ans,ans1;
    while(cin>>n)
    {
        s=0;
        for(i=1;i<=n;i++)
        {
            cin>>a[i];
            s+=a[i];
        }
            memset(dp,-1,sizeof(dp));
            ll ans=0+sum(1);
            //cout<<ans<<endl;
            s-=ans;
            cout<<s/2<<" "<<(s/2)+ans<<endl;
    }
    return 0;

}
