#ifdef _MSC_VER
#define _CRT_SECURE_NO_WARNINGS
#endif

#include <bits/stdc++.h>

using namespace std;

typedef unsigned long long ull;
typedef long long ll;
//int dx8[] = { 0, 0, 1, -1, 1, -1, 1, -1 };
//int dy8[] = { -1, 1, 0, 0, -1, -1, 1, 1 };
//int dy[] = { 1, -1, 0, 0 };
//int dx[] = { 0, 0, 1, -1 };
//int X[6] = { 0, 0, 0, -1, 0, 1 };
//int Y[6] = { 0, 0, -1, 0, 1, 0 };
//int Z[6] = { 1, -1, 0, 0, 0, 0 };
// start + (end - start) / 2;

#define OO ll(1e18)
char game[110][110];
int main() {
	int n, m, k;
	cin >> n >> m >> k;
	vector<int>v;
	int w2 = 0, w1 = 0, f = 0, s = 0;
	for (int i = 0; i < n; i++) {
		int r = 0, g = 0, d = 0;
		for (int j = 0; j < m; j++) {
			cin >> game[i][j];
			if (game[i][j] == 'R')r = j + 1;
			else if (game[i][j] == 'G')g = j + 1;
			else d = 1;
		}
		if (r && g)v.push_back(abs(r - g) - 1);
		else if (!r && g && d)f = 1;
		else if (r && !g && d)s = 1;
	}
	if (v.size())
		for (int idx = 0; idx <= 7; idx++) {
			int x = 0;
			for (int i = 0; i < v.size(); i++) {
				x += (v[i] >> idx);
			}
			if (x % (k + 1))w1 = 1;
		}
	if (f && s)cout << "Draw\n";
	else if (f)cout << "First\n";
	else if (s)cout << "Second\n";
	else if (!w1)cout << "Second\n";
	else cout << "First\n";
}