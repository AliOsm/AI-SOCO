#include<iostream>
#include<cstdio>
#include<vector>
#include<map>
#include<set>
#include<algorithm>
#include<cmath>
#include<sstream>
#include<iomanip>
#include<bitset>

using namespace std;

#define ll long long

int main()
{
//freopen("a.in","r",stdin);
    ll n,m,l,r;
    char c1,c2;
    string s;
    cin>>n>>m>>s;
    while(m--)
    {
        cin>>l>>r>>c1>>c2;
        for(int i=l-1;i<r;i++)
        {
            if(s[i]==c1)
                s[i]=c2;
        }
    }
    cout<<s<<endl;
}






