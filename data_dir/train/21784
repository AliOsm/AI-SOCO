#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
typedef pair<int,int> ii;
typedef vector<int> vi;
typedef vector<ii> vii;

int solve(){

    int n,k,t;
    scanf("%d%d%d",&n,&k,&t);
    printf("%d\n", min({t,k,k-(t-n)}) );
    return 0;
}

int main(int argc, char* argv[])
{
#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt" ,"w", stdout);
#endif // online-judge

    solve();

    return 0;
}