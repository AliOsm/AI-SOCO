// Code Starts Here
V(string) s;
V(ll) adj[26];
ll vis[26];
ll ans[26],p;
bool dfs(ll v)
{	if(vis[v]!=0)
		return (vis[v]==2?true:false);
	vis[v]=1;
	F(j,adj[v].size())
	{	if(!dfs(adj[v][j]))
			return false;
	}
	ans[p++]=v;
	vis[v]=2;
	return true;		
}
int main()
{	vasu2907;
	string st;
	ll n;
	cin>>n;
	F(i,n)
	{	cin>>st;
		s.pb(st);
	}
	for(ll i=1;i<n;i++)
	{	ll j=0;ll l1=s[i-1].length(),l2=s[i].length();
		for(;j<min(l1,l2);j++)
		{	if(s[i-1][j]!=s[i][j])
				break;
		}
		if(j==l2)
		{	cout<<"Impossible\n";
			return 0;
		}
		if(j<l1)
		{	adj[s[i-1][j]-'a'].pb(s[i][j]-'a');
		}
	}
	for(ll i=0;i<26;i++)
	{	if(!dfs(i))
		{	cout<<"Impossible\n";
			return 0;
		}
	}
	for(ll i=25;i>=0;i--)
		cout<<char(ans[i]+'a');
	cout<<endl;
}
