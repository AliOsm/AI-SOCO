#include <bits/stdc++.h>

#define pb push_back
#define all(x) (x).begin(), (x).end()
#define sqr(x) (x)*(x)
#define mp make_pair

using namespace std;

typedef long long ll;
typedef unsigned long long ull;

const int MAXN = 1e5+5;
const int INF = 2e9;
const int MOD = 1e9+7;

int main(){
   int l, r;
   scanf("%d %d", &l, &r);

   if (l == r)
      printf("%d", l);
   else
      printf("2");

   return 0;
}