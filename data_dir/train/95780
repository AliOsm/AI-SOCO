#include<bits/stdc++.h>
#define ll          long long
#define pb          push_back
#define	endl		'\n'
#define pii         pair<ll int,ll int>
#define vi          vector<ll int>
#define all(a)      (a).begin(),(a).end()
#define F           first
#define S           second
#define sz(x)       (ll int)x.size()
#define hell        1000000007
#define rep(i,a,b)	for(ll int i=a;i<b;i++)
#define lbnd        lower_bound
#define ubnd        upper_bound
#define bs          binary_search
#define mp          make_pair
using namespace std;

#define N  100005

int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	int TESTS=1;
//	cin>>TESTS;
	while(TESTS--)
	{
		ll n;
		cin>>n;
		string s;
		cin>>s;
		string s2="AB";
		map<string,ll>mp;
		rep(i,0,n-1)
		{
			s2[0]=s[i];
			s2[1]=s[i+1];
			mp[s2]++;
		}
		ll max=0;
		string ans;
		for(map<string,ll> :: iterator it=mp.begin();it!=mp.end();it++)
		{
			if(it->S>max)
			{
				max=it->S;
				ans=it->F;
			}
		}
		cout<<ans;
	}
	return 0;
}