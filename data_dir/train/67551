#include <bits/stdc++.h>
#pragma GCC optimization ("unroll-loops")
#pragma GCC optimization ("O2")
#pragma GCC target("popcnt")
#pragma GCC target ("avx2")
using namespace std;

int main(){
	set<int> st;
	for(int i = 0 ; i < 4 ; i += 1){
		int a;
		cin >> a;
		st.insert(a);
	}
	cout << 4-st.size() << endl;
}