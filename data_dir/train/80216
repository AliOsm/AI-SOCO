#include <bits/stdc++.h>

using namespace std;

string s;
                   
int main(){      
	cin >> s;
	int n = s.size();
	int cnt = 0;
	for(int i = 0; i < n; i++){
		if(s[i] == 'Q'){
			for(int j = i + 1; j < n; j++){
				if(s[j] == 'A'){
					for(int k = j + 1; k < n; k++){
						if(s[k] == 'Q') cnt++;
					}
				}
			}
		}
	}
	cout << cnt;
}
