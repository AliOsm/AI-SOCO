#include <bits/stdc++.h>
using namespace std;
 
int main(){
    int t;
    cin >> t;
    while(t--){
        int a,b,c;
        cin >> a >> b >> c;
        while(b){
            if(a/2+10 < a){
                a = a/2+10;
                b -= 1;
            }else{
                break;
            }
        }
        if(a <= 10*c){
            cout << "YES" << endl;
        }else{
            cout << "NO" << endl;
        }
    }
}