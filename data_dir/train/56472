#include <iostream>

int main() {
    std::ios_base::sync_with_stdio(false);
    std::cin.tie(nullptr);
    std::cout.tie(nullptr);

    int al, ar, bl, br;
    std::cin >> al >> ar >> bl >> br;

    if (ar - 1 <= bl && bl <= 2 * ar + 2 ||
        al - 1 <= br && br <= 2 * al + 2) {
        std::cout<< "YES";
    } else {
        std::cout << "NO";
    }

    return 0;
}
