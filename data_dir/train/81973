#include<iostream>
using namespace std;
int n,one,zero;
string s;
int main()
{
        cin>>n>>s; 
        for(int i=0; i<n; i++)
        {
                if(s[i]-'0') one++;
                else zero++;
        }
        if(s[0]-'0')
        cout<<'1';
        for(int i=1; i<=zero; i++) cout<<'0';
}